<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smarter_foodies">
    <!-- xmlns:tools="http://schemas.android.com/tools"> -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:icon="@mipmap/ic_launcher_sf_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_sf_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.Smarter_Foodies">
        <activity
            android:name=".likedRecipes"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".DashboardActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".welcomeActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ApplyChef" />
        <activity android:name=".SearchRecipe" />
        <activity android:name=".profile_page" />
        <activity android:name=".UpdateProfile" />
        <activity android:name=".RecipePage" />
        <activity
            android:name=".UpdateRecipe"
            android:theme="@style/Theme.Smarter_Foodies.BackgroundUpdateImageStyle" />
        <activity
            android:name=".AddRecipe"
            android:theme="@style/Theme.Smarter_Foodies.BackgroundImageStyle" />
        <activity android:name=".RecipeImageActivity" />
        <activity
            android:name=".LoginGoogle"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>