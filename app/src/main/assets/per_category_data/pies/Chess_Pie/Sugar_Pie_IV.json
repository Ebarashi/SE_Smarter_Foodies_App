{
    "title": "Sugar Pie IV",
    "Category": "Chess Pie",
    "Ingredients": [
        {
            "1": "2 cups brown sugar"
        },
        {
            "2": "0.25 cups all-purpose flour"
        },
        {
            "3": "0.5 cup heavy cream"
        },
        {
            "4": "1  egg"
        },
        {
            "5": "1 tablespoon corn syrup"
        },
        {
            "6": "1 teaspoon vanilla extract"
        },
        {
            "7": "0.75 cup chopped pecans"
        },
        {
            "8": "1 (9 inch) unbaked pie crust"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C). In a medium bowl, mix together brown sugar and flour; set aside."
        },
        {
            "2": "In a large bowl, whisk together the cream, egg, corn syrup and vanilla. Stir in brown sugar mixture until blended. Fold in the pecans."
        },
        {
            "3": "Pour filling into pie crust. Bake in preheated oven for 30 to 40 minutes, or until golden brown."
        }
    ],
