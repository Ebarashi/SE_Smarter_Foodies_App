{
    "title": "Mom's Spiced Pumpkin Pie",
    "Category": "Pumpkin Pie",
    "Ingredients": [
        {
            "1": "2  eggs, lightly beaten"
        },
        {
            "2": "1 (12 fluid ounce) can evaporated milk"
        },
        {
            "3": "1 (15 ounce) can pumpkin puree"
        },
        {
            "4": "0.75 cup white sugar"
        },
        {
            "5": "2 tablespoons all-purpose flour"
        },
        {
            "6": "1 teaspoon salt"
        },
        {
            "7": "1 teaspoon ground ginger"
        },
        {
            "8": "2 teaspoons ground cinnamon"
        },
        {
            "9": "1 teaspoon ground allspice"
        },
        {
            "10": "0.5 teaspoons ground cloves"
        },
        {
            "11": "1 (9 inch) unbaked pie crust"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 450 degrees F (230 degrees C)."
        },
        {
            "2": "In a bowl, mix the eggs, evaporated milk and pumpkin. In a separate bowl, mix the sugar, flour, salt, ginger, cinnamon, allspice and cloves. Stir into the pumpkin mixture. Pour into the pie crust."
        },
        {
            "3": "Bake in the preheated oven 10 minutes. Reduce heat to 350 degrees F (175 degrees C) and continue baking 45 minutes, until a knife inserted in the center of the pie comes out clean. Cool on a metal rack."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Cook Time": "55 mins"
        },
