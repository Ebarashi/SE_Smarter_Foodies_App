{
    "title": "Mom's Pumpkin Pie",
    "Category": "Pumpkin Pie",
    "Ingredients": [
        {
            "1": "1 recipe pastry for a 9 inch single crust pie"
        },
        {
            "2": "3  eggs"
        },
        {
            "3": "1  egg yolk"
        },
        {
            "4": "0.5 cup white sugar"
        },
        {
            "5": "0.5 cup packed brown sugar"
        },
        {
            "6": "1 teaspoon salt"
        },
        {
            "7": "0.5 teaspoon ground cinnamon"
        },
        {
            "8": "0.5 teaspoon ground nutmeg"
        },
        {
            "9": "0.5 teaspoon ground ginger"
        },
        {
            "10": "0.25 teaspoon ground cloves"
        },
        {
            "11": "0.5 cups milk"
        },
        {
            "12": "0.5 cup heavy whipping cream"
        },
        {
            "13": "2 cups pumpkin puree"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 425 degrees F (220 degrees C.)"
        },
        {
            "2": "In a large bowl, combine eggs, egg yolk, white sugar and brown sugar. Add salt, cinnamon, nutmeg, ginger and cloves. Gradually stir in milk and cream. Stir in pumpkin. Pour filling into pie shell."
        },
        {
            "3": "Bake for ten minutes in preheated oven. Reduce heat to 350 degrees F (175 degrees C), and bake for an additional 40 to 45 minutes, or until filling is set."
        }
    ],
    "Details": [
        {
            "Prep Time": "30 mins"
        },
        {
            "Cook Time": " 1 hrs"
        },
        {
            "Total Time": " 1 hrs 30 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "1 9-inch pie"
        }
