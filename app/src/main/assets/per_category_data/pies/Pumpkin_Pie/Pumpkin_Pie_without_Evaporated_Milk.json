{
    "title": "Pumpkin Pie without Evaporated Milk",
    "Category": "Pumpkin Pie",
    "Ingredients": [
        {
            "1": "2 cups pumpkin puree"
        },
        {
            "2": "2 cups whole milk"
        },
        {
            "3": "2 cups white sugar, or more to taste"
        },
        {
            "4": "3 large eggs"
        },
        {
            "5": "2 tablespoons all-purpose flour"
        },
        {
            "6": "2 teaspoons ground cinnamon, or more to taste"
        },
        {
            "7": "0.5 teaspoon ground nutmeg, or more to taste"
        },
        {
            "8": "0.25 teaspoon ground cloves, or more to taste"
        },
        {
            "9": "0.5 teaspoon salt"
        },
        {
            "10": "2 (9 inch)  unbaked pie shells"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 475 degrees F (245 degrees C)."
        },
        {
            "2": "Combine pumpkin puree, milk, sugar, eggs, flour, cinnamon, nutmeg, cloves, and salt in a large mixing bowl; mix with an electric mixer until smooth. Pour filling into the pie shells."
        },
        {
            "3": "Bake in the preheated oven for 10 minutes. Reduce heat to 350 degrees F (175 degrees C) and continue to bake until filling is set, 35 to 45 more minutes."
        },
        {
            "4": "Remove from the oven and set on a wire rack to cool. Serve warm or at room temperature."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "45 mins"
        },
        {
            "Total Time": " 1 hrs"
        },
        {
