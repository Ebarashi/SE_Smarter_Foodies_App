{
    "title": "No-Bake Pumpkin Cheesecake Pie",
    "Category": "Pumpkin Pie",
    "Ingredients": [
        {
            "1": "10 ounces pumpkin pie filling"
        },
        {
            "2": "6.0 ounces cream cheese (at room temperature)"
        },
        {
            "3": "2 cups frozen whipped topping, thawed and divided"
        },
        {
            "4": "1 cup confectioners' sugar"
        },
        {
            "5": "1 (9 inch) prepared shortbread pie crust"
        }
    ],
    "Directions": [
        {
            "1": "Stir pumpkin pie filling, cream cheese, 1 cup whipped topping, and confectioners' sugar together in a large bowl until smooth; spread into the prepared pie crust. Top pie with remaining whipped topping."
        },
        {
            "2": "Refrigerate pie until the filling is set, 4 to 6 hours."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Additional Time": " 4 hrs"
        },
        {
            "Total Time": " 4 hrs 10 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "1 9-inch pie"
        }
    ],
