{
    "title": "Millionaire Pie",
    "Category": "Pie Recipes",
    "Ingredients": [
        {
            "1": "1 (14 ounce) can sweetened condensed milk"
        },
        {
            "2": "1 (8 ounce) package cream cheese, softened"
        },
        {
            "3": "1 (12 ounce) container frozen whipped topping, thawed"
        },
        {
            "4": "1 (20 ounce) can crushed pineapple, drained"
        },
        {
            "5": "0.5 cups chopped pecans"
        },
        {
            "6": "2 (9 inch)  prepared graham cracker crusts"
        }
    ],
    "Directions": [
        {
            "1": "Blend condensed milk and cream cheese together in a large bowl; gently fold in the Cool Whip whipped topping. Stir in crushed pineapple and pecans."
        },
        {
            "2": "Pour filling into prepared pie crusts; refrigerate for 3 to 4 hours, until set."
        }
    ],
    "Details": [
