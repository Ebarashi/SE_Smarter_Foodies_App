{
    "title": "Chocolate Cream Pie",
    "Category": "Pie Recipes",
    "Ingredients": [
        {
            "1": "0.5 cups white sugar"
        },
        {
            "2": "3  egg yolks, beaten"
        },
        {
            "3": "0.5 cup unsweetened cocoa powder"
        },
        {
            "4": "3 tablespoons cornstarch"
        },
        {
            "5": "0.5 teaspoon salt"
        },
        {
            "6": "3 cups milk"
        },
        {
            "7": "1 tablespoon butter"
        },
        {
            "8": "0.5 teaspoons vanilla extract"
        },
        {
            "9": "1 (9 inch) pie crust, baked"
        },
        {
            "10": "1 cup frozen whipped topping, thawed"
        }
    ],
    "Directions": [
        {
            "1": "Beat sugar and eggs together in a large bowl until creamy; mix in cocoa powder, cornstarch, and salt until well blended. Gently stir in milk."
        },
        {
            "2": "Pour mixture into a large saucepan. Cook over medium heat, stirring constantly, until boiling. Remove from heat; stir in butter and vanilla extract. Set aside to cool slightly."
        },
        {
            "3": "Pour mixture into pastry shell; chill pie in the refrigerator until set, 2 to 4 hours. Garnish with whipped topping."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "5 mins"
        },
        {
            "Additional Time": " 2 hrs"
        },
        {
            "Total Time": " 2 hrs 20 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "1 9-inch pie"
        }
