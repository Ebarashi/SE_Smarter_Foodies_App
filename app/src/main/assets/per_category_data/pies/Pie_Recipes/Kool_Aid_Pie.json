{
    "title": "Kool-Aid Pie",
    "Category": "Pie Recipes",
    "Ingredients": [
        {
            "1": "1 (14 ounce) can sweetened condensed milk"
        },
        {
            "2": "1 (0.13 ounce) package unsweetened fruit-flavored drink mix (such as Kool-Aid\u00ae)"
        },
        {
            "3": "1 (8 ounce) container frozen whipped topping, thawed"
        },
        {
            "4": "1 (9 inch) prepared graham cracker crust"
        }
    ],
    "Directions": [
        {
            "1": "Whisk sweetened condensed milk and fruit-flavored drink mix together in a bowl until thoroughly combined and thick. Gently fold in whipped topping until light and fluffy."
        },
        {
            "2": "Pour filling into pie crust and refrigerate at least 1 hour before serving."
        }
    ],
