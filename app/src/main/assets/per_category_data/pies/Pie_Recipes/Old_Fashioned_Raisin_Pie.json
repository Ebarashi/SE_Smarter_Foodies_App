{
    "title": "Old Fashioned Raisin Pie",
    "Category": "Pie Recipes",
    "Ingredients": [
        {
            "1": "2 cups raisins"
        },
        {
            "2": "2 cups water"
        },
        {
            "3": "0.5 cup packed brown sugar"
        },
        {
            "4": "2 tablespoons cornstarch"
        },
        {
            "5": "0.5 teaspoon ground cinnamon"
        },
        {
            "6": "0.25 teaspoon salt"
        },
        {
            "7": "1 tablespoon distilled white vinegar"
        },
        {
            "8": "1 tablespoon butter"
        },
        {
            "9": "1 (14.1 ounce) package double-crust pie pastry, thawed"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 425 degrees F (220 degrees C)."
        },
        {
            "2": "Combine raisins and water in a small saucepan. Boil 5 minutes."
        },
        {
            "3": "Mix brown sugar, cornstarch, cinnamon, and salt together; add to hot raisins. Cook and stir until syrup is clear. Remove from heat, and stir in vinegar and butter. Cool slightly."
        },
        {
            "4": "Turn filling into a pastry-lined pie dish. Cover with top crust. Seal edges and cut slits in the top crust."
        },
        {
            "5": "Bake in the preheated oven for 30 to 35 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Cook Time": "40 mins"
        },
        {
            "Total Time": "45 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "1 9-inch pie"
        }
    ],
