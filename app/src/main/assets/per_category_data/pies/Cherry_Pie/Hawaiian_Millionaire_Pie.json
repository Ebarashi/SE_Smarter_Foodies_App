{
    "title": "Hawaiian Millionaire Pie",
    "Category": "Cherry Pie",
    "Ingredients": [
        {
            "1": "1 (9 inch) pie crust, baked"
        },
        {
            "2": "1 (15 ounce) can crushed pineapple, drained"
        },
        {
            "3": "0.25 cup lemon juice"
        },
        {
            "4": "1 (12 ounce) container frozen whipped topping, thawed"
        },
        {
            "5": "0.5 cup maraschino cherries, chopped"
        },
        {
            "6": "1 (14 ounce) can sweetened condensed milk"
        },
        {
            "7": "1 cup chopped walnuts"
        }
    ],
    "Directions": [
        {
            "1": "In a large mixing bowl, fold pineapple, lemon juice, whipped topping, cherries, condensed milk, and walnuts together. Pour into pie shell and chill for at least one hour before serving."
        }
    ],
