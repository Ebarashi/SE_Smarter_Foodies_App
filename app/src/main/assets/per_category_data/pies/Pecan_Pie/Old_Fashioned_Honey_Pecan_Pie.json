{
    "title": "Old-Fashioned Honey Pecan Pie",
    "Category": "Pecan Pie",
    "Ingredients": [
        {
            "1": "1 cup honey"
        },
        {
            "2": "3 large eggs, beaten"
        },
        {
            "3": "1 cup chopped pecans"
        },
        {
            "4": "3 tablespoons butter"
        },
        {
            "5": "1 teaspoon vanilla extract"
        },
        {
            "6": "1 pinch ground nutmeg"
        },
        {
            "7": "1 (9 inch) refrigerated pie pastry"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 325 degrees F (165 degrees C). Press pastry into a 9-inch pie pan."
        },
        {
            "2": "Bring honey to a boil in a saucepan. Remove from the heat and quickly whisk in eggs. Stir in pecans, butter, vanilla, and nutmeg. Pour into the prepared pie pan."
        },
        {
            "3": "Bake in the preheated oven until set, about 25 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "30 mins"
        },
        {
            "Total Time": "40 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "1 9-inch pie"
        }
    ],
