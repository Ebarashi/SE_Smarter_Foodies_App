{
    "title": "German Chocolate Pecan Pie",
    "Category": "Pecan Pie",
    "Ingredients": [
        {
            "1": "1 cup light corn syrup"
        },
        {
            "2": "3  eggs"
        },
        {
            "3": "1 cup white sugar"
        },
        {
            "4": "2 tablespoons butter, melted"
        },
        {
            "5": "1 teaspoon vanilla extract"
        },
        {
            "6": "2 cups coarsely chopped pecans"
        },
        {
            "7": "0.75 cup sweetened flaked coconut"
        },
        {
            "8": "0.75 cup semisweet chocolate chips"
        },
        {
            "9": "1 (9 inch) deep dish prepared pie crust"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Whisk the corn syrup, eggs, sugar, butter, and vanilla together in a mixing bowl until light-colored and well blended. Stir in the pecans and coconut. Sprinkle the chocolate chips over the bottom of the prepared pie crust. Pour the corn syrup mixture over the chocolate chips."
        },
        {
            "3": "Bake in preheated oven until top is set, 55 to 60 minutes. Cool completely on a rack before serving, about 2 hours."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "55 mins"
        },
        {
            "Additional Time": " 2 hrs"
        },
        {
            "Total Time": " 3 hrs 10 mins"
        },
        {
            "Servings": "8 "
