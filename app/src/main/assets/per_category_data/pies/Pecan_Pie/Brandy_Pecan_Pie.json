{
    "title": "Brandy Pecan Pie",
    "Category": "Pecan Pie",
    "Ingredients": [
        {
            "1": "2  eggs, beaten"
        },
        {
            "2": "0.5 cup half-and-half cream"
        },
        {
            "3": "0.75 cup white sugar"
        },
        {
            "4": "2 teaspoons all-purpose flour"
        },
        {
            "5": "0.5 teaspoon salt"
        },
        {
            "6": "1 tablespoon molasses"
        },
        {
            "7": "1 cup light corn syrup"
        },
        {
            "8": "1 teaspoon vanilla extract"
        },
        {
            "9": "0.5 tablespoons brandy"
        },
        {
            "10": "1 cup coarsely chopped pecans"
        },
        {
            "11": "1 (9 inch) unbaked pie crust"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "In a large bowl, beat eggs together with half-and-half. Mix in sugar, flour, salt, molasses, corn syrup, vanilla, and brandy. Stir in pecans. Pour into pie crust."
        },
        {
            "3": "Bake in preheated oven for 40 to 50 minutes, or until done."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Cook Time": "50 mins"
        },
        {
            "Total Time": " 1 hrs 10 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "1 to 9 - inch pie"
        }
