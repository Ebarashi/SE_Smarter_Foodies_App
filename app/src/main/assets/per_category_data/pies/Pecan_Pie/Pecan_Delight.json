{
    "title": "Pecan Delight",
    "Category": "Pecan Pie",
    "Ingredients": [
        {
            "1": "3  egg whites"
        },
        {
            "2": "1 cup white sugar"
        },
        {
            "3": "0.5 teaspoon vanilla extract"
        },
        {
            "4": "0.5 teaspoon baking powder"
        },
        {
            "5": "20  buttery round crackers, crushed fine"
        },
        {
            "6": "1 cup chopped pecans"
        },
        {
            "7": "1 (12 ounce) container frozen whipped topping, thawed"
        }
    ],
    "Directions": [
        {
            "1": "Beat egg whites until slightly stiff. Add sugar, vanilla, baking powder, Ritz crackers and pecans. Mix well, and pour into greased 9 inch pie plate."
        },
        {
            "2": "Bake at 350 degrees F (175 degrees C) for 25 minutes exactly."
        },
        {
            "3": "Allow pie to cool to room temperature, and then cover with nondairy whipped topping. Refrigerate for 2 hours before serving."
        }
    ],
