{
    "title": "Peanut Butter Chip Pecan Pie",
    "Category": "Pecan Pie",
    "Ingredients": [
        {
            "1": "0.5 cup all-purpose flour"
        },
        {
            "2": "1 cup white sugar"
        },
        {
            "3": "0.5 cup melted butter"
        },
        {
            "4": "2  eggs"
        },
        {
            "5": "1 teaspoon vanilla extract"
        },
        {
            "6": "1 cup peanut butter chips"
        },
        {
            "7": "1 cup chopped pecans"
        },
        {
            "8": "1 (9 inch) unbaked pie crust"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 325 degrees F (165 degrees C.)"
        },
        {
            "2": "In a large bowl, combine flour and sugar. Blend in melted butter, eggs and vanilla. Stir in peanut butter chips and chopped pecans. Pour filling into pie shell."
        },
        {
            "3": "Bake in the preheated oven for 30 to 40 minutes, or until golden brown."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
