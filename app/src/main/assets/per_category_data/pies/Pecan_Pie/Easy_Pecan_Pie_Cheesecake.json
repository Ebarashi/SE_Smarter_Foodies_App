{
    "title": "Easy Pecan Pie Cheesecake",
    "Category": "Pecan Pie",
    "Ingredients": [
        {
            "1": "1 Pie-"
        },
        {
            "2": "1 (8 ounce) package cream cheese, softened"
        },
        {
            "3": "0.5 cup white sugar"
        },
        {
            "4": "1 large egg, beaten"
        },
        {
            "5": "1 teaspoon vanilla extract"
        },
        {
            "6": "0.5 teaspoon salt"
        },
        {
            "7": "1 (9 inch) unbaked pie crust"
        },
        {
            "8": "0.25 cups chopped pecans"
        },
        {
            "9": "1 Topping-"
        },
        {
            "10": "1 cup light corn syrup"
        },
        {
            "11": "3 large eggs"
        },
        {
            "12": "0.25 cup white sugar"
        },
        {
            "13": "1 teaspoon vanilla extract"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 375 degrees F (190 degrees C)."
        },
        {
            "2": "Beat cream cheese, sugar, egg, vanilla extract, and salt for pie together in a bowl until creamy. Spread evenly over the bottom of the unbaked pie crust, then sprinkle pecans evenly over top."
        },
        {
            "3": "Combine corn syrup, eggs, sugar, and vanilla for topping together in a bowl. Beat until smooth. Pour over pecan layer."
        },
        {
            "4": "Bake in the preheated oven until pecan layer is golden brown, 35 to 45 minutes. Let cool on a wire rack. Serve slightly warm."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "35 mins"
        },
        {
            "Total Time": "50 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "1 9-inch pie"
        }
    ],
