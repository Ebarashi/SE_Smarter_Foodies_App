{
    "title": "Mincemeat Pie Filling",
    "Category": "Mincemeat Pie",
    "Ingredients": [
        {
            "1": "4 cups chopped apples"
        },
        {
            "2": "0.5 cups diced cooked beef"
        },
        {
            "3": "0.5 cups raisins"
        },
        {
            "4": "0.5 cups white sugar"
        },
        {
            "5": "1 cup beef broth"
        },
        {
            "6": "1 large orange - peeled, sectioned, and cut into bite-sized pieces"
        },
        {
            "7": "0.5 cup sorghum"
        },
        {
            "8": "0.25 cup sweet pickle juice"
        },
        {
            "9": "0.25 cup pineapple juice"
        },
        {
            "10": "1 teaspoon ground cinnamon"
        },
        {
            "11": "1 teaspoon ground nutmeg"
        },
        {
            "12": "0.5 teaspoon ground cloves"
        },
        {
            "13": "0.5 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Mix apples, diced beef, raisins, sugar, beef broth, orange sections, sorghum, pickle juice, pineapple juice, cinnamon, nutmeg, cloves, and salt together in a large bowl until well combined."
        },
        {
            "2": "Store filling in the refrigerator or freeze until ready to use."
        }
    ],
