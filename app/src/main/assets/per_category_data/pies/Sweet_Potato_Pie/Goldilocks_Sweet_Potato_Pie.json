{
    "title": "Goldilocks Sweet Potato Pie",
    "Category": "Sweet Potato Pie",
    "Ingredients": [
        {
            "1": "1  pastry for a 9-inch pie crust"
        },
        {
            "2": "1 pound sweet potatoes, boiled and mashed"
        },
        {
            "3": "1 cup white sugar"
        },
        {
            "4": "0.5 cup butter, melted"
        },
        {
            "5": "0.5 cup milk"
        },
        {
            "6": "2  eggs"
        },
        {
            "7": "1 teaspoon vanilla extract"
        },
        {
            "8": "0.5 teaspoon ground nutmeg"
        },
        {
            "9": "0.5 teaspoon ground cinnamon"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C). Press pie crust into a 9-inch pie pan."
        },
        {
            "2": "Mix sweet potatoes, sugar, butter, milk, eggs, vanilla extract, nutmeg, and cinnamon together in a bowl; pour sweet potato mixture into the prepared pie crust."
        },
        {
            "3": "Bake in the preheated oven until a knife inserted near the center comes out clean, about 1 hour."
        }
    ],
    "Details": [
