{
    "title": "No Bake Key Lime Pie",
    "Category": "Key Lime Pie",
    "Ingredients": [
        {
            "1": "1 (12 fluid ounce) can frozen lemonade concentrate"
        },
        {
            "2": "0.5 cup water"
        },
        {
            "3": "1 (3 ounce) package lime flavored Jell-O\u00ae mix"
        },
        {
            "4": "1 (8 ounce) package cream cheese, softened"
        },
        {
            "5": "1 (8 ounce) container frozen whipped topping, thawed"
        },
        {
            "6": "1 (9 inch) pie crust, baked"
        }
    ],
    "Directions": [
        {
            "1": "Heat the lemonade and the water to boiling. Add the gelatin and the cream cheese and blend well."
        },
        {
            "2": "Fold in the non-dairy whipped topping."
        },
        {
            "3": "Pour mixture into the pie shell and chill or freeze. It's a breeze!"
        }
    ],
    "Details": [
        {
            "Servings": "8 "
        },
        {
            "Yield": "1 pie"
        }
