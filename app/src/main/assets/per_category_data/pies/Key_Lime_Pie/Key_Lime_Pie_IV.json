{
    "title": "Key Lime Pie IV",
    "Category": "Key Lime Pie",
    "Ingredients": [
        {
            "1": "1 (14 ounce) can sweetened condensed milk"
        },
        {
            "2": "4 tablespoons fresh lime juice"
        },
        {
            "3": "4 tablespoons lemon juice"
        },
        {
            "4": "3 drops green food coloring"
        },
        {
            "5": "1 cup heavy whipping cream"
        },
        {
            "6": "1 (9 inch) pie shell, baked"
        }
    ],
    "Directions": [
        {
            "1": "Combine milk, lime juice, lemon juice, and food coloring. This mixture will thicken."
        },
        {
            "2": "Whip the cream until stiff. Fold into lime mixture. Pour into pie shell. Chill."
        }
    ],
    "Details": [
        {
            "Servings": "8 "
        },
        {
            "Yield": "1 pie"
        }
    ],
