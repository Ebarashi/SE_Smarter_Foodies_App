{
    "title": "No-Roll Pie Crust",
    "Category": "Pie Crusts",
    "Ingredients": [
        {
            "1": "0.5 cups all-purpose flour"
        },
        {
            "2": "1 teaspoon white sugar"
        },
        {
            "3": "0.5 teaspoon salt"
        },
        {
            "4": "0.5 cup canola oil"
        },
        {
            "5": "2 tablespoons milk"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 450 degrees F (230 degrees C)."
        },
        {
            "2": "Sift flour, sugar, and salt into the center of a pie plate. Make a well in the center and pour in oil and milk. Mix with a fork, then press into the bottom and sides of the pie plate."
        },
        {
            "3": "Bake in the preheated oven for 8 to 10 minutes, or until golden brown."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "8 mins"
        },
        {
            "Total Time": "23 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "1 9-inch pie cust"
        }
    ],
