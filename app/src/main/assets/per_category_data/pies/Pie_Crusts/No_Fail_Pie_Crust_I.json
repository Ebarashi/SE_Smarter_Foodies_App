{
    "title": "No Fail Pie Crust I",
    "Category": "Pie Crusts",
    "Ingredients": [
        {
            "1": "1.0 cups all-purpose flour"
        },
        {
            "2": "1 cup shortening"
        },
        {
            "3": "0.25 teaspoon salt"
        },
        {
            "4": "1  egg"
        },
        {
            "5": "0.25 cup cold water"
        },
        {
            "6": "1 tablespoon distilled white vinegar"
        }
    ],
    "Directions": [
        {
            "1": "Combine flour and salt in a large bowl. Cut in shortening until it resembles coarse crumbs."
        },
        {
            "2": "Mix egg, water, and vinegar together in a separate bowl. Pour into flour all at once and blend with a fork until dough forms into a ball. Divide into 3 equal-sized rounds."
        },
        {
            "3": "Wrap with plastic and chill in a refrigerator until ready to prepare."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Total Time": "10 mins"
        },
        {
            "Servings": "24 "
        },
        {
            "Yield": "3 single crusts"
        }
    ],
