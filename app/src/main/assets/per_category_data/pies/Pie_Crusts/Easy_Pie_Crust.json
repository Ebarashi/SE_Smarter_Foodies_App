{
    "title": "Easy Pie Crust",
    "Category": "Pie Crusts",
    "Ingredients": [
        {
            "1": "0.5 cups all-purpose flour"
        },
        {
            "2": "2 teaspoons white sugar"
        },
        {
            "3": "1 teaspoon salt"
        },
        {
            "4": "0.5 cup vegetable oil"
        },
        {
            "5": "2 tablespoons milk"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 400 degrees F (200 degrees C)."
        },
        {
            "2": "Place all ingredients in 9 inch pie pan. Stir together with fork. Pat mixture into bottom and up the sides of the pan. Poke holes in bottom and side of crust."
        },
        {
            "3": "Bake for 15 minutes in the preheated oven, or until light brown. Use as directed in favorite pie recipe."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "15 mins"
        },
        {
            "Total Time": "25 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "1 to 9 - inch pie crust"
        }
    ],
