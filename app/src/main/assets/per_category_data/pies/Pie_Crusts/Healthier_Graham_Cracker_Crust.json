{
    "title": "Healthier Graham Cracker Crust",
    "Category": "Pie Crusts",
    "Ingredients": [
        {
            "1": "0.5 cups graham cracker crumbs"
        },
        {
            "2": "2 teaspoons unsweetened cocoa powder"
        },
        {
            "3": "3 tablespoons nonfat plain yogurt"
        },
        {
            "4": "2 tablespoons brown sugar"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "In a medium bowl, mix together graham cracker crumbs, cocoa powder, yogurt, and brown sugar. Press mixture firmly into a 9 inch pie pan."
        },
        {
            "3": "Bake in preheated oven for 5 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "5 mins"
        },
