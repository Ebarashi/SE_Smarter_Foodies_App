{
    "title": "Pretzel Crust",
    "Category": "Pie Crusts",
    "Ingredients": [
        {
            "1": "0.75 cup margarine, softened"
        },
        {
            "2": "3 tablespoons brown sugar"
        },
        {
            "3": "1.0 cups crushed pretzels"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "In a medium bowl, mix together margarine, sugar, and pretzels. Press the mixture into a 9x13-inch baking pan."
        },
        {
            "3": "Bake in the preheated oven for 10 minutes; fill with desired pie filling."
        }
    ],
