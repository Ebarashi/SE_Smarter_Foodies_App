{
    "title": "No-Sugar Apple Pie",
    "Category": "Apple Pie",
    "Ingredients": [
        {
            "1": "2 (9 inch)  pie crusts"
        },
        {
            "2": "1 (12 fluid ounce) can unsweetened apple juice concentrate, thawed"
        },
        {
            "3": "3 tablespoons cornstarch"
        },
        {
            "4": "1 tablespoon ground cinnamon"
        },
        {
            "5": "6 cups sliced green apples"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Whisk 1/3 cup apple juice concentrate together with cornstarch and cinnamon in a small bowl. Set aside."
        },
        {
            "3": "Simmer apples with remaining apple juice concentrate in a large saucepan until apples are tender, about 10 minutes. Stir in cornstarch mixture and continue to simmer until thickened. Remove from heat."
        },
        {
            "4": "Spoon apple mixture into pastry-lined pie plate. Cover with top crust. Seal and flute edges. Cut steam vents in top."
        },
        {
            "5": "Bake in the preheated oven until crust is golden brown, about 45 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "25 mins"
        },
        {
