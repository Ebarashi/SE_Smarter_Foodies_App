{
    "title": "Strawberry Yogurt Pie I",
    "Category": "Strawberry Pie",
    "Ingredients": [
        {
            "1": "2 (8 ounce) containers strawberry flavored yogurt"
        },
        {
            "2": "1 (12 ounce) container frozen whipped topping, thawed"
        },
        {
            "3": "1 cup strawberries, finely chopped"
        },
        {
            "4": "1 (9 inch) prepared graham cracker crust"
        }
    ],
    "Directions": [
        {
            "1": "Mix yogurt with 3 and 1/2 cups of the dessert topping until blended. Sweeten the strawberries, if necessary. Mix in strawberries and spoon into crust."
        },
        {
            "2": "Freeze for at least 3 hours or overnight, if possible, until it's firm."
        },
        {
            "3": "Remove from freezer and top with remainder of dessert topping. Store in freezer."
        }
    ],
    "Details": [
        {
            "Servings": "8 "
        },
        {
            "Yield": "1 to 9 - inch pie"
        }
    ],
