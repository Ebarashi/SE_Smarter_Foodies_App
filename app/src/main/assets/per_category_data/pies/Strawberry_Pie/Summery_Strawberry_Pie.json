{
    "title": "Summery Strawberry Pie",
    "Category": "Strawberry Pie",
    "Ingredients": [
        {
            "1": "1 (9 inch) prepared graham cracker pie crust"
        },
        {
            "2": "1 (8 ounce) package Neufchatel cheese, softened"
        },
        {
            "3": "2 tablespoons white sugar"
        },
        {
            "4": "2 teaspoons pure vanilla extract"
        },
        {
            "5": "2 tablespoons milk"
        },
        {
            "6": "1 pint fresh strawberries, hulled"
        },
        {
            "7": "1 cup strawberry glaze"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Bake pie crust in preheated oven until lightly browed, about 5 minutes; allow to cool completely before filling."
        },
        {
            "3": "Beat together Neufchatel cheese, sugar, vanilla extract, and milk together in a bowl until slightly thinned; spread with a spatula onto the pie crust."
        },
        {
            "4": "Arrange berries atop the cheese filling in the pie crust with the points pointing upward, pushing down slightly to anchor in the cheese filling. Cover the cheese filling as completely as possible with berries."
        },
        {
            "5": "Pour strawberries glaze evenly over the pie."
        },
        {
            "6": "Refrigerate at least 1 hour before serving."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Cook Time": "5 mins"
        },
