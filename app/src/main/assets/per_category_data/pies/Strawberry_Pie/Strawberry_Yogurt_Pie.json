{
    "title": "Strawberry Yogurt Pie",
    "Category": "Strawberry Pie",
    "Ingredients": [
        {
            "1": "1 (9 inch) pie shell, baked"
        },
        {
            "2": "1 (16 ounce) package frozen strawberries, defrosted"
        },
        {
            "3": "2 (8 ounce) containers strawberry flavored yogurt"
        },
        {
            "4": "1 (8 ounce) container frozen whipped topping, thawed"
        },
        {
            "5": "1 (.25 ounce) package unflavored gelatin"
        }
    ],
    "Directions": [
        {
            "1": "Place strawberries and yogurt in a blender or food processor. Blend until strawberries are in small chunks."
        },
        {
            "2": "In a large bowl, mix together whipped topping and gelatin. Stir in strawberry mixture. Pour mixture into baked pastry shell and chill overnight."
        }
    ],
