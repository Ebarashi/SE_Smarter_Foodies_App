{
    "title": "Curried Egg Sandwiches",
    "Category": "Lunch Recipes",
    "Ingredients": [
        {
            "1": "4  hard-cooked eggs, peeled and chopped"
        },
        {
            "2": "0.5 cup mayonnaise"
        },
        {
            "3": "1 teaspoon curry powder"
        },
        {
            "4": "1 salt and pepper to taste"
        },
        {
            "5": "8 slices bread"
        }
    ],
    "Directions": [
        {
            "1": "Mix together mayonnaise and curry powder in a bowl. Gently stir in eggs, then season to taste with salt and pepper."
        },
        {
            "2": "Evenly divide between 4 slices of bread, top with remaining 4 slices."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
