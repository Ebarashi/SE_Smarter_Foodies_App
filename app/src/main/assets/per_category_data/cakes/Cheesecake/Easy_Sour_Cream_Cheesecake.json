{
    "title": "Easy Sour Cream Cheesecake",
    "Category": "Cheesecake",
    "Ingredients": [
        {
            "1": "1 (9 inch) prepared shortbread pie crust"
        },
        {
            "2": "2 (8 ounce) packages cream cheese"
        },
        {
            "3": "1 cup white sugar"
        },
        {
            "4": "2 large eggs"
        },
        {
            "5": "1 cup sour cream"
        },
        {
            "6": "2 teaspoons vanilla extract"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 325 degrees F (165 degrees C). Place pie crust in a pan."
        },
        {
            "2": "Cream together cream cheese and sugar. Add eggs one at a time, blending well. Add sour cream and vanilla. Pour into prepared crust in the pan."
        },
        {
            "3": "Bake in the preheated oven until the cake is set and jiggles evenly across the top when lightly shaken, 60 to 70 minutes."
        },
        {
            "4": "Run a knife around the outside edge but leave the cake in the pan. Let cool on the counter, then place in refrigerator. Remove from pan when completely chilled, and serve."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Cook Time": " 1 hrs"
        },
        {
            "Total Time": " 1 hrs 20 mins"
        },
        {
            "Servings": "12 "
        },
        {
            "Yield": "1 9-inch cheesecake"
        }
    ],
