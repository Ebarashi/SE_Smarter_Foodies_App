{
    "title": "No Bake Cheesecake with Cool Whip and Apples",
    "Category": "Cheesecake",
    "Ingredients": [
        {
            "1": "2 (8 ounce) packages cream cheese, softened"
        },
        {
            "2": "1 cup white sugar"
        },
        {
            "3": "1 teaspoon vanilla extract"
        },
        {
            "4": "2 cups frozen whipped topping, thawed"
        },
        {
            "5": "1 (9 inch) prepared graham cracker crust"
        },
        {
            "6": "1 (21 ounce) can apple pie filling"
        }
    ],
    "Directions": [
        {
            "1": "Beat together cream cheese, sugar, and vanilla in a large bowl with an electric mixer until smooth. Fold in whipped topping. Pour into graham crust."
        },
        {
            "2": "Top with pie filling. Chill in the refrigerator for 1 to 2 hours before serving."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
