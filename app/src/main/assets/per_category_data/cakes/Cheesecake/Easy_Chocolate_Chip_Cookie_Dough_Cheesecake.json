{
    "title": "Easy Chocolate Chip Cookie Dough Cheesecake",
    "Category": "Cheesecake",
    "Ingredients": [
        {
            "1": "2 (8 ounce) packages cream cheese, softened"
        },
        {
            "2": "1 cup white sugar"
        },
        {
            "3": "2  eggs"
        },
        {
            "4": "2 teaspoons vanilla extract"
        },
        {
            "5": "2 (18 ounce) packages chocolate chip cookie dough"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Mix cream cheese, sugar, eggs, and vanilla together in a bowl until smooth and well blended."
        },
        {
            "3": "Spread 1 roll of cookie dough on the bottom of a 9x13-inch baking pan to form the base. Spread cream cheese mixture evenly on top. Pinch off small pieces of the remaining cookie dough and distribute them on the cream cheese mixture."
        },
        {
            "4": "Bake in the preheated oven until cream cheese mixture is set and cookies on top are golden brown, 30 to 45 minutes. Let cool before serving, about 30 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "30 mins"
        },
        {
            "Additional Time": "30 mins"
        },
        {
            "Total Time": " 1 hrs 15 mins"
        },
        {
            "Servings": "12 "
        },
        {
            "Yield": "1 9x13-inch baking pan"
        }
