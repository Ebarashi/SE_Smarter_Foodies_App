{
    "title": "No-Bake Cheesecake with Cool Whip",
    "Category": "Cheesecake",
    "Ingredients": [
        {
            "1": "2 (8 ounce) packages cream cheese, softened to room temperature"
        },
        {
            "2": "1 cup white sugar"
        },
        {
            "3": "3 tablespoons vanilla extract"
        },
        {
            "4": "1 teaspoon lemon extract (Optional)"
        },
        {
            "5": "1 (8 ounce) container frozen whipped topping (such as Cool Whip\u00ae), thawed"
        },
        {
            "6": "1 (9 inch) prepared graham cracker crust"
        },
        {
            "7": "1 (21 ounce) can strawberry pie filling"
        }
    ],
    "Directions": [
        {
            "1": "Combine cream cheese, sugar, vanilla, and lemon extract in a large bowl; beat with an electric mixer until smooth and creamy. Fold in whipped topping."
        },
        {
            "2": "Pour mixture into graham cracker crust and smooth evenly. Top with pie filling."
        },
        {
            "3": "Refrigerate until firm, 2 to 3 hours."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Additional Time": " 2 hrs"
        },
        {
            "Total Time": " 2 hrs 10 mins"
        },
        {
            "Servings": "10 "
        },
        {
            "Yield": "1 9-inch cheesecake"
        }
    ],
