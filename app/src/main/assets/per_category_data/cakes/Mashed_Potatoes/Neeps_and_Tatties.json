{
    "title": "Neeps and Tatties",
    "Category": "Mashed Potatoes",
    "Ingredients": [
        {
            "1": "2 pounds russet potatoes, peeled and cubed"
        },
        {
            "2": "2 pounds swede (rutabaga), peeled and cubed"
        },
        {
            "3": "0.25 cup butter, softened"
        },
        {
            "4": "1 teaspoon mustard powder"
        },
        {
            "5": "1 bunch scallions, finely chopped"
        },
        {
            "6": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Place potatoes and swede in a large pot; cover with water. Bring to a boil and cook until tender, about 15 minutes. Drain."
        },
        {
            "2": "Mash butter and mustard powder into potatoes and swede until well incorporated. Stir in scallions, salt, and pepper."
        }
    ],
