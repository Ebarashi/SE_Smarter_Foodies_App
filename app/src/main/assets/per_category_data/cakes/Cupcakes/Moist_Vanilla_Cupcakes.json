{
    "title": "Moist Vanilla Cupcakes",
    "Category": "Cupcakes",
    "Ingredients": [
        {
            "1": "0.75 cup white sugar"
        },
        {
            "2": "0.5 cup unsalted butter, softened"
        },
        {
            "3": "1 large egg"
        },
        {
            "4": "0.25 teaspoons vanilla extract"
        },
        {
            "5": "1 teaspoon baking powder"
        },
        {
            "6": "0.5 teaspoon baking soda"
        },
        {
            "7": "0.25 teaspoon salt"
        },
        {
            "8": "1 cup all-purpose flour"
        },
        {
            "9": "0.5 cup milk"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C). Line a cupcake pan with 12 paper liners."
        },
        {
            "2": "Beat sugar and butter together in a bowl using an electric mixer until light and fluffy. Beat in egg and vanilla extract. Mix in baking powder, baking soda, and salt. Alternate adding flour and milk, stirring until just combined. Fill prepared pan with batter."
        },
        {
            "3": "Bake in the preheated oven until lightly browned on top, about 18 minutes. Cool in the pan for 10 minutes, then remove to wire racks to cool completely."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "20 mins"
        },
        {
            "Additional Time": "10 mins"
        },
        {
            "Total Time": "40 mins"
        },
        {
            "Servings": "12 "
        },
        {
            "Yield": "12 cupcakes"
        }
