{
    "title": "No Egg Chocolate Mug Cake",
    "Category": "Chocolate Cake",
    "Ingredients": [
        {
            "1": "4 tablespoons all-purpose flour"
        },
        {
            "2": "4 tablespoons white sugar"
        },
        {
            "3": "3 tablespoons milk"
        },
        {
            "4": "2 tablespoons unsweetened cocoa powder"
        },
        {
            "5": "0.5 tablespoon olive oil"
        },
        {
            "6": "1 pinch baking powder"
        },
        {
            "7": "1 tablespoon semisweet chocolate chips"
        },
        {
            "8": "1 tablespoon sliced almonds"
        }
    ],
    "Directions": [
        {
            "1": "Mix flour, sugar, milk, cocoa powder, olive oil, and baking powder in a large mug until well combined. Stir in chocolate chips and almonds."
        },
        {
            "2": "Place mug in the microwave and cook until cake has risen by 50 percent, 30 seconds to 2 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Cook Time": "2 mins"
        },
        {
            "Total Time": "7 mins"
        },
        {
            "Servings": "1 "
        },
        {
            "Yield": "1 mug cake"
        }
    ],
