{
    "title": "Flourless Chocolate Cake II",
    "Category": "Chocolate Cake",
    "Ingredients": [
        {
            "1": "4 (1 ounce) squares semisweet chocolate, chopped"
        },
        {
            "2": "0.5 cup butter"
        },
        {
            "3": "0.75 cup white sugar"
        },
        {
            "4": "0.5 cup cocoa powder"
        },
        {
            "5": "3 large eggs, beaten"
        },
        {
            "6": "1 teaspoon vanilla extract"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 300 degrees F (150 degrees C). Grease an 8-inch round cake pan, and dust with cocoa powder."
        },
        {
            "2": "In the top of a double boiler over lightly simmering water, melt chocolate and butter. Remove from heat, and stir in sugar, cocoa powder, eggs, and vanilla. Pour into prepared pan."
        },
        {
            "3": "Bake in preheated oven for 30 minutes. Let cool in pan for 10 minutes, then turn out onto a wire rack and cool completely, 45 minutes to 1 hour."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "35 mins"
        },
