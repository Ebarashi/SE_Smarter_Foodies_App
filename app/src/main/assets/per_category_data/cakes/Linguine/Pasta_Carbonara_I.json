{
    "title": "Pasta Carbonara I",
    "Category": "Linguine",
    "Ingredients": [
        {
            "1": "0.5 pound bacon, cut into small pieces"
        },
        {
            "2": "4  eggs, room temperature"
        },
        {
            "3": "0.25 cup heavy cream at room temperature"
        },
        {
            "4": "1 cup grated Parmesan cheese"
        },
        {
            "5": "16 ounces dry fettuccine pasta"
        },
        {
            "6": "0.25 cup butter, softened"
        },
        {
            "7": "0.25 cup chopped parsley"
        },
        {
            "8": "1 ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Cook bacon until crisp. Drain on paper towels."
        },
        {
            "2": "In medium bowl beat together eggs and cream just until blended. Stir in cheese and set aside."
        },
        {
            "3": "Cook pasta according to package directions. Drain and return to pan. Toss with butter until it is melted. Add bacon and cheese mixture and toss gently until mixed."
        }
    ],
