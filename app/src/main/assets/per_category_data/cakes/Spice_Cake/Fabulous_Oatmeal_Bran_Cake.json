{
    "title": "Fabulous Oatmeal-Bran Cake",
    "Category": "Spice Cake",
    "Ingredients": [
        {
            "1": "0.5 cups boiling water"
        },
        {
            "2": "0.5 cup rolled oats"
        },
        {
            "3": "0.5 cup whole bran cereal"
        },
        {
            "4": "1 cup white sugar"
        },
        {
            "5": "1 cup packed light brown sugar"
        },
        {
            "6": "0.5 cup shortening"
        },
        {
            "7": "2  eggs"
        },
        {
            "8": "1 teaspoon baking soda"
        },
        {
            "9": "1 teaspoon ground cinnamon"
        },
        {
            "10": "0.5 teaspoon salt"
        },
        {
            "11": "0.5 cups all-purpose flour"
        }
    ],
    "Directions": [
        {
            "1": "Pour boiling water over oats and bran cereal, and let stand 10 minutes."
        },
        {
            "2": "Combine the remaining ingredients, and add oat mixture to them. Pour into two 8 or 9 inch pans."
        },
        {
            "3": "Bake at 350 degrees F ( 175 degrees C) for 30 to 40 minutes. To make muffins bake about 15 minutes, or until toothpick comes out clean."
        }
    ],
    "Details": [
        {
            "Servings": "24 "
        },
        {
            "Yield": "2 - 8 or 9 inch layer cake"
        }
    ],
