{
    "title": "Backpacking Oatmeal",
    "Category": "Meal Prep",
    "Ingredients": [
        {
            "1": "1 cup instant oats"
        },
        {
            "2": "2 tablespoons chia seeds"
        },
        {
            "3": "2 tablespoons slivered almonds"
        },
        {
            "4": "4 teaspoons brown sugar"
        },
        {
            "5": "2 teaspoons powdered peanut butter (such as PB2\u00ae)"
        },
        {
            "6": "1 teaspoon ground cinnamon"
        },
        {
            "7": "0.5 teaspoon unsweetened cocoa powder"
        },
        {
            "8": "1 pinch salt"
        },
        {
            "9": "2 cups water"
        },
        {
            "10": "1 (1.2 ounce) package peanut butter (such as Justin's\u00ae)"
        }
    ],
    "Directions": [
        {
            "1": "Combine oats, chia seeds, almonds, brown sugar, powdered peanut butter, cinnamon, cocoa powder, and salt in a sandwich bag before your trip."
        },
        {
            "2": "Bring water to a boil in a pot. Pour the oatmeal mixture into the pot and briefly stir. Remove from heat. Cover until water is fully absorbed, about 5 minutes. Stir in peanut butter."
        }
    ],
