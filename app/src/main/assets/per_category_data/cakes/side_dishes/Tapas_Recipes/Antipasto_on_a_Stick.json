{
    "title": "Antipasto on a Stick",
    "Category": "Tapas Recipes",
    "Ingredients": [
        {
            "1": "8  bamboo toothpicks"
        },
        {
            "2": "8 slices salami, rolled"
        },
        {
            "3": "8 (1/2 inch) cubes mozzarella cheese"
        },
        {
            "4": "8  pitted black olives"
        },
        {
            "5": "4  grape tomatoes, halved"
        },
        {
            "6": "4  marinated artichoke hearts, drained and halved"
        },
        {
            "7": "8 leaves fresh basil, rolled"
        },
        {
            "8": "2 tablespoons olive oil, or to taste"
        }
    ],
    "Directions": [
        {
            "1": "Thread ingredients onto toothpicks in the following order: salami roll, mozzarella cube, olive, tomato half, artichoke heart, and basil leaf. Arrange toothpicks on a serving platter and drizzle olive oil over top."
        }
    ],
