{
    "title": "Garlic Butter",
    "Category": "Side Dishes",
    "Ingredients": [
        {
            "1": "1 cup unsalted butter, softened"
        },
        {
            "2": "1 tablespoon minced garlic, or more to taste"
        },
        {
            "3": "0.25 cup grated Parmesan cheese"
        },
        {
            "4": "1 teaspoon garlic salt, or more to taste"
        },
        {
            "5": "1 teaspoon Italian seasoning, or more to taste"
        },
        {
            "6": "0.5 teaspoon ground black pepper, or more to taste"
        },
        {
            "7": "0.25 teaspoon ground paprika, or more to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine softened butter, Parmesan cheese, and minced garlic in a bowl until well combined. Mix in garlic salt, Italian seasoning, pepper, and paprika until smooth and well combined."
        },
        {
            "2": "Taste and adjust seasonings."
        }
    ],
    "Details": [
