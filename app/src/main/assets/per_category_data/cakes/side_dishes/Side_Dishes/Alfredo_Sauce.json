{
    "title": "Alfredo Sauce",
    "Category": "Side Dishes",
    "Ingredients": [
        {
            "1": "0.25 cup butter"
        },
        {
            "2": "1 cup heavy cream"
        },
        {
            "3": "0.5 cups freshly grated Parmesan cheese"
        },
        {
            "4": "1 clove garlic, crushed"
        },
        {
            "5": "0.25 cup chopped fresh parsley"
        }
    ],
    "Directions": [
        {
            "1": "Melt butter in a medium saucepan over low heat. Stir in cream and simmer for 5 minutes."
        },
        {
            "2": "Add cheese and garlic; whisk continuously until heated through. Stir in parsley and serve."
        }
    ],
    "Details": [
