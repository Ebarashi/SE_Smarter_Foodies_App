{
    "title": "Stuffed Jalapenos III",
    "Category": "Jalapeno Poppers",
    "Ingredients": [
        {
            "1": "12  fresh jalapeno peppers, halved lengthwise and seeded"
        },
        {
            "2": "1 (8 ounce) package whipped cream cheese"
        },
        {
            "3": "12 slices bacon, cut in half"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 400 degrees F (200 degrees C)."
        },
        {
            "2": "Stuff each jalapeno half with whipped cream cheese. Wrap a half slice of bacon around each stuffed jalapeno half."
        },
        {
            "3": "Arrange wrapped jalapeno halves in a single layer on a medium baking sheet. Bake in the preheated oven 45 minutes, or until bacon is evenly browned and crisp."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Cook Time": "45 mins"
        },
        {
            "Total Time": " 1 hrs 5 mins"
        },
        {
            "Servings": "12 "
        },
        {
            "Yield": "24 stuffed jalapenos"
        }
    ],
