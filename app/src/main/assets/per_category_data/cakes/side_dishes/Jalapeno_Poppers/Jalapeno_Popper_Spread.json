{
    "title": "Jalapeno Popper Spread",
    "Category": "Jalapeno Poppers",
    "Ingredients": [
        {
            "1": "2 (8 ounce) packages cream cheese, softened"
        },
        {
            "2": "1 cup mayonnaise"
        },
        {
            "3": "1 (4 ounce) can chopped green chilies, drained"
        },
        {
            "4": "2 ounces canned diced jalapeno peppers, drained"
        },
        {
            "5": "1 cup grated Parmesan cheese"
        }
    ],
    "Directions": [
        {
            "1": "Stir together cream cheese and mayonnaise in a large bowl until smooth. Stir in green chiles and jalapeno peppers. Pour mixture into a microwave safe serving dish, and sprinkle with Parmesan cheese."
        },
        {
            "2": "Microwave on High until hot, about 3 minutes."
        }
    ],
