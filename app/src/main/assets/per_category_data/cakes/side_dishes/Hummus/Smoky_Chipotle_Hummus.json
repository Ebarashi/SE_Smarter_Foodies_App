{
    "title": "Smoky Chipotle Hummus",
    "Category": "Hummus",
    "Ingredients": [
        {
            "1": "2 (15.5 ounce) cans garbanzo beans, drained"
        },
        {
            "2": "0.5 cup water"
        },
        {
            "3": "0.25 cup tahini (sesame-seed paste)"
        },
        {
            "4": "0.25 cup fresh lemon juice"
        },
        {
            "5": "2 tablespoons olive oil"
        },
        {
            "6": "1  canned chipotle pepper in adobo sauce"
        },
        {
            "7": "2 cloves garlic"
        },
        {
            "8": "0.5 teaspoons cumin"
        },
        {
            "9": "1 (7 ounce) jar roasted red bell peppers, drained"
        },
        {
            "10": "6  oil-packed sun-dried tomatoes, drained"
        },
        {
            "11": "0.5 cup chopped cilantro"
        },
        {
            "12": "0.5 teaspoon salt"
        },
        {
            "13": "1 ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Place the garbanzo beans, water, tahini, lemon juice, olive oil, chipotle pepper, garlic, and cumin in the bowl of a food processor; blend until smooth. Add the red peppers, sun-dried tomatoes, cilantro, salt, and pepper. Pulse the mixture until the ingredients are coarsely chopped into the hummus base. Transfer to a serving bowl, cover, and chill until ready to serve."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Total Time": "15 mins"
        },
        {
            "Servings": "20 "
        },
        {
            "Yield": "5 cups"
        }
    ],
