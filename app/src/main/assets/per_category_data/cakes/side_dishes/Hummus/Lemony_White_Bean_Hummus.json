{
    "title": "Lemony White Bean Hummus",
    "Category": "Hummus",
    "Ingredients": [
        {
            "1": "1 (15 ounce) can great northern beans, rinsed and drained"
        },
        {
            "2": "0.5 cup nonfat sour cream"
        },
        {
            "3": "2 tablespoons lemon juice"
        },
        {
            "4": "1 tablespoon olive oil"
        },
        {
            "5": "0.5 teaspoon ground cumin"
        },
        {
            "6": "0.5 teaspoon salt"
        },
        {
            "7": "0.25 teaspoon freshly ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Combine beans, sour cream, lemon juice, olive oil, cumin, salt, and pepper in the bowl of a food processor; mix until smooth."
        }
    ],
