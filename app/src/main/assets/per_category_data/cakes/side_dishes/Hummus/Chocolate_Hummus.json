{
    "title": "Chocolate Hummus",
    "Category": "Hummus",
    "Ingredients": [
        {
            "1": "0.25 cup milk chocolate chips"
        },
        {
            "2": "1 (15 ounce) can garbanzo beans, drained and rinsed"
        },
        {
            "3": "0.5 cup unsweetened cocoa powder"
        },
        {
            "4": "2 tablespoons maple syrup, or to taste"
        },
        {
            "5": "1 teaspoon vanilla extract"
        },
        {
            "6": "0.5 teaspoon salt"
        },
        {
            "7": "2 tablespoons almond milk, or more as needed"
        }
    ],
    "Directions": [
        {
            "1": "Place chocolate chips in a microwave-safe bowl and heat in the microwave until melted, 1 to 3 minutes."
        },
        {
            "2": "Combine melted chocolate chips, garbanzo beans, cocoa powder, maple syrup, vanilla extract, and salt in the bowl of a food processor. Add almond milk 1 tablespoon at a time while blending to desired smoothness."
        }
    ],
