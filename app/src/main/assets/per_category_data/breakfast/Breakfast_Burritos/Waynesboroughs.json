{
    "title": "Waynesboroughs",
    "Category": "Breakfast Burritos",
    "Ingredients": [
        {
            "1": "5 large potatoes, peeled and diced"
        },
        {
            "2": "1 pound pork sausage"
        },
        {
            "3": "6  eggs"
        },
        {
            "4": "10 (8 inch) flour tortillas"
        }
    ],
    "Directions": [
        {
            "1": "Boil the potatoes in a large saucepan until they are soft, but not mushy. Drain and return to the pan."
        },
        {
            "2": "In a large skillet, brown the sausage until there is no pink left. Drain and set aside."
        },
        {
            "3": "Coat a small skillet with vegetable cooking spray. Over medium heat, scramble the eggs."
        },
        {
            "4": "Mix potatoes, sausage and scrambled eggs thoroughly and place into the flour tortillas."
        }
    ],
