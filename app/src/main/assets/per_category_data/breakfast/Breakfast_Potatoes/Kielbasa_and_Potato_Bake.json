{
    "title": "Kielbasa and Potato Bake",
    "Category": "Breakfast Potatoes",
    "Ingredients": [
        {
            "1": "1 (10.75 ounce) can  condensed cream of mushroom soup"
        },
        {
            "2": "2 cups milk"
        },
        {
            "3": "1 tablespoon minced garlic"
        },
        {
            "4": "1 teaspoon salt"
        },
        {
            "5": "0.5 teaspoon ground black pepper"
        },
        {
            "6": "1 pound kielbasa sausage, sliced thin"
        },
        {
            "7": "4 large russet potatoes, peeled and cubed"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 375 degrees F (190 degrees C)."
        },
        {
            "2": "In a large mixing bowl, mix together soup, milk, garlic, salt, and pepper. Stir in potatoes and kielbasa. Spoon into a 7x11 inch casserole dish."
        },
        {
            "3": "Place casserole on a baking sheet, and bake in the preheated oven for 90 minutes, or until potatoes are tender."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
