{
    "title": "Slow Cooker Cheesy Hash Brown Potatoes",
    "Category": "Breakfast Potatoes",
    "Ingredients": [
        {
            "1": "1 (30 ounce) package frozen shredded hash brown potatoes"
        },
        {
            "2": "0.5 cup butter, melted"
        },
        {
            "3": "2 (10.75 ounce) cans  condensed cream of chicken soup"
        },
        {
            "4": "0.75 cup sour cream"
        },
        {
            "5": "2 cups shredded Cheddar cheese"
        }
    ],
    "Directions": [
        {
            "1": "Place frozen hash browns in a slow cooker. Pour melted butter over hash browns. Pour in cream of chicken soup and sour cream, stirring between each addition. Add Cheddar cheese. Cook on Low for 4 to 5 hours. Stir mixture after 2 hours of cooking."
        }
    ],
