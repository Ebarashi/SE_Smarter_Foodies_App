{
    "title": "Eggs n' Fries",
    "Category": "Breakfast Potatoes",
    "Ingredients": [
        {
            "1": "1 teaspoon butter, or to taste"
        },
        {
            "2": "0.25 cup frozen French fries, or to taste"
        },
        {
            "3": "2  eggs, beaten"
        },
        {
            "4": "1 pinch salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Melt butter in a skillet over medium heat. Cook and stir French fries in hot butter until browned and hot, 3 to 5 minutes. Add eggs and season with salt and black pepper; cook and stir until eggs are scrambled and set, 3 to 5 minutes."
        }
    ],
