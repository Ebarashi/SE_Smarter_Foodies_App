{
    "title": "Sallie's Cheesy Hashbrown Casserole",
    "Category": "Breakfast Potatoes",
    "Ingredients": [
        {
            "1": "2 (16 ounce) packages frozen cubed hash brown potatoes, thawed"
        },
        {
            "2": "2 (10.5 ounce) cans cream of chicken soup"
        },
        {
            "3": "1 (16 ounce) package shredded Cheddar cheese"
        },
        {
            "4": "1 pint sour cream"
        },
        {
            "5": "0.5 cup melted butter (Optional)"
        },
        {
            "6": "0.5 cup finely chopped onion"
        },
        {
            "7": "1 teaspoon salt"
        },
        {
            "8": "0.5 teaspoon ground black pepper"
        },
        {
            "9": "1 Topping-"
        },
        {
            "10": "1 sleeve buttery round crackers (such as Ritz\u00ae), crushed (Optional)"
        },
        {
            "11": "0.5 cup melted butter (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C). Grease a glass 9x13-inch baking pan."
        },
        {
            "2": "Combine potatoes, cream of chicken soup, Cheddar cheese, sour cream, 1/2 cup melted butter, onion, salt, and pepper together in a bowl; spread into the prepared baking pan. Mix crushed crackers and 1/2 cup melted butter together in a bowl and sprinkle over potato mixture."
        },
        {
            "3": "Bake in the preheated oven until bubbling and browned on top, 60 to 90 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
