{
    "title": "Fast-and-Fabulous Egg and Cottage Cheese Casserole",
    "Category": "Breakfast Casseroles",
    "Ingredients": [
        {
            "1": "1 cooking spray"
        },
        {
            "2": "10  eggs, beaten"
        },
        {
            "3": "2 (16 ounce) packages cottage cheese"
        },
        {
            "4": "1 pound shredded Monterey Jack cheese"
        },
        {
            "5": "1 (4 ounce) can diced green chiles"
        },
        {
            "6": "0.5 cup butter, melted (Optional)"
        },
        {
            "7": "0.5 cup all-purpose flour"
        },
        {
            "8": "0.5 cup cooked crumbled bacon (Optional)"
        },
        {
            "9": "1 teaspoon baking powder"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 400 degrees F (200 degrees C). Prepare a 10x13-inch baking dish with cooking spray."
        },
        {
            "2": "Mix eggs, cottage cheese, Monterey Jack cheese, green chiles, butter, flour, bacon, and baking powder together in a large bowl; pour into prepared baking dish."
        },
        {
            "3": "Bake in the preheated oven for 15 minutes. Reduce heat to 350 degrees F (175 degrees C) and continue baking until a toothpick inserted into the center of the casserole comes out clean, about 30 minutes more."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "45 mins"
        },
        {
            "Total Time": " 1 hrs"
        },
        {
            "Servings": "10 "
        },
        {
            "Yield": "1 10x13-inch casserole dish"
        }
    ],
