{
    "title": "Pancake Casserole",
    "Category": "Breakfast Casseroles",
    "Ingredients": [
        {
            "1": "2 cups baking mix (such as Bisquick \u00ae)"
        },
        {
            "2": "2 cups shredded Cheddar cheese, divided"
        },
        {
            "3": "1 cup milk"
        },
        {
            "4": "5 tablespoons maple syrup"
        },
        {
            "5": "2  eggs"
        },
        {
            "6": "0.5 tablespoons white sugar"
        },
        {
            "7": "12 slices cooked bacon, crumbled"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C). Grease a 9x13-inch baking pan."
        },
        {
            "2": "Mix baking mix, 1 cup Cheddar cheese, milk, maple syrup, eggs, and sugar together in a bowl; pour into prepared pan."
        },
        {
            "3": "Bake in the preheated oven until a toothpick inserted in the center comes out clean, 20 to 25 minutes. Sprinkle bacon and remaining 1 cup Cheddar cheese over casserole; return to oven until cheese is melted, about 5 more minutes."
        }
    ],
