{
    "title": "Southern-Style Chocolate Gravy",
    "Category": "Breakfast and Brunch",
    "Ingredients": [
        {
            "1": "0.75 cup white sugar"
        },
        {
            "2": "0.25 cup cocoa"
        },
        {
            "3": "3 tablespoons all-purpose flour"
        },
        {
            "4": "2 cups milk"
        },
        {
            "5": "1 tablespoon butter, softened"
        },
        {
            "6": "2 teaspoons vanilla"
        }
    ],
    "Directions": [
        {
            "1": "Whisk sugar, cocoa, and flour in a medium bowl until no lumps remain. Pour in milk and whisk until well-combined."
        },
        {
            "2": "Transfer mixture to a saucepan; cook and stir over medium heat until it thickens to a gravy consistency, 7 to 10 minutes."
        },
        {
            "3": "Remove from heat; stir in butter and vanilla. Serve warm."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "10 mins"
        },
        {
            "Total Time": "20 mins"
        },
        {
            "Servings": "12 "
        },
        {
            "Yield": "3 cups"
        }
    ],
