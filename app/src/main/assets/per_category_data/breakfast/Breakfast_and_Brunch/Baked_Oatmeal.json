{
    "title": "Baked Oatmeal",
    "Category": "Breakfast and Brunch",
    "Ingredients": [
        {
            "1": "3 cups rolled oats"
        },
        {
            "2": "1 cup brown sugar"
        },
        {
            "3": "2 teaspoons ground cinnamon"
        },
        {
            "4": "2 teaspoons baking powder"
        },
        {
            "5": "1 teaspoon salt"
        },
        {
            "6": "2  eggs"
        },
        {
            "7": "1 cup milk"
        },
        {
            "8": "0.5 cup melted butter"
        },
        {
            "9": "2 teaspoons vanilla extract"
        },
        {
            "10": "0.75 cup dried cranberries"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Mix together oats, brown sugar, cinnamon, baking powder, and salt in a large bowl. Beat in eggs, milk, melted butter, and vanilla extract. Stir in dried cranberries. Spread into a 9x13-inch baking dish."
        },
        {
            "3": "Bake in the preheated oven until top is golden, about 40 minutes."
        }
    ],
