{
    "title": "Easy Chicken and Bell Pepper Fajitas",
    "Category": "Fajitas",
    "Ingredients": [
        {
            "1": "0.5 teaspoons garlic powder"
        },
        {
            "2": "0.5 teaspoons chili powder"
        },
        {
            "3": "0.5 teaspoon paprika"
        },
        {
            "4": "0.5 teaspoon dried basil"
        },
        {
            "5": "10 ounces chicken tenderloins, sliced into strips"
        },
        {
            "6": "2 tablespoons vegetable oil"
        },
        {
            "7": "1 cup chopped onion"
        },
        {
            "8": "0.5 cup chopped green bell pepper"
        },
        {
            "9": "0.5 cup chopped red bell pepper"
        }
    ],
    "Directions": [
        {
            "1": "Mix garlic powder, chili powder, paprika, and basil in a large bowl. Add chicken strips; stir to coat with garlic powder mixture."
        },
        {
            "2": "Heat oil in a large skillet over medium heat. Add chicken, onion, green bell pepper, and red bell pepper; cook and stir until chicken is no longer pink and vegetables are tender, 7 to 10 minutes."
        }
    ],
