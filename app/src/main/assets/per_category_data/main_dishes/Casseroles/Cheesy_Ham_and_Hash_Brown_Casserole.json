{
    "title": "Cheesy Ham and Hash Brown Casserole",
    "Category": "Casseroles",
    "Ingredients": [
        {
            "1": "1 cooking spray"
        },
        {
            "2": "1 (32 ounce) package frozen hash brown potatoes"
        },
        {
            "3": "2 (10.5 ounce) cans condensed cream of potato soup"
        },
        {
            "4": "1 (16 ounce) container sour cream"
        },
        {
            "5": "2 cups shredded sharp Cheddar cheese"
        },
        {
            "6": "8 ounces cooked, diced ham"
        },
        {
            "7": "0.5 cups grated Parmesan cheese"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 375 degrees F (190 degrees C). Lightly grease a 9x13-inch baking dish with cooking spray."
        },
        {
            "2": "Mix together hash browns, condensed soup, sour cream, Cheddar cheese, and ham in a bowl until well combined. Spread evenly into the prepared dish. Sprinkle with Parmesan cheese."
        },
        {
            "3": "Bake in the preheated oven until bubbly and lightly brown, about 1 hour."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": " 1 hrs"
        },
        {
            "Total Time": " 1 hrs 15 mins"
        },
        {
            "Servings": "12 "
        },
        {
            "Yield": "1 9x13-inch casserole"
        }
    ],
