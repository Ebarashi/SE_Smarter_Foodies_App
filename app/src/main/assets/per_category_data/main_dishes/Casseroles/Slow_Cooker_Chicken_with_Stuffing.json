{
    "title": "Slow Cooker Chicken with Stuffing",
    "Category": "Casseroles",
    "Ingredients": [
        {
            "1": "4  skinless, boneless chicken breasts"
        },
        {
            "2": "1 cup chicken broth"
        },
        {
            "3": "1 (10.5 ounce) can cream of chicken soup"
        },
        {
            "4": "1 (6 ounce) box stuffing mix"
        },
        {
            "5": "0.5 cup water"
        }
    ],
    "Directions": [
        {
            "1": "Place chicken into the bottom of a slow cooker. Pour broth over chicken."
        },
        {
            "2": "Mix soup, stuffing mix, and water in a bowl; spoon on top of chicken."
        },
        {
            "3": "Cook in the slow cooker on Low for 7 hours."
        }
    ],
