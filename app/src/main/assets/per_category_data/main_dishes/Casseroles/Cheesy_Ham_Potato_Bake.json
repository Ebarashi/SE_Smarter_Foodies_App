{
    "title": "Cheesy Ham Potato Bake",
    "Category": "Casseroles",
    "Ingredients": [
        {
            "1": "1 (30 ounce) package frozen hash brown potatoes, thawed"
        },
        {
            "2": "1 (12 ounce) package pre-cooked ham steak, diced"
        },
        {
            "3": "2 cups shredded sharp Cheddar cheese, or more to taste"
        },
        {
            "4": "1  onion, chopped"
        },
        {
            "5": "1 (10.5 ounce) can condensed cream of chicken soup"
        },
        {
            "6": "1 (8 ounce) container sour cream"
        },
        {
            "7": "0.5 cup butter, melted"
        },
        {
            "8": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C). Grease a 9x13-inch baking dish."
        },
        {
            "2": "Stir hash browns, ham, cheese, onion, condensed soup, sour cream, and butter in a large bowl; season with salt and pepper. Transfer mixture to the prepared baking dish and cover with aluminum foil."
        },
        {
            "3": "Bake until bubbly and cheese is melted, 45 minutes to 1 hour. Remove foil and continue baking until brown, about 15 minutes more."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": " 1 hrs"
        },
        {
            "Total Time": " 1 hrs 10 mins"
        },
        {
            "Servings": "12 "
        },
        {
            "Yield": "1 9x13-inch baking dish"
        }
