{
    "title": "CAMPBELL'S\u00ae Thai Tacos",
    "Category": "Lettuce Wraps",
    "Ingredients": [
        {
            "1": "1 pound lean ground pork"
        },
        {
            "2": "1 tablespoon minced ginger"
        },
        {
            "3": "2 cups CAMPBELL'S\u00ae 30% Less Sodium Ready To Use Thai Chicken Broth"
        },
        {
            "4": "2  green onions, sliced"
        },
        {
            "5": "1 large carrot, diced"
        },
        {
            "6": "1  red bell pepper, diced"
        },
        {
            "7": "0.25 cup hoisin sauce"
        },
        {
            "8": "16 leaves Boston lettuce"
        }
    ],
    "Directions": [
        {
            "1": "In large non-stick skillet, cook pork with ginger until pork is no longer pink, 5 to 7 minutes. Pour in broth and cook until reduced by about half, about 10 minutes."
        },
        {
            "2": "Stir in green onions, carrot, red pepper, and hoisin and cook just until vegetables are tender-crisp, about 5 minutes more."
        },
        {
            "3": "Serve pork mixture in lettuce leaves."
        }
    ],
