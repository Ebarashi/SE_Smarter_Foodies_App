{
    "title": "Creamy Ham and Cheese Quiche",
    "Category": "Quiche",
    "Ingredients": [
        {
            "1": "1 (9 inch) refrigerated pie pastry"
        },
        {
            "2": "1 cup shredded Cheddar cheese, divided"
        },
        {
            "3": "0.5 cup shredded Swiss cheese"
        },
        {
            "4": "2 tablespoons all-purpose flour, divided"
        },
        {
            "5": "0.5 cup diced cooked ham"
        },
        {
            "6": "2 tablespoons honey mustard"
        },
        {
            "7": "0.25 cups half-and-half"
        },
        {
            "8": "5  eggs, beaten"
        },
        {
            "9": "0.25 cup chopped green onions"
        },
        {
            "10": "0.25 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 400 degrees F (200 degrees C)."
        },
        {
            "2": "Press pastry into the bottom of a 9-inch pie plate."
        },
        {
            "3": "Bake in the preheated oven for 5 minutes. Poke holes into the pastry with a fork and continue cooking until lightly browned, about 5 minutes more."
        },
        {
            "4": "Remove crust from the oven, and reduce the oven temperature to 350 degrees F (175 degrees C)."
        },
        {
            "5": "Mix 1/2 cup Cheddar cheese, Swiss cheese, and 1 tablespoon flour in a bowl; spread over warm crust."
        },
        {
            "6": "Mix ham and honey mustard together in a bowl; spoon over cheese mixture."
        },
        {
            "7": "Mix half-and-half, eggs, green onions, remaining 1 tablespoon flour, and salt together in bowl. Pour mixture carefully over ham layer; top with remaining Cheddar cheese."
        },
        {
            "8": "Bake in the preheated oven for 40 to 50 minutes until set in the center. Cool for 10 minutes before cutting."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "50 mins"
        },
