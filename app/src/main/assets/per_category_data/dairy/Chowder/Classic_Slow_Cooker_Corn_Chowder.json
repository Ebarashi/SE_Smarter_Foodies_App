{
    "title": "Classic Slow Cooker Corn Chowder",
    "Category": "Chowder",
    "Ingredients": [
        {
            "1": "3 cups milk"
        },
        {
            "2": "2 (14.75 ounce) cans cream-style corn"
        },
        {
            "3": "2 (10.75 ounce) cans  condensed cream of mushroom soup"
        },
        {
            "4": "2 (4 ounce) cans chopped green chiles"
        },
        {
            "5": "2 cups frozen corn"
        },
        {
            "6": "2 cups frozen shredded hash brown potatoes"
        },
        {
            "7": "2 cups cubed cooked ham"
        },
        {
            "8": "1 large onion, chopped"
        },
        {
            "9": "2 tablespoons butter"
        },
        {
            "10": "2 tablespoons hot sauce"
        },
        {
            "11": "2 teaspoons dried parsley"
        },
        {
            "12": "1 teaspoon chili powder"
        },
        {
            "13": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Stir milk, cream-style corn, cream of mushroom soup, chopped green chiles, frozen corn, hash brown potatoes, ham, onion, butter, hot sauce, parsley, and chili powder in a slow cooker. Season with salt and black pepper. Cover and cook on High for 4 hours or on Low and for 6 hours."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": " 4 hrs"
        },
        {
            "Total Time": " 4 hrs 15 mins"
        },
        {
            "Servings": "8 "
        }
