{
    "title": "Mexican Chicken Corn Chowder",
    "Category": "Chowder",
    "Ingredients": [
        {
            "1": "0.5 pounds boneless skinless chicken breasts, cut into bite-size pieces"
        },
        {
            "2": "0.5 cup chopped onion"
        },
        {
            "3": "1 clove garlic, minced"
        },
        {
            "4": "3 tablespoons butter"
        },
        {
            "5": "2 cubes  chicken bouillon"
        },
        {
            "6": "1 cup hot water"
        },
        {
            "7": "0.75 teaspoon ground cumin"
        },
        {
            "8": "2 cups half-and-half cream"
        },
        {
            "9": "2 cups shredded Monterey Jack cheese"
        },
        {
            "10": "1 (14.75 ounce) can cream-style corn"
        },
        {
            "11": "1 (4 ounce) can diced green chiles"
        },
        {
            "12": "1 dash hot pepper sauce"
        },
        {
            "13": "1  tomato, chopped"
        },
        {
            "14": "0.25 bunch fresh cilantro sprigs, for garnish"
        }
    ],
    "Directions": [
        {
            "1": "In a Dutch oven, brown chicken, onion, and garlic in butter until chicken is no longer pink."
        },
        {
            "2": "Dissolve the bouillon in hot water; Pour into Dutch oven, and season with cumin. Bring to a boil. Reduce heat to low, cover, and simmer for 5 minutes."
        },
        {
            "3": "Stir in cream, cheese, corn, chilies, and hot pepper sauce. Cook, stirring frequently, until the cheese is melted. Stir in chopped tomato. Garnish with cilantro."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Cook Time": "35 mins"
        },
        {
            "Total Time": "55 mins"
        },
