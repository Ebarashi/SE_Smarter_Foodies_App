{
    "title": "Easy Vegetarian Corn Chowder",
    "Category": "Chowder",
    "Ingredients": [
        {
            "1": "6 tablespoons butter"
        },
        {
            "2": "0.25 cup diced onion"
        },
        {
            "3": "0.5 cup diced celery"
        },
        {
            "4": "6 tablespoons all-purpose flour"
        },
        {
            "5": "2 (14.5 ounce) cans vegetable broth"
        },
        {
            "6": "2 (15 ounce) cans creamed corn"
        },
        {
            "7": "1 (15 ounce) can whole kernel corn, drained"
        },
        {
            "8": "2 tablespoons shredded carrot"
        },
        {
            "9": "1 cup half-and-half cream"
        },
        {
            "10": "0.75 cup skim milk"
        },
        {
            "11": "0.5 teaspoon ground nutmeg"
        },
        {
            "12": "0.25 teaspoon ground black pepper"
        },
        {
            "13": "1 pinch salt"
        }
    ],
    "Directions": [
        {
            "1": "In a large saucepan over medium heat, melt butter. Cook onions and celery in butter 3 minutes. Whisk in flour and cook 6 minutes more, until light brown. Whisk in broth and simmer 10 minutes."
        },
        {
            "2": "Stir in creamed corn, corn, carrot, half-and-half, milk, nutmeg, pepper and salt. Simmer over low heat 10 minutes more."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
