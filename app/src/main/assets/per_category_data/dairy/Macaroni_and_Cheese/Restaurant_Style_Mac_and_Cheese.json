{
    "title": "Restaurant Style Mac and Cheese",
    "Category": "Macaroni and Cheese",
    "Ingredients": [
        {
            "1": "0.5 cups macaroni"
        },
        {
            "2": "6 ounces processed cheese, shredded"
        },
        {
            "3": "0.5 cup shredded Cheddar cheese"
        },
        {
            "4": "2 tablespoons heavy cream"
        },
        {
            "5": "1 salt to taste"
        }
    ],
    "Directions": [
        {
            "1": "Bring a large pot of lightly salted water to a boil. Add pasta and cook for 8 to 10 minutes or until al dente; drain."
        },
        {
            "2": "Return drained pasta to the pot. Mix in processed cheese, Cheddar cheese, and cream. Stir until cheeses melt. Sprinkle with salt."
        }
    ],
