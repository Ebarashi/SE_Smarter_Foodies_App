{
    "title": "Smoked Salmon Spread",
    "Category": "Salmon Recipes",
    "Ingredients": [
        {
            "1": "2 (8 ounce) packages cream cheese, softened"
        },
        {
            "2": "12 ounces smoked salmon, chopped"
        },
        {
            "3": "3 dashes Worcestershire sauce"
        },
        {
            "4": "3 drops hot pepper sauce"
        },
        {
            "5": "1 teaspoon chopped fresh dill weed"
        },
        {
            "6": "2 tablespoons chopped green onion"
        }
    ],
    "Directions": [
        {
            "1": "In a medium bowl, stir cream cheese until it is no longer in a hard form. Add salmon, Worcestershire sauce, hot pepper sauce, dill and onion; mix well. Serve."
        }
    ],
