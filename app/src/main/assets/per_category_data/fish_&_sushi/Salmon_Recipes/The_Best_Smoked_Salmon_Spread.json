{
    "title": "The Best Smoked Salmon Spread",
    "Category": "Salmon Recipes",
    "Ingredients": [
        {
            "1": "1 (8 ounce) package cream cheese, softened"
        },
        {
            "2": "6 ounces smoked salmon, chopped"
        },
        {
            "3": "0.25 cup capers, or to taste"
        },
        {
            "4": "2 tablespoons chopped green onion (Optional)"
        },
        {
            "5": "0.5 teaspoons chopped fresh dill"
        },
        {
            "6": "0.25 cup heavy whipping cream"
        },
        {
            "7": "3 dashes Worcestershire sauce"
        },
        {
            "8": "3 drops hot pepper sauce"
        },
        {
            "9": "1 squeeze fresh lemon juice"
        }
    ],
    "Directions": [
        {
            "1": "Process cream cheese in a food processor to soften completely."
        },
        {
            "2": "Add smoked salmon, capers, green onion, dill, cream, Worcestershire sauce, hot pepper sauce, and lemon juice. Process the mixture again until creamy and smooth."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
