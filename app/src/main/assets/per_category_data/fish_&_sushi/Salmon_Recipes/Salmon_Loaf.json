{
    "title": "Salmon Loaf",
    "Category": "Salmon Recipes",
    "Ingredients": [
        {
            "1": "1 (14.75 ounce) can salmon, undrained"
        },
        {
            "2": "0.5 cup crushed saltine crackers"
        },
        {
            "3": "0.5 cup milk"
        },
        {
            "4": "1 large egg, beaten"
        },
        {
            "5": "2 tablespoons melted butter"
        },
        {
            "6": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Combine salmon, crushed crackers, milk, egg, melted butter, salt, and pepper in a mixing bowl. Mix thoroughly."
        },
        {
            "3": "Press the salmon mixture into a lightly greased 9x5-inch loaf pan."
        },
        {
            "4": "Bake in the preheated oven for 45 minutes or until done."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Cook Time": "45 mins"
        },
        {
            "Total Time": "50 mins"
        },
        {
            "Servings": "4 "
        },
        {
            "Yield": "1 loaf"
        }
