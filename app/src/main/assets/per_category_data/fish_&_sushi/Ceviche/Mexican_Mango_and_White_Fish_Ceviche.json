{
    "title": "Mexican Mango and White Fish Ceviche",
    "Category": "Ceviche",
    "Ingredients": [
        {
            "1": "2 pounds white fish, cut into small cubes"
        },
        {
            "2": "4  limes, juiced"
        },
        {
            "3": "0.5  orange, juiced"
        },
        {
            "4": "1 tablespoon olive oil"
        },
        {
            "5": "1  green chile pepper, chopped"
        },
        {
            "6": "2  mangoes, cut into cubes"
        },
        {
            "7": "5  green onions, chopped"
        },
        {
            "8": "3  tomatoes, seeded and chopped"
        },
        {
            "9": "0.5 cup chopped fresh cilantro"
        },
        {
            "10": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine white fish, lime juice, orange juice, oil, and chile pepper in a glass or ceramic bowl. Cover with plastic wrap and refrigerate for 90 minutes."
        },
        {
            "2": "Mix in mangoes and green onions, cover, and chill for 10 minutes more."
        },
        {
            "3": "Gently fold in tomatoes and cilantro. Season with salt and pepper and serve immediately."
        }
    ],
    "Details": [
