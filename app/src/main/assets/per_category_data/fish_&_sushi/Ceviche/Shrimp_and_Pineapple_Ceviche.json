{
    "title": "Shrimp and Pineapple Ceviche",
    "Category": "Ceviche",
    "Ingredients": [
        {
            "1": "2 cups chopped cooked shrimp meat"
        },
        {
            "2": "1 cup chopped red bell pepper"
        },
        {
            "3": "1 cup chopped fresh pineapple"
        },
        {
            "4": "1  avocado, peeled, pitted and chopped"
        },
        {
            "5": "0.5  red onion, finely chopped"
        },
        {
            "6": "0.5 bunch cilantro, chopped"
        },
        {
            "7": "1 clove garlic, minced"
        },
        {
            "8": "1  serrano pepper, minced, or to taste"
        },
        {
            "9": "2  limes, juiced"
        },
        {
            "10": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Stir the shrimp, bell pepper, pineapple, avocado, onion, cilantro, garlic, serrano pepper, lime juice, salt, and pepper together in a glass or ceramic mixing bowl until evenly combined. Cover, and refrigerate 1 hour before serving."
        }
    ],
