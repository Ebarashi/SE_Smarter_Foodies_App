{
    "title": "Quick Mushroom Ceviche",
    "Category": "Ceviche",
    "Ingredients": [
        {
            "1": "6 (4 ounce) jars sliced mushrooms, drained"
        },
        {
            "2": "1 small white onion, chopped"
        },
        {
            "3": "0.5 cup chopped cilantro leaves"
        },
        {
            "4": "0.5 cup ketchup"
        },
        {
            "5": "3  limes, juiced"
        },
        {
            "6": "1 salt to taste"
        },
        {
            "7": "2  avocados - peeled, pitted, and chopped"
        }
    ],
    "Directions": [
        {
            "1": "Combine mushrooms, onion, and cilantro in a glass bowl. Add ketchup and lime juice; mix well. Season with salt. Chill for 30 minutes. Sprinkle with chopped avocado before serving."
        }
    ],
