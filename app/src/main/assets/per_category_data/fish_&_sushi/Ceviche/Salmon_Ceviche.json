{
    "title": "Salmon Ceviche",
    "Category": "Ceviche",
    "Ingredients": [
        {
            "1": "0.25 teaspoon white sugar"
        },
        {
            "2": "1.0 tablespoons sea salt"
        },
        {
            "3": "0.5 teaspoon chili paste or sauce"
        },
        {
            "4": "0.25 cup fresh lime juice"
        },
        {
            "5": "0.25 teaspoon fresh ground pepper"
        },
        {
            "6": "0.25 teaspoon cumin"
        },
        {
            "7": "0.25 cup extra virgin olive oil"
        },
        {
            "8": "1 clove garlic, minced"
        },
        {
            "9": "0.25 cup minced red onion"
        },
        {
            "10": "1  ripe tomato, finely diced"
        },
        {
            "11": "2 tablespoons minced fresh cilantro"
        },
        {
            "12": "1 pound very fresh salmon fillets (sushi-grade preferred), thinly sliced"
        },
        {
            "13": "1  avocado, sliced"
        }
    ],
    "Directions": [
        {
            "1": "In a large glass or plastic bowl, dissolve the sugar, salt, and chili paste in the lime juice. Season with pepper and cumin. Stir in the olive oil, garlic, onion, tomato, and cilantro. Gently mix in the salmon, then cover, and refrigerate for 4 hours."
        },
        {
            "2": "To serve, drain the excess liquid from the salmon, then gently stir in the avocado slices, and allow to rest at room temperature for 15 minutes."
        }
    ],
