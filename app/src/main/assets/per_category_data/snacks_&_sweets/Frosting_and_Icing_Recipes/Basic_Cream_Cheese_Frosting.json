{
    "title": "Basic Cream Cheese Frosting",
    "Category": "Frosting and Icing Recipes",
    "Ingredients": [
        {
            "1": "8 ounces cream cheese"
        },
        {
            "2": "0.5 cup butter, softened"
        },
        {
            "3": "2 teaspoons vanilla extract"
        },
        {
            "4": "4 cups confectioners' sugar"
        }
    ],
    "Directions": [
        {
            "1": "Beat cream cheese, softened butter, and vanilla together in a large bowl with an electric mixer until light and creamy."
        },
        {
            "2": "Gradually beat in confectioners' sugar until smooth."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Total Time": "10 mins"
        },
        {
            "Servings": "12 "
        },
        {
            "Yield": "2 cups"
        }
    ],
