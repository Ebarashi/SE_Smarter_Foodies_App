{
    "title": "Brownie Frosting",
    "Category": "Frosting and Icing Recipes",
    "Ingredients": [
        {
            "1": "0.5 cups white sugar"
        },
        {
            "2": "6 tablespoons milk"
        },
        {
            "3": "6 tablespoons unsalted butter, softened"
        },
        {
            "4": "1 cup chocolate chips"
        },
        {
            "5": "1 dash vanilla extract"
        }
    ],
    "Directions": [
        {
            "1": "Stir together sugar, milk, and butter in a saucepan over medium heat; bring to a boil. Cook at a rolling boil for 30 seconds, then immediately remove from the heat."
        },
        {
            "2": "Add chocolate chips and vanilla to the saucepan; stir until chips are completely melted. Pour immediately over warm brownies."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Cook Time": "5 mins"
        },
        {
            "Total Time": "10 mins"
        },
        {
            "Servings": "12 "
        },
        {
            "Yield": "1 9x13-inch pan of brownies"
        }
    ],
