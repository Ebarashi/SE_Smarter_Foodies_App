{
    "title": "Wedding Cake Icing",
    "Category": "Frosting and Icing Recipes",
    "Ingredients": [
        {
            "1": "1 cup shortening"
        },
        {
            "2": "0.5 teaspoon salt"
        },
        {
            "3": "0.5 teaspoons clear imitation vanilla extract"
        },
        {
            "4": "0.25 teaspoon almond extract"
        },
        {
            "5": "0.5 cup water"
        },
        {
            "6": "8 cups sifted confectioners' sugar"
        }
    ],
    "Directions": [
        {
            "1": "Mix shortening, water, vanilla, almond extract, and half of the powdered sugar with a mixer for 5-10 minutes. (it's very important to mix this long)"
        },
        {
            "2": "Add the rest of the powdered sugar and beat just enough to mix in."
        },
        {
            "3": "Add additional water for desired consistency."
        }
    ],
    "Details": [
        {
            "Servings": "16 "
        },
        {
            "Yield": "4 cups"
        }
    ],
