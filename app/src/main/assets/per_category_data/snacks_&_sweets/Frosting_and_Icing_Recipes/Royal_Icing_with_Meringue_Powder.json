{
    "title": "Royal Icing with Meringue Powder",
    "Category": "Frosting and Icing Recipes",
    "Ingredients": [
        {
            "1": "4 cups sifted confectioners' sugar"
        },
        {
            "2": "6 tablespoons water"
        },
        {
            "3": "3 tablespoons meringue powder"
        }
    ],
    "Directions": [
        {
            "1": "Beat confectioners' sugar, water, and meringue powder in a large bowl with an electric mixer until soft peaks form, about 7 to 10 minutes."
        }
    ],
