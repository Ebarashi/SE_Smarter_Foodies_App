{
    "title": "Lemon Icing",
    "Category": "Frosting and Icing Recipes",
    "Ingredients": [
        {
            "1": "0.5 cup unsalted butter, at room temperature"
        },
        {
            "2": "2 tablespoons lemon juice"
        },
        {
            "3": "0.5 teaspoon vanilla extract"
        },
        {
            "4": "3 cups confectioners' sugar"
        }
    ],
    "Directions": [
        {
            "1": "Combine butter, lemon juice, vanilla, and confectioners' sugar in a large mixing bowl and beat until light and fluffy. Add more confectioners' sugar, or lemon juice to adjust the consistency as needed."
        }
    ],
