{
    "title": "Easy Chocolate Bundt Cake Glaze",
    "Category": "Frosting and Icing Recipes",
    "Ingredients": [
        {
            "1": "1 (14 ounce) can sweetened condensed milk"
        },
        {
            "2": "1 cup semisweet chocolate chips"
        },
        {
            "3": "1 teaspoon vanilla extract"
        }
    ],
    "Directions": [
        {
            "1": "Combine sweetened condensed milk and chocolate chips in a saucepan over medium heat. Cook, stirring constantly, until the chips are melted and the mixture is smooth. Do not allow it to bubble."
        },
        {
            "2": "Remove from heat and stir in vanilla. Cool slightly before drizzling over cake."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Cook Time": "5 mins"
        },
        {
            "Total Time": "10 mins"
        },
        {
            "Servings": "12 "
        },
        {
            "Yield": "1 1/4 cups"
        }
    ],
