{
    "title": "Cheese Filling",
    "Category": "Frosting and Icing Recipes",
    "Ingredients": [
        {
            "1": "2 (8 ounce) packages cream cheese, room temperature"
        },
        {
            "2": "1 cup white sugar"
        },
        {
            "3": "1 large egg yolk"
        },
        {
            "4": "1 teaspoon vanilla extract"
        }
    ],
    "Directions": [
        {
            "1": "Beat together cream cheese and sugar in a large bowl with an electric mixer until smooth and creamy. Beat in egg yolk and vanilla until well combined. Cover and chill in the refrigerator before using in pastries and baking them."
        }
    ],
