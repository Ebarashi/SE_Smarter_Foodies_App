{
    "title": "Caramel Frosting",
    "Category": "Frosting and Icing Recipes",
    "Ingredients": [
        {
            "1": "2 tablespoons butter"
        },
        {
            "2": "0.5 cup packed brown sugar"
        },
        {
            "3": "3 tablespoons milk, or more as needed"
        },
        {
            "4": "1 cup confectioners' sugar"
        },
        {
            "5": "0.5 teaspoon vanilla extract"
        }
    ],
    "Directions": [
        {
            "1": "Melt butter in a saucepan over medium heat. Stir in brown sugar and 3 tablespoons milk. Boil vigorously for 1 minute."
        },
        {
            "2": "Remove from heat and stir in 1/2 cup confectioners' sugar. Cool slightly, then beat in vanilla and remaining 1/2 cup confectioners' sugar. Add more milk if mixture is too thick."
        }
    ],
