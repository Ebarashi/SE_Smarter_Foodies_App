{
    "title": "Sugar Cookie Glaze",
    "Category": "Frosting and Icing Recipes",
    "Ingredients": [
        {
            "1": "1 cup confectioners' sugar"
        },
        {
            "2": "2 tablespoons water"
        },
        {
            "3": "1 tablespoon light corn syrup"
        },
        {
            "4": "10 drops food coloring (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Stir confectioners' sugar, water, and corn syrup together in a bowl until smooth. Stir in food coloring if desired."
        }
    ],
