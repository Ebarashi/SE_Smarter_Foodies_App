{
    "title": "Sugar Cookie Icing",
    "Category": "Frosting and Icing Recipes",
    "Ingredients": [
        {
            "1": "1 cup confectioners' sugar"
        },
        {
            "2": "2 teaspoons milk"
        },
        {
            "3": "2 teaspoons light corn syrup"
        },
        {
            "4": "0.25 teaspoon almond extract"
        },
        {
            "5": "1 Recommended-"
        },
        {
            "6": "1 assorted food coloring"
        }
    ],
    "Directions": [
        {
            "1": "Stir together confectioners' sugar and milk in a small bowl until smooth. Beat in corn syrup and almond extract until icing is smooth and glossy. If icing is too thick, add more corn syrup."
        },
        {
            "2": "Divide into separate bowls, and add food colorings to each to desired intensity. Dip cookies, or paint them with a brush."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Total Time": "10 mins"
        },
        {
            "Servings": "12 "
        },
        {
            "Yield": "1 dozen cookies' worth"
        }
    ],
