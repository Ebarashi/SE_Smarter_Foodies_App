{
    "title": "Mascarpone Frosting",
    "Category": "Frosting and Icing Recipes",
    "Ingredients": [
        {
            "1": "1 cup heavy whipping cream"
        },
        {
            "2": "0.5 (8 ounce) container mascarpone cheese"
        },
        {
            "3": "1 teaspoon vanilla extract, or more to taste"
        },
        {
            "4": "2 tablespoons white sugar"
        }
    ],
    "Directions": [
        {
            "1": "Beat together cream, mascarpone, and vanilla in a large bowl with an electric mixer until smooth and creamy. Add sugar gradually, mixing continuously until frosting is smooth and stiff peaks form, about 5 minutes. Use immediately or store covered in the refrigerator."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
