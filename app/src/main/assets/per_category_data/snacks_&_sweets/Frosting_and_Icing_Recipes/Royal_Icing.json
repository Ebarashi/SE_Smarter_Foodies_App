{
    "title": "Royal Icing",
    "Category": "Frosting and Icing Recipes",
    "Ingredients": [
        {
            "1": "4  pasteurized egg whites"
        },
        {
            "2": "4 cups sifted confectioners' sugar"
        },
        {
            "3": "1 teaspoon lemon extract"
        }
    ],
    "Directions": [
        {
            "1": "Beat egg whites in a large glass, metal, or ceramic bowl with an electric mixer at high speed until foamy."
        },
        {
            "2": "Gradually add sugar and lemon extract; continue to beat on high speed until thickened."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Total Time": "10 mins"
        },
        {
            "Servings": "32 "
        },
        {
            "Yield": "2 cups"
        }
    ],
