{
    "title": "Creamy Chocolate Frosting",
    "Category": "Frosting and Icing Recipes",
    "Ingredients": [
        {
            "1": "1.5 cups confectioners' sugar"
        },
        {
            "2": "6 tablespoons unsweetened cocoa powder"
        },
        {
            "3": "6 tablespoons unsalted butter, at room temperature"
        },
        {
            "4": "5 tablespoons evaporated milk"
        },
        {
            "5": "1 teaspoon vanilla extract"
        }
    ],
    "Directions": [
        {
            "1": "Sift together confectioners' sugar and cocoa powder in a medium bowl."
        },
        {
            "2": "Cream butter in a large bowl until smooth. Gradually beat in sugar mixture alternately with evaporated milk. Blend in vanilla."
        },
        {
            "3": "Beat mixture on high speed until light and fluffy. Adjust consistency with more milk or sugar, if desired."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Total Time": "15 mins"
        },
        {
            "Servings": "12 "
        }
