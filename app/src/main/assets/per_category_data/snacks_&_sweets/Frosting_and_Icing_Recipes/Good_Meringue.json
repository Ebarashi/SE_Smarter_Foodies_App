{
    "title": "Good Meringue",
    "Category": "Frosting and Icing Recipes",
    "Ingredients": [
        {
            "1": "3  egg whites"
        },
        {
            "2": "0.25 teaspoon cream of tartar"
        },
        {
            "3": "0.75 cup white sugar"
        }
    ],
    "Directions": [
        {
            "1": "In large bowl, beat egg whites and cream of tartar with electric mixer until foamy. Gradually add sugar, beating until mixture forms stiff peaks."
        }
    ],
