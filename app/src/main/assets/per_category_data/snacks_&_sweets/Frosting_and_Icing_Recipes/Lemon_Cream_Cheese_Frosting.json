{
    "title": "Lemon Cream Cheese Frosting",
    "Category": "Frosting and Icing Recipes",
    "Ingredients": [
        {
            "1": "1 (8 ounce) package cream cheese, softened"
        },
        {
            "2": "0.25 cup butter"
        },
        {
            "3": "2 tablespoons lemon juice"
        },
        {
            "4": "2 teaspoons lemon zest"
        },
        {
            "5": "1 teaspoon vanilla extract"
        },
        {
            "6": "5 cups confectioners' sugar"
        }
    ],
    "Directions": [
        {
            "1": "Beat cream cheese, butter, lemon juice, lemon zest, and vanilla together in a large bowl using an electric mixer until smooth and fluffy; gradually beat in confectioners' sugar, 1 cup at a time, until desired consistency is reached."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Total Time": "15 mins"
        },
        {
            "Servings": "15 "
        },
        {
            "Yield": "3 2/3 cups"
        }
    ],
