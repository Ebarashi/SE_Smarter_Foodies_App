{
    "title": "Sour Cream Waffles",
    "Category": "Waffles",
    "Ingredients": [
        {
            "1": "1 cup water"
        },
        {
            "2": "0.5 cup sour cream"
        },
        {
            "3": "0.5 cup plain yogurt"
        },
        {
            "4": "0.25 teaspoon white vinegar"
        },
        {
            "5": "2 cups all-purpose flour"
        },
        {
            "6": "2 teaspoons baking powder"
        },
        {
            "7": "1 teaspoon baking soda"
        },
        {
            "8": "0.5 teaspoon salt"
        },
        {
            "9": "2  eggs, beaten"
        }
    ],
    "Directions": [
        {
            "1": "In a bowl, mix together water, sour cream, yogurt, and vinegar."
        },
        {
            "2": "Sift flour into a separate, large bowl; stir in baking powder, baking soda, and salt."
        },
        {
            "3": "Add sour cream mixture and eggs to flour mixture; blend until smooth."
        },
        {
            "4": "Cook on a lightly greased waffle iron until golden brown; lift off with a fork."
        }
    ],
    "Details": [
