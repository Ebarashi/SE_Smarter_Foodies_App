{
    "title": "Kitchen Sink Hash Brown and Egg Waffle",
    "Category": "Waffles",
    "Ingredients": [
        {
            "1": "1 cooking spray"
        },
        {
            "2": "2 cups frozen shredded hash browns, thawed"
        },
        {
            "3": "2  eggs, beaten"
        },
        {
            "4": "0.25 cup milk"
        },
        {
            "5": "0.25 cup all-purpose flour"
        },
        {
            "6": "2 tablespoons diced onion, or more to taste"
        },
        {
            "7": "1 tablespoon sriracha sauce, or to taste"
        },
        {
            "8": "2 cloves garlic, minced"
        },
        {
            "9": "0.5 teaspoon baking powder"
        },
        {
            "10": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Preheat a waffle iron according to manufacturer's instructions; spray with cooking spray."
        },
        {
            "2": "Mix hash browns, eggs, milk, flour, onion, sriracha sauce, garlic, baking powder, salt, and pepper together in a bowl to make batter."
        },
        {
            "3": "Pour 1/4 of the mixture onto the preheated waffle iron; spread to the edges with a spatula. Cook until golden and warmed through, 5 to 7 minutes. Repeat with remaining batter."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "5 mins"
        },
        {
            "Total Time": "20 mins"
        },
        {
            "Servings": "4 "
        },
        {
            "Yield": "4 waffles"
        }
    ],
