{
    "title": "Rich Yogurt Waffles",
    "Category": "Waffles",
    "Ingredients": [
        {
            "1": "3 large eggs"
        },
        {
            "2": "0.5 cups vanilla fat-free yogurt"
        },
        {
            "3": "0.25 cups all-purpose flour"
        },
        {
            "4": "0.5 cup shortening"
        },
        {
            "5": "2 teaspoons baking powder"
        },
        {
            "6": "1 teaspoon baking soda"
        },
        {
            "7": "0.5 teaspoon kosher salt"
        }
    ],
    "Directions": [
        {
            "1": "Preheat a waffle iron according to manufacturer's instructions."
        },
        {
            "2": "Beat eggs in a large mixing bowl. Add yogurt, flour, shortening, baking powder, baking soda, and salt to eggs; mix until smooth."
        },
        {
            "3": "Pour batter onto the preheated waffle iron. Cook until no longer steaming, about 5 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "20 mins"
        },
        {
            "Total Time": "30 mins"
        },
        {
            "Servings": "4 "
        },
        {
            "Yield": "4 waffles"
        }
    ],
