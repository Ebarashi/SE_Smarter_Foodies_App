{
    "title": "Norwegian Waffles",
    "Category": "Waffles",
    "Ingredients": [
        {
            "1": "0.5 cups all-purpose flour"
        },
        {
            "2": "1.5 teaspoons baking powder"
        },
        {
            "3": "2 large eggs"
        },
        {
            "4": "2 tablespoons white sugar"
        },
        {
            "5": "0.75 cups milk"
        },
        {
            "6": "3 tablespoons melted shortening"
        },
        {
            "7": "1 teaspoon salt"
        },
        {
            "8": "1 teaspoon vanilla extract"
        }
    ],
    "Directions": [
        {
            "1": "Preheat a waffle iron according to manufacturer's instructions."
        },
        {
            "2": "Sift flour and baking powder together into a bowl."
        },
        {
            "3": "Beat eggs and sugar in a separate bowl with an electric mixer until fluffy. Add milk, melted shortening, salt, and vanilla; mix until well blended. Mix in flour mixture."
        },
        {
            "4": "Spoon 2/3 cup batter onto the preheated waffle iron. Close the iron and cook until waffle is golden brown and the iron stops steaming, about 5 minutes. Repeat to cook remaining waffles."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "20 mins"
        },
        {
            "Total Time": "30 mins"
        },
        {
            "Servings": "4 "
        },
        {
            "Yield": "4 waffles"
        }
    ],
