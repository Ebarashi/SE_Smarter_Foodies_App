{
    "title": "Thai Sticky Black Rice Pudding",
    "Category": "Rice Pudding",
    "Ingredients": [
        {
            "1": "1.5 cups water"
        },
        {
            "2": "1 cup black rice"
        },
        {
            "3": "0.75 teaspoon salt"
        },
        {
            "4": "1 cup unsweetened coconut milk"
        },
        {
            "5": "0.5 cup white sugar"
        }
    ],
    "Directions": [
        {
            "1": "Combine water, black rice, and salt in a saucepan. Bring to a boil. Reduce heat and cover; simmer for 40 minutes."
        },
        {
            "2": "Add coconut milk and sugar. Bring back to a boil. Reduce heat and simmer, uncovered, until rice reaches a sticky consistency, about 30 minutes."
        },
        {
            "3": "Remove from heat and allow mixture to cool. Stir and serve."
        }
    ],
