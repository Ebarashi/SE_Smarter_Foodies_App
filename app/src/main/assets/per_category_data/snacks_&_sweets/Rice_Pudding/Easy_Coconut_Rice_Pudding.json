{
    "title": "Easy Coconut Rice Pudding",
    "Category": "Rice Pudding",
    "Ingredients": [
        {
            "1": "3 cups leftover cooked rice"
        },
        {
            "2": "1 (13.5 ounce) can coconut milk"
        },
        {
            "3": "1 cup white sugar"
        },
        {
            "4": "1 tablespoon coconut oil"
        },
        {
            "5": "1 tablespoon unsalted butter"
        },
        {
            "6": "1 teaspoon vanilla extract"
        },
        {
            "7": "0.25 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Place leftover rice in a sauce pan; add coconut milk over top. Stir in sugar, coconut oil, butter, vanilla, and salt."
        },
        {
            "2": "Cook over medium heat, stirring frequently, until sugar is dissolved and mixture is hot, about 6 minutes."
        }
    ],
