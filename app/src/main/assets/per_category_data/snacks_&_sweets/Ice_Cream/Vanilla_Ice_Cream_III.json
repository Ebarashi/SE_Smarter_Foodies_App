{
    "title": "Vanilla Ice Cream III",
    "Category": "Ice Cream",
    "Ingredients": [
        {
            "1": "1 (14 ounce) can sweetened condensed milk"
        },
        {
            "2": "1 (12 fluid ounce) can evaporated milk"
        },
        {
            "3": "2 tablespoons vanilla extract"
        },
        {
            "4": "1 pinch salt"
        },
        {
            "5": "1 cup white sugar"
        },
        {
            "6": "6 cups milk, or as needed"
        }
    ],
    "Directions": [
        {
            "1": "Combine condensed milk, evaporated milk, vanilla, salt and sugar in freezer canister of ice cream maker, and stir well. Add enough milk to bring mixture to the fill line on canister. Freeze according to manufacturer's directions."
        }
    ],
