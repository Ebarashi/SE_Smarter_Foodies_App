{
    "title": "Vanilla Ice Cream V",
    "Category": "Ice Cream",
    "Ingredients": [
        {
            "1": "2 quarts half-and-half cream"
        },
        {
            "2": "0.5 pint heavy cream"
        },
        {
            "3": "0.5 cups white sugar"
        },
        {
            "4": "4 teaspoons vanilla extract"
        },
        {
            "5": "1 pinch salt"
        }
    ],
    "Directions": [
        {
            "1": "Combine half-and-half, cream, sugar, vanilla and salt in freezer container of ice cream maker. Freeze according to manufacturer's instructions."
        }
