{
    "title": "Fancy Pear Popovers",
    "Category": "Popovers and Yorkshire Pudding",
    "Ingredients": [
        {
            "1": "1 cooking spray"
        },
        {
            "2": "0.5 cup milk"
        },
        {
            "3": "0.5 cup all-purpose flour"
        },
        {
            "4": "2  eggs, lightly beaten"
        },
        {
            "5": "0.25 teaspoon salt"
        },
        {
            "6": "2 tablespoons butter, cut into 6 pieces"
        },
        {
            "7": "0.5 cup grated sharp Cheddar cheese"
        },
        {
            "8": "0.25 cup brown sugar"
        },
        {
            "9": "0.25 teaspoon ground nutmeg"
        },
        {
            "10": "1  pear, thinly sliced"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 400 degrees F (200 degrees C). Spray 6 custard cups with cooking spray."
        },
        {
            "2": "Pour milk, flour, eggs, and salt in a bowl. Mix until batter is fully combined."
        },
        {
            "3": "Place a piece of butter in each prepared cup. Sprinkle each cup with Cheddar cheese, brown sugar, and nutmeg; line each evenly with pear slices. Pour batter evenly into the prepared cups."
        },
        {
            "4": "Bake in the preheated oven until golden brown, 15 to 20 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "25 mins"
        },
        {
            "Cook Time": "15 mins"
        },
        {
            "Total Time": "40 mins"
        },
        {
            "Servings": "6 "
        },
        {
            "Yield": "6 popovers"
