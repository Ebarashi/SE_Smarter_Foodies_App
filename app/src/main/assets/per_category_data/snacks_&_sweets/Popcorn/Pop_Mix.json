{
    "title": "Pop Mix",
    "Category": "Popcorn",
    "Ingredients": [
        {
            "1": "1 (2.9 ounce) bag microwave popcorn"
        },
        {
            "2": "1 tablespoon grated Parmesan cheese"
        },
        {
            "3": "0.25 cup melted butter"
        },
        {
            "4": "1 cup savory baked cereal snack mix (such as Bits & Bites)"
        },
        {
            "5": "1 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Place packaged popcorn in the microwave; cook on high until popping begins to slow down, about 2 1/2 minutes. Mix in Parmesan cheese. Drizzle melted butter over popcorn. Mix in savory snack mix and salt."
        }
    ],
