{
    "title": "Chocolate Peanut Butter Bars II",
    "Category": "Peanut Butter Cookies",
    "Ingredients": [
        {
            "1": "0.5 cup butter"
        },
        {
            "2": "0.5 cup packed brown sugar"
        },
        {
            "3": "1 teaspoon vanilla extract"
        },
        {
            "4": "2 cups peanut butter"
        },
        {
            "5": "1.0 cups confectioners' sugar"
        },
        {
            "6": "2 cups semisweet chocolate chips"
        }
    ],
    "Directions": [
        {
            "1": "Melt butter or margarine over low heat. Add sugars, peanut butter and vanilla. Mix well."
        },
        {
            "2": "Press into a 9 x 13 inch pan."
        },
        {
            "3": "Melt chocolate chips and spread over the top. Cool."
        }
    ],
    "Details": [
        {
            "Servings": "12 "
        },
        {
            "Yield": "2 dozen"
        }
    ],
