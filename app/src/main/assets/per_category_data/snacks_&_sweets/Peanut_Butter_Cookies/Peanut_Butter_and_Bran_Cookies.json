{
    "title": "Peanut Butter and Bran Cookies",
    "Category": "Peanut Butter Cookies",
    "Ingredients": [
        {
            "1": "1 cup butter, melted"
        },
        {
            "2": "0.75 cup packed brown sugar"
        },
        {
            "3": "0.75 cup white sugar"
        },
        {
            "4": "1 teaspoon vanilla extract"
        },
        {
            "5": "1 cup peanut butter"
        },
        {
            "6": "2  eggs, beaten"
        },
        {
            "7": "0.25 cups all-purpose flour"
        },
        {
            "8": "1 cup wheat bran"
        },
        {
            "9": "0.75 cup rolled oats"
        },
        {
            "10": "2 teaspoons baking soda"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "In a bowl, beat together the butter, brown sugar, white sugar, vanilla, peanut butter and eggs. In a separate bowl, mix the flour, bran, oats, and baking soda. Stir the flour mixture into the butter mixture until smooth. Drop by rounded teaspoons onto an ungreased cookie sheet."
        },
        {
            "3": "Bake for 15 to 18 minutes in the preheated oven. Remove to a rack to cool."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "18 mins"
        },
        {
            "Total Time": "33 mins"
        },
        {
            "Servings": "84 "
        },
        {
            "Yield": "7 dozen"
