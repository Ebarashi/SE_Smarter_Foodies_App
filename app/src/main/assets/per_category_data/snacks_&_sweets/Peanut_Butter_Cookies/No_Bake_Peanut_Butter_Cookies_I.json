{
    "title": "No Bake Peanut Butter Cookies I",
    "Category": "Peanut Butter Cookies",
    "Ingredients": [
        {
            "1": "2 cups white sugar"
        },
        {
            "2": "0.5 cup milk"
        },
        {
            "3": "0.25 cup butter"
        },
        {
            "4": "2 tablespoons unsweetened cocoa powder"
        },
        {
            "5": "3 cups quick cooking oats"
        },
        {
            "6": "0.5 cup peanut butter"
        },
        {
            "7": "1 teaspoon vanilla extract"
        },
        {
            "8": "1 cup chopped walnuts (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Combine sugar, milk, butter and cocoa in a saucepan and bring to a boil, cooking 1 minute."
        },
        {
            "2": "Remove from heat and add remaining ingredients. Stir and mix well."
        },
        {
            "3": "Drop by teaspoons onto wax paper, letting stand 30 minutes or when dry and cool. Store in well-sealed container."
        }
    ],
    "Details": [
        {
            "Servings": "12 "
        },
        {
            "Yield": "2 dozen"
        }
    ],
