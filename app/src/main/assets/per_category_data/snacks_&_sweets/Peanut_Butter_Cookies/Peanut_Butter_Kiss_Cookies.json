{
    "title": "Peanut Butter Kiss Cookies",
    "Category": "Peanut Butter Cookies",
    "Ingredients": [
        {
            "1": "1 cup white sugar"
        },
        {
            "2": "1 cup creamy peanut butter"
        },
        {
            "3": "1 large egg"
        },
        {
            "4": "18  milk chocolate candy kisses, unwrapped"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Stir together sugar, peanut butter, and egg in a mixing bowl until well combined."
        },
        {
            "3": "Shape dough into 1-inch balls and place on an ungreased cookie sheet. If dough is too sticky, refrigerate until easy to handle, about 30 minutes."
        },
        {
            "4": "Bake in the preheated oven until edges are set, about 10 minutes."
        },
        {
            "5": "Press a chocolate kiss into the center of each warm cookie. Remove to a wire rack to cool."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "10 mins"
        },
