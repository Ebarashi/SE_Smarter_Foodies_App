{
    "title": "Joey's Peanut Butter Cookies",
    "Category": "Peanut Butter Cookies",
    "Ingredients": [
        {
            "1": "1 cup peanut butter"
        },
        {
            "2": "0.5 cup butter, softened"
        },
        {
            "3": "0.5 cup white sugar"
        },
        {
            "4": "0.5 cup packed brown sugar"
        },
        {
            "5": "1 large egg"
        },
        {
            "6": "3 tablespoons milk"
        },
        {
            "7": "1 teaspoon vanilla extract"
        },
        {
            "8": "0.25 cups all-purpose flour"
        },
        {
            "9": "0.75 teaspoon baking powder"
        },
        {
            "10": "0.25 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 375 degrees F (190 degrees C)."
        },
        {
            "2": "Cream together peanut butter, butter, white sugar, and brown sugar in a large bowl until well blended. Beat in egg until well combined. Mix in milk and vanilla. Combine flour, baking powder, and salt in a separate bowl; stir into peanut butter mixture until incorporated."
        },
        {
            "3": "Roll tablespoonfuls of dough into balls. Place cookies 2 inches apart onto ungreased cookie sheets. Press each ball once with fork tines."
        },
        {
            "4": "Bake in the preheated oven until edges are lightly browned, 8 to 10 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "10 mins"
        },
        {
            "Total Time": "25 mins"
        },
