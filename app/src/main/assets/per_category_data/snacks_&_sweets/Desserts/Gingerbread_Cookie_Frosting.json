{
    "title": "Gingerbread Cookie Frosting",
    "Category": "Desserts",
    "Ingredients": [
        {
            "1": "1 cup confectioners' sugar"
        },
        {
            "2": "0.25 cup milk, or as needed"
        },
        {
            "3": "10 drops food coloring"
        }
    ],
    "Directions": [
        {
            "1": "Sift confectioners' sugar into a small bowl. Stir in enough milk to make a smooth but not too runny consistency."
        },
        {
            "2": "Divide mixture into small containers, depending on how many colors you want. Drop food coloring into each container, using as much food coloring as needed to create desired hues."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
