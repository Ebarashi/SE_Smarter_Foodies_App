{
    "title": "Mom's Peach Crisp",
    "Category": "Crisps and Crumbles",
    "Ingredients": [
        {
            "1": "4 cups sliced fresh peaches"
        },
        {
            "2": "0.5 cup all-purpose flour"
        },
        {
            "3": "0.5 cup brown sugar"
        },
        {
            "4": "0.5 cup cold butter"
        },
        {
            "5": "1 teaspoon ground cinnamon"
        },
        {
            "6": "0.25 teaspoon salt"
        },
        {
            "7": "1 cup rolled oats"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Arrange sliced peaches evenly in an 8x8-inch baking dish."
        },
        {
            "3": "Mix flour, brown sugar, butter, cinnamon, and salt in a bowl using a pastry cutter until crumbly. Fold oats into flour mixture; sprinkle mixture evenly over peaches, pressing down lightly."
        },
        {
            "4": "Bake in the preheated oven until crispy and golden brown on top, about 30 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "30 mins"
        },
