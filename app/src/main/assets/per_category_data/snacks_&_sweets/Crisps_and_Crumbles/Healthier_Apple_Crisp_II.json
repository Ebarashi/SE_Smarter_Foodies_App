{
    "title": "Healthier Apple Crisp II",
    "Category": "Crisps and Crumbles",
    "Ingredients": [
        {
            "1": "10 cups unpeeled, cored, and sliced apples"
        },
        {
            "2": "0.5 cup white sugar"
        },
        {
            "3": "1 tablespoon white whole wheat flour"
        },
        {
            "4": "1 teaspoon ground cinnamon"
        },
        {
            "5": "0.5 cup water"
        },
        {
            "6": "1 cup quick cooking oats"
        },
        {
            "7": "1 cup white whole wheat flour"
        },
        {
            "8": "0.5 cup packed brown sugar"
        },
        {
            "9": "0.5 cup chopped walnuts"
        },
        {
            "10": "0.25 teaspoon baking powder"
        },
        {
            "11": "0.25 teaspoon baking soda"
        },
        {
            "12": "0.5 cup butter, melted"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degree C)."
        },
        {
            "2": "Place the sliced apples in a 9x13 inch pan. Mix the white sugar, 1 tablespoon flour and ground cinnamon together, and sprinkle over apples. Pour water evenly over all."
        },
        {
            "3": "Combine the oats, 1 cup flour, brown sugar, baking powder, baking soda, walnuts and melted butter together. Crumble evenly over the apple mixture."
        },
        {
            "4": "Bake at 350 degrees F (175 degrees C) for about 45 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "30 mins"
        },
        {
            "Cook Time": "45 mins"
        },
        {
            "Total Time": " 1 hrs 15 mins"
        },
        {
            "Servings": "12 "
        },
        {
            "Yield": "1 9x13-inch pan"
        }
