{
    "title": "Easy Rhubarb Crisp",
    "Category": "Crisps and Crumbles",
    "Ingredients": [
        {
            "1": "1 cup all-purpose flour"
        },
        {
            "2": "1 cup brown sugar"
        },
        {
            "3": "0.75 cup oats"
        },
        {
            "4": "0.5 cup melted butter"
        },
        {
            "5": "1 teaspoon ground cinnamon"
        },
        {
            "6": "4 cups diced rhubarb"
        },
        {
            "7": "1 cup white sugar"
        },
        {
            "8": "1 cup water"
        },
        {
            "9": "2 tablespoons cornstarch"
        },
        {
            "10": "1 teaspoon vanilla extract"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C). Grease a 9x13-inch baking dish."
        },
        {
            "2": "Mix flour, brown sugar, oats, butter, and cinnamon together in a bowl until crumbly. Press 1/2 of the oat mixture into the bottom of prepared baking dish. Sprinkle rhubarb over oat mixture."
        },
        {
            "3": "Combine white sugar, water, cornstarch, and vanilla in a saucepan over medium heat; cook and stir until thick and clear, about 5 minutes. Pour sugar mixture over rhubarb. Sprinkle remaining oat mixture over rhubarb."
        },
        {
            "4": "Bake in the preheated oven until the crisp is lightly brown and bubbling, about 1 hour."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Cook Time": " 1 hrs 5 mins"
        },
        {
            "Total Time": " 1 hrs 25 mins"
        },
        {
            "Servings": "10 "
        },
        {
            "Yield": "1 9x13-inch crisp"
        }
    ],
