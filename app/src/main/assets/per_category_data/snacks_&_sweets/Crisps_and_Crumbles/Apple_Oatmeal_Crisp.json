{
    "title": "Apple Oatmeal Crisp",
    "Category": "Crisps and Crumbles",
    "Ingredients": [
        {
            "1": "1 cup brown sugar"
        },
        {
            "2": "1 cup rolled oats"
        },
        {
            "3": "1 cup all-purpose flour"
        },
        {
            "4": "0.5 cup butter, melted"
        },
        {
            "5": "3 cups apples - peeled, cored and chopped"
        },
        {
            "6": "0.5 cup white sugar"
        },
        {
            "7": "2 teaspoons ground cinnamon"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C). Lightly grease an 8-inch square pan."
        },
        {
            "2": "Place brown sugar, oats, flour, and butter in a large bowl; mix until crumbly. Place 1/2 of the crumb mixture in the bottom of the prepared pan. Spread apples evenly over crumb mixture. Sprinkle with sugar and cinnamon; top with remaining crumb mixture."
        },
        {
            "3": "Bake in the preheated oven until golden brown, 40 to 45 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Cook Time": "40 mins"
        },
        {
            "Total Time": " 1 hrs"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "1 8-inch pan"
        }
