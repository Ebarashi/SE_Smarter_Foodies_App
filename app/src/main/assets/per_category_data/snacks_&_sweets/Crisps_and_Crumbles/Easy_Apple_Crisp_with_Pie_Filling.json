{
    "title": "Easy Apple Crisp with Pie Filling",
    "Category": "Crisps and Crumbles",
    "Ingredients": [
        {
            "1": "1 (21 ounce) can apple pie filling"
        },
        {
            "2": "0.5 teaspoon ground cinnamon"
        },
        {
            "3": "0.5 teaspoon ground nutmeg"
        },
        {
            "4": "1 cup margarine"
        },
        {
            "5": "0.25 cup brown sugar"
        },
        {
            "6": "2 cups all-purpose flour"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Spread pie filling in an 8x8-inch baking dish. Sprinkle with cinnamon and nutmeg."
        },
        {
            "3": "Cream together margarine and sugar. Cut in flour with a pastry blender until the mixture resembles coarse crumbs. Sprinkle mixture over pie filling."
        },
        {
            "4": "Bake in the preheated oven until the topping is brown and crisp and the filling is hot and bubbly, about 20 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "20 mins"
        },
        {
            "Total Time": "30 mins"
        },
        {
            "Servings": "6 "
        },
        {
            "Yield": "1 8x8-inch dish"
        }
    ],
