{
    "title": "Microwaveable Maple Fudge",
    "Category": "Fudge",
    "Ingredients": [
        {
            "1": "2 cups brown sugar"
        },
        {
            "2": "1 cup whipping cream"
        },
        {
            "3": "3 teaspoons maple syrup"
        },
        {
            "4": "2 teaspoons butter"
        },
        {
            "5": "1 teaspoon vanilla extract"
        }
    ],
    "Directions": [
        {
            "1": "Combine brown sugar, cream, and maple syrup in a large microwave-safe bowl. Cook on high in the microwave for 6 minutes."
        },
        {
            "2": "Remove and stir for 1 minute. Cook for 6 minutes more. Let cool for 1 to 2 minutes. Add butter and vanilla extract. Beat with an electric mixer on medium-low speed until thick and light."
        },
        {
            "3": "Pour mixture into a small pan no larger than 8x8 inches. Cut into squares before refrigerating."
        }
    ],
