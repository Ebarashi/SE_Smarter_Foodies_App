{
    "title": "Slow Cooker Peanut Butter Fudge",
    "Category": "Fudge",
    "Ingredients": [
        {
            "1": "1 (14 ounce) can sweetened condensed milk"
        },
        {
            "2": "1 (11.5 ounce) package milk chocolate chips"
        },
        {
            "3": "10  miniature peanut butter cup candies, chopped"
        }
    ],
    "Directions": [
        {
            "1": "Combine sweetened condensed milk and chocolate chips in a slow cooker."
        },
        {
            "2": "Cook on Low with lid off, stirring every 10 minutes, until thick, about 50 minutes."
        },
        {
            "3": "Turn off slow cooker and stir in peanut butter cups. Pour mixture into a 9x13-inch pan and refrigerate for 2 hours."
        },
        {
            "4": "Cut cooled fudge into squares."
        }
    ],
