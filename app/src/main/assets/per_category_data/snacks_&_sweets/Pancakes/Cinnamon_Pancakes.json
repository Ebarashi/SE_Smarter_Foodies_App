{
    "title": "Cinnamon Pancakes",
    "Category": "Pancakes",
    "Ingredients": [
        {
            "1": "1 cup all-purpose flour"
        },
        {
            "2": "1.0 tablespoons white sugar"
        },
        {
            "3": "2 teaspoons baking powder"
        },
        {
            "4": "2 teaspoons ground cinnamon"
        },
        {
            "5": "1 teaspoon baking soda"
        },
        {
            "6": "1 cup milk"
        },
        {
            "7": "0.25 cup butter, melted"
        },
        {
            "8": "2 large eggs"
        },
        {
            "9": "1 tablespoon vegetable oil, or as needed"
        }
    ],
    "Directions": [
        {
            "1": "Mix flour, sugar, baking powder, cinnamon, and baking soda together in a bowl."
        },
        {
            "2": "Whisk milk, melted butter, and eggs together in a separate bowl. Pour milk mixture into flour mixture; whisk until well blended."
        },
        {
            "3": "Heat oil on a griddle over medium heat. Drop batter in 1/4-cup scoops onto the hot griddle. Cook until bubbles form and the edges are dry, 3 to 4 minutes. Flip and cook until browned on the other side, 2 to 3 minutes. Remove to a plate and keep warm while you cook the remaining pancakes."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "10 mins"
        },
        {
            "Total Time": "20 mins"
        },
        {
            "Servings": "10 "
        },
        {
            "Yield": "10 pancakes"
        }
