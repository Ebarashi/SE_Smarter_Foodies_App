{
    "title": "Old-Fashioned Pancakes",
    "Category": "Pancakes",
    "Ingredients": [
        {
            "1": "0.5 cups all-purpose flour"
        },
        {
            "2": "1.5 teaspoons baking powder"
        },
        {
            "3": "1 tablespoon white sugar"
        },
        {
            "4": "1 teaspoon salt"
        },
        {
            "5": "0.25 cups milk"
        },
        {
            "6": "3 tablespoons butter, melted"
        },
        {
            "7": "1  egg"
        },
        {
            "8": "1 cooking spray"
        }
    ],
    "Directions": [
        {
            "1": "Sift together flour, baking powder, sugar, and salt in a large bowl. Whisk in milk, melted butter, and egg until combined. Let batter rest for 5 minutes."
        },
        {
            "2": "Preheat a large skillet over medium-high heat. Spray with cooking spray."
        },
        {
            "3": "Pour batter into the hot skillet, about 1/4 cup batter for each pancake. Cook until bubbles appear on the sides and center of each pancake, 2 to 3 minutes. Flip and cook until the bottom is golden, 1 to 2 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Cook Time": "20 mins"
        },
