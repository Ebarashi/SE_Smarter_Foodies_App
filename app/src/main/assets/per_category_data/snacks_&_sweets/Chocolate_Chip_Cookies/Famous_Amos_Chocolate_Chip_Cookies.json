{
    "title": "Famous Amos Chocolate Chip Cookies",
    "Category": "Chocolate Chip Cookies",
    "Ingredients": [
        {
            "1": "1 cup white sugar"
        },
        {
            "2": "1 cup packed light brown sugar"
        },
        {
            "3": "1 cup butter"
        },
        {
            "4": "1 cup vegetable oil"
        },
        {
            "5": "1 large egg"
        },
        {
            "6": "1 tablespoon milk"
        },
        {
            "7": "1 teaspoon vanilla extract"
        },
        {
            "8": "4 cups all-purpose flour"
        },
        {
            "9": "1 teaspoon salt"
        },
        {
            "10": "1 teaspoon cream of tartar"
        },
        {
            "11": "1 teaspoon baking soda"
        },
        {
            "12": "2 cups semisweet chocolate chips"
        },
        {
            "13": "0.5 cup chopped walnuts"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Cream together white sugar, brown sugar, butter, oil, egg, milk, and vanilla until thoroughly combined."
        },
        {
            "3": "Add in flour, salt, cream of tartar, and baking soda. Mix in chocolate chips and walnuts."
        },
        {
            "4": "Drop dough by teaspoons onto ungreased cookie sheets. Bake in the preheated oven for 8 to 12 minutes. Cookies are light in color when done."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "10 mins"
        },
        {
            "Total Time": "25 mins"
        },
        {
            "Servings": "72 "
        },
        {
            "Yield": "6 dozen"
        }
    ],
