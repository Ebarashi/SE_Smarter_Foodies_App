{
    "title": "5-Ingredient Easy Chocolate Chip Cookies",
    "Category": "Chocolate Chip Cookies",
    "Ingredients": [
        {
            "1": "3 cups all-purpose flour"
        },
        {
            "2": "0.75 cup brown sugar"
        },
        {
            "3": "2 large eggs"
        },
        {
            "4": "1 cup butter, softened"
        },
        {
            "5": "1 cup semi-sweet chocolate chips"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Sift flour and brown sugar into a bowl; stir in eggs. Mix butter into flour mixture until dough is creamy; fold in chocolate chips. Spoon dough onto a baking sheet, keeping them about 3 inches apart."
        },
        {
            "3": "Bake in the preheated oven until slightly brown, 10 to 15 minutes. Cool."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "10 mins"
        },
        {
            "Total Time": "20 mins"
        },
        {
            "Servings": "24 "
        },
        {
            "Yield": "2 dozen cookies"
        }
    ],
