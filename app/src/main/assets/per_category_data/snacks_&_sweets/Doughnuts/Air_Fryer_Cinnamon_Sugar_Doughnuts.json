{
    "title": "Air Fryer Cinnamon-Sugar Doughnuts",
    "Category": "Doughnuts",
    "Ingredients": [
        {
            "1": "0.25 cup butter, melted"
        },
        {
            "2": "0.5 cup white sugar"
        },
        {
            "3": "0.25 cup brown sugar"
        },
        {
            "4": "1 teaspoon ground cinnamon"
        },
        {
            "5": "0.25 teaspoon ground nutmeg (Optional)"
        },
        {
            "6": "1 (16.3 ounce) package refrigerated flaky biscuit dough (such as Pillsbury\u2122 Grands!\u2122 Flaky Layers)"
        }
    ],
    "Directions": [
        {
            "1": "Place melted butter in a bowl. Combine white sugar, brown sugar, cinnamon and nutmeg in a second bowl."
        },
        {
            "2": "Separate biscuit dough into individual biscuits and cut out the centers using a biscuit cutter (or the bottom of a piping tip) to create a doughnut shape. Place doughnuts in the air fryer basket."
        },
        {
            "3": "Air fry at 350 degrees F (175 degrees C) until golden brown, 4 to 6 minutes. Flip doughnuts and cook an additional 1 to 3 minutes."
        },
        {
            "4": "Remove doughnuts from air fryer. Dip each doughnut into melted butter (making sure to coat top, bottom, and sides), followed by sugar-cinnamon mixture until completed coated. Serve immediately."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "10 mins"
        },
        {
            "Total Time": "20 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "8 doughnuts"
        }
