{
    "title": "Aunt Rose's Cheese Ball",
    "Category": "Cheese Balls",
    "Ingredients": [
        {
            "1": "2 (8 ounce) packages cream cheese, softened"
        },
        {
            "2": "0.5 cup shredded sharp Cheddar cheese"
        },
        {
            "3": "3 ounces blue cheese"
        },
        {
            "4": "0.5 cup butter, melted"
        },
        {
            "5": "0.5 cup finely chopped black olives"
        },
        {
            "6": "2 tablespoons finely chopped green onions"
        },
        {
            "7": "1 teaspoon garlic salt"
        },
        {
            "8": "2 dashes Worcestershire sauce"
        },
        {
            "9": "1 cup chopped walnuts"
        }
    ],
    "Directions": [
        {
            "1": "In a large bowl, mix the cream cheese, Cheddar cheese, blue cheese, butter, olives, green onions, garlic salt, and Worcestershire sauce. Cover, and refrigerate 30 minutes, until firm."
        },
        {
            "2": "Form the mixture into 2 balls, and roll in the walnuts to coat."
        }
    ],
