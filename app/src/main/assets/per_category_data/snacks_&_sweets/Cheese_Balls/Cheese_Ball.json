{
    "title": "Cheese Ball",
    "Category": "Cheese Balls",
    "Ingredients": [
        {
            "1": "2 (8 ounce) packages cream cheese"
        },
        {
            "2": "1 (8 ounce) can crushed pineapple, drained"
        },
        {
            "3": "1 tablespoon diced onion"
        },
        {
            "4": "1 tablespoon chopped green bell pepper"
        },
        {
            "5": "0.25 tablespoon seasoning salt"
        },
        {
            "6": "1 cup chopped pecans"
        }
    ],
    "Directions": [
        {
            "1": "Mix together the cream cheese, pineapple, onion, bell pepper and seasoning salt."
        },
        {
            "2": "Form into a ball and roll in chopped pecans. Chill and serve with butter crackers."
        }
    ],
