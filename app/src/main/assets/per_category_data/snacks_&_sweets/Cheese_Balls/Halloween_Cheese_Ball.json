{
    "title": "Halloween Cheese Ball",
    "Category": "Cheese Balls",
    "Ingredients": [
        {
            "1": "2 (8 ounce) packages cream cheese, softened"
        },
        {
            "2": "2 cups shredded Cheddar cheese"
        },
        {
            "3": "2 teaspoons chili powder"
        },
        {
            "4": "1 (8 ounce) package nacho cheese-flavored corn chips (such as Doritos\u00ae), crushed"
        },
        {
            "5": "1 bell pepper stem"
        }
    ],
    "Directions": [
        {
            "1": "Combine cream cheese, Cheddar, and chili powder until well mixed. Form into a ball and refrigerate until ready to serve."
        },
        {
            "2": "Just before serving, roll in corn chip crumbs and top with bell pepper stem to look like a pumpkin."
        }
    ],
