{
    "title": "Feta Cheese Ball",
    "Category": "Cheese Balls",
    "Ingredients": [
        {
            "1": "2 (8 ounce) packages cream cheese, softened"
        },
        {
            "2": "4 ounces feta cheese, crumbled"
        },
        {
            "3": "4 tablespoons olive oil"
        },
        {
            "4": "5  green onions, finely chopped"
        },
        {
            "5": "3 cloves garlic, minced"
        },
        {
            "6": "4 teaspoons dried dill weed"
        },
        {
            "7": "2 teaspoons dried oregano"
        },
        {
            "8": "1 teaspoon cracked black pepper"
        }
    ],
    "Directions": [
        {
            "1": "In a large bowl, combine cream cheese, feta cheese, olive oil, green onions and garlic. Season with dill, oregano and pepper. Mix well, and form into 2 balls. Cover, and chill for 2 hours."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
