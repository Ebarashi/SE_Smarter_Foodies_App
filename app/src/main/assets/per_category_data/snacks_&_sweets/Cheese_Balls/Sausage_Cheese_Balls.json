{
    "title": "Sausage Cheese Balls",
    "Category": "Cheese Balls",
    "Ingredients": [
        {
            "1": "2 pounds pork sausage"
        },
        {
            "2": "0.5 cups buttermilk baking mix"
        },
        {
            "3": "16 ounces shredded Cheddar cheese"
        },
        {
            "4": "0.5 cup diced onion"
        },
        {
            "5": "0.5 cup chopped celery"
        },
        {
            "6": "0.5 teaspoon garlic powder"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 375 degrees F (190 degrees C)."
        },
        {
            "2": "Combine the sausage, baking mix, cheddar cheese, onion, celery and garlic powder. Mix well and form into 1 inch balls."
        },
        {
            "3": "Place on ungreased cookie sheet 1/2 inch apart. Bake for 15 minutes or until golden brown."
        }
    ],
    "Details": [
        {
            "Servings": "36 "
        },
        {
            "Yield": "6 dozen"
        }
    ],
