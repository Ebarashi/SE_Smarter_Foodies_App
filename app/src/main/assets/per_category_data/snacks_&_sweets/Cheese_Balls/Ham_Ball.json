{
    "title": "Ham Ball",
    "Category": "Cheese Balls",
    "Ingredients": [
        {
            "1": "2 (8 ounce) packages cream cheese, slightly softened"
        },
        {
            "2": "4 slices ham, chopped"
        },
        {
            "3": "4  green onions, chopped"
        },
        {
            "4": "0.25 teaspoon seasoned salt, or to taste"
        },
        {
            "5": "1 cup chopped pecans, or more to taste"
        }
    ],
    "Directions": [
        {
            "1": "Mix cream cheese, ham, green onions, and seasoned salt until evenly mixed; roll into a ball."
        },
        {
            "2": "Spread chopped pecans onto a flat surface. Roll ham ball in chopped pecans to coat."
        }
    ],
