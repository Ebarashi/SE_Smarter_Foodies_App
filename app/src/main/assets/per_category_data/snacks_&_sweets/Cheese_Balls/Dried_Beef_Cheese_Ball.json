{
    "title": "Dried Beef Cheese Ball",
    "Category": "Cheese Balls",
    "Ingredients": [
        {
            "1": "8 ounces cream cheese"
        },
        {
            "2": "3 ounces dried beef"
        },
        {
            "3": "2  green onions, chopped"
        },
        {
            "4": "1 tablespoon Worcestershire sauce"
        },
        {
            "5": "0.25 teaspoons monosodium glutamate (MSG)"
        },
        {
            "6": "0.25 teaspoon onion salt"
        }
    ],
    "Directions": [
        {
            "1": "Mix cream cheese, dried beef, and green onions in a medium bowl until combined; season with Worcestershire sauce, monosodium glutamate, and onion salt. Form into a ball and refrigerate for at least 30 minutes before serving."
        }
    ],
