{
    "title": "Corned Beef Cheese Ball",
    "Category": "Cheese Balls",
    "Ingredients": [
        {
            "1": "8 ounces cream cheese, softened"
        },
        {
            "2": "1 (2.5 ounce) package deli-sliced corned beef (such as Buddig \u00ae), diced"
        },
        {
            "3": "3  green onions, finely chopped"
        },
        {
            "4": "1 tablespoon mayonnaise"
        },
        {
            "5": "2 teaspoons Worcestershire sauce"
        },
        {
            "6": "0.5 teaspoon prepared horseradish"
        },
        {
            "7": "0.5 cup chopped pecans"
        }
    ],
    "Directions": [
        {
            "1": "Combine cream cheese, corned beef, green onions, mayonnaise, Worcestershire sauce, and horseradish in a bowl until thoroughly mixed."
        },
        {
            "2": "Shape cream cheese mixture into a ball."
        },
        {
            "3": "Place pecans into a shallow bowl. Roll and coat ball in chopped pecans. Refrigerate until ready to serve."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
