{
    "title": "Party Cheese Ball",
    "Category": "Cheese Balls",
    "Ingredients": [
        {
            "1": "2 (8 ounce) packages PHILADELPHIA Cream Cheese, softened"
        },
        {
            "2": "2 (8 ounce) packages KRAFT Shredded Sharp Cheddar Cheese"
        },
        {
            "3": "1 tablespoon finely chopped onion"
        },
        {
            "4": "1 tablespoon chopped red bell pepper"
        },
        {
            "5": "2 teaspoons Worcestershire sauce"
        },
        {
            "6": "1 teaspoon lemon juice"
        },
        {
            "7": "1 dash ground red pepper"
        },
        {
            "8": "1 dash salt"
        },
        {
            "9": "1 cup chopped PLANTERS Pecans"
        }
    ],
    "Directions": [
        {
            "1": "Beat cream cheese and cheddar cheese in small bowl with electric mixer on medium speed until well blended."
        },
        {
            "2": "Mix in all remaining ingredients except pecans; cover. Refrigerate several hours or overnight."
        },
        {
            "3": "Shape into ball; roll in pecans. Serve with assorted NABISCO Crackers."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Additional Time": " 3 hrs"
        },
        {
            "Total Time": " 3 hrs 15 mins"
        },
        {
            "Servings": "24 "
        },
        {
            "Yield": "24 servings"
        }
    ],
