{
    "title": "Dried Fruit Cheese Ball",
    "Category": "Cheese Balls",
    "Ingredients": [
        {
            "1": "1 (8 ounce) package cream cheese, softened"
        },
        {
            "2": "2 tablespoons honey"
        },
        {
            "3": "0.5 (8 ounce) package mild Cheddar cheese, shredded"
        },
        {
            "4": "1 (6 ounce) package dried mixed fruit, chopped"
        },
        {
            "5": "1 cup chopped pecans"
        }
    ],
    "Directions": [
        {
            "1": "In a medium bowl combine cream cheese and honey; beat until smooth. Stir in cheese and chopped fruit; mix well."
        },
        {
            "2": "Form into a ball and roll in chopped nuts. Chill for at least 3 hours."
        }
    ],
