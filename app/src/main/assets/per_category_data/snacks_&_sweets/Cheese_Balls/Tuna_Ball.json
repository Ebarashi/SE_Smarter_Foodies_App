{
    "title": "Tuna Ball",
    "Category": "Cheese Balls",
    "Ingredients": [
        {
            "1": "2 (3 ounce) packages cream cheese, softened"
        },
        {
            "2": "1 (5 ounce) can tuna, drained and flaked"
        },
        {
            "3": "0.75 cup chopped pecans (Optional)"
        },
        {
            "4": "0.25 cup chopped onion"
        }
    ],
    "Directions": [
        {
            "1": "Blend cream cheese, tuna, 1/2 of the pecans, and onion in a bowl. Shape into a ball or loaf, then coat with remaining pecans. Refrigerate until serving."
        }
