{
    "title": "Avocado Toast (Vegan)",
    "Category": "Road Trip Snacks",
    "Ingredients": [
        {
            "1": "4 slices whole-grain bread"
        },
        {
            "2": "1  avocado, halved and pitted"
        },
        {
            "3": "2 tablespoons chopped fresh parsley"
        },
        {
            "4": "0.5 teaspoons extra-virgin olive oil"
        },
        {
            "5": "0.5  lemon, juiced"
        },
        {
            "6": "0.5 teaspoon salt"
        },
        {
            "7": "0.5 teaspoon ground black pepper"
        },
        {
            "8": "0.5 teaspoon onion powder"
        },
        {
            "9": "0.5 teaspoon garlic powder"
        }
    ],
    "Directions": [
        {
            "1": "Toast bread in a toaster or toaster oven."
        },
        {
            "2": "Scoop avocado into a bowl. Add parsley, olive oil, lemon juice, salt, pepper, onion powder, and garlic powder; mash together using a potato masher. Spread avocado mixture into each piece of toast."
        }
    ],
    "Details": [
