{
    "title": "Easy Brownie In A Mug",
    "Category": "Brownies",
    "Ingredients": [
        {
            "1": "0.25 cup all-purpose flour"
        },
        {
            "2": "0.25 cup white sugar"
        },
        {
            "3": "2 tablespoons cocoa powder"
        },
        {
            "4": "1 pinch ground cinnamon"
        },
        {
            "5": "1 pinch salt"
        },
        {
            "6": "0.25 cup water"
        },
        {
            "7": "2 tablespoons canola oil"
        },
        {
            "8": "1 splash vanilla extract"
        }
    ],
    "Directions": [
        {
            "1": "Stir flour, sugar, cocoa powder, cinnamon, and salt together in a microwave-safe mug. Stir water, oil, and vanilla extract into flour mixture with a fork until smooth."
        },
        {
            "2": "Microwave until set, 1 to 5 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
