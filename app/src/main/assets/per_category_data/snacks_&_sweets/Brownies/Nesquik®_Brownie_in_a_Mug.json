{
    "title": "Nesquik\u00ae Brownie in a Mug",
    "Category": "Brownies",
    "Ingredients": [
        {
            "1": "0.25 cup powdered chocolate drink mix (such as Nesquik\u00ae)"
        },
        {
            "2": "2 tablespoons powdered chocolate drink mix (such as Nesquik\u00ae)"
        },
        {
            "3": "0.25 cup all-purpose flour"
        },
        {
            "4": "1 pinch salt"
        },
        {
            "5": "3 tablespoons water"
        },
        {
            "6": "2 tablespoons olive oil"
        }
    ],
    "Directions": [
        {
            "1": "Mix 1/4 cup plus 2 tablespoons powdered chocolate drink mix, flour, and salt together in a microwave-safe mug. Add water and olive oil; stir thoroughly until mixture is smooth."
        },
        {
            "2": "Microwave mixture until center is fudgy and top looks dry, about 1 minute 55 seconds. Let cool for 2 minutes before serving."
        }
    ],
