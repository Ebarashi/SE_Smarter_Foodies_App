{
    "title": "Butter Swim Biscuits",
    "Category": "Biscuit",
    "Ingredients": [
        {
            "1": "0.5 cup unsalted butter"
        },
        {
            "2": "1.0 cups all-purpose flour"
        },
        {
            "3": "4 teaspoons baking powder"
        },
        {
            "4": "4 teaspoons white sugar"
        },
        {
            "5": "1 teaspoon salt"
        },
        {
            "6": "0.75 cups buttermilk"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 450 degrees F (230 degrees C)."
        },
        {
            "2": "Place butter in an 8x8-inch glass or ceramic baking dish. Microwave for 1 minute. Stir butter and keep microwaving at 20-second intervals until butter is fully melted."
        },
        {
            "3": "Sift flour, baking powder, sugar, and salt into a bowl. Add buttermilk and stir until combined. Pour over melted butter and spread over the melted butter. Use a bench scraper to cut dough into 9 equal pieces."
        },
        {
            "4": "Bake in the preheated oven until biscuit tops start to brown, 20 to 25 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Cook Time": "25 mins"
        },
        {
            "Total Time": "30 mins"
        },
        {
            "Servings": "9 "
        },
        {
            "Yield": "9 biscuits"
        }
    ],
