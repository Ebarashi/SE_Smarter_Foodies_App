{
    "title": "Easy Batter Fruit Cobbler",
    "Category": "Cobbler",
    "Ingredients": [
        {
            "1": "4 tablespoons butter"
        },
        {
            "2": "0.75 cup all-purpose flour"
        },
        {
            "3": "0.75 cup sugar"
        },
        {
            "4": "1 teaspoon baking powder"
        },
        {
            "5": "0.25 teaspoon salt"
        },
        {
            "6": "0.75 cup milk"
        },
        {
            "7": "2 cups of sliced fresh peaches or nectarines, or whole blueberries, strawberries, raspberries, blackberries or a combination of fruits (or a 12-ounce package of frozen berries)"
        },
        {
            "8": "1 tablespoon sugar"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C). Adjust the oven rack to the upper-middle position."
        },
        {
            "2": "Put butter in an 8-inch square or 9-inch round pan; place in the oven until butter is melted. Remove the pan from the oven and set aside."
        },
        {
            "3": "Whisk flour, 3/4 cup sugar, baking powder, and salt in a medium bowl. Whisk in milk until a smooth batter forms. Pour batter into the prepared pan, then scatter fruit over batter. Sprinkle with 1 tablespoon sugar."
        },
        {
            "4": "Bake in the preheated oven until the top is lightly browned and fruit bubbles, 50 to 60 minutes. Serve warm or at room temperature."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "55 mins"
        },
        {
            "Total Time": " 1 hrs 10 mins"
        },
        {
            "Servings": "4 "
        }
    ],
