{
    "title": "Easy Nachos with Refried Beans",
    "Category": "Nachos",
    "Ingredients": [
        {
            "1": "1 (16 ounce) can refried beans"
        },
        {
            "2": "1 teaspoon garlic powder"
        },
        {
            "3": "1 teaspoon onion powder"
        },
        {
            "4": "1 teaspoon ground cumin"
        },
        {
            "5": "1 (10 ounce) bag scoop-shaped tortilla chips"
        },
        {
            "6": "8 ounces Colby-Monterey Jack cheese, thinly sliced"
        },
        {
            "7": "1 (4 ounce) can sliced jalapeno peppers"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Combine refried beans, garlic powder, onion powder, and cumin in a bowl; mix well."
        },
        {
            "3": "Pour tortilla chips onto a baking sheet in one layer. Spoon bean mixture over tortilla chips. Top with Colby-Monterey Jack cheese and jalapenos."
        },
        {
            "4": "Bake in the preheated oven until cheese is melted, about 5 minutes."
        }
    ],
