{
    "title": "Buffalo Chicken Nacho Bowl",
    "Category": "Nachos",
    "Ingredients": [
        {
            "1": "1 pound shredded rotisserie chicken"
        },
        {
            "2": "0.25 cup Buffalo wing sauce"
        },
        {
            "3": "1 (16 ounce) package tortilla chips"
        },
        {
            "4": "1 (15 ounce) can black beans, rinsed and drained"
        },
        {
            "5": "1 (8 ounce) package shredded sharp Cheddar cheese"
        },
        {
            "6": "0.5 cup sour cream"
        },
        {
            "7": "0.25 cup crumbled blue cheese"
        },
        {
            "8": "1 tablespoon chopped jalapeno peppers, or to taste"
        },
        {
            "9": "1 tablespoon chopped green onion, or to taste"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 425 degrees F (220 degrees C)."
        },
        {
            "2": "Combine chicken and Buffalo wing sauce in a bowl."
        },
        {
            "3": "Place 1 layer tortilla chips in the bottom of a baking dish; top with a layer of Buffalo chicken mixture, black beans, and Cheddar cheese. Continue layering with remaining tortilla chips, Buffalo chicken mixture, black beans, and Cheddar cheese."
        },
        {
            "4": "Bake in the preheated oven until cheese is melted, 10 to 15 minutes. Cover the nachos with aluminum foil if the top is getting too dark."
        },
        {
            "5": "Top nachos with sour cream, blue cheese, jalapeno peppers, and green onion."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
