{
    "title": "Blueberry Turnovers",
    "Category": "Danishes",
    "Ingredients": [
        {
            "1": "1 (8 ounce) package refrigerated crescent rolls"
        },
        {
            "2": "0.5 cup fresh blueberries"
        },
        {
            "3": "0.25 cup confectioners' sugar"
        },
        {
            "4": "0.25 cup prepared vanilla frosting (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 375 degrees F (190 degrees C)."
        },
        {
            "2": "Roll out crescent dough triangles onto a baking sheet."
        },
        {
            "3": "Place 1 tablespoon blueberries on the widest end of each triangle. Sprinkle 1/2 teaspoon confectioners' sugar over blueberries on each roll. Beginning with the wide end, roll up each crescent around blueberries; pinch both sides to seal completely."
        },
        {
            "4": "Bake in the preheated oven until golden, about 12 minutes. Remove to cool on a wire rack for 5 minutes."
        },
        {
            "5": "Dust turnovers with remaining confectioners' sugar. Drizzle with vanilla frosting."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "15 mins"
        },
        {
            "Additional Time": "5 mins"
        },
        {
