{
    "title": "Portuguese Coconut Cakes (Bolos de Coco)",
    "Category": "Macaroons",
    "Ingredients": [
        {
            "1": "0.75 cups shredded coconut"
        },
        {
            "2": "0.75 cup white sugar"
        },
        {
            "3": "3  eggs"
        },
        {
            "4": "1 tablespoon grated lemon zest"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C). Line 3 muffin cups with paper liners."
        },
        {
            "2": "Mix coconut, sugar, eggs, and lemon zest together in a large bowl. Scoop coconut mixture into lined muffin cups."
        },
        {
            "3": "Bake in the preheated oven until firm and golden, 15 to 20 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "15 mins"
        },
        {
            "Total Time": "25 mins"
        },
        {
            "Servings": "3 "
        },
        {
            "Yield": "3 coconut cakes"
        }
    ],
