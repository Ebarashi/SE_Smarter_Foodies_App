{
    "title": "Caramel Graham Crackers",
    "Category": "Toffee",
    "Ingredients": [
        {
            "1": "0.5 cup butter"
        },
        {
            "2": "0.5 cup margarine"
        },
        {
            "3": "1 cup brown sugar"
        },
        {
            "4": "24  graham crackers, broken into rectangles"
        },
        {
            "5": "1 cup chopped pecans"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C). Line a jelly roll pan with aluminum foil."
        },
        {
            "2": "Melt butter and margarine together in a saucepan; stir brown sugar into butter mixture until dissolved; bring to a boil, reduce heat to medium, and cook at a boil until caramelized, about 2 minutes."
        },
        {
            "3": "Spread graham crackers in a single layer onto prepared aluminum foil. Pour butter mixture over graham crackers. Sprinkle pecans over the crackers."
        },
        {
            "4": "Bake in preheated oven until topping begins to solidify, about 12 minutes. Let crackers cool before breaking into pieces to serve."
        }
    ],
