{
    "title": "Grandma's Lace Cookies",
    "Category": "Oatmeal Cookies",
    "Ingredients": [
        {
            "1": "1 cooking spray"
        },
        {
            "2": "0.5 cup butter"
        },
        {
            "3": "0.75 cup white sugar"
        },
        {
            "4": "1  egg, beaten"
        },
        {
            "5": "1 cup quick cooking oats (such as Quaker\u2122)"
        },
        {
            "6": "3 tablespoons all-purpose flour"
        },
        {
            "7": "1 teaspoon vanilla extract"
        },
        {
            "8": "0.25 teaspoon salt"
        },
        {
            "9": "0.25 teaspoon baking powder"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C). Line a baking sheet with aluminum foil and spray with cooking spray."
        },
        {
            "2": "Melt butter in a microwave-safe bowl in a microwave. Add sugar and stir to combine. Add beaten egg and mix well. Stir in oats, flour, vanilla, salt, and baking powder."
        },
        {
            "3": "Drop teaspoonfuls of dough onto the prepared baking sheet, leaving plenty of room to spread."
        },
        {
            "4": "Bake in the preheated oven until edges are golden brown, about 8 minutes. Let cool completely before removing cookies from foil."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "8 mins"
        },
        {
            "Total Time": "18 mins"
        },
        {
            "Servings": "10 "
        },
        {
            "Yield": "10 cookies"
        }
    ],
