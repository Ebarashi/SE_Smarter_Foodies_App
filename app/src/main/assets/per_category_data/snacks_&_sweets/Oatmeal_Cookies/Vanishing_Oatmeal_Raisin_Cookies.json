{
    "title": "Vanishing Oatmeal Raisin Cookies",
    "Category": "Oatmeal Cookies",
    "Ingredients": [
        {
            "1": "14 tablespoons butter, softened"
        },
        {
            "2": "0.75 cup firmly packed brown sugar"
        },
        {
            "3": "0.5 cup granulated sugar"
        },
        {
            "4": "2  eggs"
        },
        {
            "5": "1 teaspoon vanilla"
        },
        {
            "6": "0.5 cups all-purpose flour"
        },
        {
            "7": "1 teaspoon baking soda"
        },
        {
            "8": "1 teaspoon ground cinnamon"
        },
        {
            "9": "0.5 teaspoon salt (Optional)"
        },
        {
            "10": "3 cups Quaker\u00ae Oats (Quick or Old Fashioned, uncooked)"
        },
        {
            "11": "1 cup raisins"
        }
    ],
    "Directions": [
        {
            "1": "Heat oven to 350 degrees F."
        },
        {
            "2": "In large bowl, beat butter and sugars on medium speed of electric mixer until creamy. Add eggs and vanilla; beat well."
        },
        {
            "3": "Add combined flour, baking soda, cinnamon and salt; mix well."
        },
        {
            "4": "Add oats and raisins; mix well."
        },
        {
            "5": "Drop dough by rounded tablespoonfuls onto ungreased cookie sheets."
        },
        {
            "6": "Bake 8 to 10 minutes or until light golden brown."
        },
        {
            "7": "Cool 1 minute on cookie sheets; remove to wire rack. Cool completely. Store tightly covered."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Cook Time": "8 mins"
        },
