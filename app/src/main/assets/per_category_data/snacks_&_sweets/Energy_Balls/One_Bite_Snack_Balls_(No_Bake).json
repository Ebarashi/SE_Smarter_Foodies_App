{
    "title": "One-Bite Snack Balls (No-Bake)",
    "Category": "Energy Balls",
    "Ingredients": [
        {
            "1": "0.5 cup rolled oats"
        },
        {
            "2": "0.25 cup grated carrots"
        },
        {
            "3": "0.25 cup chopped dates"
        },
        {
            "4": "3 tablespoons brown sugar"
        },
        {
            "5": "1 teaspoon ground cinnamon"
        },
        {
            "6": "1 teaspoon vanilla extract"
        },
        {
            "7": "0.25 cup raisins"
        },
        {
            "8": "0.25 cup honey"
        }
    ],
    "Directions": [
        {
            "1": "Combine oats and carrots in the bowl of a food processor; pulse until blended. Add dates, brown sugar, cinnamon, and vanilla; blend again."
        },
        {
            "2": "Transfer mixture to a bowl and stir in raisins and honey. Roll into 8 balls."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Total Time": "10 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "8 balls"
        }
    ],
