{
    "title": "Black Garlic Chocolate Protein Bites",
    "Category": "Energy Balls",
    "Ingredients": [
        {
            "1": "0.5 cup roasted honey peanut butter (such as Skippy\u00ae)"
        },
        {
            "2": "0.5 cup chocolate protein powder"
        },
        {
            "3": "2  solo black garlic bulbs"
        },
        {
            "4": "2 tablespoons Neufchatel cheese, softened"
        },
        {
            "5": "2 tablespoons sanding sugar, or as needed"
        }
    ],
    "Directions": [
        {
            "1": "Combine peanut butter, protein powder, black garlic, and Neufchatel cheese in a mini food processor. Pulse until well combined."
        },
        {
            "2": "Roll mixture into sixteen 1-inch balls. Roll in sanding sugar. Serve immediately or store in an airtight container in the refrigerator."
        }
    ],
