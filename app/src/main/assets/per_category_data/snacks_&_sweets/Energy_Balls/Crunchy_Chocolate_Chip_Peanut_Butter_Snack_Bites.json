{
    "title": "Crunchy Chocolate Chip Peanut Butter Snack Bites",
    "Category": "Energy Balls",
    "Ingredients": [
        {
            "1": "1 cup large flake rolled oats"
        },
        {
            "2": "0.5 cup KRAFT Crunchy Peanut Butter"
        },
        {
            "3": "0.25 cup honey"
        },
        {
            "4": "0.5 cup salted peanuts, chopped"
        },
        {
            "5": "0.5 cup BAKER'S Semi-Sweet Chocolate Chips"
        }
    ],
    "Directions": [
        {
            "1": "Mix together all ingredients in a medium bowl until blended. Refrigerate for 20 to 30 min."
        },
        {
            "2": "Roll into 20 (1-inch) balls, using about 2 Tbsp. for each."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
