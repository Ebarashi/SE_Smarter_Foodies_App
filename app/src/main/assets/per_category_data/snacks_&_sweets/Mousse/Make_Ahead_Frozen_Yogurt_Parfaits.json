{
    "title": "Make-Ahead Frozen Yogurt Parfaits",
    "Category": "Mousse",
    "Ingredients": [
        {
            "1": "1 For the Parfaits-"
        },
        {
            "2": "0.5 cups raspberries, rinsed and drained"
        },
        {
            "3": "0.5 cup white sugar"
        },
        {
            "4": "1 cup blueberries, rinsed and drained"
        },
        {
            "5": "0.5 cups granola"
        },
        {
            "6": "1 For the Frozen Yogurt-"
        },
        {
            "7": "2 cups full-fat plain Greek yogurt"
        },
        {
            "8": "1 teaspoon vanilla extract"
        },
        {
            "9": "1 teaspoon lemon zest"
        },
        {
            "10": "0.5 cup white sugar"
        }
    ],
    "Directions": [
        {
            "1": "Combine raspberries and \u00bd cup white sugar in a bowl. Crush with a potato masher into a puree. Cover and let sit out for 1 hour."
        },
        {
            "2": "Pass berry puree through a fine mesh strainer to remove seeds. Add blueberries, stir, and keep in the refrigerator until needed."
        },
        {
            "3": "Whisk yogurt, vanilla, lemon zest, and sugar for frozen yogurt together in a bowl until sugar dissolves and the mixture is light and fluffy. Transfer into a piping bag and refrigerate until needed."
        }
    ],
