{
    "title": "Pumpkin Pie Mousse",
    "Category": "Mousse",
    "Ingredients": [
        {
            "1": "1 cup canned pumpkin"
        },
        {
            "2": "0.5 cup confectioners' sugar"
        },
        {
            "3": "2 teaspoons ground cinnamon"
        },
        {
            "4": "0.25 teaspoon ground nutmeg"
        },
        {
            "5": "2 cups whipped topping"
        }
    ],
    "Directions": [
        {
            "1": "Stir pumpkin, sugar, cinnamon, and nutmeg together in a medium bowl. Fold in whipped topping. Cover and refrigerate for 30 minutes."
        }
