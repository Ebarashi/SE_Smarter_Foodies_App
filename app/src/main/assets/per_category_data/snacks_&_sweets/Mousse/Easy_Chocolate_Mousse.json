{
    "title": "Easy Chocolate Mousse",
    "Category": "Mousse",
    "Ingredients": [
        {
            "1": "1 (3.9 ounce) package instant chocolate pudding mix"
        },
        {
            "2": "0.5 cups milk"
        },
        {
            "3": "1 (16 ounce) container frozen whipped topping, thawed"
        }
    ],
    "Directions": [
        {
            "1": "Prepare the pudding in a large bowl using 1 1/2 cups milk instead of the suggested 2 cups."
        },
        {
            "2": "Fold in the whipped topping until blended. Refrigerate until chilled and serve."
        }
    ],
