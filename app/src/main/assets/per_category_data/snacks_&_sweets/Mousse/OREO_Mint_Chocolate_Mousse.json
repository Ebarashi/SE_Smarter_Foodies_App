{
    "title": "OREO Mint Chocolate Mousse",
    "Category": "Mousse",
    "Ingredients": [
        {
            "1": "1 (3.9 ounce) package chocolate instant pudding mix"
        },
        {
            "2": "2 cups cold milk"
        },
        {
            "3": "14  Mint OREO Fudge Cremes, divided"
        },
        {
            "4": "1 cup thawed frozen whipped topping"
        },
        {
            "5": "4  fresh strawberries, cut in half"
        },
        {
            "6": "8  fresh mint leaves"
        }
    ],
    "Directions": [
        {
            "1": "Beat pudding mix and milk in medium bowl with whisk 2 minutes."
        },
        {
            "2": "Chop 10 cookies coarsely; stir into pudding with whipped topping until blended. Spoon into 8 dessert dishes."
        },
        {
            "3": "Refrigerate 1 hour. Cut remaining cookies in half. Top desserts with cookie halves and fruit. Garnish with mint, if desired."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Additional Time": " 1 hrs"
        },
        {
            "Total Time": " 1 hrs 15 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "8 servings"
        }
    ],
