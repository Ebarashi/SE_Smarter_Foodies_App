{
    "title": "Papaya Boats",
    "Category": "Mousse",
    "Ingredients": [
        {
            "1": "1 cup fat-free plain yogurt"
        },
        {
            "2": "0.25 cup walnuts"
        },
        {
            "3": "0.25 cup raisins"
        },
        {
            "4": "1 cup chopped fresh strawberries"
        },
        {
            "5": "2  medium papayas, cut in half lengthwise and seeded"
        },
        {
            "6": "2 tablespoons honey"
        }
    ],
    "Directions": [
        {
            "1": "In a bowl, mix the yogurt, walnuts, and raisins. Fold in the strawberries. Spoon the mixture into the centers of the papaya halves. Drizzle with honey to serve."
        }
    ],
