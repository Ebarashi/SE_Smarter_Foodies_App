{
    "title": "Fresh Fig Cookies",
    "Category": "Drop Cookies",
    "Ingredients": [
        {
            "1": "1 cup white sugar"
        },
        {
            "2": "0.5 cup shortening"
        },
        {
            "3": "1  egg"
        },
        {
            "4": "2 cups all-purpose flour"
        },
        {
            "5": "1 teaspoon baking soda"
        },
        {
            "6": "1 teaspoon baking powder"
        },
        {
            "7": "0.5 teaspoon salt"
        },
        {
            "8": "0.5 teaspoon ground cloves"
        },
        {
            "9": "1 cup chopped fresh figs"
        },
        {
            "10": "0.5 cup chopped walnuts"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (180 degrees C)."
        },
        {
            "2": "Cream sugar and shortening and add beaten egg."
        },
        {
            "3": "Sift dry ingredients and blend with creamed mixture. Fold in figs and nuts."
        },
        {
            "4": "Drop by spoonfuls on greased sheet. Bake for 15 to 20 minutes."
        }
    ],
    "Details": [
        {
            "Servings": "18 "
        },
        {
            "Yield": "3 dozen"
        }
    ],
