{
    "title": "Mincemeat Cookies II",
    "Category": "Drop Cookies",
    "Ingredients": [
        {
            "1": "0.5 cups packed brown sugar"
        },
        {
            "2": "1 cup shortening"
        },
        {
            "3": "1  egg"
        },
        {
            "4": "2 cups prepared mincemeat pie filling"
        },
        {
            "5": "0.75 cups all-purpose flour"
        },
        {
            "6": "0.25 teaspoon salt"
        },
        {
            "7": "0.5 teaspoons baking soda"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 425 degrees F (220 degrees C)."
        },
        {
            "2": "Cream together brown sugar and shortening."
        },
        {
            "3": "Add in egg and mincemeat and mix well. Sift together flour, salt and baking soda and stir into mincemeat mixture."
        },
        {
            "4": "Drop onto cookie sheet and bake 7 - 10 minutes."
        }
    ],
    "Details": [
        {
            "Servings": "18 "
        },
        {
            "Yield": "3 dozen"
        }
    ],
