{
    "title": "Easy Kids' Recipe for Fluffy Banana Cookies",
    "Category": "Drop Cookies",
    "Ingredients": [
        {
            "1": "2  ripe bananas, mashed, or more to taste"
        },
        {
            "2": "0.5 cups all-purpose flour"
        },
        {
            "3": "0.5 cup white sugar, or more to taste"
        },
        {
            "4": "0.25 cup butter, melted"
        },
        {
            "5": "0.25 cup chopped walnuts (Optional)"
        },
        {
            "6": "1  egg"
        },
        {
            "7": "1 teaspoon baking soda"
        },
        {
            "8": "0.25 teaspoon salt"
        },
        {
            "9": "1 tablespoon white sugar"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 325 degrees F (165 degrees C). Line a baking sheet with parchment paper."
        },
        {
            "2": "Place bananas, flour, 1/2 cup sugar, butter, walnuts, egg, baking soda, and salt in a bowl. Mix until well combined. Drop 3 tablespoonfuls of dough 2 to 3 inches apart onto baking sheet. Sprinkle 1 tablespoon sugar on top."
        },
        {
            "3": "Bake in the preheated oven until golden brown and edges are set, 15 to 20 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "15 mins"
        },
        {
            "Total Time": "25 mins"
        },
        {
            "Servings": "12 "
        },
        {
            "Yield": "1 9x13-inch baking sheet"
        }
    ],
