{
    "title": "Salted Cashew Sweet Potato Fudge",
    "Category": "Chocolate Fudge",
    "Ingredients": [
        {
            "1": "12 ounces white chocolate chips"
        },
        {
            "2": "0.5 cup Bruce's\u00ae Yams Cut Sweet Potatoes in Syrup, drained and mashed"
        },
        {
            "3": "1 tablespoon heavy cream, warmed"
        },
        {
            "4": "1 teaspoon vanilla extract, warmed"
        },
        {
            "5": "1 teaspoon salt"
        },
        {
            "6": "0.25 teaspoon nutmeg"
        },
        {
            "7": "0.25 cup smooth cashew butter"
        },
        {
            "8": "0.25 cup chopped salted cashews"
        }
    ],
    "Directions": [
        {
            "1": "Spray a 4 x 5-inch glass dish with non-stick baking spray; set aside."
        },
        {
            "2": "Warm a glass bowl and add white chocolate chips. Heat in microwave, stopping every 20 seconds to stir."
        },
        {
            "3": "When melted, stir in mashed Bruce's\u00ae Yams Cut Sweet Potatoes, cream, vanilla, salt and nutmeg. Mix quickly, then add cashew butter and continue to mix until very smooth. Pour mixture into prepared dish. Sprinkle with chopped cashews (patting them gently into fudge)."
        },
        {
            "4": "Refrigerate for a minimum of 2 hours."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Additional Time": " 2 hrs"
        },
        {
            "Total Time": " 2 hrs 10 mins"
        },
        {
            "Servings": "16 "
        },
        {
            "Yield": "16 small squares"
        }
    ],
