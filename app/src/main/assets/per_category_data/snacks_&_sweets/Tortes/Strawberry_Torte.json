{
    "title": "Strawberry Torte",
    "Category": "Tortes",
    "Ingredients": [
        {
            "1": "11 ounces cream cheese, softened"
        },
        {
            "2": "0.75 cup white sugar"
        },
        {
            "3": "2 teaspoons vanilla extract"
        },
        {
            "4": "1 pint heavy whipping cream"
        },
        {
            "5": "2 (12 ounce) packages ladyfinger cookies"
        },
        {
            "6": "1 (16 ounce) package frozen strawberries"
        },
        {
            "7": "1 tablespoon cornstarch"
        }
    ],
    "Directions": [
        {
            "1": "Strain strawberries and set aside, reserving juices. In a saucepan bring cornstarch and strawberry juice to a gentle boil. Remove from heat and let cool."
        },
        {
            "2": "Whip cream cheese, sugar, and vanilla together in a large bowl."
        },
        {
            "3": "In a separate bowl beat whipping cream until stiff peaks form. Fold whipped cream into cream cheese mixture."
        },
        {
            "4": "Arrange ladyfingers around the sides and bottom of an 8-inch springform pan, standing ladyfingers lengthwise around the sides of the pan. Pour 1/2 of the filling into the pan, then arrange a layer of ladyfingers on top of the filling. Pour remaining filling over ladyfingers."
        },
        {
            "5": "Spread strawberry sauce over top of cake and arrange whole strawberries on top. Refrigerate overnight until thoroughly chilled."
        },
        {
            "6": "Remove from pan to serve."
        }
    ],
    "Details": [
        {
            "Prep Time": "25 mins"
        },
        {
            "Cook Time": "10 mins"
        },
        {
            "Additional Time": " 8 hrs"
        },
