{
    "title": "Bavarian-Style Apple Torte",
    "Category": "Tortes",
    "Ingredients": [
        {
            "1": "0.75 cup butter or margarine, softened"
        },
        {
            "2": "0.5 teaspoon vanilla extract"
        },
        {
            "3": "0.5 cups all-purpose flour"
        },
        {
            "4": "0.5 cup white sugar"
        },
        {
            "5": "2 (8 ounce) packages cream cheese, softened"
        },
        {
            "6": "0.25 cup white sugar"
        },
        {
            "7": "2  eggs"
        },
        {
            "8": "0.75 teaspoon vanilla extract"
        },
        {
            "9": "3 cups apples - peeled, cored and sliced thinly"
        },
        {
            "10": "1 teaspoon ground cinnamon"
        },
        {
            "11": "0.5 cup white sugar"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Mix together the butter, 1/2 teaspoon vanilla extract, all-purpose flour, and 1/2 cup of sugar in a bowl. Press the mixture into the bottom of an ungreased 9-inch springform pan."
        },
        {
            "3": "Beat together the cream cheese and sugar in a large bowl. Stir in the eggs and 3/4 teaspoons vanilla extract. Pour the egg mixture over the crust."
        },
        {
            "4": "Combine the sliced apples, cinnamon, and 1/2 cup sugar in a bowl. Spoon apple mixture evenly over the cream cheese mixture. Place the springform pan on a baking sheet."
        },
        {
            "5": "Bake in preheated oven until center is set, 50 to 65 minutes."
        }
    ],
