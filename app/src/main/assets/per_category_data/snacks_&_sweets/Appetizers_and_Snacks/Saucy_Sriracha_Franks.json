{
    "title": "Saucy Sriracha Franks",
    "Category": "Appetizers and Snacks",
    "Ingredients": [
        {
            "1": "1 cup ketchup"
        },
        {
            "2": "0.25 cup soy sauce"
        },
        {
            "3": "0.25 cup rice vinegar"
        },
        {
            "4": "3 tablespoons sriracha sauce"
        },
        {
            "5": "1 tablespoon brown sugar"
        },
        {
            "6": "1 teaspoon sesame oil"
        },
        {
            "7": "1 teaspoon grated ginger root"
        },
        {
            "8": "1 clove garlic, crushed"
        },
        {
            "9": "1 (12 ounce) package all-beef franks, cut into 1-inch slices"
        },
        {
            "10": "1 box toothpicks, or as needed"
        }
    ],
    "Directions": [
        {
            "1": "Mix ketchup, soy sauce, vinegar, sriracha, brown sugar, sesame oil, ginger, and garlic in a saucepan and bring to a simmer over low heat. Add franks and heat until warm, about 5 minutes. Serve with toothpicks."
        }
    ],
