{
    "title": "Slow Cooker Split Pea Sausage Soup",
    "Category": "Split Pea Soup",
    "Ingredients": [
        {
            "1": "1 pound dried split peas"
        },
        {
            "2": "10 cups water"
        },
        {
            "3": "1 pound smoked sausage of your choice, sliced"
        },
        {
            "4": "5 cubes  chicken bouillon"
        },
        {
            "5": "0.5 cups chopped carrot"
        },
        {
            "6": "1 cup chopped celery"
        },
        {
            "7": "2  potatoes, peeled and chopped"
        },
        {
            "8": "0.5 teaspoon garlic powder"
        },
        {
            "9": "0.5 teaspoon dried oregano"
        },
        {
            "10": "2  bay leaves"
        },
        {
            "11": "1  onion, chopped"
        }
    ],
    "Directions": [
        {
            "1": "In a 5-quart slow cooker, combine peas, water, sausage, bouillon, carrot, celery, potatoes, garlic powder, oregano, bay leaves, and onion."
        },
        {
            "2": "Cover and cook on High for 4 to 5 hours. Remove bay leaves before ladling into bowls."
        }
    ],
    "Details": [
