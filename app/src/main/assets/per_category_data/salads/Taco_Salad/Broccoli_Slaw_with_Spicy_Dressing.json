{
    "title": "Broccoli Slaw with Spicy Dressing",
    "Category": "Taco Salad",
    "Ingredients": [
        {
            "1": "1 (12 ounce) package broccoli coleslaw mix"
        },
        {
            "2": "0.5 cup light mayonnaise"
        },
        {
            "3": "2  limes, juiced"
        },
        {
            "4": "2 teaspoons white sugar"
        },
        {
            "5": "1 teaspoon brown sugar"
        },
        {
            "6": "0.5 teaspoon chili powder"
        },
        {
            "7": "0.5 teaspoon ground cumin"
        },
        {
            "8": "0.25 teaspoon ground cayenne pepper"
        }
    ],
    "Directions": [
        {
            "1": "Place broccoli coleslaw mix in a bowl."
        },
        {
            "2": "Mix mayonnaise, lime juice, white sugar, brown sugar, chili powder, cumin, and cayenne pepper together in a separate bowl. Pour over the broccoli slaw."
        }
    ],
