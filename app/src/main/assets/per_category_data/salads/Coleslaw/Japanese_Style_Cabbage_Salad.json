{
    "title": "Japanese-Style Cabbage Salad",
    "Category": "Coleslaw",
    "Ingredients": [
        {
            "1": "0.5 large head cabbage, cored and shredded"
        },
        {
            "2": "1 bunch green onions, thinly sliced"
        },
        {
            "3": "1 cup almond slivers"
        },
        {
            "4": "0.25 cup toasted sesame seeds"
        },
        {
            "5": "1 Dressing-"
        },
        {
            "6": "3 tablespoons sesame oil"
        },
        {
            "7": "3 tablespoons rice vinegar"
        },
        {
            "8": "1 tablespoon white sugar (Optional)"
        },
        {
            "9": "1 clove garlic, minced (Optional)"
        },
        {
            "10": "1 teaspoon grated fresh ginger root (Optional)"
        },
        {
            "11": "1 teaspoon salt"
        },
        {
            "12": "1 teaspoon black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Combine cabbage, green onions, almond, and sesame seeds in a large bowl."
        },
        {
            "2": "Make dressing: Whisk together sesame oil, vinegar, sugar, garlic, ginger, salt, and pepper in a small bowl until sugar is dissolved."
        },
        {
            "3": "Pour dressing over cabbage mixture and toss until well combined."
        }
    ],
    "Details": [
        {
            "Prep Time": "25 mins"
        },
        {
            "Total Time": "25 mins"
        },
        {
            "Servings": "12 "
