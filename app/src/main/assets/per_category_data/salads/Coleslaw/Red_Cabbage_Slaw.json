{
    "title": "Red Cabbage Slaw",
    "Category": "Coleslaw",
    "Ingredients": [
        {
            "1": "1 small head red cabbage, thinly sliced and chopped"
        },
        {
            "2": "0.5 cup grated carrot"
        },
        {
            "3": "0.5 cup mayonnaise"
        },
        {
            "4": "0.25 cup dried cranberries"
        },
        {
            "5": "0.25 cup chopped walnuts"
        },
        {
            "6": "1 tablespoon milk, or more as needed"
        },
        {
            "7": "1 tablespoon apple cider vinegar"
        },
        {
            "8": "1 teaspoon white sugar, or more to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine cabbage, carrot, mayonnaise, cranberries, walnuts, milk, cider vinegar, and sugar in a bowl; stir well. Cover and refrigerate until chilled, about 4 hours."
        }
    ],
