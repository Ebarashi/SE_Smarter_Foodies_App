{
    "title": "Claremont Salad",
    "Category": "Coleslaw",
    "Ingredients": [
        {
            "1": "1 medium head cabbage, shredded"
        },
        {
            "2": "2  cucumbers, thinly sliced"
        },
        {
            "3": "3  carrots, thinly sliced"
        },
        {
            "4": "1  Bermuda onion, thinly sliced"
        },
        {
            "5": "0.75 cup distilled white vinegar"
        },
        {
            "6": "0.75 cup white sugar"
        },
        {
            "7": "0.5 cup vegetable oil"
        },
        {
            "8": "4 tablespoons water"
        },
        {
            "9": "3 teaspoons salt"
        }
    ],
    "Directions": [
        {
            "1": "Combine cabbage, cucumbers, carrots, and onion in a large bowl."
        },
        {
            "2": "Whisk together vinegar, sugar, oil, water, and salt in a small bowl. Pour over the cabbage mixture, toss to coat, and refrigerate for at least 12 hours. Drain excess liquid before serving."
        }
    ],
    "Details": [
