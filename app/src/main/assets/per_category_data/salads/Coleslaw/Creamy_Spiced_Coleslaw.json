{
    "title": "Creamy Spiced Coleslaw",
    "Category": "Coleslaw",
    "Ingredients": [
        {
            "1": "1 cup mayonnaise"
        },
        {
            "2": "2 tablespoons sugar"
        },
        {
            "3": "0.5 teaspoon salt"
        },
        {
            "4": "0.5 teaspoon pepper"
        },
        {
            "5": "0.5 teaspoon celery seed"
        },
        {
            "6": "0.5 teaspoon garlic powder"
        },
        {
            "7": "0.5 teaspoon onion powder"
        },
        {
            "8": "2 tablespoons cider vinegar"
        },
        {
            "9": "1 (16 ounce) package shredded coleslaw mix"
        }
    ],
    "Directions": [
        {
            "1": "In a large bowl, mix mayonnaise, sugar, salt, pepper, celery seed, garlic powder, onion powder and cider vinegar. Toss coleslaw mix with dressing to coat. Chill at least 1 hour before serving."
        }
    ],
