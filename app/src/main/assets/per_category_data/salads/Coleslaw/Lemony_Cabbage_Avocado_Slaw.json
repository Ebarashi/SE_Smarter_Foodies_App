{
    "title": "Lemony Cabbage-Avocado Slaw",
    "Category": "Coleslaw",
    "Ingredients": [
        {
            "1": "6 cups finely shredded white and red cabbage"
        },
        {
            "2": "1  ripe avocado, diced"
        },
        {
            "3": "1 small red bell pepper, chopped"
        },
        {
            "4": "0.25 cup shelled hemp seeds"
        },
        {
            "5": "3 tablespoons lemon juice"
        },
        {
            "6": "3 tablespoons chopped fresh cilantro"
        },
        {
            "7": "2 tablespoons finely chopped red onion"
        },
        {
            "8": "0.25 teaspoon fine sea salt"
        }
    ],
    "Directions": [
        {
            "1": "Combine cabbage, avocado, red bell pepper, hemp seeds, lemon juice, cilantro, onion, and sea salt in a large bowl; toss together until slaw has a creamy consistency."
        }
    ],
