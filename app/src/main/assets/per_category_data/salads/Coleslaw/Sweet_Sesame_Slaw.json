{
    "title": "Sweet Sesame Slaw",
    "Category": "Coleslaw",
    "Ingredients": [
        {
            "1": "1 head green cabbage, shredded"
        },
        {
            "2": "1 large carrot, grated"
        },
        {
            "3": "1 cup grated jicama"
        },
        {
            "4": "1 tablespoon black sesame seeds"
        },
        {
            "5": "1 cup mayonnaise"
        },
        {
            "6": "2 tablespoons rice vinegar"
        },
        {
            "7": "1 tablespoon sesame oil"
        },
        {
            "8": "3 tablespoons honey"
        }
    ],
    "Directions": [
        {
            "1": "Place cabbage, carrot, jicama, and sesame seeds in a large bowl."
        },
        {
            "2": "Whisk together mayonnaise, vinegar, sesame oil, and honey in a small bowl."
        },
        {
            "3": "Drizzle the dressing over the salad; toss until evenly coated. Chill at least 4 hours before serving."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
