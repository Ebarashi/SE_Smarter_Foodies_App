{
    "title": "California Coleslaw",
    "Category": "Coleslaw",
    "Ingredients": [
        {
            "1": "1 small head cabbage, shredded"
        },
        {
            "2": "1 small white onion, chopped"
        },
        {
            "3": "1  green bell pepper, chopped"
        },
        {
            "4": "1  red bell pepper, chopped"
        },
        {
            "5": "1 small carrot, shredded"
        },
        {
            "6": "0.5 cup cider vinegar"
        },
        {
            "7": "3 tablespoons white sugar"
        },
        {
            "8": "0.5 teaspoon salt"
        },
        {
            "9": "0.25 teaspoon freshly ground black pepper"
        },
        {
            "10": "0.5 cup vegetable oil"
        }
    ],
    "Directions": [
        {
            "1": "In a large salad bowl, place the cabbage, onion, green bell pepper, red bell pepper and carrot. Combine the vinegar, sugar, salt, pepper and vegetable oil in the bowl with the vegetables. Toss the mixture until the vegetables are fully coated with the marinade."
        }
    ],
