{
    "title": "Cucumber Slaw",
    "Category": "Coleslaw",
    "Ingredients": [
        {
            "1": "2  cucumbers - quartered, seeded, and thinly sliced"
        },
        {
            "2": "2  carrots, shredded"
        },
        {
            "3": "0.25  onion, grated"
        },
        {
            "4": "2 tablespoons white sugar"
        },
        {
            "5": "0.5 teaspoons ground coriander"
        },
        {
            "6": "1 teaspoon kosher salt"
        },
        {
            "7": "0.5 teaspoon coarse ground black pepper"
        },
        {
            "8": "0.25 cup apple cider vinegar"
        }
    ],
    "Directions": [
        {
            "1": "Combine cucumbers, carrots, onion, sugar, coriander, salt, and black pepper together in a bowl; add vinegar and mix well."
        }
    ],
