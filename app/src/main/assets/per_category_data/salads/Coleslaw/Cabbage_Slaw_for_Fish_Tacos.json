{
    "title": "Cabbage Slaw for Fish Tacos",
    "Category": "Coleslaw",
    "Ingredients": [
        {
            "1": "0.5 medium head cabbage, shredded"
        },
        {
            "2": "0.5 cup chopped fresh cilantro leaves"
        },
        {
            "3": "0.25 cup thinly sliced red onion"
        },
        {
            "4": "1 medium jalapeno pepper, seeded and diced"
        },
        {
            "5": "0.25 cup olive oil"
        },
        {
            "6": "0.5 tablespoons fresh lime juice"
        },
        {
            "7": "0.5 tablespoons red wine vinegar"
        },
        {
            "8": "0.5 teaspoon salt"
        },
        {
            "9": "1 pinch white sugar"
        }
    ],
    "Directions": [
        {
            "1": "Place cabbage in a large bowl with cilantro, onion, and jalapeno; toss to mix."
        },
        {
            "2": "Mix oil, lime juice, vinegar, salt, and sugar together in a small bowl. Pour over cabbage mixture and toss to coat. Let sit on the counter for 1 to 2 hours, then drain. Serve at room temperature."
        }
    ],
