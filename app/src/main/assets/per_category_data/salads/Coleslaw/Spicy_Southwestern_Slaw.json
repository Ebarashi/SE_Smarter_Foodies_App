{
    "title": "Spicy Southwestern Slaw",
    "Category": "Coleslaw",
    "Ingredients": [
        {
            "1": "3 cups shredded cabbage"
        },
        {
            "2": "1  carrot, shredded"
        },
        {
            "3": "1  onion, diced"
        },
        {
            "4": "1  red bell pepper, diced"
        },
        {
            "5": "1  jalapeno pepper, seeded and diced"
        },
        {
            "6": "1 tablespoon chopped fresh cilantro"
        },
        {
            "7": "0.5 cup canola oil"
        },
        {
            "8": "2 tablespoons apple cider vinegar"
        },
        {
            "9": "1 tablespoon white sugar"
        },
        {
            "10": "1 teaspoon cayenne pepper"
        },
        {
            "11": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "In a large bowl, toss together the cabbage, carrot, onion, red pepper, jalapeno and cilantro. Set aside."
        },
        {
            "2": "In a small bowl, whisk together the oil, vinegar, sugar, cayenne pepper, salt and pepper. Pour over vegetable mixture and toss to combine. Cover and refrigerate for at least one hour."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
