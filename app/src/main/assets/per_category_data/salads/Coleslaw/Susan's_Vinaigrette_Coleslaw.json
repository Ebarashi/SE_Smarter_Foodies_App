{
    "title": "Susan's Vinaigrette Coleslaw",
    "Category": "Coleslaw",
    "Ingredients": [
        {
            "1": "3 tablespoons apple cider vinegar"
        },
        {
            "2": "3 tablespoons canola oil"
        },
        {
            "3": "3 tablespoons white sugar"
        },
        {
            "4": "0.25 teaspoon dry mustard"
        },
        {
            "5": "0.25 teaspoon poppy seeds"
        },
        {
            "6": "0.25 teaspoon ground black pepper"
        },
        {
            "7": "0.25 teaspoon salt"
        },
        {
            "8": "0.25 teaspoon hot pepper sauce (Optional)"
        },
        {
            "9": "4 cups shredded green cabbage"
        },
        {
            "10": "2  carrots, shredded"
        }
    ],
    "Directions": [
        {
            "1": "Whisk apple cider vinegar, canola oil, sugar, dry mustard, poppy seeds, black pepper, salt, and hot pepper sauce in a bowl, stirring until sugar has dissolved."
        },
        {
            "2": "Mix cabbage with carrots in a large salad bowl and pour dressing over slaw. Stir to coat. Refrigerate at least 2 hours before serving."
        }
    ],
