{
    "title": "Homemade Coleslaw",
    "Category": "Coleslaw",
    "Ingredients": [
        {
            "1": "1 tablespoon distilled white vinegar"
        },
        {
            "2": "1 cup mayonnaise"
        },
        {
            "3": "2 tablespoons prepared Dijon-style mustard"
        },
        {
            "4": "3 cups shredded cabbage"
        },
        {
            "5": "3 cups shredded red cabbage"
        },
        {
            "6": "1 cup grated carrot"
        },
        {
            "7": "0.5 cup shredded green bell pepper"
        },
        {
            "8": "0.5 cup shredded rell bell pepper"
        },
        {
            "9": "0.5 cup shredded celery"
        }
    ],
    "Directions": [
        {
            "1": "In a small bowl combine vinegar, mayonnaise and mustard."
        },
        {
            "2": "In a large bowl combine cabbage, carrots, bell peppers and celery."
        },
        {
            "3": "Pour dressing over vegetables, mixing to coat evenly. Chill, and serve over lettuce leaves."
        }
    ],
