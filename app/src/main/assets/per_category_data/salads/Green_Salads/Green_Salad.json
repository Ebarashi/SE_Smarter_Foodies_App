{
    "title": "Green Salad",
    "Category": "Green Salads",
    "Ingredients": [
        {
            "1": "0.5 cup chopped onion"
        },
        {
            "2": "0.5 cup chopped green bell pepper"
        },
        {
            "3": "2 (10 ounce) packages mixed salad greens"
        },
        {
            "4": "4  thinly sliced chicken deli meat, chopped"
        },
        {
            "5": "1  tomato, chopped"
        },
        {
            "6": "0.25 teaspoon onion powder"
        },
        {
            "7": "3 dashes garlic powder"
        },
        {
            "8": "2 pinches salt and ground black pepper to taste"
        },
        {
            "9": "3 tablespoons balsamic vinaigrette salad dressing"
        }
    ],
    "Directions": [
        {
            "1": "Place onion and bell pepper in a microwave-safe bowl; heat in microwave on high until soft, about 1 to 2 minutes. Set aside to cool."
        },
        {
            "2": "Combine onion, bell pepper, salad greens, deli meat, and tomato in a large salad bowl. Sprinkle with onion powder, garlic powder, salt, and black pepper; toss well to mix."
        },
        {
            "3": "Pour on salad dressing; toss well and serve."
        }
    ],
