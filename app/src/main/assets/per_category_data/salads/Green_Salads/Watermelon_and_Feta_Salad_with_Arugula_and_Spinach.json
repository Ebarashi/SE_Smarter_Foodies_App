{
    "title": "Watermelon and Feta Salad with Arugula and Spinach",
    "Category": "Green Salads",
    "Ingredients": [
        {
            "1": "3 tablespoons extra-virgin olive oil"
        },
        {
            "2": "2 teaspoons white balsamic vinegar"
        },
        {
            "3": "0.5 teaspoon kosher salt"
        },
        {
            "4": "2 cups arugula"
        },
        {
            "5": "2 cups baby spinach leaves"
        },
        {
            "6": "1 cup thinly sliced red onion"
        },
        {
            "7": "1 cup grape tomatoes, halved"
        },
        {
            "8": "0.5 cup crumbled feta cheese"
        },
        {
            "9": "2 cups bite sized watermelon chunks"
        }
    ],
    "Directions": [
        {
            "1": "Whisk the olive oil, white balsamic vinegar, and salt together in a small bowl; set aside."
        },
        {
            "2": "Combine the arugula, spinach, onions, and tomatoes in a large salad bowl. Drizzle the vinaigrette over the salad mixture; toss to coat. Add the feta cheese and watermelon to serve."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Total Time": "20 mins"
        },
        {
            "Servings": "4 "
        },
        {
            "Yield": "8 cups"
        }
