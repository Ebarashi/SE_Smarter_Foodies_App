{
    "title": "Easy Arugula Salad",
    "Category": "Green Salads",
    "Ingredients": [
        {
            "1": "4 cups young arugula leaves, rinsed and dried"
        },
        {
            "2": "1 cup cherry tomatoes, halved"
        },
        {
            "3": "0.25 cup pine nuts"
        },
        {
            "4": "2 tablespoons grapeseed oil or olive oil"
        },
        {
            "5": "1 tablespoon rice vinegar"
        },
        {
            "6": "1 salt to taste"
        },
        {
            "7": "1 freshly ground black pepper to taste"
        },
        {
            "8": "0.25 cup grated Parmesan cheese"
        },
        {
            "9": "1 large avocado - peeled, pitted and sliced"
        }
    ],
    "Directions": [
        {
            "1": "In a large plastic bowl with a lid, combine arugula, cherry tomatoes, pine nuts, oil, vinegar, and Parmesan cheese. Season with salt and pepper to taste. Cover, and shake to mix."
        },
        {
            "2": "Divide salad onto plates, and top with slices of avocado."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Total Time": "15 mins"
        },
