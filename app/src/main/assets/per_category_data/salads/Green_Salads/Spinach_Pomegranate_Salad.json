{
    "title": "Spinach Pomegranate Salad",
    "Category": "Green Salads",
    "Ingredients": [
        {
            "1": "1 (10 ounce) bag baby spinach leaves, rinsed and drained"
        },
        {
            "2": "0.5 cup walnut pieces"
        },
        {
            "3": "0.5 cup crumbled feta"
        },
        {
            "4": "0.25 medium red onion, sliced very thin"
        },
        {
            "5": "0.25 cup alfalfa sprouts (Optional)"
        },
        {
            "6": "0.5 cup pomegranate seeds, or to taste"
        },
        {
            "7": "4 tablespoons balsamic vinaigrette"
        }
    ],
    "Directions": [
        {
            "1": "Place spinach into a salad bowl. Top with walnuts, feta, red onion, alfalfa sprouts, and pomegranate seeds. Drizzle with vinaigrette."
        }
    ],
