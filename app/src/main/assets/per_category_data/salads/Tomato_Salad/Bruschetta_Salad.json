{
    "title": "Bruschetta Salad",
    "Category": "Tomato Salad",
    "Ingredients": [
        {
            "1": "6  Roma tomatoes, sliced into rounds"
        },
        {
            "2": "0.5 pound mozzarella cheese, cut into bite-size cubes"
        },
        {
            "3": "1 cup crushed garlic-flavored bagel chips"
        },
        {
            "4": "0.5 cup torn fresh basil leaves"
        },
        {
            "5": "0.5  red onion, chopped"
        },
        {
            "6": "0.25 cup light olive oil"
        },
        {
            "7": "3 tablespoons red wine vinegar"
        },
        {
            "8": "2 large cloves garlic, minced"
        },
        {
            "9": "1 tablespoon dried basil"
        },
        {
            "10": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine tomatoes, mozzarella cheese, bagel chips, basil, red onion, olive oil, red wine vinegar, garlic, basil, salt, and black pepper together in a bowl; toss until evenly combined. Refrigerate until chilled, 15 to 30 minutes."
        }
    ],
