{
    "title": "Israeli Tomato and Cucumber Salad",
    "Category": "Tomato Salad",
    "Ingredients": [
        {
            "1": "1 Salad-"
        },
        {
            "2": "4  English cucumbers, diced"
        },
        {
            "3": "4  Roma (plum) tomatoes, seeded and diced"
        },
        {
            "4": "0.5  purple onion, diced"
        },
        {
            "5": "1  red bell pepper, seeded and diced"
        },
        {
            "6": "2 tablespoons chopped garlic"
        },
        {
            "7": "1 cup chopped fresh parsley"
        },
        {
            "8": "3 tablespoons chopped fresh mint"
        },
        {
            "9": "1 Dressing-"
        },
        {
            "10": "0.5 cup olive oil"
        },
        {
            "11": "2 tablespoons fresh lemon juice"
        },
        {
            "12": "1 tablespoon kosher salt"
        },
        {
            "13": "1 tablespoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Toss cucumbers, tomatoes, onion, bell pepper, garlic, parsley, and mint together in a bowl. Drizzle olive oil and lemon juice over the salad and toss to coat; season with salt and pepper."
        }
    ],
