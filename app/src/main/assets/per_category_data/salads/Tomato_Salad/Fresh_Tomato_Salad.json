{
    "title": "Fresh Tomato Salad",
    "Category": "Tomato Salad",
    "Ingredients": [
        {
            "1": "5  tomatoes, diced"
        },
        {
            "2": "1  onion, chopped"
        },
        {
            "3": "1  cucumber, sliced"
        },
        {
            "4": "1  green bell pepper, chopped"
        },
        {
            "5": "0.5 cup chopped fresh basil"
        },
        {
            "6": "0.5 cup chopped parsley"
        },
        {
            "7": "2 tablespoons crushed garlic"
        },
        {
            "8": "1 salt and pepper to taste"
        },
        {
            "9": "2 tablespoons white wine vinegar"
        }
    ],
    "Directions": [
        {
            "1": "Combine tomatoes, onion, cucumber, bell pepper, basil, parsley, garlic, and vinegar in a large bowl; toss well. Season with salt and pepper."
        },
        {
            "2": "Chill salad in the refrigerator for 15 minutes before serving."
        }
