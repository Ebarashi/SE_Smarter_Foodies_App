{
    "title": "Cucumber Watermelon Salad",
    "Category": "Tomato Salad",
    "Ingredients": [
        {
            "1": "1 large cucumber, halved lengthwise and thinly sliced crosswise"
        },
        {
            "2": "2 medium tomatoes, diced"
        },
        {
            "3": "1 small onion, thinly sliced"
        },
        {
            "4": "2 cups cubed seedless watermelon"
        },
        {
            "5": "3 tablespoons red wine vinegar"
        },
        {
            "6": "2 tablespoons olive oil"
        },
        {
            "7": "1 teaspoon sea salt"
        },
        {
            "8": "1 teaspoon white sugar"
        },
        {
            "9": "0.5 teaspoon dried oregano"
        },
        {
            "10": "0.5 teaspoon dried basil"
        },
        {
            "11": "0.5 teaspoon ground thyme"
        },
        {
            "12": "0.5 teaspoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Combine cucumber, tomatoes, onion, and watermelon in a large bowl."
        },
        {
            "2": "Mix vinegar, olive oil, salt, sugar, oregano, basil, thyme, and pepper together in a separate, smaller bowl and pour over salad. Stir to coat well."
        },
        {
            "3": "Cover and refrigerate for 20 minutes. Serve chilled."
        }
    ],
    "Details": [
