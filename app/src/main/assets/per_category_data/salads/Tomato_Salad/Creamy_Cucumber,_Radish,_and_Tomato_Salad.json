{
    "title": "Creamy Cucumber, Radish, and Tomato Salad",
    "Category": "Tomato Salad",
    "Ingredients": [
        {
            "1": "0.5 cup mayonnaise"
        },
        {
            "2": "0.5 cup crumbled feta cheese"
        },
        {
            "3": "1 tablespoon chopped fresh chives"
        },
        {
            "4": "1 tablespoon chopped fresh dill"
        },
        {
            "5": "1 tablespoon fresh lemon juice"
        },
        {
            "6": "0.25 teaspoon salt"
        },
        {
            "7": "0.25 teaspoon ground black pepper"
        },
        {
            "8": "1 bunch radishes, diced"
        },
        {
            "9": "1 pint cherry tomatoes, quartered"
        },
        {
            "10": "1 large English cucumber, diced"
        },
        {
            "11": "0.25 medium red onion, chopped"
        }
    ],
    "Directions": [
        {
            "1": "Combine mayonnaise, feta, chives, dill, lemon juice, salt, and pepper in a large bowl. Add radishes, tomatoes, cucumber, and onion and toss together."
        }
    ],
