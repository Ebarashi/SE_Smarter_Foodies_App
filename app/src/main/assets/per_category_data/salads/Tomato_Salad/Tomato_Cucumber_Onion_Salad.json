{
    "title": "Tomato Cucumber Onion Salad",
    "Category": "Tomato Salad",
    "Ingredients": [
        {
            "1": "1 cup diced cucumber"
        },
        {
            "2": "1 cup diced fresh tomato"
        },
        {
            "3": "0.5 cup olive oil"
        },
        {
            "4": "0.5 cup red wine vinegar"
        },
        {
            "5": "0.25 cup diced red onion"
        },
        {
            "6": "1 teaspoon white pepper"
        },
        {
            "7": "1 teaspoon salt, or to taste"
        }
    ],
    "Directions": [
        {
            "1": "Toss cucumber, tomato, olive oil, red wine vinegar, red onion, and white pepper together in a large bowl; season with salt. Refrigerate until slightly chilled, 10 to 15 minutes."
        }
    ],
