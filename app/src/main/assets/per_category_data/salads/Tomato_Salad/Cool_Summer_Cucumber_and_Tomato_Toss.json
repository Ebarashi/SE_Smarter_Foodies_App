{
    "title": "Cool Summer Cucumber and Tomato Toss",
    "Category": "Tomato Salad",
    "Ingredients": [
        {
            "1": "1 large cucumber, peeled and sliced"
        },
        {
            "2": "2  ripe fresh tomatoes, chopped"
        },
        {
            "3": "2 tablespoons balsamic vinegar"
        },
        {
            "4": "2 tablespoons olive oil"
        },
        {
            "5": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Place cucumber and tomatoes in a bowl. Pour in olive oil and balsamic vinegar. Season with salt and pepper. Toss gently to coat. Refrigerate until ready to serve."
        }
    ],
