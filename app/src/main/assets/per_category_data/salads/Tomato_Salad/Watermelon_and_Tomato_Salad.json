{
    "title": "Watermelon and Tomato Salad",
    "Category": "Tomato Salad",
    "Ingredients": [
        {
            "1": "1 pint cherry tomatoes, halved"
        },
        {
            "2": "2 cups cubed seeded watermelon"
        },
        {
            "3": "6 large basil leaves, thinly sliced"
        },
        {
            "4": "0.5 cup crumbled feta cheese"
        },
        {
            "5": "1 teaspoon lemon zest"
        },
        {
            "6": "2 tablespoons extra-virgin olive oil"
        },
        {
            "7": "0.5 tablespoons white balsamic vinegar"
        },
        {
            "8": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Place the tomatoes and watermelon into a mixing bowl. Sprinkle with the basil, feta cheese, and lemon zest. Drizzle with the olive oil and balsamic vinegar. Toss gently. Season to taste with salt and pepper before serving."
        }
    ],
