{
    "title": "Tomato, Basil, and Feta Salad",
    "Category": "Tomato Salad",
    "Ingredients": [
        {
            "1": "6  roma (plum) tomatoes, diced"
        },
        {
            "2": "1 small cucumber - peeled, quartered lengthwise, and chopped"
        },
        {
            "3": "3  green onions, chopped"
        },
        {
            "4": "0.25 cup fresh basil leaves, cut into thin strips"
        },
        {
            "5": "3 tablespoons olive oil"
        },
        {
            "6": "2 tablespoons balsamic vinegar"
        },
        {
            "7": "3 tablespoons crumbled feta cheese"
        },
        {
            "8": "1 salt and freshly ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "In a large bowl, toss together the tomatoes, cucumber, green onions, basil, olive oil, balsamic vinegar, and feta cheese. Season with salt and pepper."
        }
    ],
