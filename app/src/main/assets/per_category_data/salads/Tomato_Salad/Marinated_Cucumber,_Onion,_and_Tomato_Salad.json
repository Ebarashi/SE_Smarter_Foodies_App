{
    "title": "Marinated Cucumber, Onion, and Tomato Salad",
    "Category": "Tomato Salad",
    "Ingredients": [
        {
            "1": "1 cup water"
        },
        {
            "2": "0.5 cup distilled white vinegar"
        },
        {
            "3": "0.25 cup vegetable oil"
        },
        {
            "4": "0.25 cup sugar"
        },
        {
            "5": "1 teaspoon salt, or to taste"
        },
        {
            "6": "1 teaspoon freshly ground black pepper, or to taste"
        },
        {
            "7": "3  cucumbers, peeled and sliced 1/4-inch thick"
        },
        {
            "8": "3  tomatoes, cut into wedges"
        },
        {
            "9": "1  onion, sliced and separated into rings"
        }
    ],
    "Directions": [
        {
            "1": "Whisk water, vinegar, oil, sugar, salt, and pepper together in a large bowl until smooth; add cucumbers, tomatoes, and onion and stir to coat."
        },
        {
            "2": "Cover bowl with plastic wrap; refrigerate for at least 2 hours."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Additional Time": " 2 hrs"
        },
        {
            "Total Time": " 2 hrs 15 mins"
        },
        {
            "Servings": "6 "
        },
