{
    "title": "Spinach Pasta Salad",
    "Category": "Pasta Salad",
    "Ingredients": [
        {
            "1": "1 (12 ounce) package farfalle pasta"
        },
        {
            "2": "10 ounces baby spinach, rinsed and torn into bite-size piece"
        },
        {
            "3": "2 ounces crumbled feta cheese with basil and tomato"
        },
        {
            "4": "1  red onion, chopped"
        },
        {
            "5": "1 (15 ounce) can black olives, drained and chopped"
        },
        {
            "6": "1 cup Italian-style salad dressing"
        },
        {
            "7": "4 cloves garlic, minced"
        },
        {
            "8": "1  lemon, juiced"
        },
        {
            "9": "0.5 teaspoon garlic salt"
        },
        {
            "10": "0.5 teaspoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "In a large pot of salted boiling water, cook pasta until al dente, rinse under cold water and drain."
        },
        {
            "2": "In a large bowl, combine the pasta, spinach, cheese, red onion and olives."
        },
        {
            "3": "Whisk together the salad dressing, garlic, lemon juice, garlic salt and pepper. Pour over salad and toss. Refrigerate for 2 hours and serve chilled."
        }
    ],
