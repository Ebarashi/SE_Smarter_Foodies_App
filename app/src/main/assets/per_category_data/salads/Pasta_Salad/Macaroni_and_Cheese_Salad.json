{
    "title": "Macaroni and Cheese Salad",
    "Category": "Pasta Salad",
    "Ingredients": [
        {
            "1": "0.5 cups macaroni"
        },
        {
            "2": "2 tablespoons cider vinegar"
        },
        {
            "3": "1 cup shredded Cheddar cheese"
        },
        {
            "4": "0.5 cup chopped green bell pepper"
        },
        {
            "5": "0.25 cup chopped celery"
        },
        {
            "6": "3 tablespoons thinly sliced green onion"
        },
        {
            "7": "8  cherry tomatoes"
        },
        {
            "8": "1 cup mayonnaise"
        },
        {
            "9": "1 salt and pepper to taste"
        },
        {
            "10": "0.25 teaspoon Beau Monde \u2122 seasoning"
        }
    ],
    "Directions": [
        {
            "1": "In a large pot of salted boiling water, cook pasta until al dente, rinse under cold water and drain."
        },
        {
            "2": "In a large bowl, combine pasta and vinegar. Mix well and allow to sit 15-20 minutes."
        },
        {
            "3": "Add cheese, peppers, celery, green onions, tomatoes, mayonnaise and seasonings. Mix well and refrigerate until chilled."
        }
    ],
