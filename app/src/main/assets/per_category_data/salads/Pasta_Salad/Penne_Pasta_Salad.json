{
    "title": "Penne Pasta Salad",
    "Category": "Pasta Salad",
    "Ingredients": [
        {
            "1": "16 ounces penne pasta"
        },
        {
            "2": "1 cup creamy salad dressing, e.g. Miracle Whip \u2122"
        },
        {
            "3": "0.5 cup Dijon-style prepared mustard"
        },
        {
            "4": "0.5 cups grated Parmesan cheese"
        },
        {
            "5": "1 tablespoon dried dill weed"
        },
        {
            "6": "1  red onion, julienned"
        },
        {
            "7": "1  yellow bell pepper, thinly sliced"
        },
        {
            "8": "1  orange bell pepper, julienned"
        }
    ],
    "Directions": [
        {
            "1": "In a large pot of salted boiling water, cook pasta until al dente, rinse under cold water and drain."
        },
        {
            "2": "In a large bowl, mix together the salad dressing, mustard, cheese and dill."
        },
        {
            "3": "Add pasta, red onion and bell peppers. Toss well; season with salt and pepper. Chill before serving."
        }
    ],
