{
    "title": "Italian Pasta Salad I",
    "Category": "Pasta Salad",
    "Ingredients": [
        {
            "1": "1 (16 ounce) package rotini pasta"
        },
        {
            "2": "1 cup Italian-style salad dressing"
        },
        {
            "3": "1 cup creamy Caesar salad dressing"
        },
        {
            "4": "1 cup grated Parmesan cheese"
        },
        {
            "5": "1  red bell pepper, diced"
        },
        {
            "6": "1  green bell pepper, chopped"
        },
        {
            "7": "1  red onion, diced"
        }
    ],
    "Directions": [
        {
            "1": "In a large pot of salted boiling water, cook pasta until al dente, rinse under cold water and drain."
        },
        {
            "2": "In a large bowl, combine the pasta, Italian salad dressing, Caesar dressing, Parmesan cheese, red bell pepper, green bell pepper, and red onion. Mix well and serve chilled or at room temperature."
        }
    ],
