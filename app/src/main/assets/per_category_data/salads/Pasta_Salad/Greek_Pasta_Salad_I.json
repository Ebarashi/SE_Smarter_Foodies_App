{
    "title": "Greek Pasta Salad I",
    "Category": "Pasta Salad",
    "Ingredients": [
        {
            "1": "0.5 cup olive oil"
        },
        {
            "2": "0.5 cup red wine vinegar"
        },
        {
            "3": "0.5 teaspoons garlic powder"
        },
        {
            "4": "0.5 teaspoons dried basil"
        },
        {
            "5": "0.5 teaspoons dried oregano"
        },
        {
            "6": "0.75 teaspoon ground black pepper"
        },
        {
            "7": "0.75 teaspoon white sugar"
        },
        {
            "8": "1.0 cups cooked elbow macaroni"
        },
        {
            "9": "3 cups fresh sliced mushrooms"
        },
        {
            "10": "15  cherry tomatoes, halved"
        },
        {
            "11": "1 cup sliced red bell peppers"
        },
        {
            "12": "0.75 cup crumbled feta cheese"
        },
        {
            "13": "0.5 cup chopped green onions"
        },
        {
            "14": "1 (4 ounce) can whole black olives"
        },
        {
            "15": "0.75 cup sliced pepperoni sausage, cut into strips"
        }
    ],
    "Directions": [
        {
            "1": "In a large bowl, whisk together olive oil, vinegar, garlic powder, basil, oregano, black pepper, and sugar. Add cooked pasta, mushrooms, tomatoes, red peppers, feta cheese, green onions, olives, and pepperoni. Toss until evenly coated. Cover, and chill 2 hours or overnight."
        }
    ],
