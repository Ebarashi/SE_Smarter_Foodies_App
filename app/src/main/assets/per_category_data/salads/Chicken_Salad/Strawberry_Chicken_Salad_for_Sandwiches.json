{
    "title": "Strawberry Chicken Salad for Sandwiches",
    "Category": "Chicken Salad",
    "Ingredients": [
        {
            "1": "0.5 cup mayonnaise"
        },
        {
            "2": "2 teaspoons lemon juice"
        },
        {
            "3": "1 teaspoon honey"
        },
        {
            "4": "2 teaspoons poppy seeds"
        },
        {
            "5": "2 cups chopped, cooked chicken"
        },
        {
            "6": "0.75 cup sliced fresh strawberries"
        },
        {
            "7": "0.25 cup pecan pieces"
        },
        {
            "8": "1 stalk chopped celery"
        },
        {
            "9": "2 stalks green onions, chopped"
        },
        {
            "10": "2 tablespoons chopped fresh parsley"
        },
        {
            "11": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Mix mayonnaise, lemon juice, honey, and poppy seeds together in a medium bowl. Add chicken, strawberries, pecans, celery, green onions, parsley; toss to combine. Season with salt and pepper."
        }
    ],
