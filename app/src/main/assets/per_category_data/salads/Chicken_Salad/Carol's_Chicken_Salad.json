{
    "title": "Carol's Chicken Salad",
    "Category": "Chicken Salad",
    "Ingredients": [
        {
            "1": "0.5 cup mayonnaise"
        },
        {
            "2": "0.5 teaspoon salt"
        },
        {
            "3": "0.75 teaspoon poultry seasoning"
        },
        {
            "4": "0.25 teaspoon onion powder"
        },
        {
            "5": "0.25 teaspoon garlic powder"
        },
        {
            "6": "0.25 teaspoon ground black pepper"
        },
        {
            "7": "1 tablespoon lemon juice"
        },
        {
            "8": "3 cups diced, cooked chicken breast meat"
        },
        {
            "9": "0.5 cup finely chopped celery"
        },
        {
            "10": "0.5 cup chopped green onions"
        },
        {
            "11": "1 (8 ounce) can water chestnuts, drained and chopped"
        },
        {
            "12": "0.5 cups diced Swiss cheese"
        },
        {
            "13": "0.5 cups halved green grapes"
        }
    ],
    "Directions": [
        {
            "1": "In a medium bowl, whisk together the mayonnaise, salt, poultry seasoning, onion powder, garlic powder, pepper, and lemon juice."
        },
        {
            "2": "In a large bowl, toss together the chicken, celery, green onions, water chestnuts, Swiss cheese, and grapes. Add the mayonnaise mixture, and stir to coat. Chill until serving."
        }
    ],
    "Details": [
