{
    "title": "Creamy Avocado Chicken Salad",
    "Category": "Chicken Salad",
    "Ingredients": [
        {
            "1": "2  cooked chicken breast halves, diced"
        },
        {
            "2": "1  ripe avocado, sliced"
        },
        {
            "3": "0.5 cup sour cream"
        },
        {
            "4": "0.5 teaspoons ground black pepper"
        },
        {
            "5": "1 teaspoon lime juice"
        },
        {
            "6": "0.5 teaspoon garlic powder"
        },
        {
            "7": "0.5 teaspoon onion powder"
        },
        {
            "8": "0.25 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Process chicken in a food processor until finely chopped. Add avocado, sour cream, black pepper, lime juice, garlic powder, onion powder, and salt; process until completely combined and almost pureed."
        }
    ],
