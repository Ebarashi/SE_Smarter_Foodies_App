{
    "title": "Easy Leftover Rotisserie Chicken Salad",
    "Category": "Chicken Salad",
    "Ingredients": [
        {
            "1": "8 ounces shredded rotisserie chicken"
        },
        {
            "2": "0.5 cup chopped pecans"
        },
        {
            "3": "1  apple, cored and chopped"
        },
        {
            "4": "10  seedless green grapes, quartered"
        },
        {
            "5": "1 cup mayonnaise"
        },
        {
            "6": "1 tablespoon low-fat vanilla yogurt"
        },
        {
            "7": "1 teaspoon Dijon mustard"
        },
        {
            "8": "1 teaspoon lemon juice"
        },
        {
            "9": "0.25 teaspoon ground cumin"
        },
        {
            "10": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Place shredded chicken, pecans, apple, and quartered grapes in a bowl. Stir."
        },
        {
            "2": "Mix mayonnaise, yogurt, mustard, lemon juice, cumin, salt, and pepper together in a small bowl. Pour over chicken mixture and toss to coat. Cover and refrigerate until chilled, about 1 hour."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
