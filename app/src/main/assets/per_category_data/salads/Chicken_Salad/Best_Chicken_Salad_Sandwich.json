{
    "title": "Best Chicken Salad Sandwich",
    "Category": "Chicken Salad",
    "Ingredients": [
        {
            "1": "0.5 cup light mayonnaise"
        },
        {
            "2": "0.5 cup low-fat plain yogurt"
        },
        {
            "3": "1 teaspoon paprika"
        },
        {
            "4": "1 teaspoon seasoned salt"
        },
        {
            "5": "0.5 cups dried cranberries"
        },
        {
            "6": "1 cup chopped celery"
        },
        {
            "7": "1 cup cubed green apple"
        },
        {
            "8": "1 cup sliced red grapes"
        },
        {
            "9": "1 cup chopped pecans"
        },
        {
            "10": "4 cups drained canned chicken"
        },
        {
            "11": "1 ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Mix mayonnaise, yogurt, paprika, and seasoned salt together in a bowl; fold in cranberries, celery, apple, grapes, and pecans. Stir in chicken and season with black pepper."
        }
    ],
