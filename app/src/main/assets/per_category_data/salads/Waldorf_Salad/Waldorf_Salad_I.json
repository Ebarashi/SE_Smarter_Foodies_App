{
    "title": "Waldorf Salad I",
    "Category": "Waldorf Salad",
    "Ingredients": [
        {
            "1": "2 cups chopped apples"
        },
        {
            "2": "0.5  lemons, juiced"
        },
        {
            "3": "0.25 cup chopped celery"
        },
        {
            "4": "0.25 cup chopped walnuts"
        },
        {
            "5": "0.25 cup raisins"
        },
        {
            "6": "0.25 cup seedless grapes, halved"
        },
        {
            "7": "0.5 cup heavy cream, whipped"
        },
        {
            "8": "0.25 cup mayonnaise"
        },
        {
            "9": "1 pinch ground nutmeg"
        }
    ],
    "Directions": [
        {
            "1": "In a large bowl, toss together the apples and lemon juice. Stir in the celery, nuts, raisins and grapes."
        },
        {
            "2": "Fold together the whipped cream and mayonnaise; add to salad and mix well."
        },
        {
            "3": "Sprinkle with nutmeg and chill before serving."
        }
    ],
    "Details": [
        {
            "Servings": "5 "
        },
        {
            "Yield": "4 to 6 servings"
        }
    ],
