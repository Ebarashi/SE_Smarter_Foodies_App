{
    "title": "Simple Waldorf Salad",
    "Category": "Waldorf Salad",
    "Ingredients": [
        {
            "1": "0.5 cup mayonnaise"
        },
        {
            "2": "1.0 tablespoons distilled white vinegar"
        },
        {
            "3": "1  apple - peeled, cored, and chopped"
        },
        {
            "4": "1 large stalk celery, diced"
        },
        {
            "5": "0.5 cup raisins"
        },
        {
            "6": "0.5 cup chopped walnuts"
        },
        {
            "7": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Whisk together mayonnaise and vinegar in a mixing bowl."
        },
        {
            "2": "Add apples, celery, raisins, and walnuts to the bowl. Season with salt and pepper; mix well."
        },
        {
            "3": "Place in the refrigerator until chilled, about 30 minutes."
        }
    ],
