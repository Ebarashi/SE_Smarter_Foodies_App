    "Ingredients": [
        {
            "1": "2 cups red leaf lettuce - rinsed, dried and torn"
        },
        {
            "2": "2 tablespoons raspberry walnut vinaigrette"
        },
        {
            "3": "0.5 cup seedless red grapes, halved"
        },
        {
