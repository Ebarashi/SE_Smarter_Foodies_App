{
    "title": "Pasta Salad Dressing",
    "Category": "Salad Dressings",
    "Ingredients": [
        {
            "1": "0.5 cup mayonnaise"
        },
        {
            "2": "0.5 cup olive oil"
        },
        {
            "3": "0.25 cup red wine vinegar"
        },
        {
            "4": "0.5 teaspoons Dijon mustard"
        },
        {
            "5": "1 clove garlic, minced"
        },
        {
            "6": "1 teaspoon white sugar"
        },
        {
            "7": "0.5 teaspoon salt"
        },
        {
            "8": "0.5 teaspoon freshly ground black pepper"
        },
        {
            "9": "0.25 teaspoon dried oregano"
        },
        {
            "10": "0.25 teaspoon ground thyme"
        },
        {
            "11": "0.25 teaspoon dried basil"
        }
    ],
    "Directions": [
        {
            "1": "Beat mayonnaise, olive oil, vinegar, mustard, garlic, sugar, salt, pepper, oregano, thyme, and basil together in a bowl with a whisk until smooth."
        }
    ],
