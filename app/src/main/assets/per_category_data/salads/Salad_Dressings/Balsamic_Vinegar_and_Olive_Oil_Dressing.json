    "Category": "Salad Dressings",
    "Ingredients": [
        {
            "1": "1 sprig fresh oregano"
        },
        {
            "2": "1 sprig fresh thyme"
        },
        {
            "3": "1 cup olive oil"
        },
        {
            "4": "0.25 cup balsamic vinegar"
        },
        {
