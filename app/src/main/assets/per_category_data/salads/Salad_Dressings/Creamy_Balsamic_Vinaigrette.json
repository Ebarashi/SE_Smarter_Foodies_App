{
    "title": "Creamy Balsamic Vinaigrette",
    "Category": "Salad Dressings",
    "Ingredients": [
        {
            "1": "3 tablespoons olive oil"
        },
        {
            "2": "3 tablespoons balsamic vinegar"
        },
        {
            "3": "3 tablespoons mayonnaise"
        },
        {
            "4": "2 tablespoons water"
        },
        {
            "5": "2 cloves garlic, pressed"
        },
        {
            "6": "1 teaspoon Dijon mustard"
        },
        {
            "7": "1 teaspoon brown sugar"
        },
        {
            "8": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Place olive oil, balsamic vinegar, mayonnaise, water, garlic, Dijon mustard, and brown sugar into a blender; blend until the salad dressing is smooth and emulsified, 10 to 15 seconds. Season with salt and black pepper."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Total Time": "10 mins"
        },
        {
            "Servings": "6 "
        },
        {
            "Yield": "3 /4 cup"
        }
    ],
