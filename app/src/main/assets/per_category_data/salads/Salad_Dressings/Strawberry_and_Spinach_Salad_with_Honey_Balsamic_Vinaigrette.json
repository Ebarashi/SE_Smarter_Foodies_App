{
    "title": "Strawberry and Spinach Salad with Honey Balsamic Vinaigrette",
    "Category": "Salad Dressings",
    "Ingredients": [
        {
            "1": "1 bunch fresh spinach"
        },
        {
            "2": "1 cup sliced fresh strawberries"
        },
        {
            "3": "0.5 cup crumbled Gorgonzola cheese"
        },
        {
            "4": "0.5 cup raw pecans"
        },
        {
            "5": "0.25 cup balsamic vinegar"
        },
        {
            "6": "2 tablespoons honey"
        },
        {
            "7": "0.5 cup olive oil"
        },
        {
            "8": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine spinach, strawberries, Gorgonzola cheese, and pecans in a large bowl."
        },
        {
            "2": "Stir together balsamic vinegar and honey in a medium bowl; slowly stream in olive oil while whisking continuously. Season with salt and pepper."
        },
        {
            "3": "Drizzle dressing over spinach mixture just before serving."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
