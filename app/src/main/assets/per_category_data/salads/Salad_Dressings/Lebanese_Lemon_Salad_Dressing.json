{
    "title": "Lebanese Lemon Salad Dressing",
    "Category": "Salad Dressings",
    "Ingredients": [
        {
            "1": "0.5 cup fresh lemon juice"
        },
        {
            "2": "0.5 cup mild extra-virgin olive oil"
        },
        {
            "3": "3 cloves garlic, minced"
        },
        {
            "4": "1 teaspoon kosher salt"
        },
        {
            "5": "1 ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "In a medium bowl, whisk together the lemon juice, olive oil, garlic, salt and pepper. Stir just before serving."
        }
    ],
