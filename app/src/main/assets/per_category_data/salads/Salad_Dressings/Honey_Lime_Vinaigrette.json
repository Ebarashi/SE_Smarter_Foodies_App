{
    "title": "Honey-Lime Vinaigrette",
    "Category": "Salad Dressings",
    "Ingredients": [
        {
            "1": "0.25 cup fresh lime juice"
        },
        {
            "2": "2 tablespoons honey"
        },
        {
            "3": "1 teaspoon white sugar"
        },
        {
            "4": "1 teaspoon Dijon mustard"
        },
        {
            "5": "0.5 teaspoon garlic powder"
        },
        {
            "6": "0.5 teaspoon kosher salt"
        },
        {
            "7": "0.25 teaspoon ground black pepper"
        },
        {
            "8": "0.25 teaspoon ground cumin"
        },
        {
            "9": "0.25 cup olive oil"
        },
        {
            "10": "0.25 cup canola oil"
        }
    ],
    "Directions": [
        {
            "1": "Blend lime juice, honey, sugar, mustard, garlic powder, salt, black pepper, and cumin together in a blender; stream olive oil and canola oil into lime juice mixture while blending until well combined."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Total Time": "5 mins"
        },
        {
            "Servings": "8 "
        }
    ],
