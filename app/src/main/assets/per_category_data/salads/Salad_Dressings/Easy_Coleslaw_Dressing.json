{
    "title": "Easy Coleslaw Dressing",
    "Category": "Salad Dressings",
    "Ingredients": [
        {
            "1": "0.5 cup mayonnaise"
        },
        {
            "2": "2 tablespoons white sugar"
        },
        {
            "3": "0.5 tablespoons lemon juice"
        },
        {
            "4": "1 tablespoon vinegar"
        },
        {
            "5": "0.5 teaspoon ground black pepper"
        },
        {
            "6": "0.25 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Whisk mayonnaise, sugar, lemon juice, vinegar, pepper, and salt together in a bowl until smooth and creamy."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
