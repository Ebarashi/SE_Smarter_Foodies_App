{
    "title": "Creamy Bacon and Broccoli Salad",
    "Category": "Broccoli Salad",
    "Ingredients": [
        {
            "1": "1 (270 g) tub PHILADELPHIA Savoury Garlic Cooking Creme"
        },
        {
            "2": "3 tablespoons white vinegar"
        },
        {
            "3": "8 cups small broccoli florets"
        },
        {
            "4": "0.5 cup Oscar Mayer Real Bacon Recipe Pieces"
        },
        {
            "5": "0.5 cup shredded Cracker Barrel Old Cheddar Cheese"
        },
        {
            "6": "0.5 cup chopped red onions"
        },
        {
            "7": "0.25 cup sunflower seeds"
        }
    ],
    "Directions": [
        {
            "1": "Mix first 2 ingredients in large bowl."
        },
        {
            "2": "Add remaining ingredients; toss to coat."
        },
        {
            "3": "Refrigerate 1 hour."
        }
    ],
