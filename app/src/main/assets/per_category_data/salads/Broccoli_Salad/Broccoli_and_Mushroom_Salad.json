{
    "title": "Broccoli and Mushroom Salad",
    "Category": "Broccoli Salad",
    "Ingredients": [
        {
            "1": "1 Dressing-"
        },
        {
            "2": "1 cup vegetable oil"
        },
        {
            "3": "0.5 cup white sugar"
        },
        {
            "4": "0.25 cup cider vinegar"
        },
        {
            "5": "1 teaspoon salt"
        },
        {
            "6": "1 teaspoon ground paprika"
        },
        {
            "7": "1 teaspoon celery seed"
        },
        {
            "8": "1 teaspoon onion powder"
        },
        {
            "9": "1 Salad-"
        },
        {
            "10": "1 large head broccoli, cut into bite-sized florets"
        },
        {
            "11": "1 (16 ounce) package sliced fresh mushrooms"
        },
        {
            "12": "2 stalks green onion, sliced"
        }
    ],
    "Directions": [
        {
            "1": "Combine oil, sugar, vinegar, salt, paprika, celery seed, and onion powder in a jar. Seal the jar, shake, and refrigerate for 30 minutes."
        },
        {
            "2": "Combine broccoli, mushrooms, and green onion in a large bowl. Remove dressing from the refrigerator and shake. Pour over veggies, toss to coat, and refrigerate for at least 1 hour before serving."
        }
    ],
