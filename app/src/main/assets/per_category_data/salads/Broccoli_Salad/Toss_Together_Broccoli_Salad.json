{
    "title": "Toss-Together Broccoli Salad",
    "Category": "Broccoli Salad",
    "Ingredients": [
        {
            "1": "3 cups fresh broccoli florets"
        },
        {
            "2": "1 cup shredded Cheddar cheese"
        },
        {
            "3": "1 cup finely chopped onion"
        },
        {
            "4": "8  crispy cooked bacon, crumbled"
        },
        {
            "5": "0.5 cup sunflower seeds"
        },
        {
            "6": "0.5 cup raisins"
        },
        {
            "7": "6 tablespoons mayonnaise"
        },
        {
            "8": "0.25 cup white vinegar"
        },
        {
            "9": "2 tablespoons white sugar"
        }
    ],
    "Directions": [
        {
            "1": "Mix the broccoli, Cheddar cheese, onion, bacon, sunflower seeds, and raisins in a large bowl. Whisk mayonnaise, vinegar, and sugar together in a separate bowl; pour over the broccoli mixture and toss to coat."
        },
        {
            "2": "Cover bowl with plastic wrap and refrigerate at least 1 hour before serving."
        }
    ],
