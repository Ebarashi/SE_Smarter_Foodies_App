{
    "title": "Broccoli Buffet Salad",
    "Category": "Broccoli Salad",
    "Ingredients": [
        {
            "1": "1 Dressing-"
        },
        {
            "2": "0.5 cup plain low-fat yogurt"
        },
        {
            "3": "0.25 cup light mayonnaise"
        },
        {
            "4": "2 tablespoons white sugar"
        },
        {
            "5": "1 tablespoon lemon juice"
        },
        {
            "6": "1 Salad-"
        },
        {
            "7": "3 cups broccoli florets"
        },
        {
            "8": "0.5 cup chopped red onion"
        },
        {
            "9": "0.5 cup chopped raisins"
        },
        {
            "10": "0.5 cup crumbled feta cheese"
        },
        {
            "11": "0.25 cup sunflower seeds"
        },
        {
            "12": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Make the dressing: Mix yogurt, mayonnaise, sugar, and lemon juice together in a 2-cup liquid measure."
        },
        {
            "2": "Make the salad: Stir broccoli, onion, raisins, sunflower seeds, and feta together in a salad bowl."
        },
        {
            "3": "Pour dressing over salad and toss to coat. Season with salt and pepper. Cover and refrigerate for 2 hours before serving."
        }
    ],
