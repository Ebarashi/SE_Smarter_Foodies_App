{
    "title": "Lime Salad",
    "Category": "Jell-O\u00ae Salad",
    "Ingredients": [
        {
            "1": "1 (6 ounce) package lime flavored Jell-O\u00ae mix"
        },
        {
            "2": "0.75 cup boiling water"
        },
        {
            "3": "0.25 teaspoon salt"
        },
        {
            "4": "0.75 cup cold water"
        },
        {
            "5": "1 cup shredded cabbage"
        },
        {
            "6": "0.5 cup grated carrots"
        },
        {
            "7": "0.5 cup chopped walnuts"
        }
    ],
    "Directions": [
        {
            "1": "Dissolve gelatin mix in boiling water. Stir in salt. Stir in cold water. Chill until slightly thickened."
        },
        {
            "2": "Fold cabbage, carrots, and nuts into thickened gelatin. Pour mixture into an 8 inch mold that has been sprayed with a non-stick spray. Chill until firm."
        },
        {
            "3": "Set mold in a sink of hot water for a few seconds when ready to unmold. Place serving plate on top of mold, and turn over. Lift off mold."
        }
    ],
    "Details": [
        {
            "Servings": "8 "
        },
        {
            "Yield": "8 servings"
        }
    ],
