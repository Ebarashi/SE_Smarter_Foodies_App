{
    "title": "Orange Gelatin Salad",
    "Category": "Jell-O\u00ae Salad",
    "Ingredients": [
        {
            "1": "1 (11 ounce) can mandarin oranges, drained"
        },
        {
            "2": "1 (8 ounce) can crushed pineapple, drained"
        },
        {
            "3": "1 (6 ounce) package orange flavored Jell-O\u00ae mix"
        },
        {
            "4": "16 ounces cottage cheese"
        },
        {
            "5": "8 ounces frozen whipped topping, thawed"
        }
    ],
    "Directions": [
        {
            "1": "Combine oranges, pineapple, and gelatin mix in a mixing bowl. Mix well and chill for 30 minutes."
        },
        {
            "2": "Add cottage cheese to the fruit mixture; stir to combine."
        },
        {
            "3": "Gently fold in whipped topping; chill and serve."
        }
    ],
    "Details": [
