{
    "title": "Thanksgiving Jell-O\u00ae Salad",
    "Category": "Jell-O\u00ae Salad",
    "Ingredients": [
        {
            "1": "1 (16 ounce) can crushed pineapple, drained"
        },
        {
            "2": "1 (16 ounce) container small curd cottage cheese"
        },
        {
            "3": "1 (12 ounce) container frozen whipped topping (such as Cool Whip\u00ae), thawed"
        },
        {
            "4": "1 (3 ounce) package lime-flavored gelatin mix (such as Jell-O\u00ae)"
        }
    ],
    "Directions": [
        {
            "1": "Stir pineapple, cottage cheese, and whipped topping together in a bowl; sprinkle gelatin mix over the top and stir until well-mixed. Refrigerate until chilled and set, at least 2 hours."
        }
    ],
