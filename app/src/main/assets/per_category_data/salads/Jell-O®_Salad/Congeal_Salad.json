{
    "title": "Congeal Salad",
    "Category": "Jell-O\u00ae Salad",
    "Ingredients": [
        {
            "1": "1 (6 ounce) package fruit flavored Jell-O\u00ae mix"
        },
        {
            "2": "1 (8 ounce) package cream cheese, softened"
        },
        {
            "3": "32 ounces large curd cottage cheese"
        },
        {
            "4": "1 (15.25 ounce) can fruit cocktail, drained"
        },
        {
            "5": "1 (20 ounce) can pineapple chunks, drained"
        },
        {
            "6": "1 (11 ounce) can mandarin orange segments"
        },
        {
            "7": "1  apple - peeled, cored and diced"
        },
        {
            "8": "1  banana, peeled and sliced"
        },
        {
            "9": "1 (10 ounce) jar maraschino cherries, drained"
        },
        {
            "10": "1 cup chopped walnuts"
        }
    ],
    "Directions": [
        {
            "1": "Prepare gelatin according to package directions, substituting 1 cup cold water with 1 cup crushed ice. Place gelatin into freezer until almost congealed."
        },
        {
            "2": "Place cream cheese in a bowl and beat until smooth. Fold cream cheese into gelatin. Add cottage cheese, fruit cocktail, pineapple, oranges, apples, banana, cherries and walnuts. Mix well."
        },
        {
            "3": "Pour into mold or dish and refrigerate until completely set."
        }
    ],
    "Details": [
        {
            "Servings": "9 "
        },
        {
            "Yield": "8 to 10 servings"
        }
    ],
