{
    "title": "Orange Sherbet Salad",
    "Category": "Jell-O\u00ae Salad",
    "Ingredients": [
        {
            "1": "2 (3 ounce) packages orange flavored gelatin (such as JELL-O\u00ae)"
        },
        {
            "2": "2 cups boiling water"
        },
        {
            "3": "1 pint orange sherbet"
        },
        {
            "4": "1 (10 ounce) can mandarin oranges, drained"
        }
    ],
    "Directions": [
        {
            "1": "Whisk gelatin into boiling water until dissolved; allow to cool for 10 minutes."
        },
        {
            "2": "Stir in orange sherbet until completely melted. Once gelatin begins to thicken, stir in mandarin oranges. Pour into a gelatin mold; refrigerate until set, about 6 hours."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Additional Time": " 6 hrs 10 mins"
        },
        {
            "Total Time": " 6 hrs 15 mins"
        },
        {
            "Servings": "6 "
        }
    ],
