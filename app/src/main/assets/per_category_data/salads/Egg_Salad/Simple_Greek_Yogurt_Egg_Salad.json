{
    "title": "Simple Greek Yogurt Egg Salad",
    "Category": "Egg Salad",
    "Ingredients": [
        {
            "1": "0.25 cup Greek yogurt"
        },
        {
            "2": "1 tablespoon brown sugar"
        },
        {
            "3": "8  hard-boiled eggs, peeled and chopped"
        },
        {
            "4": "0.25 cup chopped celery"
        },
        {
            "5": "0.25 cup chopped green onion"
        },
        {
            "6": "2 tablespoons sweet pickle relish"
        },
        {
            "7": "2 tablespoons yellow mustard"
        },
        {
            "8": "0.25 teaspoon dill weed"
        },
        {
            "9": "0.25 teaspoon paprika"
        },
        {
            "10": "1 ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Whisk yogurt and brown sugar together in a bowl. Add eggs, celery, green onion, relish, mustard, dill, paprika, and black pepper; mix well."
        }
    ],
