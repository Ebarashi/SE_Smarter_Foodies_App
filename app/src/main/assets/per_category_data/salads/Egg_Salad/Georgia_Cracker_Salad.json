{
    "title": "Georgia Cracker Salad",
    "Category": "Egg Salad",
    "Ingredients": [
        {
            "1": "1 (4 ounce) packet saltine crackers"
        },
        {
            "2": "1 large tomato, chopped"
        },
        {
            "3": "3  green onions, finely chopped"
        },
        {
            "4": "0.5 cups mayonnaise"
        },
        {
            "5": "1  hard-cooked egg, chopped"
        },
        {
            "6": "1 salt and freshly ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Coarsely crumble the saltines into a medium bowl. Mix in the tomato, onions and mayonnaise. Stir in the hard-cooked egg, and season with salt and pepper."
        }
    ],
