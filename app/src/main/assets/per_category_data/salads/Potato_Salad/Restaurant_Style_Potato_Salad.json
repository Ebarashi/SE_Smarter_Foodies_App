{
    "title": "Restaurant-Style Potato Salad",
    "Category": "Potato Salad",
    "Ingredients": [
        {
            "1": "2 pounds russet potatoes"
        },
        {
            "2": "1 cup mayonnaise"
        },
        {
            "3": "4 teaspoons sweet pickle relish"
        },
        {
            "4": "4 teaspoons white sugar"
        },
        {
            "5": "2 teaspoons chopped white onion"
        },
        {
            "6": "2 teaspoons prepared mustard"
        },
        {
            "7": "1 teaspoon white wine vinegar"
        },
        {
            "8": "1 tablespoon minced celery"
        },
        {
            "9": "1 teaspoon minced pimento"
        },
        {
            "10": "0.5 teaspoon shredded carrot"
        },
        {
            "11": "0.25 teaspoon dried parsley"
        },
        {
            "12": "0.25 teaspoon ground black pepper"
        },
        {
            "13": "1 salt to taste"
        }
    ],
    "Directions": [
        {
            "1": "Bring a large pot of salted water to a boil. Add potatoes and cook until tender but still firm, about 15 minutes. Drain, cool, and chop."
        },
        {
            "2": "In a large bowl, combine potatoes, mayonnaise, sweet pickle relish, sugar, onion, mustard, vinegar, celery, pimentos, carrot, parsley, pepper, and salt. Mix well, chill and serve."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "15 mins"
        },
        {
            "Total Time": "30 mins"
        },
        {
            "Servings": "8 "
        }
    ],
