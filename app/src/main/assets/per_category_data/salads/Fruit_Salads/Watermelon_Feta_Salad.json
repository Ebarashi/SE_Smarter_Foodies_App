{
    "title": "Watermelon Feta Salad",
    "Category": "Fruit Salads",
    "Ingredients": [
        {
            "1": "3 tablespoons olive oil"
        },
        {
            "2": "2 teaspoons white balsamic vinegar"
        },
        {
            "3": "0.5 teaspoon kosher salt"
        },
        {
            "4": "8 ounces grape tomatoes, halved"
        },
        {
            "5": "3 cups arugula"
        },
        {
            "6": "1 cup sliced red onion"
        },
        {
            "7": "2 pounds watermelon, cubed"
        },
        {
            "8": "4 ounces feta cheese, cut into 1/4-inch cubes"
        }
    ],
    "Directions": [
        {
            "1": "Whisk olive oil, vinegar, and salt together in a large mixing bowl. Add tomatoes, arugula, and red onion; toss to coat. Gently stir in watermelon and feta cheese to serve."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Total Time": "20 mins"
        },
        {
            "Servings": "8 "
        }
    ],
