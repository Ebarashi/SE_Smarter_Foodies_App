{
    "title": "Summer Fruit Salad with a Lemon, Honey, and Mint Dressing",
    "Category": "Fruit Salads",
    "Ingredients": [
        {
            "1": "4 cups cubed seeded watermelon"
        },
        {
            "2": "2 cups sliced fresh strawberries"
        },
        {
            "3": "2 large  fresh peaches, cut into cubes"
        },
        {
            "4": "2 large  nectarines, cut into cubes"
        },
        {
            "5": "1  red Anjou pear, cut into cubes"
        },
        {
            "6": "1 cup seedless grapes, halved"
        },
        {
            "7": "2  lemons, juiced"
        },
        {
            "8": "0.25 cup minced fresh mint (chocolate mint preferred)"
        },
        {
            "9": "0.5  lemon, zested"
        },
        {
            "10": "1 tablespoon honey (fireweed honey preferred)"
        }
    ],
    "Directions": [
        {
            "1": "Combine watermelon, strawberries, peaches, nectarines, pear, and grapes in a large mixing bowl."
        },
        {
            "2": "Whisk lemon juice, mint, lemon zest, and honey together in a small bowl; drizzle over the fruit mixture and toss to coat."
        },
        {
            "3": "Refrigerate 1 hour before serving."
        }
    ],
