{
    "title": "Easy Caramel Apple Salad",
    "Category": "Fruit Salads",
    "Ingredients": [
        {
            "1": "1 (8 ounce) container frozen whipped topping, thawed"
        },
        {
            "2": "1 (8 ounce) can crushed pineapple"
        },
        {
            "3": "1 (3.4 ounce) package instant butterscotch pudding mix"
        },
        {
            "4": "2 cups chopped apples"
        },
        {
            "5": "1 cup skinless peanuts"
        }
    ],
    "Directions": [
        {
            "1": "Stir whipped topping, pineapple, and butterscotch pudding mix together in a bowl until smooth. Fold apples and peanuts into pudding mixture until salad is well mixed."
        },
        {
            "2": "Refrigerate salad until completely chilled, at least 30 minutes."
        }
    ],
