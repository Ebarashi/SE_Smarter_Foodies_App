{
    "title": "Christmas Cranberry Salad",
    "Category": "Fruit Salads",
    "Ingredients": [
        {
            "1": "1 pound cranberries, finely ground"
        },
        {
            "2": "2 cups white sugar"
        },
        {
            "3": "1 (20 ounce) can crushed pineapple, drained"
        },
        {
            "4": "1 (16 ounce) package miniature marshmallows"
        },
        {
            "5": "1 cup chopped pecans (Optional)"
        },
        {
            "6": "1 pint whipped cream, beaten stiff"
        }
    ],
    "Directions": [
        {
            "1": "Mix together the cranberries and sugar; cover and refrigerate overnight."
        },
        {
            "2": "The next day, combine the cranberry mixture with the pineapple, marshmallows, pecans and whipped cream. Mix well."
        },
        {
            "3": "Pour into a 3 quart dish, cover and refrigerate or freeze until ready to serve."
        }
    ],
    "Details": [
        {
            "Servings": "6 "
        },
        {
            "Yield": "6 to 8 servings"
        }
    ],
