{
    "title": "Avocado Watermelon Salad",
    "Category": "Fruit Salads",
    "Ingredients": [
        {
            "1": "4 cups cubed watermelon"
        },
        {
            "2": "4 cups fresh baby spinach, torn"
        },
        {
            "3": "2 large avocados - peeled, pitted, and diced"
        },
        {
            "4": "0.25 cup walnut oil"
        },
        {
            "5": "0.25 cup olive oil"
        },
        {
            "6": "1  lime, juiced"
        },
        {
            "7": "0.5 teaspoon sweet paprika"
        }
    ],
    "Directions": [
        {
            "1": "Combine watermelon, spinach, and avocados together in a bowl."
        },
        {
            "2": "Whisk walnut oil, olive oil, lime juice, and paprika together in a bowl; pour over watermelon mixture. Toss to coat."
        }
    ],
