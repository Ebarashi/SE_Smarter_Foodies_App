{
    "title": "Lazy Days Ambrosia",
    "Category": "Fruit Salads",
    "Ingredients": [
        {
            "1": "1 (20 ounce) can crushed pineapple, undrained"
        },
        {
            "2": "1 (3.4 ounce) package instant vanilla pudding mix"
        },
        {
            "3": "2 (15 ounce) cans fruit cocktail, drained"
        },
        {
            "4": "2 (11 ounce) cans mandarin oranges, drained"
        },
        {
            "5": "1 (8 ounce) container frozen whipped topping, thawed"
        },
        {
            "6": "2 tablespoons sweetened flaked coconut, or more to taste (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Mix undrained pineapple and pudding mix together in a bowl until smooth, about 2 minutes. Fold in fruit cocktail and oranges, then fold in whipped topping. Sprinkle coconut over top. Cover and refrigerate until chilled, at least 30 minutes."
        }
    ],
