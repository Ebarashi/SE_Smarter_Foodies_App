{
    "title": "Tajin\u00ae Fruit Salad",
    "Category": "Fruit Salads",
    "Ingredients": [
        {
            "1": "2 large  oranges, peeled and segmented"
        },
        {
            "2": "1  mango, diced"
        },
        {
            "3": "1 cup diced fresh pineapple"
        },
        {
            "4": "1 cup diced jicama"
        },
        {
            "5": "1 small cucumber, sliced"
        },
        {
            "6": "1  banana, sliced"
        },
        {
            "7": "2  limes, juiced"
        },
        {
            "8": "1 tablespoon chile-lime seasoning (such as Tajin\u00ae), or more to taste"
        },
        {
            "9": "0.5 teaspoon chopped fresh mint, or to taste (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Cut orange segments in half and place in a bowl. Add mango, pineapple, jicama, cucumber, and banana. Add lime juice and sprinkle with chile-lime seasoning; toss to coat. Sprinkle with mint."
        }
    ],
