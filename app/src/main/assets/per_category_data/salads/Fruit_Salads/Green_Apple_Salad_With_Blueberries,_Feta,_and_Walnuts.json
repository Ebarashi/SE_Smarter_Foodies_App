{
    "title": "Green Apple Salad With Blueberries, Feta, and Walnuts",
    "Category": "Fruit Salads",
    "Ingredients": [
        {
            "1": "1 Salad-"
        },
        {
            "2": "4 cups mixed salad greens such as leaf lettuce, endive, and radicchio"
        },
        {
            "3": "1 large Granny Smith apple, cubed"
        },
        {
            "4": "0.5 cup chopped walnuts"
        },
        {
            "5": "0.5 cup crumbled feta cheese"
        },
        {
            "6": "0.5 cup dried cranberries"
        },
        {
            "7": "2 tablespoons finely chopped green onions (Optional)"
        },
        {
            "8": "1 Dressing-"
        },
        {
            "9": "0.25 cup vegetable oil"
        },
        {
            "10": "0.25 cup blueberries"
        },
        {
            "11": "2 tablespoons extra-virgin olive oil"
        },
        {
            "12": "2 tablespoons balsamic vinegar"
        },
        {
            "13": "0.25 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Make the salad: Layer salad greens, apple cubes, walnuts, feta cheese, cranberries, and green onions in a large bowl."
        },
        {
            "2": "Make the dressing: Combine vegetable oil, blueberries, olive oil, vinegar, and salt in a food processor or blender; pulse until well blended and smooth."
        },
        {
            "3": "Pour dressing over salad and toss until well combined. Chill for 30 minutes before serving."
        }
    ],
