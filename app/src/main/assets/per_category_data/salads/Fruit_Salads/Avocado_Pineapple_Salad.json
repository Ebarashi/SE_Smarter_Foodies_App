{
    "title": "Avocado Pineapple Salad",
    "Category": "Fruit Salads",
    "Ingredients": [
        {
            "1": "0.5 cups cucumbers - peeled, seeded, and cubed"
        },
        {
            "2": "0.5 cup chopped red onion"
        },
        {
            "3": "2 tablespoons chopped serrano pepper"
        },
        {
            "4": "2 tablespoons lime juice"
        },
        {
            "5": "0.75 teaspoon salt"
        },
        {
            "6": "1.0 cups pineapple, peeled and cut into 1/2-inch dice"
        },
        {
            "7": "2  avocado - peeled, pitted and diced"
        }
    ],
    "Directions": [
        {
            "1": "Combine cucumbers, onion, serrano pepper, lime juice, and salt in a large bowl. Add pineapple and avocado; gently toss to combine."
        }
    ],
