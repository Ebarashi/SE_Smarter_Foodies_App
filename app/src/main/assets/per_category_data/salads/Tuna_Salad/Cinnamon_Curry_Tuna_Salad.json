{
    "title": "Cinnamon-Curry Tuna Salad",
    "Category": "Tuna Salad",
    "Ingredients": [
        {
            "1": "2 (5 ounce) cans water packed tuna, drained and flaked"
        },
        {
            "2": "2 teaspoons mayonnaise"
        },
        {
            "3": "1 teaspoon Dijon mustard"
        },
        {
            "4": "1 tablespoon sweet pickle relish"
        },
        {
            "5": "2 teaspoons lemon juice"
        },
        {
            "6": "0.5 teaspoons ground cinnamon"
        },
        {
            "7": "1 teaspoon curry powder"
        },
        {
            "8": "1 teaspoon ground black pepper"
        },
        {
            "9": "1 salt to taste"
        }
    ],
    "Directions": [
        {
            "1": "In a bowl, mix the tuna, mayonnaise, mustard, relish, lemon juice, cinnamon, curry powder, pepper, and salt. Cover, and refrigerate until ready to serve."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Total Time": "10 mins"
        },
        {
            "Servings": "4 "
        },
        {
            "Yield": "4 servings"
        }
    ],
