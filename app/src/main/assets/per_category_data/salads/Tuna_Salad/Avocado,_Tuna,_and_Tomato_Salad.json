{
    "title": "Avocado, Tuna, and Tomato Salad",
    "Category": "Tuna Salad",
    "Ingredients": [
        {
            "1": "1  avocado"
        },
        {
            "2": "1 (5 ounce) can tuna, drained"
        },
        {
            "3": "1 small tomato, chopped"
        },
        {
            "4": "0.25 cup crumbled feta cheese"
        },
        {
            "5": "1  jalapeno pepper, finely chopped (Optional)"
        },
        {
            "6": "1  green onion, chopped"
        },
        {
            "7": "2 cloves garlic, minced"
        },
        {
            "8": "3 tablespoons chopped fresh cilantro"
        },
        {
            "9": "2 tablespoons lime juice"
        },
        {
            "10": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Split avocado. Save skins and discard pit. Scoop contents into a bowl; slightly mash."
        },
        {
            "2": "Mix mashed avocado, tuna, tomato, feta cheese, jalapeno pepper, green onion, feta cheese, garlic, cilantro, lime juice, salt, and pepper together in a bowl."
        },
        {
            "3": "Scoop mixture back into reserved avocado skins and serve immediately or chill until serving."
        }
    ],
