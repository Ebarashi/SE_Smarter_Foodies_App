{
    "title": "Honey Mustard Tuna Salad",
    "Category": "Tuna Salad",
    "Ingredients": [
        {
            "1": "2 (6 ounce) cans albacore tuna in water, drained and flaked"
        },
        {
            "2": "3 tablespoons honey mustard"
        },
        {
            "3": "2 tablespoons sweet pickle relish"
        },
        {
            "4": "1 tablespoon mayonnaise"
        },
        {
            "5": "0.25 teaspoon salt"
        },
        {
            "6": "0.25 teaspoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Combine tuna, honey mustard, pickle relish, mayonnaise, salt, and pepper in a bowl. Mix until well combined."
        }
    ],
