{
    "title": "Asian Tuna Salad",
    "Category": "Tuna Salad",
    "Ingredients": [
        {
            "1": "5 (5 ounce) cans chunk light tuna in water, drained and flaked"
        },
        {
            "2": "0.5 cup sweetened dried cranberries (such as Craisins\u00ae)"
        },
        {
            "3": "0.5 cup finely chopped carrot"
        },
        {
            "4": "0.25 cup chopped green onions"
        },
        {
            "5": "0.25 cup finely chopped cilantro"
        },
        {
            "6": "1 tablespoon sesame seeds"
        },
        {
            "7": "0.5 teaspoons ground ginger"
        },
        {
            "8": "0.5 teaspoons ground red chile pepper"
        },
        {
            "9": "0.75 teaspoon kosher salt"
        },
        {
            "10": "0.5 cup light mayonnaise"
        },
        {
            "11": "0.25 cup Asian chile paste (such as sambal oelek)"
        },
        {
            "12": "2 tablespoons sesame oil"
        },
        {
            "13": "1 tablespoon light soy sauce"
        },
        {
            "14": "1 tablespoon honey"
        }
    ],
    "Directions": [
        {
            "1": "Mix tuna, cranberries, carrot, green onions, cilantro, sesame seeds, ginger, ground chile pepper, and kosher salt together in a large bowl."
        },
        {
            "2": "Whisk mayonnaise, chile paste, sesame oil, soy sauce, and honey together in a separate bowl; add to tuna mixture and stir to coat."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Total Time": "20 mins"
        },
        {
            "Servings": "12 "
        },
