{
    "title": "Zesty Tuna Salad",
    "Category": "Tuna Salad",
    "Ingredients": [
        {
            "1": "1 (5 ounce) can tuna, drained"
        },
        {
            "2": "1 teaspoon mayonnaise"
        },
        {
            "3": "1 teaspoon sweet pickle relish"
        },
        {
            "4": "1 teaspoon Dijon-style prepared mustard"
        },
        {
            "5": "1 stalk celery, chopped"
        },
        {
            "6": "0.25 cup chopped onion"
        },
        {
            "7": "0.25 teaspoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Mash tuna in a small bowl with a fork. Add mayonnaise, pickle relish, mustard, celery, onion, and black pepper; stir to combine. Chill and serve."
        }
    ],
