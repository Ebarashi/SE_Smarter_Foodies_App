{
    "title": "Zippy Tuna",
    "Category": "Tuna Salad",
    "Ingredients": [
        {
            "1": "1 (5 ounce) can chunk light tuna in water, drained and flaked"
        },
        {
            "2": "2 tablespoons mayonnaise"
        },
        {
            "3": "1 tablespoon prepared yellow mustard"
        },
        {
            "4": "0.5 teaspoons creamy prepared horseradish"
        },
        {
            "5": "1 teaspoon lemon juice"
        },
        {
            "6": "1 teaspoon garlic powder"
        },
        {
            "7": "1 pinch salt"
        }
    ],
    "Directions": [
        {
            "1": "Mix tuna, mayonnaise, yellow mustard, horseradish, lemon juice, garlic powder, and salt together in a bowl."
        }
    ],
