{
    "title": "Healthier Mediterranean Tuna Salad",
    "Category": "Tuna Salad",
    "Ingredients": [
        {
            "1": "2 (5 ounce) cans chunk light tuna in water, drained"
        },
        {
            "2": "0.25 cup finely chopped red onion"
        },
        {
            "3": "0.25 cup chopped fresh parsley"
        },
        {
            "4": "3 tablespoons olive oil"
        },
        {
            "5": "2 tablespoons freshly squeezed lemon juice"
        },
        {
            "6": "0.5 teaspoon lemon zest"
        },
        {
            "7": "0.25 teaspoon salt"
        },
        {
            "8": "0.25 teaspoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Combine tuna, onion, and parsley in a medium bowl."
        },
        {
            "2": "Whisk oil, lemon juice, lemon zest, salt, and pepper to make the dressing. Toss with the tuna mixture."
        }
    ],
