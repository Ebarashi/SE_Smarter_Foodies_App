{
    "title": "Cold Tuna Macaroni Salad",
    "Category": "Tuna Salad",
    "Ingredients": [
        {
            "1": "1 (12 ounce) package macaroni"
        },
        {
            "2": "3  tomatoes - peeled, seeded and diced"
        },
        {
            "3": "3 stalks celery, chopped"
        },
        {
            "4": "1 (12 ounce) can tuna"
        },
        {
            "5": "0.5 cups light mayonnaise"
        },
        {
            "6": "1 tablespoon Italian-style salad dressing"
        },
        {
            "7": "1 tablespoon white sugar"
        },
        {
            "8": "1 pinch ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "In a large pot of salted boiling water, cook pasta until al dente; rinse under cold water and drain."
        },
        {
            "2": "In a large bowl, combine the pasta, tomatoes, celery, and tuna."
        },
        {
            "3": "Prepare the dressing by whisking together the mayonnaise, salad dressing, sugar, and pepper. Add to pasta salad, mix together, and refrigerate for 1 hour."
        }
    ],
