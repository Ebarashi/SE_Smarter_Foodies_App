{
    "title": "Tuna Salad",
    "Category": "Tuna Salad",
    "Ingredients": [
        {
            "1": "1 (5 ounce) can tuna, drained"
        },
        {
            "2": "1 (15 ounce) can chickpeas (garbanzo beans), drained"
        },
        {
            "3": "2  hard-boiled eggs, chopped"
        },
        {
            "4": "1  cucumber, peeled and diced"
        },
        {
            "5": "1 head iceberg lettuce, torn into bite-sized pieces"
        },
        {
            "6": "0.5 cup French dressing"
        }
    ],
    "Directions": [
        {
            "1": "In a large mixing bowl, combine tuna, chickpeas (garbanzo beans), chopped hard-boiled eggs, diced cucumber, and iceberg lettuce."
        },
        {
            "2": "Toss well and add French dressing to taste."
        }
    ],
