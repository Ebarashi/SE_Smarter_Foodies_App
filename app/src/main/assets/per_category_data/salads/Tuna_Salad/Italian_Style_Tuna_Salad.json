{
    "title": "Italian-Style Tuna Salad",
    "Category": "Tuna Salad",
    "Ingredients": [
        {
            "1": "3 tablespoons olive oil"
        },
        {
            "2": "1 tablespoon red wine vinegar"
        },
        {
            "3": "1 salt and ground black pepper to taste"
        },
        {
            "4": "2 slices Italian bread, cubed"
        },
        {
            "5": "3  tomatoes, cut into small pieces"
        },
        {
            "6": "1 medium cucumber, peeled and thinly sliced"
        },
        {
            "7": "1 small onion, thinly sliced"
        },
        {
            "8": "1 (5 ounce) can tuna, drained"
        },
        {
            "9": "20  black olives"
        },
        {
            "10": "1 tablespoon capers"
        }
    ],
    "Directions": [
        {
            "1": "Mix olive oil, red wine vinegar, salt, and pepper together for the dressing."
        },
        {
            "2": "Mix bread, tomatoes, cucumber, onion, tuna, olives, and capers together gently. Pour dressing on top and serve."
        }
    ],
