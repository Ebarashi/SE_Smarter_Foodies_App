{
    "title": "Tuna Salad without Mayonnaise",
    "Category": "Tuna Salad",
    "Ingredients": [
        {
            "1": "1 (5 ounce) can tuna in water (such as StarKist\u00ae Chunk Light), drained"
        },
        {
            "2": "0.25 cup Italian-style salad dressing"
        },
        {
            "3": "2 teaspoons prepared yellow mustard"
        },
        {
            "4": "1 teaspoon tomato relish"
        },
        {
            "5": "1 teaspoon celery seed"
        },
        {
            "6": "1 teaspoon onion powder"
        },
        {
            "7": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Mix tuna, salad dressing, mustard, relish, celery seed, onion powder, salt, and pepper together in a bowl."
        },
        {
            "2": "Place in the refrigerator for 1 hour."
        }
    ],
