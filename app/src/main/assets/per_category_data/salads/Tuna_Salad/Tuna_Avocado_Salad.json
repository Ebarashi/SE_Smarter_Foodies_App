{
    "title": "Tuna Avocado Salad",
    "Category": "Tuna Salad",
    "Ingredients": [
        {
            "1": "2 medium avocados"
        },
        {
            "2": "1 (5 ounce) can light tuna (such as Century\u00ae)"
        },
        {
            "3": "2 small tomatoes, chopped"
        },
        {
            "4": "1 tablespoon chopped fresh cilantro, or to taste"
        },
        {
            "5": "0.5 tablespoon ground black pepper"
        },
        {
            "6": "1 pinch chili powder, or more to taste"
        },
        {
            "7": "1 salt to taste"
        }
    ],
    "Directions": [
        {
            "1": "Scoop out avocado contents and place in a bowl. Set aside shells."
        },
        {
            "2": "Mix in tuna, tomatoes, cilantro, pepper, chili powder, and salt. Divide mixture between shells to serve."
        }
    ],
