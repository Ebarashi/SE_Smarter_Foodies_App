{
    "title": "Shrimp Etouffee II",
    "Category": "Etouffee",
    "Ingredients": [
        {
            "1": "0.25 cup margarine"
        },
        {
            "2": "0.5 cup chopped onion"
        },
        {
            "3": "0.5 cup chopped green onion"
        },
        {
            "4": "0.5 cup chopped green bell pepper"
        },
        {
            "5": "4 cloves minced garlic"
        },
        {
            "6": "0.5 cup celery, diced"
        },
        {
            "7": "0.5 cup chopped fresh parsley"
        },
        {
            "8": "3 tablespoons tomato paste"
        },
        {
            "9": "1 (10.75 ounce) can  condensed cream of chicken soup"
        },
        {
            "10": "1 pound cleaned shrimp"
        },
        {
            "11": "1 salt to taste"
        },
        {
            "12": "0.25 teaspoon hot pepper sauce to taste"
        },
        {
            "13": "0.25 teaspoon cayenne pepper"
        }
    ],
    "Directions": [
        {
            "1": "In a 2 quart microwave safe dish, combine margarine, onion, green onion, bell pepper, garlic and celery. Heat on High settings for 8 to 9 minutes."
        },
        {
            "2": "Stir in parsley, tomato paste, soup, shrimp, salt, hot pepper sauce and cayenne. Heat on High setting for 5 minutes. Stir and cook for another 5 minutes until mixture thickens. Serve over white rice."
        }
    ],
