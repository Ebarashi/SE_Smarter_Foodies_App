{
    "title": "Gnocchi and Zucchini with Garlic-Parmesan Sauce",
    "Category": "Gnocchi",
    "Ingredients": [
        {
            "1": "1 pound gnocchi"
        },
        {
            "2": "2 tablespoons salted butter"
        },
        {
            "3": "1 tablespoon olive oil"
        },
        {
            "4": "2  zucchini, sliced"
        },
        {
            "5": "2 teaspoons chopped garlic"
        },
        {
            "6": "0.5 cup grated Parmesan cheese"
        }
    ],
    "Directions": [
        {
            "1": "Bring a large pot of lightly salted water to a boil. Cook gnocchi in the boiling water until they float to the top, 2 to 4 minutes. Drain, reserving 1/4 cup cooking liquid."
        },
        {
            "2": "Melt butter in a saucepan over medium heat and add olive oil. Add zucchini and garlic. Saute until tender, 5 to 7 minutes. Add cooked and drained gnocchi, Parmesan cheese, and reserved cooking liquid. Stir until nice and creamy."
        }
    ],
