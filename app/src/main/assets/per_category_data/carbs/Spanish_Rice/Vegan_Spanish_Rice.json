{
    "title": "Vegan Spanish Rice",
    "Category": "Spanish Rice",
    "Ingredients": [
        {
            "1": "2 tablespoons olive oil"
        },
        {
            "2": "1 cup uncooked brown rice"
        },
        {
            "3": "0.5  red bell pepper, chopped"
        },
        {
            "4": "1  onion, chopped"
        },
        {
            "5": "2 teaspoons crushed garlic"
        },
        {
            "6": "2 cups water"
        },
        {
            "7": "1 (10 ounce) can diced tomatoes"
        },
        {
            "8": "2 teaspoons chili powder"
        },
        {
            "9": "1 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Heat oil in a deep skillet over medium heat. Add rice, red bell pepper, onion, and garlic. Cook and stir until rice is browned and onion is tender, about 5 minutes. Stir in water and diced tomatoes. Season with chili powder and salt. Cover and simmer until rice is cooked, about 30 minutes."
        }
    ],
