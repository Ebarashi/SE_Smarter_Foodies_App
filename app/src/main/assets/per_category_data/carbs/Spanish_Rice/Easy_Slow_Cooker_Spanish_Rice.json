{
    "title": "Easy Slow Cooker Spanish Rice",
    "Category": "Spanish Rice",
    "Ingredients": [
        {
            "1": "2 pounds ground beef"
        },
        {
            "2": "1 (28 ounce) can whole peeled tomatoes"
        },
        {
            "3": "2 (8 ounce) cans tomato sauce"
        },
        {
            "4": "2  green bell peppers, chopped"
        },
        {
            "5": "1 cup water"
        },
        {
            "6": "2  yellow onions, chopped"
        },
        {
            "7": "1 cup converted raw rice"
        },
        {
            "8": "2 tablespoons Worcestershire sauce"
        },
        {
            "9": "2 tablespoons chili powder"
        },
        {
            "10": "0.5 teaspoon hot sauce, or to taste"
        },
        {
            "11": "1 cup shredded Cheddar cheese, or to taste"
        }
    ],
    "Directions": [
        {
            "1": "Heat a large skillet over medium-high heat. Cook and stir beef in the hot skillet until lightly browned and crumbly, 5 to 7 minutes. Drain and discard fat."
        },
        {
            "2": "Transfer beef to a slow cooker. Add tomatoes, tomato sauce, green bell peppers, water, onions, rice, Worcestershire sauce, chili powder, and hot sauce. Cook on High until rice is tender, about 3 hours. Serve in bowls and top with Cheddar cheese."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": " 3 hrs 5 mins"
        },
