{
    "title": "Spaghetti With Marinara Sauce",
    "Category": "Spaghetti",
    "Ingredients": [
        {
            "1": "1 pound spaghetti"
        },
        {
            "2": "1 (28 ounce) can crushed tomatoes"
        },
        {
            "3": "1 (14.5 ounce) can diced tomatoes"
        },
        {
            "4": "1 (15 ounce) can tomato sauce"
        },
        {
            "5": "1 tablespoon minced garlic"
        },
        {
            "6": "2 teaspoons white sugar"
        },
        {
            "7": "2 teaspoons dried parsley"
        },
        {
            "8": "1 teaspoon garlic powder"
        },
        {
            "9": "0.5 teaspoon salt"
        },
        {
            "10": "0.25 teaspoon dried oregano"
        },
        {
            "11": "0.25 teaspoon dried basil"
        },
        {
            "12": "0.25 teaspoon ground black pepper"
        },
        {
            "13": "0.5 tablespoons capers"
        },
        {
            "14": "1 pinch crushed red pepper flakes (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "In a large saucepan combine crushed tomatoes, diced tomatoes, tomato sauce, minced garlic, sugar, parsley, garlic powder, salt, oregano, basil, and ground black pepper. Add capers and crushed red pepper if desired. Cover. Bring to a boil."
        },
        {
            "2": "Lower heat and simmer, with cover, for 45 to 60 minutes."
        },
        {
            "3": "As simmering time nears, in a large pot with boiling salted water cook spaghetti until al dente."
        },
        {
            "4": "Toss spaghetti with cooked sauce. Serve warm."
        }
    ],
    "Details": [
        {
            "Servings": "8 "
        },
        {
            "Yield": "8 servings"
        }
    ],
