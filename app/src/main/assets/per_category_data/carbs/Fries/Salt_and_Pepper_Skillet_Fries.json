{
    "title": "Salt and Pepper Skillet Fries",
    "Category": "Fries",
    "Ingredients": [
        {
            "1": "2 tablespoons olive oil"
        },
        {
            "2": "1 teaspoon butter"
        },
        {
            "3": "3 large potatoes, sliced lengthwise into 1/2-inch circles and cut into 1/2-inch strips"
        },
        {
            "4": "0.25 teaspoon coarse sea salt"
        },
        {
            "5": "0.25 teaspoon cracked black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Heat olive oil and butter in a skillet over medium heat."
        },
        {
            "2": "Cook and stir potato strips in hot oil mixture until browned on all sides, 20 to 25 minutes."
        },
        {
            "3": "Transfer fries to a paper towel-lined plate; season with sea salt and cracked black pepper."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
