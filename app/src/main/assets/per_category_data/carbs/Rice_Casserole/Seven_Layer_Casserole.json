{
    "title": "Seven Layer Casserole",
    "Category": "Rice Casserole",
    "Ingredients": [
        {
            "1": "0.5 cup uncooked long-grain white rice"
        },
        {
            "2": "3  potatoes, sliced"
        },
        {
            "3": "1  onion, sliced into rings"
        },
        {
            "4": "2  carrots, cut into 1 inch pieces"
        },
        {
            "5": "1 (15 ounce) can green peas, with liquid"
        },
        {
            "6": "1 (10.75 ounce) can  condensed tomato soup"
        },
        {
            "7": "1 (10.75 ounce) can  water"
        },
        {
            "8": "1 pound pork sausage, sliced and browned"
        },
        {
            "9": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "In a lightly greased 9x13 inch baking dish layer the rice, potatoes, onion, carrots, peas, tomato soup with water, and sausage. Salt and pepper to taste."
        },
        {
            "3": "Bake covered at 350 degrees F (175 degrees C) for 45 minutes, or until fork tender. Remove cover for last 5 minutes of baking."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
