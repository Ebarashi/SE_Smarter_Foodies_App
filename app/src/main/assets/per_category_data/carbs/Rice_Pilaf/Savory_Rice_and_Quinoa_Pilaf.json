{
    "title": "Savory Rice and Quinoa Pilaf",
    "Category": "Rice Pilaf",
    "Ingredients": [
        {
            "1": "6 cups vegetable broth"
        },
        {
            "2": "2 cups basmati rice"
        },
        {
            "3": "1 cup quinoa"
        },
        {
            "4": "2  tomatoes, chopped (Optional)"
        },
        {
            "5": "0.25 cup raisins (Optional)"
        },
        {
            "6": "1 tablespoon chopped fresh parsley"
        },
        {
            "7": "1 teaspoon salt"
        },
        {
            "8": "1 teaspoon ground black pepper"
        },
        {
            "9": "0.5 teaspoon ground allspice"
        },
        {
            "10": "0.5 teaspoon dried sage"
        }
    ],
    "Directions": [
        {
            "1": "Bring vegetable broth to a boil in a large saucepan. Stir in basmati rice, quinoa, tomatoes, raisins, parsley, salt, pepper, allspice, and sage. Reduce heat and simmer until broth is absorbed and quinoa is tender, about 20 minutes."
        }
    ],
