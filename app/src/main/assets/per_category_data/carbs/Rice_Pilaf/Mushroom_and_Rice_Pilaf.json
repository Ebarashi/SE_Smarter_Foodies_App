{
    "title": "Mushroom and Rice Pilaf",
    "Category": "Rice Pilaf",
    "Ingredients": [
        {
            "1": "1 tablespoon butter"
        },
        {
            "2": "0.75 cup chopped yellow onion"
        },
        {
            "3": "1 (10.5 ounce) can condensed beef consomme"
        },
        {
            "4": "1 (7 ounce) can mushroom stems and pieces"
        },
        {
            "5": "0.25 cups instant white rice (such as Minute\u00ae)"
        }
    ],
    "Directions": [
        {
            "1": "Melt butter in a skillet over medium heat. Add onion and saute until the onion has softened and turned golden brown, 5 to 8 minutes."
        },
        {
            "2": "Add consomme and mushrooms; bring to a boil."
        },
        {
            "3": "Remove from heat and stir in rice. Cover and let sit until rice has absorbed all liquid, about 5 minutes."
        }
    ],
