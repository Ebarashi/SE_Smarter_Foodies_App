{
    "title": "Ground Beef and Potato Tostadas",
    "Category": "Tostadas",
    "Ingredients": [
        {
            "1": "1 pound ground beef"
        },
        {
            "2": "1 small russet potato, peeled and diced"
        },
        {
            "3": "1 (1.25 ounce) package taco seasoning mix"
        },
        {
            "4": "0.5 cups shredded lettuce"
        },
        {
            "5": "1 (3.25 ounce) can sliced black olives, drained"
        },
        {
            "6": "1 cup shredded Mexican cheese blend"
        },
        {
            "7": "6  crispy tostada shells"
        }
    ],
    "Directions": [
        {
            "1": "Crumble beef in a large skillet and cook over medium-high heat until the fat has started to render, about 2 minutes. Stir in potatoes and cook for 10 minutes. Stir in taco seasoning and cook for 2 minutes more."
        },
        {
            "2": "Divide beef mixture, lettuce, olives, and Mexican cheese blend among the tostada shells."
        }
    ],
