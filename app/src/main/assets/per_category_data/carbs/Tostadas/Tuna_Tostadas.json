{
    "title": "Tuna Tostadas",
    "Category": "Tostadas",
    "Ingredients": [
        {
            "1": "1 (8 ounce) can tuna"
        },
        {
            "2": "5 tablespoons mayonnaise"
        },
        {
            "3": "4 tablespoons salsa"
        },
        {
            "4": "6 tablespoons vegetable oil"
        },
        {
            "5": "4 (6 inch) corn tortillas"
        },
        {
            "6": "1 large tomato, diced"
        },
        {
            "7": "0.5 cup chopped fresh spinach"
        },
        {
            "8": "0.5 cup diced onion"
        },
        {
            "9": "4 tablespoons shredded Cheddar cheese"
        },
        {
            "10": "1  avocado, sliced"
        }
    ],
    "Directions": [
        {
            "1": "Mix tuna, mayonnaise, and salsa together in a bowl. Set aside."
        },
        {
            "2": "Heat vegetable oil over medium-high heat in a frying pan. Add tortillas in batches and fry until golden and slightly crisp, 2 to 3 minutes."
        },
        {
            "3": "Spread tuna mixture over tortillas evenly. Top with tomato, spinach, and onions. Sprinkle with Cheddar cheese and top with avocado slices."
        }
    ],
