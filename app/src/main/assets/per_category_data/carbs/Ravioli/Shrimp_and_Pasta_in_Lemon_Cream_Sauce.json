{
    "title": "Shrimp and Pasta in Lemon Cream Sauce",
    "Category": "Ravioli",
    "Ingredients": [
        {
            "1": "1 (9 ounce) package BUITONI\u00ae Refrigerated Light Four Cheese Ravioli, prepared according to package directions"
        },
        {
            "2": "12 ounces cooked medium shrimp"
        },
        {
            "3": "1 (10 ounce) container BUITONI\u00ae Refrigerated Light Alfredo Sauce"
        },
        {
            "4": "1 tablespoon lemon juice"
        },
        {
            "5": "1 teaspoon grated lemon peel"
        },
        {
            "6": "1 tablespoon Chopped fresh parsley"
        }
    ],
    "Directions": [
        {
            "1": "Combine shrimp, sauce, lemon juice and lemon peel in small saucepan. Heat over medium-low heat, stirring occasionally, until heated through (do not boil)."
        },
        {
            "2": "Serve over pasta. Sprinkle with parsley."
        }
    ],
