{
    "title": "Easy Quinoa and Edamame Salad",
    "Category": "Quinoa",
    "Ingredients": [
        {
            "1": "1 Dressing-"
        },
        {
            "2": "0.25 cup olive oil"
        },
        {
            "3": "0.25 cup rice wine vinegar"
        },
        {
            "4": "1 tablespoon maple syrup"
        },
        {
            "5": "1 tablespoon toasted sesame oil"
        },
        {
            "6": "0.5 teaspoons liquid aminos (such as Bragg\u00ae)"
        },
        {
            "7": "1 teaspoon freshly squeezed lemon juice"
        },
        {
            "8": "1 clove garlic, minced"
        },
        {
            "9": "0.25 teaspoon red pepper flakes"
        },
        {
            "10": "1 salt and ground black pepper to taste"
        },
        {
            "11": "1 Salad-"
        },
        {
            "12": "0.5 cups cooked quinoa"
        },
        {
            "13": "1 cup shelled edamame"
        },
        {
            "14": "0.5 cup chopped red bell pepper"
        },
        {
            "15": "0.5 cup chopped Persian cucumber"
        },
        {
            "16": "0.5 cup shredded carrot"
        },
        {
            "17": "0.5 cup sliced green onion"
        },
        {
            "18": "0.5 cup dried cranberries"
        },
        {
            "19": "0.25 cup slivered almonds"
        }
    ],
    "Directions": [
        {
            "1": "Combine olive oil, vinegar, maple syrup, sesame oil, liquid amino, lemon juice, garlic, red pepper flakes, salt, and pepper in a mason jar. Seal the lid and shake dressing until fully blended."
        },
        {
            "2": "Place cooked quinoa, edamame, bell pepper, cucumber, carrot, green onion, cranberries in a large mixing bowl. Add 1/2 cup of the dressing and mix until well combined; save any remaining dressing for another use. Place in the refrigerator until chilled, at least 1 hour."
        },
        {
            "3": "Garnished with slivered almonds to serve."
        }
    ],
