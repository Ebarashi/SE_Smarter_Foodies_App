{
    "title": "Garlic Shrimp with Three Cheese Tortellini",
    "Category": "Tortellini",
    "Ingredients": [
        {
            "1": "1 (9 ounce) package BUITONI\u00ae Refrigerated Three Cheese Tortellini"
        },
        {
            "2": "6  raw large shrimp, peeled and deveined"
        },
        {
            "3": "2 tablespoons extra virgin olive oil"
        },
        {
            "4": "2 teaspoons finely chopped garlic"
        },
        {
            "5": "0.5 cup BUITONI\u00ae Refrigerated All Natural Pesto with Basil"
        },
        {
            "6": "0.5 cup chopped sun-dried tomatoes"
        },
        {
            "7": "2 tablespoons chopped fresh basil"
        },
        {
            "8": "2 tablespoons BUITONI\u00ae Refrigerated Freshly Shredded Parmesan Cheese"
        }
    ],
    "Directions": [
        {
            "1": "Prepare pasta according to package directions."
        },
        {
            "2": "Heat a large skillet over medium-high heat; cook shrimp, oil, and garlic, stirring frequently, until shrimp turn pink. Remove from heat."
        },
        {
            "3": "Toss pasta with pesto. Add sun-dried tomatoes and basil; toss well. Top with shrimp and cheese."
        }
    ],
