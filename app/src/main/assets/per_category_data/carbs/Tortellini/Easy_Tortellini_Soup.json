{
    "title": "Easy Tortellini Soup",
    "Category": "Tortellini",
    "Ingredients": [
        {
            "1": "1 tablespoon olive oil"
        },
        {
            "2": "1 small red onion, chopped"
        },
        {
            "3": "1  zucchini, chopped"
        },
        {
            "4": "1 tablespoon minced garlic"
        },
        {
            "5": "1 (28 ounce) can crushed tomatoes"
        },
        {
            "6": "2 (14.5 ounce) cans chicken broth"
        },
        {
            "7": "1 tablespoon white sugar"
        },
        {
            "8": "1 tablespoon Italian seasoning"
        },
        {
            "9": "0.25 cup red wine"
        },
        {
            "10": "1 dash hot pepper sauce"
        },
        {
            "11": "1 (11 ounce) can white corn, undrained"
        },
        {
            "12": "0.5 cup freshly grated Parmesan cheese"
        },
        {
            "13": "8 ounces cheese tortellini"
        }
    ],
    "Directions": [
        {
            "1": "In a large pot over medium heat, cook onion, zucchini and garlic in oil three minutes, until onion is translucent. Stir in crushed tomatoes, broth, sugar, Italian seasoning, wine and pepper sauce and bring to a boil."
        },
        {
            "2": "Reduce heat and stir in corn and Parmesan. Simmer 30 minutes."
        },
        {
            "3": "Stir in tortellini and simmer 10 minutes more, until pasta is tender."
        }
    ],
    "Details": [
