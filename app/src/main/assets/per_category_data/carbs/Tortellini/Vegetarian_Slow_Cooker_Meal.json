{
    "title": "Vegetarian Slow Cooker Meal",
    "Category": "Tortellini",
    "Ingredients": [
        {
            "1": "1 (32 fluid ounce) container vegetable broth"
        },
        {
            "2": "1 (16 ounce) package frozen cheese-filled tortellini"
        },
        {
            "3": "1 (8 ounce) package cream cheese, cut into pieces"
        },
        {
            "4": "1 (8 ounce) package fresh spinach"
        },
        {
            "5": "1 cup sliced crimini ('baby bella') mushrooms"
        }
    ],
    "Directions": [
        {
            "1": "Stir vegetable broth, tortellini, cream cheese, spinach, and mushrooms together in a slow cooker."
        },
        {
            "2": "Cook on Low for 5 to 6 hours."
        }
    ],
