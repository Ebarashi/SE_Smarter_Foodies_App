{
    "title": "Tortellini Soup II",
    "Category": "Tortellini",
    "Ingredients": [
        {
            "1": "6 (3.5 ounce) links sweet Italian sausage, casings removed"
        },
        {
            "2": "5 cups water"
        },
        {
            "3": "1 (14.5 ounce) can stewed tomatoes, with juice"
        },
        {
            "4": "2 teaspoons dried basil"
        },
        {
            "5": "1 (1 ounce) package dry onion soup mix"
        },
        {
            "6": "4  tomatoes, diced"
        },
        {
            "7": "2 cups cheese tortellini"
        },
        {
            "8": "3 cups shredded cabbage"
        }
    ],
    "Directions": [
        {
            "1": "In a large pot over medium high heat, saute the sausage for 5 minutes, or until browned and crumbly. Pour in the water, stewed tomatoes, basil, onion soup mix and fresh tomatoes."
        },
        {
            "2": "Bring just to a boil, reduce heat to low and simmer for 15 minutes. Stir in the tortellini and cabbage. Simmer for 10 more minutes."
        }
    ],
