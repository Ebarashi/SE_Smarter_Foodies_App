{
    "title": "Quick-Pickled Spicy Asparagus",
    "Category": "Pickles",
    "Ingredients": [
        {
            "1": "1 pound thick asparagus spears, trimmed"
        },
        {
            "2": "1 tablespoon minced garlic"
        },
        {
            "3": "2 teaspoons crushed red pepper flakes"
        },
        {
            "4": "1 teaspoon mustard seed"
        },
        {
            "5": "0.5 teaspoon pink peppercorns (Optional)"
        },
        {
            "6": "0.75 cups distilled white vinegar"
        },
        {
            "7": "1 cup water"
        },
        {
            "8": "1 tablespoon salt"
        },
        {
            "9": "1 teaspoon white sugar"
        }
    ],
    "Directions": [
        {
            "1": "Place trimmed asparagus in a quart-sized canning jar. Add garlic, pepper flakes, mustard seed, and peppercorns."
        },
        {
            "2": "Combine vinegar, water, salt, and sugar in a medium saucepan over medium heat; bring to a boil. Reduce heat and simmer for 5 minutes. Turn heat off."
        },
        {
            "3": "Pour vinegar mixture over the asparagus. Screw on the lid and let sit until lid has sealed, about 1 hour."
        },
        {
            "4": "Place in the refrigerator until pickled, at least 2 days."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "10 mins"
        },
        {
            "Additional Time": "2 days 1 hrs"
        },
        {
            "Total Time": "2 days 1 hrs 20 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "1 quart"
        }
