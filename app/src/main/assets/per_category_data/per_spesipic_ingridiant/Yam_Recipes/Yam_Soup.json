{
    "title": "Yam Soup",
    "Category": "Yam Recipes",
    "Ingredients": [
        {
            "1": "3 cups chicken broth"
        },
        {
            "2": "2 pounds yams, diced"
        },
        {
            "3": "1 medium onion, chopped"
        },
        {
            "4": "1 tablespoon curry powder"
        },
        {
            "5": "0.25 teaspoon black pepper"
        },
        {
            "6": "0.5 teaspoon salt"
        },
        {
            "7": "0.5 cup uncooked wild rice"
        },
        {
            "8": "0.5 teaspoon onion powder"
        },
        {
            "9": "0.5 teaspoon garlic powder"
        }
    ],
    "Directions": [
        {
            "1": "Pour chicken broth into a saucepan, with curry powder, salt, pepper, onion powder and garlic powder. Bring to a boil over medium heat. Add yam and onion, and simmer for about 20 minutes. Add rice, and cook for another 20 minutes, or until rice is tender."
        }
    ],
