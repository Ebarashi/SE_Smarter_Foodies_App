{
    "title": "Vegan Ube-Blueberry Ice Cream",
    "Category": "Yam Recipes",
    "Ingredients": [
        {
            "1": "1 (14 ounce) can coconut cream, chilled"
        },
        {
            "2": "1 (14.5 ounce) package frozen ube (purple yam)"
        },
        {
            "3": "0.5 cups frozen blueberries"
        },
        {
            "4": "1 tablespoon light agave syrup"
        }
    ],
    "Directions": [
        {
            "1": "Pour coconut cream into a high-speed blender (such as Vitamix\u00ae). Add ube, blueberries, and agave syrup. Cover and blend for about 1 minute, starting at Variable 1 and slowly increasing to the highest speed. Use the tamper to press the ingredients toward the blades."
        },
        {
            "2": "Serve immediately for a soft-serve consistency. Store in the freezer for a firmer consistency."
        }
    ],
