{
    "title": "Yam Casserole",
    "Category": "Yam Recipes",
    "Ingredients": [
        {
            "1": "1 (29 ounce) can yams, drained and mashed"
        },
        {
            "2": "0.25 cup melted butter"
        },
        {
            "3": "0.5 cup white sugar"
        },
        {
            "4": "0.5 cup evaporated milk"
        },
        {
            "5": "2  eggs, beaten"
        },
        {
            "6": "0.5 teaspoon nutmeg"
        },
        {
            "7": "0.5 teaspoon ground cinnamon"
        },
        {
            "8": "1 teaspoon vanilla extract"
        },
        {
            "9": "0.25 cup butter, softened"
        },
        {
            "10": "0.5 cup brown sugar"
        },
        {
            "11": "0.5 cup chopped walnuts"
        },
        {
            "12": "1 cup sugar frosted corn flake cereal"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "In a large bowl combine yams, melted butter, white sugar, evaporated milk, eggs, nutmeg, cinnamon and vanilla extract. Mix well and transfer to a 9x9 inch casserole dish."
        },
        {
            "3": "In a medium bowl cream butter with brown sugar. Stir in walnuts and frosted cereal. Spread on top of yam mixture."
        },
        {
            "4": "Bake in preheated oven for 35 minutes."
        }
    ],
