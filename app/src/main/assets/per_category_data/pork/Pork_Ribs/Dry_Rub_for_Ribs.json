{
    "title": "Dry Rub for Ribs",
    "Category": "Pork Ribs",
    "Ingredients": [
        {
            "1": "3 tablespoons brown sugar"
        },
        {
            "2": "0.5 tablespoons paprika"
        },
        {
            "3": "0.5 tablespoons salt"
        },
        {
            "4": "0.5 tablespoons ground black pepper"
        },
        {
            "5": "1 teaspoon garlic powder"
        }
    ],
    "Directions": [
        {
            "1": "Mix brown sugar, paprika, salt, pepper, and garlic powder together in a small bowl. Rub into pork ribs."
        },
        {
            "2": "For best results, allow ribs to marinate overnight."
        }
    ],
