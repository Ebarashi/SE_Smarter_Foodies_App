{
    "title": "Jerre's Black Bean and Pork Tenderloin Slow Cooker Chili",
    "Category": "Pork Tenderloin",
    "Ingredients": [
        {
            "1": "0.5 pounds pork tenderloin, cut into 2 inch strips"
        },
        {
            "2": "1 small onion, coarsely chopped"
        },
        {
            "3": "1 small red bell pepper, coarsely chopped"
        },
        {
            "4": "3 (15 ounce) cans black beans"
        },
        {
            "5": "1 (16 ounce) jar salsa"
        },
        {
            "6": "0.5 cup chicken broth"
        },
        {
            "7": "1 teaspoon dried oregano"
        },
        {
            "8": "1 teaspoon ground cumin"
        },
        {
            "9": "2 teaspoons chili powder"
        }
    ],
    "Directions": [
        {
            "1": "Combine pork tenderloin, onion, red pepper, black beans, salsa, chicken broth, oregano, cumin, and chili powder in a slow cooker. Set to Low and cook for 8 to 10 hours."
        },
        {
            "2": "Break up pieces of cooked pork to thicken the chili before serving."
        }
    ],
