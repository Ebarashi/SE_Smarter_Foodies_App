{
    "title": "Slow Cooker Pork Tenderloin and Potatoes",
    "Category": "Pork Tenderloin",
    "Ingredients": [
        {
            "1": "1 (2 pound) pork tenderloin"
        },
        {
            "2": "2 cloves garlic, minced"
        },
        {
            "3": "6  potatoes, peeled and cubed"
        },
        {
            "4": "1 cup baby carrots"
        },
        {
            "5": "4 cubes  chicken bouillon"
        },
        {
            "6": "1 cup water"
        }
    ],
    "Directions": [
        {
            "1": "Place pork in the slow cooker and sprinkle with garlic. Arrange potatoes, carrots, and bouillon cubes around pork. Add water."
        },
        {
            "2": "Cover and cook on Low until pork is slightly pink in the center and potatoes are tender, about 4 hours. An instant-read thermometer inserted into the center should read at least 145 degrees F (63 degrees C)."
        }
    ],
