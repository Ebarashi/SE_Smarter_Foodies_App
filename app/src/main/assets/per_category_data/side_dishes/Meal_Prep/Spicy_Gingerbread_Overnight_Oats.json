{
    "title": "Spicy Gingerbread Overnight Oats",
    "Category": "Meal Prep",
    "Ingredients": [
        {
            "1": "0.5 cup milk"
        },
        {
            "2": "0.5 cup rolled oats"
        },
        {
            "3": "0.25 cup plain Greek yogurt"
        },
        {
            "4": "1 tablespoon brown sugar"
        },
        {
            "5": "1 tablespoon flaxseed meal"
        },
        {
            "6": "1 teaspoon molasses"
        },
        {
            "7": "0.25 teaspoon vanilla extract"
        },
        {
            "8": "0.25 teaspoon ground cinnamon"
        },
        {
            "9": "0.25 teaspoon ground ginger"
        },
        {
            "10": "1 pinch salt"
        },
        {
            "11": "1 pinch ground nutmeg"
        },
        {
            "12": "1 pinch ground cloves"
        },
        {
            "13": "1 pinch ground allspice"
        },
        {
            "14": "3 tablespoons raisins (Optional)"
        },
        {
            "15": "3 tablespoons chopped walnuts (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Combine milk, oats, yogurt, brown sugar, flaxseed meal, molasses, vanilla extract, cinnamon, ginger, salt, nutmeg, cloves, and allspice in a glass container."
        },
        {
            "2": "Refrigerate mixture 8 hours to overnight. Top with raisins and walnuts just before serving."
        }
    ],
    "Details": [
