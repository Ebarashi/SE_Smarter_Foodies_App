{
    "title": "Quick Tartar Sauce",
    "Category": "Side Dishes",
    "Ingredients": [
        {
            "1": "1 cup mayonnaise"
        },
        {
            "2": "2 teaspoons sweet pickle relish"
        },
        {
            "3": "1 teaspoon prepared yellow mustard"
        },
        {
            "4": "1 teaspoon lemon juice"
        }
    ],
    "Directions": [
        {
            "1": "Stir mayonnaise, relish, mustard, and lemon juice together in a bowl."
        }
    ],
