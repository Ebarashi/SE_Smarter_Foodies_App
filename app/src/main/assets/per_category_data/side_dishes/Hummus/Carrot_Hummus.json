{
    "title": "Carrot Hummus",
    "Category": "Hummus",
    "Ingredients": [
        {
            "1": "1 cup cashews"
        },
        {
            "2": "4 cloves garlic, peeled"
        },
        {
            "3": "1 pound carrots, peeled and roughly chopped"
        },
        {
            "4": "1 large lemon, juiced"
        },
        {
            "5": "1 tablespoon tamari"
        },
        {
            "6": "1 bunch flat-leaf parsley, chopped"
        },
        {
            "7": "0.5 tablespoons mayonnaise, or to taste"
        },
        {
            "8": "1 salt to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine cashews and garlic in a food processor and pulse until finely chopped. Add carrots, lemon juice, tamari, parsley, and mayonnaise and process until smooth. Season with salt."
        }
    ],
