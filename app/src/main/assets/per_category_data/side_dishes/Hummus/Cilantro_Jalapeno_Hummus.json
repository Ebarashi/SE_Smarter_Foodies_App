{
    "title": "Cilantro Jalapeno Hummus",
    "Category": "Hummus",
    "Ingredients": [
        {
            "1": "1 (15 ounce) can garbanzo beans, drained"
        },
        {
            "2": "1 cup fresh cilantro, or to taste"
        },
        {
            "3": "1  jalapeno pepper, stemmed and diced - or to taste"
        },
        {
            "4": "2 cloves garlic, diced"
        },
        {
            "5": "1 tablespoon olive oil, or as needed"
        },
        {
            "6": "1 salt and ground black pepper to taste"
        },
        {
            "7": "1 pinch garlic powder, or to taste"
        }
    ],
    "Directions": [
        {
            "1": "Blend garbanzo beans, cilantro, jalapeno pepper, garlic, olive oil, salt, black pepper, and garlic powder in a blender until smooth."
        }
    ],
