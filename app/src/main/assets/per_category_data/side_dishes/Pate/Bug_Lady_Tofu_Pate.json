{
    "title": "Bug Lady Tofu Pate",
    "Category": "Pate",
    "Ingredients": [
        {
            "1": "1 (16 ounce) package soft tofu"
        },
        {
            "2": "0.25 cup mayonnaise"
        },
        {
            "3": "0.25 teaspoon garlic powder"
        },
        {
            "4": "0.25 teaspoon ground white pepper"
        },
        {
            "5": "0.25 teaspoon prepared mustard"
        },
        {
            "6": "0.25 teaspoon seasoning salt"
        },
        {
            "7": "0.25 teaspoon nutritional yeast"
        },
        {
            "8": "0.25 teaspoon paprika"
        },
        {
            "9": "1  orange bell pepper, finely chopped"
        },
        {
            "10": "1  fresh red chile pepper, finely chopped"
        },
        {
            "11": "2 stalks celery, finely chopped"
        },
        {
            "12": "2 tablespoons chopped fresh parsley"
        },
        {
            "13": "2 tablespoons chopped fresh cilantro"
        },
        {
            "14": "2 tablespoons chopped fresh dill weed"
        },
        {
            "15": "2 cloves garlic, minced"
        }
    ],
    "Directions": [
        {
            "1": "Wrap soft tofu in cheese cloth. Place over a medium bowl, and press to drain as much liquid as possible."
        },
        {
            "2": "Place tofu in a medium bowl. Mix in mayonnaise, garlic powder, white pepper, mustard, seasoning salt, nutritional yeast, paprika, orange bell pepper, red chile pepper, celery, parsley, cilantro, dill weed and garlic. Blend until smooth. Cover and chill in the refrigerator until serving."
        }
    ],
