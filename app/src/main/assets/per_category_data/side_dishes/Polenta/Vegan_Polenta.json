{
    "title": "Vegan Polenta",
    "Category": "Polenta",
    "Ingredients": [
        {
            "1": "2 cups vegetable broth"
        },
        {
            "2": "1 cup water, divided"
        },
        {
            "3": "0.5 cup unsweetened oat milk"
        },
        {
            "4": "1 teaspoon salt"
        },
        {
            "5": "0.5 cups Italian polenta"
        },
        {
            "6": "0.5 cup shredded vegan white cheese"
        },
        {
            "7": "0.25 cup vegan butter"
        }
    ],
    "Directions": [
        {
            "1": "Stir together vegetable broth, 1/2 cup water, oat milk, and salt in a saucepan over medium-high heat. Bring to a boil. Reduce heat to medium-low and sprinkle in polenta, stirring constantly. Continue stirring until polenta thickens, about 5 minutes."
        },
        {
            "2": "Reduce temperature to low and cook, stirring often, for about 10 minutes. Add vegan cheese and butter. Stir until smooth. Add remaining water, if necessary, to keep consistency velvety and smooth. Pour polenta onto an oiled plate and let stand a few minutes before serving."
        }
    ],
