{
    "title": "Tomato and Mozzarella Bites",
    "Category": "Tapas Recipes",
    "Ingredients": [
        {
            "1": "20  toothpicks"
        },
        {
            "2": "20  grape or cherry tomatoes, halved"
        },
        {
            "3": "20  fresh basil leaves"
        },
        {
            "4": "20  small balls fresh mozzarella cheese (often labeled bocconcini)"
        },
        {
            "5": "1 salt and pepper to taste"
        },
        {
            "6": "0.5 cup balsamic vinegar"
        },
        {
            "7": "0.25 cup extra virgin olive oil"
        }
    ],
    "Directions": [
        {
            "1": "Thread a tomato half, a basil leaf, a mozzarella ball, and another tomato half onto a toothpick. Repeat with remaining ingredients."
        },
        {
            "2": "Arrange toothpicks on a serving platter and season with salt and pepper."
        },
        {
            "3": "Mix vinegar and oil together in a small bowl to serve as a dipping sauce."
        }
    ],
    "Details": [
        {
            "Prep Time": "30 mins"
        },
        {
            "Total Time": "30 mins"
        },
        {
            "Servings": "10 "
        },
        {
            "Yield": "20 skewers"
        }
    ],
