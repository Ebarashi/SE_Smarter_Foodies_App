{
    "title": "Spring Salad",
    "Category": "Broccoli Salad",
    "Ingredients": [
        {
            "1": "12 slices bacon"
        },
        {
            "2": "2 heads fresh broccoli, florets only"
        },
        {
            "3": "1 cup chopped celery"
        },
        {
            "4": "0.5 cup chopped green onions"
        },
        {
            "5": "1 cup seedless green grapes"
        },
        {
            "6": "1 cup seedless red grapes"
        },
        {
            "7": "0.5 cup raisins"
        },
        {
            "8": "0.5 cup blanched slivered almonds"
        },
        {
            "9": "1 cup mayonnaise"
        },
        {
            "10": "1 tablespoon white wine vinegar"
        },
        {
            "11": "0.25 cup white sugar"
        }
    ],
    "Directions": [
        {
            "1": "Place bacon in a large, deep skillet. Cook over medium high heat until evenly brown. Drain, crumble and set aside."
        },
        {
            "2": "In a large salad bowl, toss together the bacon, broccoli, celery, green onions, green grapes, red grapes, raisins and almonds."
        },
        {
            "3": "Whisk together the mayonnaise, vinegar and sugar. Pour dressing over salad and toss to coat. Refrigerate until ready to serve."
        }
    ],
