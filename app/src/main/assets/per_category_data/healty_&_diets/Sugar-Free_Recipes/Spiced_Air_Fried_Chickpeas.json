{
    "title": "Spiced Air-Fried Chickpeas",
    "Category": "Sugar-Free Recipes",
    "Ingredients": [
        {
            "1": "1 (15 ounce) can chickpeas, rinsed and drained"
        },
        {
            "2": "1 tablespoon nutritional yeast"
        },
        {
            "3": "1 tablespoon olive oil"
        },
        {
            "4": "1 teaspoon smoked paprika"
        },
        {
            "5": "1 teaspoon granulated garlic"
        },
        {
            "6": "0.5 teaspoon salt"
        },
        {
            "7": "1 pinch cumin"
        }
    ],
    "Directions": [
        {
            "1": "Spread chickpeas on a double layer of paper towels, cover with another layer of paper towels, and let dry for 30 minutes."
        },
        {
            "2": "Preheat air fryer to 355 degrees F (180 degrees C)."
        },
        {
            "3": "Combine dry chickpeas, nutritional yeast, olive oil, smoked paprika, garlic, salt, and cumin in a bowl; toss to coat."
        },
        {
            "4": "Add chickpeas to the air fryer and cook until crispy, 20 to 22 minutes, shaking them every 4 minutes."
        }
    ],
