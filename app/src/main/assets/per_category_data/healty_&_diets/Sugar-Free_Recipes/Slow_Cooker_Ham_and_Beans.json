{
    "title": "Slow Cooker Ham and Beans",
    "Category": "Sugar-Free Recipes",
    "Ingredients": [
        {
            "1": "1 pound dried great Northern beans, soaked overnight"
        },
        {
            "2": "0.5 pound cooked ham, chopped"
        },
        {
            "3": "0.5 cup brown sugar"
        },
        {
            "4": "1 tablespoon onion powder"
        },
        {
            "5": "1 tablespoon dried parsley"
        },
        {
            "6": "0.5 teaspoon garlic salt"
        },
        {
            "7": "0.5 teaspoon black pepper"
        },
        {
            "8": "0.25 teaspoon cayenne pepper"
        },
        {
            "9": "1 water to cover"
        }
    ],
    "Directions": [
        {
            "1": "Combine the beans, ham, brown sugar, onion powder, parsley, garlic salt, black pepper, and cayenne pepper in a slow cooker. Pour enough water into the slow cooker to cover the mixture by about 2 inches. Set slow cooker to Low; simmer 12 hours, stirring occasionally."
        }
    ],
