{
    "title": "Ginny's Cauliflower and Pea Salad",
    "Category": "Sugar-Free Recipes",
    "Ingredients": [
        {
            "1": "1 large head cauliflower, broken into small florets"
        },
        {
            "2": "1 (10 ounce) package frozen peas, thawed"
        },
        {
            "3": "1 small onion, chopped"
        },
        {
            "4": "2 stalks celery, chopped"
        },
        {
            "5": "1 cup shredded reduced-fat Cheddar cheese"
        },
        {
            "6": "0.5 cup grated Parmesan cheese"
        },
        {
            "7": "0.25 cup bacon bits"
        },
        {
            "8": "1 cup light mayonnaise"
        },
        {
            "9": "1 (8 ounce) container reduced fat sour cream"
        },
        {
            "10": "2 tablespoons granular no-calorie sucralose sweetener (such as Splenda\u00ae)"
        },
        {
            "11": "2 tablespoons distilled white vinegar"
        },
        {
            "12": "1 tablespoon prepared mustard"
        },
        {
            "13": "1 teaspoon seasoned salt (such as LAWRY'S\u00ae)"
        }
    ],
    "Directions": [
        {
            "1": "Toss together cauliflower, peas, onion, and celery in a large bowl. Mix in Cheddar, Parmesan, and bacon bits."
        },
        {
            "2": "Mix mayonnaise, sour cream, sweetener, vinegar, mustard, and seasoned salt in a separate bowl. Pour over vegetables and mix well."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Total Time": "15 mins"
        },
        {
            "Servings": "12 "
        }
