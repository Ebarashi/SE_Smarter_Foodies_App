{
    "title": "Smoked Chicken Thighs",
    "Category": "Sugar-Free Recipes",
    "Ingredients": [
        {
            "1": "1 teaspoon salt"
        },
        {
            "2": "1 teaspoon ground black pepper"
        },
        {
            "3": "1 teaspoon red pepper flakes"
        },
        {
            "4": "1 teaspoon paprika"
        },
        {
            "5": "1 teaspoon garlic powder"
        },
        {
            "6": "0.5 teaspoon thyme"
        },
        {
            "7": "0.5 teaspoon oregano"
        },
        {
            "8": "8  skinless, boneless chicken thighs"
        },
        {
            "9": "1 cup cherry wood chips, or as needed"
        }
    ],
    "Directions": [
        {
            "1": "Preheat a smoker to 200 degrees F (93 degrees C)."
        },
        {
            "2": "Combine salt, black pepper, red pepper flakes, paprika, garlic powder, thyme, and oregano in a bowl. Apply to chicken thighs as a dry rub."
        },
        {
            "3": "Smoke thighs using cherry wood for 1 hour. Rotate and flip thighs and continue smoking until internal temperature reaches 165 degrees F (74 degrees C), about 1 hour more."
        }
    ],
