{
    "title": "Skinny Cosmopolitan",
    "Category": "Sugar-Free Recipes",
    "Ingredients": [
        {
            "1": "2  (1-inch x 1/2-inch) strips lemon zest"
        },
        {
            "2": "2  (1-inch x 1/2-inch) strips orange zest"
        },
        {
            "3": "1 packet Sweet'N Low granulated sugar substitute"
        },
        {
            "4": "0.5 ounce unsweetened pomegranate juice"
        },
        {
            "5": "0.5 ounces vodka"
        },
        {
            "6": "1 slice lime, for garnish"
        }
    ],
    "Directions": [
        {
            "1": "Muddle lemon and orange zest with sugar substitute in a cocktail shaker. Add pomegranate juice and vodka. Fill the shaker halfway with ice and shake vigorously. Strain into a chilled stemmed glass. Garnish with lime slice."
        }
    ],
