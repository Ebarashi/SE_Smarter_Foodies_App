{
    "title": "Sweet Dijon Salad Dressing",
    "Category": "Sugar-Free Recipes",
    "Ingredients": [
        {
            "1": "0.25 cup apple cider vinegar"
        },
        {
            "2": "0.25 cup Dijon mustard"
        },
        {
            "3": "0.25 cup olive oil"
        },
        {
            "4": "4 (1 gram) packets granular sucrolose sweetener (such as Splenda\u00ae)"
        }
    ],
    "Directions": [
        {
            "1": "Place vinegar, Dijon mustard, oil, and sugar substitute into a jar with a tight-fitting lid. Close the jar, and shake vigorously to blend. Refrigerate until chilled before using."
        }
    ],
