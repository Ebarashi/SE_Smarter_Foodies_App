{
    "title": "Sugar-Free Hot Chocolate Mix",
    "Category": "Sugar-Free Recipes",
    "Ingredients": [
        {
            "1": "2 cups nonfat dry milk powder"
        },
        {
            "2": "0.5 cup low-fat powdered non-dairy creamer"
        },
        {
            "3": "0.5 cup unsweetened cocoa powder"
        },
        {
            "4": "1 tablespoon granular sweetener (such as Equal\u00ae)"
        },
        {
            "5": "0.75 teaspoon ground cinnamon"
        }
    ],
    "Directions": [
        {
            "1": "Combine milk powder, creamer, cocoa powder, sweetener, and cinnamon in a bowl; mix until thoroughly blended. Transfer to a jar with a tight lid."
        }
    ],
