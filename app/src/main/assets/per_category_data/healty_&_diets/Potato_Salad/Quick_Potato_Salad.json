{
    "title": "Quick Potato Salad",
    "Category": "Potato Salad",
    "Ingredients": [
        {
            "1": "2  potatoes, peeled and diced"
        },
        {
            "2": "3  hard-cooked eggs, peeled and diced"
        },
        {
            "3": "0.5  cucumber, peeled and diced"
        },
        {
            "4": "1  tomato, diced"
        },
        {
            "5": "1  celery stalk, diced"
        },
        {
            "6": "0.25  onion, diced"
        },
        {
            "7": "0.25 cup chopped green onion"
        },
        {
            "8": "0.75 cup low-fat mayonnaise"
        },
        {
            "9": "1 tablespoon prepared yellow mustard"
        },
        {
            "10": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Bring a large pot of lightly salted water to a boil. Add potatoes and cook until soft, 5 to 7 minutes; drain and rinse with cold water."
        },
        {
            "2": "Combine the potatoes, eggs, cucumber, tomato, celery, onion, green onion, mayonnaise, and mustard in a large bowl; stir until evenly combined. Season with salt and pepper."
        }
    ],
