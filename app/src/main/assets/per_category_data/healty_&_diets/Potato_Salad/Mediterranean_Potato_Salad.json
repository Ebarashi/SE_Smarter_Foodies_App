{
    "title": "Mediterranean Potato Salad",
    "Category": "Potato Salad",
    "Ingredients": [
        {
            "1": "2 pounds potatoes"
        },
        {
            "2": "1  green bell pepper, minced"
        },
        {
            "3": "1  cucumber, sliced and quartered"
        },
        {
            "4": "0.5 cup sliced red onion"
        },
        {
            "5": "8 ounces crumbled feta cheese"
        },
        {
            "6": "1  lemon, juiced"
        },
        {
            "7": "0.5 cup Italian-style salad dressing"
        },
        {
            "8": "1 salt and pepper to taste"
        },
        {
            "9": "3  pita breads, cut into wedges"
        }
    ],
    "Directions": [
        {
            "1": "Bring a large pot of salted water to a boil. Add potatoes and cook until tender but still firm, about 15 minutes. Drain, cool, and chop."
        },
        {
            "2": "In a large bowl, combine potatoes, green peppers, cucumbers, red onion, and cheese."
        },
        {
            "3": "Whisk together lemon juice, salad dressing, salt, and pepper. Pour over salad and toss to coat. Serve with pita bread wedges, if desired."
        }
    ],
