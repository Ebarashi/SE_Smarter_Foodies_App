{
    "title": "Spaghetti Sauce",
    "Category": "Vegetable Recipes",
    "Ingredients": [
        {
            "1": "2 teaspoons olive oil"
        },
        {
            "2": "0.5 small onion, chopped"
        },
        {
            "3": "2  green onions, chopped"
        },
        {
            "4": "2 teaspoons crushed garlic"
        },
        {
            "5": "1 (28 ounce) can peeled and diced tomatoes"
        },
        {
            "6": "4 teaspoons dried basil"
        },
        {
            "7": "4 teaspoons dried oregano"
        },
        {
            "8": "1 teaspoon white sugar"
        }
    ],
    "Directions": [
        {
            "1": "Heat oil in a large saucepan over medium heat. Add onion, green onion, and garlic; cook until onions are translucent, 3 to 4 minutes. Stir in tomatoes, basil, oregano, and sugar. Bring to a boil, reduce heat to low, and simmer for 20 minutes."
        }
    ],
