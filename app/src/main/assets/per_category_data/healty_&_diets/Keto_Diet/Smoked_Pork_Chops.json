{
    "title": "Smoked Pork Chops",
    "Category": "Keto Diet",
    "Ingredients": [
        {
            "1": "0.5 teaspoons kosher salt"
        },
        {
            "2": "0.5 teaspoons light brown sugar"
        },
        {
            "3": "0.5 teaspoons ground black pepper"
        },
        {
            "4": "1 teaspoon paprika"
        },
        {
            "5": "0.5 teaspoon ground mustard"
        },
        {
            "6": "0.5 teaspoon garlic powder"
        },
        {
            "7": "0.5 teaspoon onion powder"
        },
        {
            "8": "0.25 teaspoon oregano"
        },
        {
            "9": "6  bone-in center-cut pork chops (about 1-inch thick)"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the smoker to 250 degrees F (120 degrees C). Add wood chips according to manufacturer's directions."
        },
        {
            "2": "Combine salt, brown sugar, pepper, paprika, ground mustard, garlic powder, onion powder, and oregano in a small bowl. Stir until evenly combined. Sprinkle over both sides of each pork chop."
        },
        {
            "3": "Place pork chops directly on the smoker grate. Smoke for 2 hours 30 minutes. Remove chops from the smoker and let rest for 10 minutes."
        }
    ],
