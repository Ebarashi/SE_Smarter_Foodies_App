{
    "title": "Oven-Baked Bacon",
    "Category": "Keto Diet",
    "Ingredients": [
        {
            "1": "1 (16 ounce) package bacon"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C). Line a baking sheet with parchment paper."
        },
        {
            "2": "Place bacon slices, one next to the other, on the prepared baking sheet."
        },
        {
            "3": "Bake in the preheated oven for 15 to 20 minutes."
        },
        {
            "4": "Remove from the oven to flip bacon slices with kitchen tongs and then return to the oven."
        },
        {
            "5": "Bake until crispy, 15 to 20 minutes more. Thinner slices will need less time, about 20 minutes total. Drain on a plate lined with paper towels."
        }
    ],
