{
    "title": "Easy Keto Homemade Tomato Sauce",
    "Category": "Keto Diet",
    "Ingredients": [
        {
            "1": "1 (28 ounce) can canned San Marzano-style peeled plum tomatoes"
        },
        {
            "2": "0.25 cup extra-virgin olive oil"
        },
        {
            "3": "1 teaspoon garlic powder"
        },
        {
            "4": "1 teaspoon dried basil"
        },
        {
            "5": "1 teaspoon salt"
        },
        {
            "6": "0.5 teaspoon dried oregano"
        },
        {
            "7": "0.5 teaspoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Place tomatoes and olive oil in a blender; pulse until smooth. Add garlic powder, basil, salt, oregano, and pepper; mix until well combined."
        }
    ],
