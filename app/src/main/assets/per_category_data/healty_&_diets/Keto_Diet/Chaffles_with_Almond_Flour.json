{
    "title": "Chaffles with Almond Flour",
    "Category": "Keto Diet",
    "Ingredients": [
        {
            "1": "1 large egg"
        },
        {
            "2": "1 tablespoon blanched almond flour"
        },
        {
            "3": "0.25 teaspoon baking powder"
        },
        {
            "4": "0.5 cup shredded mozzarella cheese"
        },
        {
            "5": "1 cooking spray"
        }
    ],
    "Directions": [
        {
            "1": "Whisk egg, almond flour, and baking powder together in a bowl. Stir in mozzarella cheese; set batter aside."
        },
        {
            "2": "Preheat a waffle iron according to the manufacturer's instructions."
        },
        {
            "3": "Spray both sides of the preheated waffle iron with cooking spray. Pour 1/2 of the batter onto the waffle iron and spread it out from the center with a spoon. Close the waffle maker and cook until chaffle reaches your desired doneness, about 3 minutes. Carefully lift chaffle out of the waffle iron and repeat with remaining batter. Allow chaffles to cool and crisp up, about 2 to 3 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Cook Time": "10 mins"
        },
        {
            "Additional Time": "5 mins"
        },
        {
            "Total Time": "20 mins"
        },
        {
            "Servings": "2 "
        },
        {
            "Yield": "2 4-inch chaffles"
        }
