{
    "title": "Traditional Creamy Coleslaw",
    "Category": "Salad Recipes",
    "Ingredients": [
        {
            "1": "1 cup mayonnaise"
        },
        {
            "2": "0.25 cup white sugar"
        },
        {
            "3": "2 tablespoons seasoned rice wine vinegar"
        },
        {
            "4": "0.5 tablespoons lemon juice"
        },
        {
            "5": "1 tablespoon prepared horseradish"
        },
        {
            "6": "0.5 teaspoon onion powder"
        },
        {
            "7": "0.5 teaspoon dry mustard"
        },
        {
            "8": "0.5 teaspoon celery salt"
        },
        {
            "9": "0.5 teaspoon salt"
        },
        {
            "10": "0.5 teaspoon ground black pepper"
        },
        {
            "11": "1 (28 ounce) package coleslaw mix"
        }
    ],
    "Directions": [
        {
            "1": "Whisk mayonnaise, sugar, vinegar, lemon juice, horseradish, onion powder, dry mustard, celery salt, salt, and pepper in a large mixing bowl until sugar has dissolved."
        },
        {
            "2": "Fold coleslaw mix into dressing until well combined. Cover and refrigerate for at least 1 hour before serving."
        }
    ],
    "Details": [
