{
    "title": "Italian Peas",
    "Category": "Low-Calorie Recipes",
    "Ingredients": [
        {
            "1": "2 tablespoons olive oil"
        },
        {
            "2": "1  onion, chopped"
        },
        {
            "3": "2 cloves garlic, minced"
        },
        {
            "4": "16 ounces frozen green peas"
        },
        {
            "5": "1 tablespoon chicken stock"
        },
        {
            "6": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Heat olive oil in a skillet over medium heat. Stir in onion; cook until softened, about 5 minutes. Stir in garlic and cook for 1 minute. Add frozen peas and stir in stock. Season with salt and pepper. Cover and cook until the peas are tender, about 5 minutes."
        }
    ],
