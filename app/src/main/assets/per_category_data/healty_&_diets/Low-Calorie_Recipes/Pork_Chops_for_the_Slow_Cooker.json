{
    "title": "Pork Chops for the Slow Cooker",
    "Category": "Low-Calorie Recipes",
    "Ingredients": [
        {
            "1": "6  boneless pork chops"
        },
        {
            "2": "0.5 cup soy sauce"
        },
        {
            "3": "0.25 cup ketchup"
        },
        {
            "4": "0.25 cup brown sugar"
        },
        {
            "5": "2 cloves garlic, crushed"
        },
        {
            "6": "1 teaspoon ground ginger"
        },
        {
            "7": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Place pork chops in a slow cooker."
        },
        {
            "2": "Combine soy sauce, ketchup, brown sugar, garlic, and ginger, and pour over pork chops."
        },
        {
            "3": "Cover and cook on Low for 6 hours."
        }
    ],
