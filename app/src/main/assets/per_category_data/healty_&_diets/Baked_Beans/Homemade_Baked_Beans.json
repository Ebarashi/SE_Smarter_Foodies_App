{
    "title": "Homemade Baked Beans",
    "Category": "Baked Beans",
    "Ingredients": [
        {
            "1": "0.5 pound bacon"
        },
        {
            "2": "2 (14.5 ounce) cans Great Northern beans, rinsed and drained"
        },
        {
            "3": "2 small onions, chopped"
        },
        {
            "4": "1 cup water (Optional)"
        },
        {
            "5": "0.75 cup packed brown sugar"
        },
        {
            "6": "3 tablespoons ketchup"
        },
        {
            "7": "3 tablespoons molasses"
        },
        {
            "8": "1 tablespoon vinegar"
        },
        {
            "9": "1 tablespoon mustard"
        },
        {
            "10": "1 teaspoon salt"
        },
        {
            "11": "1 teaspoon garlic powder"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C). Grease a 2 1/2-quart baking dish."
        },
        {
            "2": "Place bacon in a large skillet and cook over medium-high heat until crisp, about 10 minutes; drain and cool on paper towels. Crumble bacon once cooled."
        },
        {
            "3": "Mix bacon, beans, onions, water, brown sugar, ketchup, molasses, vinegar, mustard, salt, and garlic powder together in the prepared baking dish. Cover the dish with aluminum foil."
        },
        {
            "4": "Bake in preheated oven until bubbling, 45 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "55 mins"
        },
        {
            "Total Time": " 1 hrs 10 mins"
        },
        {
            "Servings": "8 "
        }
    ],
