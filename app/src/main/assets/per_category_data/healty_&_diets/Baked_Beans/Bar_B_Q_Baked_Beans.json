{
    "title": "Bar-B-Q Baked Beans",
    "Category": "Baked Beans",
    "Ingredients": [
        {
            "1": "1 (15 ounce) can kidney beans, drained (Optional)"
        },
        {
            "2": "1 (15 ounce) can pinto beans, drained"
        },
        {
            "3": "1 (15 ounce) can lima beans, drained"
        },
        {
            "4": "1 (16 ounce) can great Northern beans, drained"
        },
        {
            "5": "1 (12 ounce) bottle chili sauce"
        },
        {
            "6": "2 tablespoons brown sugar"
        },
        {
            "7": "1 tablespoon Dijon mustard"
        },
        {
            "8": "1 tablespoon Worcestershire sauce"
        },
        {
            "9": "2 tablespoons molasses"
        },
        {
            "10": "3 slices bacon, cut in half"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 325 degrees F (165 degrees C)."
        },
        {
            "2": "In a medium baking dish, mix kidney beans, pinto beans, lima beans, great northern beans, chili sauce, brown sugar, Dijon mustard, Worcestershire sauce and molasses. Top with bacon."
        },
        {
            "3": "Bake 1 hour in the preheated oven, until thick and bubbly."
        }
    ],
