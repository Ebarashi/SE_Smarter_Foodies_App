{
    "title": "Slow Cooker Homemade Beans",
    "Category": "Baked Beans",
    "Ingredients": [
        {
            "1": "3 cups dry navy beans, soaked overnight or boiled for one hour"
        },
        {
            "2": "6 slices thick cut bacon, cut into 1 inch pieces"
        },
        {
            "3": "1 large onion, chopped"
        },
        {
            "4": "0.5 cups ketchup"
        },
        {
            "5": "0.5 cups water"
        },
        {
            "6": "1 cup brown sugar"
        },
        {
            "7": "0.25 cup molasses"
        },
        {
            "8": "1 tablespoon dry mustard"
        },
        {
            "9": "1 tablespoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Drain soaking liquid from beans; place them in a slow cooker."
        },
        {
            "2": "Stir bacon, onion, ketchup, water, brown sugar, molasses, mustard, and salt into beans until well mixed."
        },
        {
            "3": "Cover and cook on Low for 8 to 10 hours; stir occasionally if possible."
        }
    ],
