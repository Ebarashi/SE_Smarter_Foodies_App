{
    "title": "Vegan Pancakes",
    "Category": "Vegan Recipes",
    "Ingredients": [
        {
            "1": "0.25 cups all-purpose flour"
        },
        {
            "2": "2 tablespoons white sugar"
        },
        {
            "3": "2 teaspoons baking powder"
        },
        {
            "4": "0.5 teaspoon salt"
        },
        {
            "5": "0.25 cups water"
        },
        {
            "6": "1 tablespoon oil"
        }
    ],
    "Directions": [
        {
            "1": "Sift flour, sugar, baking powder, and salt into a large bowl; make a well in the center. Whisk together water and oil in a small bowl; pour into flour mixture. Stir just until blended; mixture will be lumpy."
        },
        {
            "2": "Heat a lightly oiled griddle over medium-high heat."
        },
        {
            "3": "Drop batter by large spoonfuls onto the griddle. Cook until bubbles form and edges are dry. Flip, then cook until bottoms are browned, 1 to 2 minutes. Repeat with remaining batter."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Cook Time": "10 mins"
        },
        {
            "Total Time": "15 mins"
        },
        {
            "Servings": "3 "
        },
        {
            "Yield": "9 pancakes"
        }
