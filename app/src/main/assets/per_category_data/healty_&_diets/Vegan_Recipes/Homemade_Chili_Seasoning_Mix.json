{
    "title": "Homemade Chili Seasoning Mix",
    "Category": "Vegan Recipes",
    "Ingredients": [
        {
            "1": "0.25 cup all-purpose flour"
        },
        {
            "2": "4 teaspoons chili powder"
        },
        {
            "3": "1 tablespoon crushed red pepper"
        },
        {
            "4": "1 tablespoon dried minced onion"
        },
        {
            "5": "1 tablespoon dried, minced garlic"
        },
        {
            "6": "2 teaspoons white sugar"
        },
        {
            "7": "2 teaspoons ground cumin"
        },
        {
            "8": "2 teaspoons dried parsley"
        },
        {
            "9": "2 teaspoons salt"
        },
        {
            "10": "1 teaspoon dried basil"
        },
        {
            "11": "0.25 teaspoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Stir together flour, chili powder, red pepper, onion, garlic, sugar, cumin, parsley, salt, basil, and black pepper in a bowl. Store mixture in an airtight container."
        }
    ],
