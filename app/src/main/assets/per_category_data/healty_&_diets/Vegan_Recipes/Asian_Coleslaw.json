{
    "title": "Asian Coleslaw",
    "Category": "Vegan Recipes",
    "Ingredients": [
        {
            "1": "1 Peanut Dressing-"
        },
        {
            "2": "6 tablespoons rice wine vinegar"
        },
        {
            "3": "6 tablespoons vegetable oil"
        },
        {
            "4": "5 tablespoons creamy peanut butter"
        },
        {
            "5": "3 tablespoons soy sauce"
        },
        {
            "6": "3 tablespoons brown sugar"
        },
        {
            "7": "2 tablespoons minced fresh ginger root"
        },
        {
            "8": "0.5 tablespoons minced garlic"
        },
        {
            "9": "1 Cabbage Salad-"
        },
        {
            "10": "5 cups thinly sliced green cabbage"
        },
        {
            "11": "2 cups thinly sliced red cabbage"
        },
        {
            "12": "2 cups shredded napa cabbage"
        },
        {
            "13": "2  red bell peppers, thinly sliced"
        },
        {
            "14": "2  carrots, julienned"
        },
        {
            "15": "6  green onions, chopped"
        },
        {
            "16": "0.5 cup chopped fresh cilantro"
        }
    ],
    "Directions": [
        {
            "1": "Prepare the peanut dressing: Whisk together rice vinegar, oil, peanut butter, soy sauce, brown sugar, ginger, and garlic in a medium bowl. Set aside."
        },
        {
            "2": "Make the cabbage salad: Mix green cabbage, red cabbage, napa cabbage, red bell peppers, carrots, green onions, and cilantro in a large bowl."
        },
        {
            "3": "Toss salad together with peanut dressing just before serving."
        }
    ],
