{
    "title": "Autumn Apple Salad II",
    "Category": "Fruit Salads",
    "Ingredients": [
        {
            "1": "4  tart green apples, cored and chopped"
        },
        {
            "2": "0.25 cup blanched slivered almonds, toasted"
        },
        {
            "3": "0.25 cup dried cranberries"
        },
        {
            "4": "0.25 cup chopped dried cherries"
        },
        {
            "5": "1 (8 ounce) container vanilla yogurt"
        }
    ],
    "Directions": [
        {
            "1": "In a medium bowl, stir together the apples, almonds, cranberries, cherries and yogurt until evenly coated."
        }
