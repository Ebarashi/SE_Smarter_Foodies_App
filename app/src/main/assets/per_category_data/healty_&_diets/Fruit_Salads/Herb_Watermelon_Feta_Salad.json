{
    "title": "Herb Watermelon Feta Salad",
    "Category": "Fruit Salads",
    "Ingredients": [
        {
            "1": "0.5 large chilled seedless watermelon, cut into 1-inch cubes"
        },
        {
            "2": "1 small red onion, sliced"
        },
        {
            "3": "1 cup thinly sliced fresh basil leaves"
        },
        {
            "4": "1 cup chopped fresh cilantro"
        },
        {
            "5": "0.5 cup minced fresh mint leaves"
        },
        {
            "6": "2  limes, juiced"
        },
        {
            "7": "1 (4 ounce) package crumbled feta cheese"
        },
        {
            "8": "3 tablespoons olive oil"
        },
        {
            "9": "2 tablespoons balsamic vinegar, or more to taste"
        },
        {
            "10": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Gently toss watermelon, onion, basil, cilantro, mint, lime juice, feta cheese, olive oil, balsamic vinegar, salt, and black pepper together in a large bowl."
        }
    ],
