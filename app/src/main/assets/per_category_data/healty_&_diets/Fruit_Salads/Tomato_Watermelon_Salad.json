{
    "title": "Tomato Watermelon Salad",
    "Category": "Fruit Salads",
    "Ingredients": [
        {
            "1": "1 large seedless watermelon, cut into 1-inch cubes"
        },
        {
            "2": "4  tomatoes, cut into 1-inch dice"
        },
        {
            "3": "1 cup crumbled feta cheese"
        },
        {
            "4": "1 large sweet onion, thinly sliced"
        },
        {
            "5": "12  mint leaves, sliced thinly"
        },
        {
            "6": "0.25 cup olive oil"
        },
        {
            "7": "2 tablespoons white balsamic vinegar"
        },
        {
            "8": "1 pinch coarse salt, or to taste"
        },
        {
            "9": "1 pinch ground black pepper, or to taste"
        }
    ],
    "Directions": [
        {
            "1": "Gently mix watermelon, tomatoes, feta cheese, onion, and mint in a large bowl."
        },
        {
            "2": "Whisk olive oil, vinegar, salt, and pepper together in a small bowl; drizzle over salad and gently toss to coat."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Total Time": "20 mins"
        },
        {
            "Servings": "12 "
        }
