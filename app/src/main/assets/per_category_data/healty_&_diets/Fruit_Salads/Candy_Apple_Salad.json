{
    "title": "Candy Apple Salad",
    "Category": "Fruit Salads",
    "Ingredients": [
        {
            "1": "4 cups Granny Smith apple - peeled, cored and chopped"
        },
        {
            "2": "0.5 cup white sugar"
        },
        {
            "3": "1 (8 ounce) can crushed pineapple, with juice"
        },
        {
            "4": "1 tablespoon all-purpose flour"
        },
        {
            "5": "1  egg, beaten"
        },
        {
            "6": "1 tablespoon white vinegar"
        },
        {
            "7": "1 (8 ounce) container frozen whipped topping, thawed"
        },
        {
            "8": "1 cup coarsely chopped dry roasted peanuts"
        }
    ],
    "Directions": [
        {
            "1": "In a saucepan over medium heat, stir together the egg, flour, sugar, vinegar and pineapple. Cook until thick, about 6 minutes. Set aside until completely cool."
        },
        {
            "2": "In a large serving bowl, fold together the pineapple mixture and whipped topping. Carefully mix in the apples and 1/2 cup of peanuts. Sprinkle the remaining nuts on the top."
        }
    ],
