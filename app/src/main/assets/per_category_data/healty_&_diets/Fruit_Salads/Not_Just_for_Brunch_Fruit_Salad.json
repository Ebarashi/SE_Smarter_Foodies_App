{
    "title": "Not Just for Brunch Fruit Salad",
    "Category": "Fruit Salads",
    "Ingredients": [
        {
            "1": "2 cups green grapes"
        },
        {
            "2": "2 cups sliced strawberries"
        },
        {
            "3": "2 cups sliced peaches"
        },
        {
            "4": "2 cups orange segments"
        },
        {
            "5": "1 cup peeled sliced kiwi"
        },
        {
            "6": "0.5 cup orange juice"
        },
        {
            "7": "0.5 cup orange-flavored liqueur (such as Cointreau\u00ae)"
        },
        {
            "8": "2 tablespoons white sugar"
        }
    ],
    "Directions": [
        {
            "1": "Mix grapes, strawberries, peaches, orange segments, and kiwi in a large bowl."
        },
        {
            "2": "Whisk orange juice, orange-flavored liqueur, and sugar together in a bowl until sugar dissolves; gently stir into fruit until evenly coated. Cover bowl with plastic wrap and refrigerate up to 8 hours."
        }
    ],
