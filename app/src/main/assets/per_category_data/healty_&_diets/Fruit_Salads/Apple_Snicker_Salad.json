{
    "title": "Apple Snicker Salad",
    "Category": "Fruit Salads",
    "Ingredients": [
        {
            "1": "1 (3.4 ounce) package instant butterscotch pudding mix"
        },
        {
            "2": "1 cup milk"
        },
        {
            "3": "1 (6 ounce) package whipped cream topping (such as Cool Whip\u00ae)"
        },
        {
            "4": "6  chocolate-coated caramel-peanut nougat candy bars (such as Snickers\u00ae), cut into bite-size pieces"
        },
        {
            "5": "3  Granny Smith apples - peeled, cored, and cut into bite-size pieces"
        }
    ],
    "Directions": [
        {
            "1": "Whisk butterscotch pudding mix into milk in a bowl until dissolved."
        },
        {
            "2": "Stir whipped cream topping into pudding mixture."
        },
        {
            "3": "Fold the candy bar pieces into the pudding mixture."
        },
        {
            "4": "Fold the apples into the pudding mixture."
        }
    ],
