{
    "title": "Baked Tofu Slices",
    "Category": "Tofu Recipes",
    "Ingredients": [
        {
            "1": "1 (18 ounce) package extra-firm tofu, slice 1/2-inch thick"
        },
        {
            "2": "1 tablespoon olive oil"
        },
        {
            "3": "1 pinch salt"
        },
        {
            "4": "1 pinch ground black pepper"
        },
        {
            "5": "2 tablespoons soy sauce"
        },
        {
            "6": "2 tablespoons Sriracha hot sauce, or to taste"
        },
        {
            "7": "0.5 teaspoons sesame oil"
        },
        {
            "8": "1 teaspoon chile-garlic sauce"
        },
        {
            "9": "1 teaspoon sesame seeds"
        },
        {
            "10": "0.5 teaspoon minced garlic"
        },
        {
            "11": "1  green onion, thinly sliced"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 450 degrees F (230 degrees C)."
        },
        {
            "2": "Arrange tofu slices onto a baking sheet. Brush olive oil over the top of each slice. Sprinkle salt and pepper over the tofu."
        },
        {
            "3": "Bake tofu in preheated oven until lightly browned on top, 8 to 10 minutes."
        },
        {
            "4": "Stir soy sauce, Sriracha, sesame oil, chile-garlic sauce, sesame seeds, garlic, and green onion together in a bowl; spoon atop the tofu slices."
        },
        {
            "5": "Return baking sheet to oven and continue baking tofu until topping is warmed, 3 to 5 minutes more."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "15 mins"
        },
        {
            "Total Time": "25 mins"
        },
        {
            "Servings": "6 "
        },
        {
            "Yield": "12 slices"
        }
