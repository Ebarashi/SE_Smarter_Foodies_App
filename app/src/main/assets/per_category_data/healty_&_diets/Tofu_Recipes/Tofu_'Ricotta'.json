{
    "title": "Tofu 'Ricotta'",
    "Category": "Tofu Recipes",
    "Ingredients": [
        {
            "1": "1 pound extra-firm tofu"
        },
        {
            "2": "1 tablespoon garlic powder"
        },
        {
            "3": "1 teaspoon oregano"
        },
        {
            "4": "1 teaspoon basil"
        },
        {
            "5": "1 teaspoon lemon juice"
        },
        {
            "6": "1 extra-virgin olive oil, or as needed"
        }
    ],
    "Directions": [
        {
            "1": "Blend tofu, garlic powder, oregano, basil, and lemon juice in a blender until combined. Stream olive oil into the tofu mixture while blending until your obtain the desired consistency."
        }
    ],
