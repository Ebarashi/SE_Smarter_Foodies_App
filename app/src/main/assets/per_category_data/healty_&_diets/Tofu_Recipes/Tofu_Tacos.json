{
    "title": "Tofu Tacos",
    "Category": "Tofu Recipes",
    "Ingredients": [
        {
            "1": "1 (16 ounce) package garden herb tofu, crumbled"
        },
        {
            "2": "2 tablespoons vegetable oil"
        },
        {
            "3": "1 clove garlic, minced"
        },
        {
            "4": "0.5 cup chopped onion"
        },
        {
            "5": "2 teaspoons chili powder"
        },
        {
            "6": "0.25 teaspoon paprika"
        },
        {
            "7": "0.25 teaspoon cayenne pepper"
        },
        {
            "8": "0.25 teaspoon ground cumin"
        },
        {
            "9": "0.25 teaspoon salt"
        },
        {
            "10": "0.5  lime, juiced"
        },
        {
            "11": "0.5 cup tomato sauce"
        },
        {
            "12": "0.25 cup chopped fresh cilantro"
        },
        {
            "13": "8 medium taco shells, heated"
        },
        {
            "14": "2 cups shredded lettuce"
        },
        {
            "15": "2  tomatoes, chopped"
        },
        {
            "16": "1  avocado - peeled, pitted and diced"
        },
        {
            "17": "1 cup shredded Cheddar cheese"
        },
        {
            "18": "0.25 cup salsa"
        }
    ],
    "Directions": [
        {
            "1": "Cook tofu, oil, garlic, and onion in a large skillet over medium heat for 5 minutes. Add chili powder, paprika, cayenne, cumin, salt, lime juice, and tomato sauce to the skillet and stir. Cook for 3 minutes."
        },
        {
            "2": "Stir in cilantro. Spoon mixture into a bowl."
        },
        {
            "3": "Spoon tofu mixture into taco shells. Top the mixture with lettuce, tomatoes, avocado, cheese, and salsa."
        }
    ],
    "Details": [
        {
            "Prep Time": "25 mins"
        },
        {
            "Cook Time": "10 mins"
        },
        {
            "Total Time": "35 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "8 tacos"
        }
