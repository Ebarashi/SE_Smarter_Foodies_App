{
    "title": "Chocolate-Banana Tofu Pudding",
    "Category": "Tofu Recipes",
    "Ingredients": [
        {
            "1": "1  banana, broken into chunks"
        },
        {
            "2": "1 (12 ounce) package soft silken tofu"
        },
        {
            "3": "0.25 cup confectioners' sugar"
        },
        {
            "4": "5 tablespoons unsweetened cocoa powder"
        },
        {
            "5": "3 tablespoons soy milk"
        },
        {
            "6": "1 pinch ground cinnamon"
        }
    ],
    "Directions": [
        {
            "1": "Place banana, tofu, sugar, cocoa powder, soy milk, and cinnamon into a blender. Cover, and puree until smooth."
        },
        {
            "2": "Pour into individual serving dishes, and refrigerate for 1 hour before serving."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Additional Time": " 1 hrs"
        },
        {
            "Total Time": " 1 hrs 10 mins"
        },
        {
            "Servings": "4 "
        }
    ],
