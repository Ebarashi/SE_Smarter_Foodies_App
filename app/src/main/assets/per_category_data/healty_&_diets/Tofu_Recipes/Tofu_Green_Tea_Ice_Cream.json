{
    "title": "Tofu Green Tea Ice Cream",
    "Category": "Tofu Recipes",
    "Ingredients": [
        {
            "1": "1 (12 ounce) package silken soft tofu, drained"
        },
        {
            "2": "1 cup unsweetened soy milk"
        },
        {
            "3": "1 cup white sugar, or to taste"
        },
        {
            "4": "0.25 cup green tea powder (matcha)"
        },
        {
            "5": "2 tablespoons canola oil"
        }
    ],
    "Directions": [
        {
            "1": "Blend tofu in a blender or food processor until smooth; add soy milk, sugar, green tea powder, and canola oil and blend until combined."
        },
        {
            "2": "Pour mixture into bowl of ice cream maker; freeze according to manufacturers directions."
        }
    ],
