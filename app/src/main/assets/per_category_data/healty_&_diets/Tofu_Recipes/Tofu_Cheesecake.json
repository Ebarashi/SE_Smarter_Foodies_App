{
    "title": "Tofu Cheesecake",
    "Category": "Tofu Recipes",
    "Ingredients": [
        {
            "1": "2 (12 ounce) packages extra firm tofu, drained and cubed"
        },
        {
            "2": "1 cup white sugar"
        },
        {
            "3": "1 teaspoon vanilla extract"
        },
        {
            "4": "0.25 teaspoon salt"
        },
        {
            "5": "0.25 cup vegetable oil"
        },
        {
            "6": "2 tablespoons lemon juice"
        },
        {
            "7": "1 (9 inch) prepared graham cracker crust"
        }
    ],
    "Directions": [
        {
            "1": "Preheat an oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "In a blender or food processor, combine tofu, sugar, vanilla, salt, vegetable oil, and lemon juice. Blend until smooth. Pour into the pie crust."
        },
        {
            "3": "Bake in the preheated oven until slightly brown, 20 to 30 minutes. Remove from the oven to cool. Refrigerate until chilled."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "20 mins"
        },
