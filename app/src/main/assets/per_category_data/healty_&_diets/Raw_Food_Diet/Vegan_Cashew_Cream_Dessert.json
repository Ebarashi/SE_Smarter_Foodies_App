{
    "title": "Vegan Cashew Cream Dessert",
    "Category": "Raw Food Diet",
    "Ingredients": [
        {
            "1": "1 cup cashews"
        },
        {
            "2": "1 cold water to cover"
        },
        {
            "3": "1 cup coconut milk"
        },
        {
            "4": "4 tablespoons agave syrup, or to taste"
        },
        {
            "5": "1 tablespoon vanilla extract"
        },
        {
            "6": "0.5 teaspoon rose water"
        },
        {
            "7": "2 tablespoons pomegranate seeds, or to taste"
        }
    ],
    "Directions": [
        {
            "1": "Cover cashews with cold water in a bowl and soak for 2 to 3 hours or overnight. Drain."
        },
        {
            "2": "Combine cashews, coconut milk, agave syrup, vanilla extract, and rose water in a blender; blend until smooth. Pour into a bowl and garnish with pomegranate seeds."
        }
    ],
