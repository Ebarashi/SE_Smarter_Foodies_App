{
    "title": "Ahi Shoyu Poke",
    "Category": "Raw Food Diet",
    "Ingredients": [
        {
            "1": "1 pound fresh ahi steaks, cut into small cubes"
        },
        {
            "2": "0.25 cup soy sauce"
        },
        {
            "3": "0.25 cup chopped Maui onion"
        },
        {
            "4": "0.25 cup chopped green onion"
        },
        {
            "5": "1  chile pepper, seeded and diced (Optional)"
        },
        {
            "6": "2 teaspoons sesame oil"
        },
        {
            "7": "2 teaspoons toasted sesame seeds (Optional)"
        },
        {
            "8": "2 teaspoons finely chopped toasted macadamia nuts (Optional)"
        },
        {
            "9": "1 teaspoon grated fresh ginger"
        },
        {
            "10": "1 sea salt to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine ahi cubes, soy sauce, Maui onion, green onion, chile pepper, sesame oil, sesame seeds, macadamia nuts, ginger, and sea salt in a large bowl."
        }
    ],
