{
    "title": "Salmon Tartare",
    "Category": "Raw Food Diet",
    "Ingredients": [
        {
            "1": "1 (5 ounce) very fresh salmon fillet"
        },
        {
            "2": "1 teaspoon minced shallot"
        },
        {
            "3": "1 teaspoon minced fresh flat-leaf parsley"
        },
        {
            "4": "1 teaspoon minced fresh chives"
        },
        {
            "5": "1 teaspoon minced cornichon (small pickled cucumber)"
        },
        {
            "6": "1 teaspoon fresh lemon juice, or more to taste"
        },
        {
            "7": "1 sea salt and freshly ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Remove salmon skin and cut out the grey-colored bloodline so you have only the shiny pink flesh. Cut salmon into small cubes and place in a bowl."
        },
        {
            "2": "Stir in shallot, parsley, chives, pickle, lemon juice, sea salt, and pepper. Let marinate for 5 minutes before serving."
        }
    ],
