{
    "title": "Spicy Ahi Poke Salad",
    "Category": "Raw Food Diet",
    "Ingredients": [
        {
            "1": "1 pound ahi tuna, cut into 1/2-inch cubes"
        },
        {
            "2": "0.25 cup minced green onion"
        },
        {
            "3": "2 tablespoons ground roasted macadamia nuts"
        },
        {
            "4": "2 tablespoons chopped fresh cilantro, or more to taste"
        },
        {
            "5": "1 tablespoon fresh lime juice"
        },
        {
            "6": "2 teaspoons sesame oil, or more to taste"
        },
        {
            "7": "1 teaspoon minced fresh ginger"
        },
        {
            "8": "1 teaspoon red pepper flakes"
        },
        {
            "9": "1 teaspoon sriracha sauce, or to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine tuna, green onion, macadamia nuts, cilantro, lime juice, sesame oil, ginger, red pepper flakes, and sriracha sauce in a bowl; refrigerate for 2 hours."
        }
    ],
