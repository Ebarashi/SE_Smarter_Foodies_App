{
    "title": "Sweet Chili Thai Sauce",
    "Category": "Vegetarian Recipes",
    "Ingredients": [
        {
            "1": "1 cup water"
        },
        {
            "2": "1 cup rice vinegar"
        },
        {
            "3": "1 cup sugar"
        },
        {
            "4": "2 teaspoons fresh ginger root, minced"
        },
        {
            "5": "2 teaspoons hot chile pepper, minced"
        },
        {
            "6": "2 teaspoons ketchup"
        },
        {
            "7": "1 teaspoon garlic, minced"
        },
        {
            "8": "2 teaspoons cornstarch"
        }
    ],
    "Directions": [
        {
            "1": "Pour water and vinegar into a saucepan; bring to a boil over high heat. Stir in sugar, ginger, chile pepper, ketchup, and garlic; simmer for 5 minutes. Whisk in cornstarch."
        },
        {
            "2": "Remove saucepan from stove to cool. Transfer to a bowl, cover, and refrigerate until needed."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
