{
    "title": "Quaker\u00ae Banana Bread Overnight Oats",
    "Category": "Overnight Oats",
    "Ingredients": [
        {
            "1": "3 cups skim or 1% milk"
        },
        {
            "2": "2 tablespoons honey"
        },
        {
            "3": "2 tablespoons brown sugar"
        },
        {
            "4": "1 teaspoon vanilla extract"
        },
        {
            "5": "0.5 teaspoon ground cinnamon"
        },
        {
            "6": "0.25 teaspoon ground nutmeg"
        },
        {
            "7": "0.25 teaspoon salt"
        },
        {
            "8": "3  bananas, divided"
        },
        {
            "9": "3 cups (750 mL) Large Flake Quaker\u00ae Oats"
        },
        {
            "10": "0.25 cup chopped toasted pecans"
        },
        {
            "11": "1 cup Plain or vanilla non-fat yogurt"
        }
    ],
    "Directions": [
        {
            "1": "Stir together milk, honey, brown sugar, vanilla, cinnamon, nutmeg and salt; stir in oats. Cover and refrigerate overnight."
        },
        {
            "2": "Mash two bananas and stir into oatmeal. Slice remaining banana. Divide oatmeal evenly among bowls or small Mason jars; top with sliced banana, pecans, and yogurt (if using)."
        }
    ],
