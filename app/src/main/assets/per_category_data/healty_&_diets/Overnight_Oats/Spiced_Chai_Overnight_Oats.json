{
    "title": "Spiced Chai Overnight Oats",
    "Category": "Overnight Oats",
    "Ingredients": [
        {
            "1": "0.5 cup rolled oats"
        },
        {
            "2": "0.5 cup brewed chai tea"
        },
        {
            "3": "0.5 cup unsweetened almond milk"
        },
        {
            "4": "2 tablespoons apple butter"
        }
    ],
    "Directions": [
        {
            "1": "Combine rolled oats, chai tea, almond milk, and apple butter in a jar or bowl."
        },
        {
            "2": "Refrigerate until oats are soft, 2 hours to overnight. Stir before eating."
        }
    ],
