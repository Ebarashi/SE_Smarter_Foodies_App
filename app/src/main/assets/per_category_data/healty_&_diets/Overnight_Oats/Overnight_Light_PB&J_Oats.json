{
    "title": "Overnight Light PB&J Oats",
    "Category": "Overnight Oats",
    "Ingredients": [
        {
            "1": "0.5 cup almond milk"
        },
        {
            "2": "0.25 cup fresh raspberries"
        },
        {
            "3": "0.25 cup rolled oats"
        },
        {
            "4": "2 tablespoons powdered peanut butter (such as PB2\u00ae)"
        },
        {
            "5": "0.5 teaspoons chia seeds"
        },
        {
            "6": "1 teaspoon white sugar"
        }
    ],
    "Directions": [
        {
            "1": "Mix almond milk, raspberries, rolled oats, powdered peanut butter, chia seeds, and sugar together in a container. Cover and refrigerate until oats are soft, 8 hours to overnight."
        }
