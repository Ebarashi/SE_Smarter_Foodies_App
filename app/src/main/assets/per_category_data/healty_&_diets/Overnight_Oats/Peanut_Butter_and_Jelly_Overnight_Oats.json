{
    "title": "Peanut Butter and Jelly Overnight Oats",
    "Category": "Overnight Oats",
    "Ingredients": [
        {
            "1": "0.5 cups almond milk"
        },
        {
            "2": "1 cup old-fashioned rolled oats"
        },
        {
            "3": "0.25 cup peanut butter"
        },
        {
            "4": "2 tablespoons chia seeds"
        },
        {
            "5": "2 tablespoons pure maple syrup, or more to taste"
        },
        {
            "6": "1 pinch coarse salt"
        },
        {
            "7": "0.25 cup strawberry preserves"
        },
        {
            "8": "1 tablespoon peanut butter, or more to taste"
        },
        {
            "9": "0.5 cup sliced fresh strawberries"
        },
        {
            "10": "0.25 cup slivered almonds"
        }
    ],
    "Directions": [
        {
            "1": "Whisk together almond milk, oats, 1/4 cup peanut butter, chia seeds, maple syrup, and salt in a bowl. Cover and refrigerate, 8 hours to overnight."
        },
        {
            "2": "Before serving, stir in strawberry preserves, remaining peanut butter, and more maple syrup if needed. Top with sliced strawberries and slivered almonds."
        }
    ],
