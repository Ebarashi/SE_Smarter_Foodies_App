{
    "title": "Overnight Steel-Cut Oats with Yogurt and Blueberries",
    "Category": "Overnight Oats",
    "Ingredients": [
        {
            "1": "0.25 cup steel-cut oats"
        },
        {
            "2": "1 (8 ounce) container plain yogurt"
        },
        {
            "3": "2 ounces fresh blueberries"
        },
        {
            "4": "0.5 ounce stevia powder (such as Pure Via\u00ae)"
        },
        {
            "5": "2.0 ounces coconut milk"
        }
    ],
    "Directions": [
        {
            "1": "Place oats into the bottom of a 1-pint Mason jar. Stir in yogurt, blueberries, and stevia powder. Pour in coconut milk and stir to combine."
        },
        {
            "2": "Cover the jar and refrigerate, 8 hours to overnight."
        }
    ],
