{
    "title": "Protein Powder Overnight Oats with Blueberries and Peanut Butter",
    "Category": "Overnight Oats",
    "Ingredients": [
        {
            "1": "0.75 cup almond milk"
        },
        {
            "2": "0.5 cup oats"
        },
        {
            "3": "2 tablespoons peanut butter"
        },
        {
            "4": "2 tablespoons chia seeds"
        },
        {
            "5": "2 tablespoons agave nectar"
        },
        {
            "6": "1 scoop vanilla protein powder"
        },
        {
            "7": "1 cup frozen blueberries"
        }
    ],
    "Directions": [
        {
            "1": "Mix almond milk, oats, peanut butter, chia seeds, agave, and protein powder in a bowl until well combined. Divide mixture between 2 Mason jars. Top each jar with 1/2 cup blueberries. Refrigerate for 8 hours, or overnight."
        }
    ],
