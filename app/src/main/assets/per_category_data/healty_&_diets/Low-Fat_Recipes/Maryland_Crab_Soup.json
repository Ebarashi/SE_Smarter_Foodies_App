{
    "title": "Maryland Crab Soup",
    "Category": "Low-Fat Recipes",
    "Ingredients": [
        {
            "1": "2 (14.5 ounce) cans stewed tomatoes"
        },
        {
            "2": "3 cups water"
        },
        {
            "3": "2 cups beef broth"
        },
        {
            "4": "1 cup fresh lima beans"
        },
        {
            "5": "1 cup frozen corn kernels"
        },
        {
            "6": "1 cup sliced carrots"
        },
        {
            "7": "2 tablespoons chopped onion"
        },
        {
            "8": "2 tablespoons Old Bay Seasoning TM"
        },
        {
            "9": "1 gallon water"
        },
        {
            "10": "10  blue crab claws, steamed (Optional)"
        },
        {
            "11": "1 pound blue crab crabmeat"
        }
    ],
    "Directions": [
        {
            "1": "Place stewed tomatoes, 3 cups water, beef broth, lima beans, corn, sliced carrots, chopped onion, and Old Bay seasoning in a 4-quart pot. Bring to a simmer over medium heat; cover and cook for 5 minutes."
        },
        {
            "2": "Bring 1 gallon water to a boil in a large pot. Add crab claws and boil for 6 minutes; drain."
        },
        {
            "3": "Stir crabmeat and boiled crab claws into tomato and vegetable mixture. Cover and simmer for 10 to 15 minutes. Serve hot."
        }
    ],
