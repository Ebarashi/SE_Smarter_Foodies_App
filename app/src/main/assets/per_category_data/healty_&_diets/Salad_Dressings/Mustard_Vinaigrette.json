{
    "title": "Mustard Vinaigrette",
    "Category": "Salad Dressings",
    "Ingredients": [
        {
            "1": "1 cup vegetable oil"
        },
        {
            "2": "0.5 cup white vinegar"
        },
        {
            "3": "1 tablespoon honey"
        },
        {
            "4": "1 tablespoon prepared Dijon-style mustard"
        },
        {
            "5": "2 teaspoons minced garlic"
        },
        {
            "6": "2 teaspoons salt"
        },
        {
            "7": "0.5 teaspoon ground black pepper"
        },
        {
            "8": "4 drops hot sauce"
        }
    ],
    "Directions": [
        {
            "1": "Whisk oil, vinegar, honey, mustard, garlic, salt, black pepper, and hot sauce together in a small bowl until thoroughly combined. Chill until serving."
        }
    ],
    "Details": [
