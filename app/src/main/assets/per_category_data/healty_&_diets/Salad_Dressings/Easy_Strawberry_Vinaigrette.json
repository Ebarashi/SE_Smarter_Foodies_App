{
    "title": "Easy Strawberry Vinaigrette",
    "Category": "Salad Dressings",
    "Ingredients": [
        {
            "1": "8 ounces frozen strawberries"
        },
        {
            "2": "2 tablespoons honey"
        },
        {
            "3": "2 tablespoons apple cider vinegar"
        },
        {
            "4": "2 tablespoons olive oil"
        },
        {
            "5": "0.25 teaspoon salt"
        },
        {
            "6": "0.25 teaspoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Blend strawberries, honey, apple cider vinegar, olive oil, salt, and black pepper together in a blender until smooth."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Total Time": "10 mins"
        },
        {
            "Servings": "7 "
