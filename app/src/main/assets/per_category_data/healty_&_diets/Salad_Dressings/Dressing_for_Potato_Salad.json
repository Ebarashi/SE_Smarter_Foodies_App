{
    "title": "Dressing for Potato Salad",
    "Category": "Salad Dressings",
    "Ingredients": [
        {
            "1": "0.5 cup mayonnaise"
        },
        {
            "2": "0.5 cup sour cream"
        },
        {
            "3": "2 teaspoons prepared mustard"
        },
        {
            "4": "1 tablespoon ketchup"
        },
        {
            "5": "0.5 teaspoons Worcestershire sauce"
        },
        {
            "6": "0.5 cup diced onion (Optional)"
        },
        {
            "7": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Mix together the mayonnaise, sour cream, mustard, ketchup, Worcestershire sauce, onion and salt and pepper. Refrigerate until ready to toss with potatoes."
        }
    ],
