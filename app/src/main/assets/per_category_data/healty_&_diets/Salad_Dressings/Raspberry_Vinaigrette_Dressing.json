{
    "title": "Raspberry Vinaigrette Dressing",
    "Category": "Salad Dressings",
    "Ingredients": [
        {
            "1": "0.5 cup vegetable oil"
        },
        {
            "2": "0.5 cup raspberry wine vinegar"
        },
        {
            "3": "0.5 cup white sugar"
        },
        {
            "4": "2 teaspoons Dijon mustard"
        },
        {
            "5": "0.25 teaspoon dried oregano"
        },
        {
            "6": "0.25 teaspoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "In a jar with a tight fitting lid, combine the oil, vinegar, sugar, mustard, oregano, and pepper. Shake well."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Total Time": "5 mins"
        },
        {
            "Servings": "12 "
        },
        {
            "Yield": "1 1/2 cups"
        }
    ],
