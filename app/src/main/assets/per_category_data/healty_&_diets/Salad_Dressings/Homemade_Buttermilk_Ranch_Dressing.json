{
    "title": "Homemade Buttermilk Ranch Dressing",
    "Category": "Salad Dressings",
    "Ingredients": [
        {
            "1": "1 cup mayonnaise"
        },
        {
            "2": "1 cup buttermilk"
        },
        {
            "3": "1 tablespoon dried parsley"
        },
        {
            "4": "1 teaspoon garlic powder"
        },
        {
            "5": "1 teaspoon salt"
        },
        {
            "6": "0.5 teaspoon onion powder"
        },
        {
            "7": "0.25 teaspoon dried dill weed"
        },
        {
            "8": "1 freshly ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Whisk mayonnaise and buttermilk in a medium mixing bowl until smooth. Add parsley, garlic powder, salt, onion powder, dill, and pepper; whisk until blended together. Place in the refrigerator until flavors have melded, about 20 minutes."
        }
    ],
