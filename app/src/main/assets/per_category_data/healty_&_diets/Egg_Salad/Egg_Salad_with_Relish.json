{
    "title": "Egg Salad with Relish",
    "Category": "Egg Salad",
    "Ingredients": [
        {
            "1": "8  hard-boiled eggs, finely chopped"
        },
        {
            "2": "0.25  sweet onion (such as Vidalia\u00ae), finely chopped"
        },
        {
            "3": "1 cup mayonnaise"
        },
        {
            "4": "2 tablespoons dill pickle relish"
        },
        {
            "5": "1 tablespoon prepared yellow mustard"
        },
        {
            "6": "1 dash hot sauce, or more to taste"
        },
        {
            "7": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine eggs and onion in a bowl. Add mayonnaise, relish, mustard, and hot sauce. Mix until ingredients are well combined. Season with salt and pepper."
        },
        {
            "2": "Chill in the refrigerator for 2 hours before serving."
        }
    ],
