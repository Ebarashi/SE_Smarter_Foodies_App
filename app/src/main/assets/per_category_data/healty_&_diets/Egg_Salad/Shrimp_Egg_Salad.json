{
    "title": "Shrimp Egg Salad",
    "Category": "Egg Salad",
    "Ingredients": [
        {
            "1": "1 pound cooked shrimp - peeled, deveined, and chopped"
        },
        {
            "2": "4  hard-cooked eggs, chopped"
        },
        {
            "3": "4 tablespoons mayonnaise"
        },
        {
            "4": "1 teaspoon Dijon mustard"
        },
        {
            "5": "1 sprig chopped fresh dill"
        },
        {
            "6": "4 leaves green leaf lettuce"
        }
    ],
    "Directions": [
        {
            "1": "In a medium bowl, mix together the shrimp, eggs, mayonnaise and mustard. Spoon onto lettuce leaves to serve."
        }
    ],
