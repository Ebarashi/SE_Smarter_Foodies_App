{
    "title": "Zippy Egg Salad",
    "Category": "Egg Salad",
    "Ingredients": [
        {
            "1": "0.25 cup mayonnaise"
        },
        {
            "2": "2 tablespoons grated Parmesan cheese"
        },
        {
            "3": "1 tablespoon light soy sauce"
        },
        {
            "4": "0.25  lime, juiced"
        },
        {
            "5": "1 teaspoon crushed red pepper"
        },
        {
            "6": "1 salt and ground black pepper to taste"
        },
        {
            "7": "4  hard-cooked eggs, chopped"
        },
        {
            "8": "1 stalk celery, finely sliced"
        }
    ],
    "Directions": [
        {
            "1": "Whisk mayonnaise, Parmesan cheese, soy sauce, lime juice, red pepper, salt, and black pepper in a bowl."
        },
        {
            "2": "Stir eggs and celery into the mayonnaise mixture until well coated."
        },
        {
            "3": "Cover bowl with plastic wrap and refrigerate until chilled before serving."
        }
    ],
