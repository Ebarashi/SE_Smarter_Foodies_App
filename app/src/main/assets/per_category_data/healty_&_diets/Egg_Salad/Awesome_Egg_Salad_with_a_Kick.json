{
    "title": "Awesome Egg Salad with a Kick",
    "Category": "Egg Salad",
    "Ingredients": [
        {
            "1": "12  hard-cooked eggs, peeled and chopped"
        },
        {
            "2": "0.5 cup light mayonnaise"
        },
        {
            "3": "0.5 cup low-fat sour cream"
        },
        {
            "4": "0.25 teaspoon paprika"
        },
        {
            "5": "3 tablespoons Dijon mustard"
        },
        {
            "6": "2 tablespoons prepared horseradish"
        },
        {
            "7": "2 tablespoons chopped red onion"
        },
        {
            "8": "0.25 cup real bacon bits"
        },
        {
            "9": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "In a medium bowl, stir together the eggs, mayonnaise, sour cream, paprika, mustard, horseradish, red onion and bacon bits. Season with salt and pepper to taste. Serve immediately, or refrigerate until serving."
        }
    ],
