{
    "title": "Cauliflower and Egg Salad",
    "Category": "Egg Salad",
    "Ingredients": [
        {
            "1": "4 cups cauliflower, broken into small florets"
        },
        {
            "2": "3  hard-boiled eggs, diced"
        },
        {
            "3": "0.5 small carrot, peeled and shaved into strips using a vegetable peeler"
        },
        {
            "4": "3 small green onions, chopped"
        },
        {
            "5": "5 tablespoons sweet pickle relish"
        },
        {
            "6": "5 tablespoons light mayonnaise"
        },
        {
            "7": "1 tablespoon mustard"
        },
        {
            "8": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Bring a large pot of lightly salted water to a boil. Cook cauliflower in the boiling water until tender, about 10 minutes; drain."
        },
        {
            "2": "Mix cooked cauliflower, hard-boiled eggs, carrot, green onions, sweet pickle relish, mayonnaise, and mustard in a large bowl; season with salt and black pepper."
        }
    ],
