{
    "title": "Strawberry Oatmeal Breakfast Smoothie",
    "Category": "Healthy Recipes",
    "Ingredients": [
        {
            "1": "1 cup soy milk"
        },
        {
            "2": "0.5 cup rolled oats"
        },
        {
            "3": "14  frozen strawberries"
        },
        {
            "4": "1  banana, broken into chunks"
        },
        {
            "5": "0.5 teaspoons white sugar (Optional)"
        },
        {
            "6": "0.5 teaspoon vanilla extract (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Blend soy milk, oats, strawberries, and banana in a blender until smooth. Add sugar and vanilla and blend again until smooth."
        },
        {
            "2": "Pour into glasses and serve."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Total Time": "5 mins"
        },
        {
            "Servings": "2 "
        }
