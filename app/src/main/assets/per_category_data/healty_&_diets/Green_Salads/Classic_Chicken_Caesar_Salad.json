{
    "title": "Classic Chicken Caesar Salad",
    "Category": "Green Salads",
    "Ingredients": [
        {
            "1": "6 cups tightly packed chopped romaine lettuce"
        },
        {
            "2": "1 pound cooked boneless skinless chicken breasts, cut into strips"
        },
        {
            "3": "0.5 cup CRACKER BARREL Finely Shredded 100% Parmesan Cheese"
        },
        {
            "4": "0.5 cup seasoned croutons"
        },
        {
            "5": "0.25 cup KRAFT Creamy Caesar Dressing"
        }
    ],
    "Directions": [
        {
            "1": "Cover 4 plates with lettuce."
        },
        {
            "2": "Top with all remaining ingredients except dressing."
        },
        {
            "3": "Drizzle with dressing just before serving."
        }
    ],
