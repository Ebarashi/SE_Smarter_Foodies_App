{
    "title": "Slow Cooker Steel-Cut Oats",
    "Category": "Oatmeal",
    "Ingredients": [
        {
            "1": "2 cups water"
        },
        {
            "2": "1 cup milk"
        },
        {
            "3": "1 cup steel-cut oats"
        },
        {
            "4": "1  apple - peeled, cored, and chopped"
        },
        {
            "5": "2 tablespoons brown sugar"
        },
        {
            "6": "0.5 tablespoons butter, cut into 6 pieces (Optional)"
        },
        {
            "7": "0.5 teaspoon ground cinnamon"
        },
        {
            "8": "0.25 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Combine water, milk, oats, apple, brown sugar, butter, cinnamon, and salt in a slow cooker; mix well."
        },
        {
            "2": "Cook oatmeal on Low for 1 hour. Reduce heat to Simmer or Warm and cook for 7 to 8 hours."
        }
    ],
