{
    "title": "Delicious Oat Bran Cereal",
    "Category": "Oatmeal",
    "Ingredients": [
        {
            "1": "1 cup water"
        },
        {
            "2": "0.25 teaspoon ground cinnamon"
        },
        {
            "3": "5  dried pitted prunes, chopped"
        },
        {
            "4": "1 teaspoon sugar substitute (such as Splenda)"
        },
        {
            "5": "0.25 cup oat bran"
        }
    ],
    "Directions": [
        {
            "1": "Combine water, cinnamon, prunes, and sugar substitute in a saucepan over medium heat; bring to a boil; stir in the oat bran and boil for 2 minutes."
        }
    ],
