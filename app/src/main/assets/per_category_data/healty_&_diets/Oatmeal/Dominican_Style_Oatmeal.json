{
    "title": "Dominican Style Oatmeal",
    "Category": "Oatmeal",
    "Ingredients": [
        {
            "1": "0.5 cups milk"
        },
        {
            "2": "0.5 cup quick cooking oats"
        },
        {
            "3": "2 tablespoons white sugar"
        },
        {
            "4": "0.25 teaspoon ground cinnamon"
        },
        {
            "5": "1 pinch ground nutmeg"
        },
        {
            "6": "1 pinch salt"
        },
        {
            "7": "0.25 teaspoon lemon zest (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Combine milk, oats, sugar, cinnamon, nutmeg, and salt in a saucepan. Add lemon peel, if using. Bring to a boil, stirring constantly for 2 minutes."
        }
    ],
