{
    "title": "Overnight Chai Oatmeal",
    "Category": "Oatmeal",
    "Ingredients": [
        {
            "1": "1 cup oats"
        },
        {
            "2": "1 cup almond-coconut milk"
        },
        {
            "3": "2 tablespoons chia seeds"
        },
        {
            "4": "2 tablespoons shredded coconut"
        },
        {
            "5": "0.25 teaspoon ground cardamom"
        },
        {
            "6": "0.25 teaspoon ground cinnamon"
        },
        {
            "7": "0.25 teaspoon vanilla extract"
        },
        {
            "8": "0.25 teaspoon ground ginger"
        },
        {
            "9": "0.25 teaspoon nutmeg"
        }
    ],
    "Directions": [
        {
            "1": "Combine oats, almond-coconut milk, chia seeds, coconut, cardamom, cinnamon, vanilla extract, ginger, and nutmeg in a bowl. Cover bowl with plastic wrap and refrigerate, 8 hours to overnight."
        }
    ],
