{
    "title": "Banana Bread Oatmeal",
    "Category": "Oatmeal",
    "Ingredients": [
        {
            "1": "3 cups fat-free milk"
        },
        {
            "2": "3 tablespoons firmly packed brown sugar"
        },
        {
            "3": "0.75 teaspoon ground cinnamon"
        },
        {
            "4": "0.25 teaspoon salt"
        },
        {
            "5": "0.25 teaspoon ground nutmeg"
        },
        {
            "6": "2 cups Quaker\u00ae Oats (Quick or Old Fashioned, uncooked)"
        },
        {
            "7": "1 cup mashed ripe bananas"
        },
        {
            "8": "2 tablespoons coarsely chopped toasted pecans"
        },
        {
            "9": "1 teaspoon Plain or vanilla non-fat yogurt"
        },
        {
            "10": "1 small Banana slices"
        },
        {
            "11": "1 tablespoon Pecan halves"
        }
    ],
    "Directions": [
        {
            "1": "Bring milk, sugar, cinnamon, salt, and nutmeg to a gentle boil in a medium saucepan (watch carefully); stir in oats. Return to boil; reduce heat to medium and cook, stirring occasionally, until most of liquid is absorbed, about 1 minute for quick oats and 5 minutes for old-fashioned oats."
        },
        {
            "2": "Remove oatmeal from heat. Stir in mashed bananas and pecans."
        },
        {
            "3": "Spoon oatmeal into 6 cereal bowls."
        },
        {
            "4": "Top with yogurt, sliced bananas, and pecan halves, if desired."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "5 mins"
        },
