{
    "title": "Josh's Baked Oatmeal",
    "Category": "Oatmeal",
    "Ingredients": [
        {
            "1": "3 cups rolled oats"
        },
        {
            "2": "0.75 cup brown sugar"
        },
        {
            "3": "2 teaspoons ground cinnamon (such as McCormick\u00ae Roasted Saigon Cinnamon)"
        },
        {
            "4": "2 teaspoons baking powder"
        },
        {
            "5": "1 teaspoon salt"
        },
        {
            "6": "1 cup milk"
        },
        {
            "7": "0.5 cup melted butter"
        },
        {
            "8": "0.25 cup maple syrup"
        },
        {
            "9": "2  eggs"
        },
        {
            "10": "2 teaspoons vanilla extract"
        },
        {
            "11": "0.25 cup dried cranberries, or to taste"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C). Grease an 8x8-inch baking dish."
        },
        {
            "2": "Mix oats, brown sugar, cinnamon, baking powder, and salt together in a bowl. Beat in milk, butter, maple syrup, eggs, and vanilla extract; fold in cranberries. Spread mixture into the prepared baking dish."
        },
        {
            "3": "Bake in the preheated oven until bubbling, about 40 minutes."
        }
    ],
    "Details": [
