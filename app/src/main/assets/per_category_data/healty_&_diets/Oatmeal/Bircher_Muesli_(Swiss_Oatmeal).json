{
    "title": "Bircher Muesli (Swiss Oatmeal)",
    "Category": "Oatmeal",
    "Ingredients": [
        {
            "1": "2 cups rolled oats"
        },
        {
            "2": "1  green apple, chopped"
        },
        {
            "3": "1 cup 2% milk"
        },
        {
            "4": "1 cup nonfat plain yogurt"
        },
        {
            "5": "1  banana, chopped"
        },
        {
            "6": "0.25 cup raisins"
        },
        {
            "7": "1 tablespoon chopped walnuts"
        },
        {
            "8": "1 tablespoon slivered almonds"
        },
        {
            "9": "1 tablespoon brown sugar"
        },
        {
            "10": "1 tablespoon honey"
        },
        {
            "11": "1 tablespoon orange juice"
        }
    ],
    "Directions": [
        {
            "1": "Mix oats, apple, milk, yogurt, banana, raisins, walnuts, almonds, brown sugar, honey, and orange juice together in a bowl. Chill in refrigerator for at least 1 hour to set."
        }
    ],
