{
    "title": "Berry Overnight Oats",
    "Category": "Oatmeal",
    "Ingredients": [
        {
            "1": "0.5 cup Quaker\u00ae Oats (Quick or Old Fashioned, uncooked)"
        },
        {
            "2": "0.5 cup non-fat milk"
        },
        {
            "3": "0.5 cup non-fat plain Greek yogurt"
        },
        {
            "4": "1 teaspoon chia seeds (Optional)"
        },
        {
            "5": "1 cup fresh mixed berries and fruit"
        }
    ],
    "Directions": [
        {
            "1": "Add Quaker\u00ae Oats to your container of choice and pour in milk."
        },
        {
            "2": "Layer Greek yogurt, chia seeds and mixed fruit and berries."
        },
        {
            "3": "Refrigerate overnight and enjoy in the morning!"
        }
    ],
