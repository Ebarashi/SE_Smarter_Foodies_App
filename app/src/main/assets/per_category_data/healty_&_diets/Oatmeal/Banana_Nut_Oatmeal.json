{
    "title": "Banana Nut Oatmeal",
    "Category": "Oatmeal",
    "Ingredients": [
        {
            "1": "0.25 cup quick cooking oats"
        },
        {
            "2": "0.5 cup skim milk"
        },
        {
            "3": "1 teaspoon flax seeds"
        },
        {
            "4": "2 tablespoons chopped walnuts"
        },
        {
            "5": "3 tablespoons honey"
        },
        {
            "6": "1  banana, peeled"
        }
    ],
    "Directions": [
        {
            "1": "Combine the oats, milk, flax seeds, walnuts, honey, and banana in a microwave-safe bowl. Cook in microwave on High for 2 minutes. Mash the banana with a fork and stir into the mixture. Serve hot."
        }
    ],
