{
    "title": "Fideo",
    "Category": "Low-Cholesterol Recipes",
    "Ingredients": [
        {
            "1": "2 tablespoons vegetable oil"
        },
        {
            "2": "1 (8 ounce) package fideo pasta"
        },
        {
            "3": "0.5 cups water, divided"
        },
        {
            "4": "1 (8 ounce) can tomato sauce, divided"
        },
        {
            "5": "1 tablespoon chili powder"
        },
        {
            "6": "0.5 teaspoon ground cumin"
        },
        {
            "7": "0.5 teaspoon garlic salt"
        }
    ],
    "Directions": [
        {
            "1": "Heat oil in a saucepan over medium-high heat. Add fideo; cook and stir until browned."
        },
        {
            "2": "Add 1 cup water and 1/2 of the tomato sauce to fideo. Season with chili powder, cumin, and garlic salt; stir to blend. Bring to a boil and cook until liquid has almost evaporated."
        },
        {
            "3": "Stir in remaining 1/2 cup water and remaining tomato sauce. Cover and cook over medium heat until fideo is tender and sauce thickens, about 10 minutes."
        }
    ],
