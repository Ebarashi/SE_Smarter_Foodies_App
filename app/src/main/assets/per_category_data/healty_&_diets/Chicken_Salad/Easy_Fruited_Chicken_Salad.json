{
    "title": "Easy Fruited Chicken Salad",
    "Category": "Chicken Salad",
    "Ingredients": [
        {
            "1": "1  apple, cored and diced"
        },
        {
            "2": "0.5 cup seedless red grapes, halved"
        },
        {
            "3": "0.5 cup chopped toasted pecans"
        },
        {
            "4": "0.5 teaspoon apple pie spice, or to taste"
        },
        {
            "5": "0.75 cup mayonnaise"
        },
        {
            "6": "4  skinless, boneless chicken breast halves - cooked and diced"
        }
    ],
    "Directions": [
        {
            "1": "In a bowl, lightly mix the apple, grapes, pecans, apple pie spice, and mayonnaise together until thoroughly combined; stir in the chicken breast meat. Chill until cold."
        }
    ],
