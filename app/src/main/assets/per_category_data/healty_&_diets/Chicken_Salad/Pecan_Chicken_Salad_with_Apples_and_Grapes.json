{
    "title": "Pecan Chicken Salad with Apples and Grapes",
    "Category": "Chicken Salad",
    "Ingredients": [
        {
            "1": "2 cups chopped cold rotisserie or roasted chicken"
        },
        {
            "2": "0.5 cup chopped celery"
        },
        {
            "3": "1  Fuji apple - washed, cored, and chopped"
        },
        {
            "4": "1 cup red grapes, halved"
        },
        {
            "5": "0.5 cup chopped pecans"
        },
        {
            "6": "0.5 cup mayonnaise"
        },
        {
            "7": "3 tablespoons Thousand Island dressing"
        },
        {
            "8": "1 tablespoon minced red onion, or to taste"
        },
        {
            "9": "0.5 teaspoon ground ginger"
        },
        {
            "10": "0.25 teaspoon curry powder"
        },
        {
            "11": "0.25 teaspoon dry mustard"
        },
        {
            "12": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine chicken, celery, apple, grapes, and pecans in a large bowl."
        },
        {
            "2": "Combine mayonnaise, Thousand Island Dressing, onion, ginger, curry powder, and dry mustard in a small bowl. Whisk together and pour dressing over salad ingredients. Mix to combine. Season with salt and pepper."
        }
    ],
