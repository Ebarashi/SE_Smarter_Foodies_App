{
    "title": "Curried Chicken Salad with Greek Yogurt",
    "Category": "Chicken Salad",
    "Ingredients": [
        {
            "1": "1 pound cooked chicken breast, chopped"
        },
        {
            "2": "2 stalks celery, chopped"
        },
        {
            "3": "0.5 cup sliced almonds"
        },
        {
            "4": "0.75 cup Greek yogurt"
        },
        {
            "5": "2 teaspoons honey"
        },
        {
            "6": "0.5 teaspoons lemon juice"
        },
        {
            "7": "0.5 teaspoons curry powder"
        },
        {
            "8": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine chicken with celery and almonds in a bowl."
        },
        {
            "2": "Mix Greek yogurt, honey, lemon juice, curry powder, salt, and pepper together in a small bowl. Pour dressing over the chicken salad and stir to combine."
        }
    ],
