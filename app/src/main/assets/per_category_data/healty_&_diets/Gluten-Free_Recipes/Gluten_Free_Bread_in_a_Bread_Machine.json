{
    "title": "Gluten-Free Bread in a Bread Machine",
    "Category": "Gluten-Free Recipes",
    "Ingredients": [
        {
            "1": "0.5 cups warm water"
        },
        {
            "2": "2 large eggs"
        },
        {
            "3": "0.5 tablespoons vegetable oil"
        },
        {
            "4": "1 teaspoon cider vinegar"
        },
        {
            "5": "1.0 cups gluten-free all-purpose baking flour"
        },
        {
            "6": "2 teaspoons xanthan gum"
        },
        {
            "7": "1 teaspoon salt"
        },
        {
            "8": "1 tablespoon white sugar"
        },
        {
            "9": "1 tablespoon active dry yeast"
        }
    ],
    "Directions": [
        {
            "1": "Place water, eggs, oil, vinegar, flour, xanthan gum, salt, sugar, and yeast, in that order, in a bread machine."
        },
        {
            "2": "Run Basic cycle and select Light or Medium for the crust."
        },
        {
            "3": "Remove when cycle ends. Cool completely before slicing."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": " 1 hrs"
        },
        {
            "Additional Time": " 1 hrs 30 mins"
        },
        {
            "Total Time": " 2 hrs 40 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "1 loaf"
        }
    ],
