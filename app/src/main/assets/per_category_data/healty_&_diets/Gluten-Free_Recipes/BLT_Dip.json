{
    "title": "BLT Dip",
    "Category": "Gluten-Free Recipes",
    "Ingredients": [
        {
            "1": "1 pound bacon"
        },
        {
            "2": "1 cup mayonnaise"
        },
        {
            "3": "1 cup sour cream"
        },
        {
            "4": "1  tomato - peeled, seeded and diced"
        }
    ],
    "Directions": [
        {
            "1": "Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until evenly browned, about 10 minutes. Drain bacon slices on paper towels."
        },
        {
            "2": "Combine mayonnaise and sour cream in a medium bowl. Crumble bacon into mayonnaise mixture; mix until combined. Stir in tomatoes right before serving."
        }
    ],
