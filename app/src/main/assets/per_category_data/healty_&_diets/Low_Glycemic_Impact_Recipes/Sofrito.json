{
    "title": "Sofrito",
    "Category": "Low Glycemic Impact Recipes",
    "Ingredients": [
        {
            "1": "10  ajies dulces peppers, tops removed"
        },
        {
            "2": "4  onions, cut into large chunks"
        },
        {
            "3": "3 medium tomatoes, chopped"
        },
        {
            "4": "2  green bell peppers, seeded and chopped"
        },
        {
            "5": "1  red bell peppers, seeded and chopped"
        },
        {
            "6": "3 medium heads garlic, peeled"
        },
        {
            "7": "25  cilantro stems (with leaves)"
        },
        {
            "8": "25  recao stems (with leaves)"
        },
        {
            "9": "1 tablespoon salt"
        },
        {
            "10": "1 tablespoon black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Combine ajies dulces peppers, onions, tomatoes, green peppers, red peppers, and garlic in a food processor; pulse a few times. Add cilantro stems, recao stems, salt, and pepper. Process until a semi-chunky salsa (not watery) consistency is reached."
        },
        {
            "2": "Store in a resealable plastic freezer bag. Use as needed or freeze in portions."
        }
    ],
