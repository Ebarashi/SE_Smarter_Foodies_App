{
    "title": "Pasta Fagioli",
    "Category": "Low Glycemic Impact Recipes",
    "Ingredients": [
        {
            "1": "1 tablespoon olive oil"
        },
        {
            "2": "1 medium onion, chopped"
        },
        {
            "3": "2 stalks celery, chopped"
        },
        {
            "4": "3 cloves garlic, minced"
        },
        {
            "5": "2 teaspoons dried parsley"
        },
        {
            "6": "1 teaspoon Italian seasoning"
        },
        {
            "7": "0.25 teaspoon crushed red pepper flakes"
        },
        {
            "8": "1 salt to taste"
        },
        {
            "9": "1 (14.5 ounce) can chicken broth"
        },
        {
            "10": "2 medium tomatoes, peeled and chopped"
        },
        {
            "11": "1 (8 ounce) can tomato sauce"
        },
        {
            "12": "0.5 cup uncooked spinach pasta"
        },
        {
            "13": "1 (15 ounce) can cannellini beans, with liquid"
        }
    ],
    "Directions": [
        {
            "1": "Heat olive oil in a large saucepan over medium heat. Add onion, celery, garlic, parsley, Italian seasoning, pepper flakes, and salt; cook and stir until onion is translucent, about 5 minutes. Stir in chicken broth, tomato sauce, and tomatoes and simmer on low for 15 to 20 minutes."
        },
        {
            "2": "Add pasta and cook until tender, about 10 minutes."
        },
        {
            "3": "Stir in undrained beans and cook until heated through, 3 to 4 minutes."
        }
    ],
