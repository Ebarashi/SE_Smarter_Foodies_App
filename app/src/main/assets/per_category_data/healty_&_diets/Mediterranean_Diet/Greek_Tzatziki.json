{
    "title": "Greek Tzatziki",
    "Category": "Mediterranean Diet",
    "Ingredients": [
        {
            "1": "1 (32 ounce) container plain low-fat yogurt"
        },
        {
            "2": "0.5  English cucumber with peel, grated"
        },
        {
            "3": "2 tablespoons fresh lemon juice"
        },
        {
            "4": "2 tablespoons extra-virgin olive oil"
        },
        {
            "5": "1 clove garlic, pressed"
        },
        {
            "6": "3 tablespoons chopped fresh dill"
        },
        {
            "7": "1 tablespoon salt, or to taste"
        },
        {
            "8": "1 tablespoon freshly ground black pepper, or to taste"
        },
        {
            "9": "2 teaspoons grated lemon zest"
        }
    ],
    "Directions": [
        {
            "1": "Combine yogurt, grated cucumber, lemon juice, olive oil, and garlic together in a large bowl. Add dill, salt, pepper, and lemon zest; mix until smooth."
        },
        {
            "2": "Pour into a serving dish, cover tightly, and refrigerate 8 hours before serving."
        }
    ],
    "Details": [
