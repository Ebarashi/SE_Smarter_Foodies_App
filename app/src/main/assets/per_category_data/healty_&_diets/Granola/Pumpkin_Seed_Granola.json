{
    "title": "Pumpkin Seed Granola",
    "Category": "Granola",
    "Ingredients": [
        {
            "1": "1 cup raw pumpkin seeds"
        },
        {
            "2": "0.5 cups rolled oats"
        },
        {
            "3": "0.5 cup pumpkin puree"
        },
        {
            "4": "0.5 cup sugar-free maple-flavored syrup"
        },
        {
            "5": "0.25 cup shredded coconut"
        },
        {
            "6": "2 tablespoons unsalted butter, melted"
        },
        {
            "7": "0.5 teaspoon pumpkin pie spice"
        },
        {
            "8": "0.5 teaspoon ground cinnamon"
        },
        {
            "9": "0.25 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 325 degrees F (165 degrees C). Line a baking sheet with parchment paper."
        },
        {
            "2": "Mix pumpkin seeds, oats, pumpkin puree, maple-flavored syrup, coconut, butter, pumpkin pie spice, cinnamon, and salt together in a bowl until well combined. Spread mixture evenly over the prepared baking sheet."
        },
        {
            "3": "Bake in the preheated oven, stirring every 15 minutes, until desired crispness is reached, 30 to 45 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "30 mins"
        },
