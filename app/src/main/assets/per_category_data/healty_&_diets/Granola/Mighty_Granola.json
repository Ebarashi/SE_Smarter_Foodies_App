{
    "title": "Mighty Granola",
    "Category": "Granola",
    "Ingredients": [
        {
            "1": "8 cups rolled oats"
        },
        {
            "2": "1 cup amaranth"
        },
        {
            "3": "1 cup sunflower seeds"
        },
        {
            "4": "1 cup finely chopped pecans"
        },
        {
            "5": "1 cup finely chopped walnuts"
        },
        {
            "6": "1 cup sliced almonds"
        },
        {
            "7": "1 cup flaxseed meal"
        },
        {
            "8": "1 cup ground chia seeds"
        },
        {
            "9": "1 cup honey"
        },
        {
            "10": "1 cup maple syrup"
        },
        {
            "11": "1 cup coconut oil"
        },
        {
            "12": "0.5 cup brown sugar"
        },
        {
            "13": "0.25 cup butter"
        },
        {
            "14": "2 tablespoons vanilla extract"
        },
        {
            "15": "2 tablespoons ground cinnamon"
        },
        {
            "16": "2 teaspoons salt"
        },
        {
            "17": "2 cups raisins"
        },
        {
            "18": "1 cup dried cherries"
        },
        {
            "19": "0.5 cup dried blueberries"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 325 degrees F (165 degrees C). Line two baking sheets with parchment paper."
        },
        {
            "2": "Combine oats, amaranth, sunflower seeds, pecans, walnuts, almonds, flaxseed meal, and ground chia seeds in a very large bowl; stir until thoroughly mixed."
        },
        {
            "3": "Mix honey, maple syrup, coconut oil, brown sugar, butter, vanilla extract, cinnamon, and salt together in a large saucepan over medium heat. Bring to a boil, stirring constantly. Cook and stir, about 1 minute. Remove maple syrup mixture from heat."
        },
        {
            "4": "Pour maple syrup mixture over oat mixture in the bowl; stir until oat mixture is moist. Spread evenly on the baking sheets."
        },
        {
            "5": "Bake in the preheated oven until toasted, stirring mixture halfway through, about 20 minutes. Remove from oven and let cool, about 5 minutes. Transfer to a very large bowl. Add raisins, cherries, and blueberries; stir until thoroughly mixed."
        }
    ],
