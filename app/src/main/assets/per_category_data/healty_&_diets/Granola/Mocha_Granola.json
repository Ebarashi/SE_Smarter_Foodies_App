{
    "title": "Mocha Granola",
    "Category": "Granola",
    "Ingredients": [
        {
            "1": "2 cups rolled oats"
        },
        {
            "2": "0.5 cup coconut oil, melted"
        },
        {
            "3": "0.5 cup chopped hazelnuts (Optional)"
        },
        {
            "4": "0.25 cup cocoa nibs"
        },
        {
            "5": "3 tablespoons maple syrup"
        },
        {
            "6": "2 tablespoons fresh ground coffee beans"
        },
        {
            "7": "2 tablespoons dark brown sugar"
        },
        {
            "8": "0.5 tablespoons unsweetened cocoa powder"
        },
        {
            "9": "2 teaspoons instant coffee granules"
        },
        {
            "10": "2 teaspoons coffee-flavored extract"
        },
        {
            "11": "0.5 teaspoons kosher salt"
        },
        {
            "12": "1 teaspoon ground cinnamon"
        },
        {
            "13": "0.5 teaspoon ground black pepper"
        },
        {
            "14": "0.5 teaspoon vanilla extract"
        },
        {
            "15": "0.25 cup miniature chocolate chips"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C). Line a baking sheet with parchment paper."
        },
        {
            "2": "Mix oats, coconut oil, hazelnuts, cocoa nibs, maple syrup, ground coffee beans, brown sugar, cocoa powder, instant coffee granules, coffee-flavored extract, kosher salt, cinnamon, black pepper, and vanilla extract together in a bowl; spread onto the prepared baking sheet, smoothing with a spatula."
        },
        {
            "3": "Bake in the preheated oven for 10 minutes; stir and continue cooking until lightly browned and beginning to crisp, about 10 minutes more. Cool granola completely."
        },
        {
            "4": "Mix chocolate chips into cooled granola and store in an air-tight container."
        }
    ],
