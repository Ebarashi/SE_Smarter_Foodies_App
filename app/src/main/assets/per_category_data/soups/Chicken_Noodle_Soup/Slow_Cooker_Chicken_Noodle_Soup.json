{
    "title": "Slow Cooker Chicken Noodle Soup",
    "Category": "Chicken Noodle Soup",
    "Ingredients": [
        {
            "1": "2 pounds skinless, boneless chicken breast halves"
        },
        {
            "2": "8 cups water"
        },
        {
            "3": "2 stalks celery, chopped"
        },
        {
            "4": "1  carrot, chopped"
        },
        {
            "5": "1  green onion, chopped"
        },
        {
            "6": "4 cubes  chicken bouillon"
        },
        {
            "7": "4 teaspoons dried parsley"
        },
        {
            "8": "1  bay leaf"
        },
        {
            "9": "1 teaspoon seasoned salt"
        },
        {
            "10": "1 teaspoon salt, or to taste"
        },
        {
            "11": "0.5 teaspoon dried basil"
        },
        {
            "12": "0.25 teaspoon ground black pepper"
        },
        {
            "13": "6 ounces egg noodles"
        }
    ],
    "Directions": [
        {
            "1": "Place chicken, water, celery, carrot, green onion, chicken bouillon, parsley, bay leaf, seasoned salt, salt, dried basil, and pepper into the bottom of a slow cooker. Cover and cook on Low until chicken breasts are no longer pink in the centers, 5 to 6 hours."
        },
        {
            "2": "Remove and discard bay leaf. Remove chicken, shred using 2 forks, and return to the pot. Stir in egg noodles."
        },
        {
            "3": "Cover and continue to cook on Low until noodles are tender, 20 to 30 minutes more."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": " 5 hrs 45 mins"
        },
        {
            "Total Time": " 6 hrs"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "8 servings"
        }
    ],
