{
    "title": "Chicken Gumbo Soup",
    "Category": "Chicken Noodle Soup",
    "Ingredients": [
        {
            "1": "8 cups water"
        },
        {
            "2": "1 teaspoon garlic powder"
        },
        {
            "3": "1 tablespoon hot pepper sauce"
        },
        {
            "4": "2  carrots, sliced thin"
        },
        {
            "5": "4 ounces fresh mushrooms"
        },
        {
            "6": "1 (10 ounce) package frozen okra, thawed and sliced"
        },
        {
            "7": "0.25 cup uncooked wild rice"
        },
        {
            "8": "1  skinless, boneless chicken breast half - cut into cubes"
        },
        {
            "9": "0.5 cups uncooked rotini pasta"
        },
        {
            "10": "1 salt and ground black pepper to taste"
        },
        {
            "11": "3  green onions, thinly sliced"
        }
    ],
    "Directions": [
        {
            "1": "Bring water to a boil. Season with garlic powder and hot pepper sauce. Add in carrots and mushrooms. Cook for 5 minutes."
        },
        {
            "2": "Add okra, wild rice, and chicken. Reduce the heat to low, and cook for 3 hours."
        },
        {
            "3": "Add pasta and cook for 10 minutes. Season with salt and pepper. Serve hot, garnished with green onions."
        }
    ],
