{
    "title": "Spicy Chicken Noodle Soup",
    "Category": "Chicken Noodle Soup",
    "Ingredients": [
        {
            "1": "2  skinless, boneless chicken breast halves, diced"
        },
        {
            "2": "2 cloves garlic, minced"
        },
        {
            "3": "6 cups vegetable broth"
        },
        {
            "4": "3  carrots, sliced"
        },
        {
            "5": "0.5 cup frozen corn"
        },
        {
            "6": "1 (8 ounce) package dried thin rice noodles"
        },
        {
            "7": "1  red bell pepper, diced"
        },
        {
            "8": "1 teaspoon dried basil"
        },
        {
            "9": "0.5 teaspoon chili pepper flakes, or to taste"
        }
    ],
    "Directions": [
        {
            "1": "Cook and stir chicken in a large pot over medium heat until lightly browned, about 5 minutes; stir in garlic and cook until fragrant, about 1 minute."
        },
        {
            "2": "Stir vegetable broth, carrots, and corn into chicken mixture; bring to a boil. Stir rice noodles, red pepper, basil, and chili flakes into chicken mixture; reduce heat and simmer until noodles are tender, about 5 minutes."
        }
    ],
