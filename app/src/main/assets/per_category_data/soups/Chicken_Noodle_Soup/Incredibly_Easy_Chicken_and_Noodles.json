{
    "title": "Incredibly Easy Chicken and Noodles",
    "Category": "Chicken Noodle Soup",
    "Ingredients": [
        {
            "1": "3 (14.5 ounce) cans chicken broth"
        },
        {
            "2": "1 (26 ounce) can condensed cream of chicken soup"
        },
        {
            "3": "1 (10.75 ounce) can  condensed cream of mushroom soup"
        },
        {
            "4": "2 cups diced, cooked chicken breast meat"
        },
        {
            "5": "2 teaspoons onion powder"
        },
        {
            "6": "1 teaspoon seasoning salt"
        },
        {
            "7": "0.5 teaspoon garlic powder"
        },
        {
            "8": "2 (9 ounce) packages frozen egg noodles"
        }
    ],
    "Directions": [
        {
            "1": "Combine chicken broth, both condensed soups, and diced chicken in a large pot. Season with onion powder, seasoning salt, and garlic powder."
        },
        {
            "2": "Bring to a boil over high heat and stir in noodles. Reduce heat to low and simmer until desired consistency is reached, 20 to 30 minutes."
        }
    ],
