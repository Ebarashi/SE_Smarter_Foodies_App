{
    "title": "Super Easy Chicken Noodle Soup",
    "Category": "Chicken Noodle Soup",
    "Ingredients": [
        {
            "1": "1 pound boneless, skinless chicken meat"
        },
        {
            "2": "4 cubes  chicken bouillon"
        },
        {
            "3": "8 cups water"
        },
        {
            "4": "2 cups thin egg noodles"
        }
    ],
    "Directions": [
        {
            "1": "Measure water into a large pot, and add chicken and chicken bouillon. Boil until chicken is fully cooked."
        },
        {
            "2": "Remove chicken from the broth. Chop into small pieces, and return meat to the pot."
        },
        {
            "3": "Add egg noodles, and cook until tender."
        }
    ],
