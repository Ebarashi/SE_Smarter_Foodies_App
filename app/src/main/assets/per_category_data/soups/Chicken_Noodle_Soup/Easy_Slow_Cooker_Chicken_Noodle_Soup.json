{
    "title": "Easy Slow Cooker Chicken Noodle Soup",
    "Category": "Chicken Noodle Soup",
    "Ingredients": [
        {
            "1": "4 cups water"
        },
        {
            "2": "3  carrots, sliced"
        },
        {
            "3": "1 large onion, sliced"
        },
        {
            "4": "4 stalks celery, sliced"
        },
        {
            "5": "1 tablespoon dried parsley"
        },
        {
            "6": "2 teaspoons salt"
        },
        {
            "7": "1 teaspoon garlic salt"
        },
        {
            "8": "0.5 teaspoon freshly ground black pepper"
        },
        {
            "9": "0.5 teaspoon dried thyme"
        },
        {
            "10": "4  skinless, boneless chicken breasts"
        },
        {
            "11": "1.5 cups egg noodles"
        }
    ],
    "Directions": [
        {
            "1": "Combine water, carrots, onion, celery, parsley, salt, garlic salt, pepper, and thyme in a slow cooker and mix well. Set chicken on top and push down slightly into the liquid."
        },
        {
            "2": "Cover and cook on High for 4 hours or Low for 8 hours."
        },
        {
            "3": "Add noodles and cook on High until noodles are tender, 30 to 45 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
