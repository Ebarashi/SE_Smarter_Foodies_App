{
    "title": "Gazpacho",
    "Category": "Gazpacho",
    "Ingredients": [
        {
            "1": "4 cups tomato juice"
        },
        {
            "2": "1  onion, minced"
        },
        {
            "3": "1  green bell pepper, minced"
        },
        {
            "4": "1  cucumber, chopped"
        },
        {
            "5": "2 cups chopped tomatoes"
        },
        {
            "6": "2  green onions, chopped"
        },
        {
            "7": "1 clove garlic, minced"
        },
        {
            "8": "3 tablespoons fresh lemon juice"
        },
        {
            "9": "2 tablespoons red wine vinegar"
        },
        {
            "10": "1 teaspoon dried tarragon"
        },
        {
            "11": "1 teaspoon dried basil"
        },
        {
            "12": "0.25 cup chopped fresh parsley"
        },
        {
            "13": "1 teaspoon white sugar"
        },
        {
            "14": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "In a blender or food processor, combine tomato juice, onion, bell pepper, cucumber, tomatoes, green onions, garlic, lemon juice, red wine vinegar, tarragon, basil, parsley, sugar, salt, and pepper. Blend until well-combined but still slightly chunky. Chill at least 2 hours before serving."
        }
    ],
