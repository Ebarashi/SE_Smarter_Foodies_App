{
    "title": "Cold Gazpacho",
    "Category": "Gazpacho",
    "Ingredients": [
        {
            "1": "6 large ripe beefsteak tomatoes, chopped"
        },
        {
            "2": "1 large cucumber, chopped"
        },
        {
            "3": "1 medium green bell pepper, chopped"
        },
        {
            "4": "1 medium yellow bell pepper, chopped"
        },
        {
            "5": "1 large sweet onion, chopped"
        },
        {
            "6": "1 small jalapeno pepper, seeded and diced, or to taste"
        },
        {
            "7": "1 clove garlic, minced"
        },
        {
            "8": "4 tablespoons extra-virgin olive oil"
        },
        {
            "9": "3 tablespoons sherry vinegar"
        },
        {
            "10": "1 salt and freshly ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Set aside a few pieces of tomato, cucumber, and bell peppers for garnish."
        },
        {
            "2": "Combine remaining tomatoes, cucumber, and bell peppers with onion, jalapeno, and garlic in a food processor; process until all vegetable are finely chopped. Mix in olive oil, sherry vinegar, and salt."
        },
        {
            "3": "Cover and refrigerate until chilled, at least 1 hour."
        },
        {
            "4": "Season with pepper before serving and garnish with reserved tomatoes, cucumbers, and bell peppers."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
