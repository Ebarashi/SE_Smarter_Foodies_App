{
    "title": "Summer Watermelon Gazpacho",
    "Category": "Gazpacho",
    "Ingredients": [
        {
            "1": "4 cups cubed seeded watermelon"
        },
        {
            "2": "2  roma (plum) tomatoes, seeded and chopped"
        },
        {
            "3": "1  red bell pepper, chopped"
        },
        {
            "4": "0.5  English (seedless) cucumber - peeled, seeded, and cubed"
        },
        {
            "5": "2 tablespoons minced shallot"
        },
        {
            "6": "2 tablespoons fresh lime juice"
        },
        {
            "7": "1 tablespoon apple cider vinegar"
        },
        {
            "8": "0.5 teaspoons salt, or to taste"
        },
        {
            "9": "0.25 teaspoon freshly ground black pepper"
        },
        {
            "10": "2 tablespoons crumbled feta cheese"
        },
        {
            "11": "2 tablespoons minced fresh cilantro"
        }
    ],
    "Directions": [
        {
            "1": "Combine watermelon, tomatoes, red pepper, cucumber, shallot, lime juice, vinegar, salt, and pepper in a high-powered blender. Blend until smooth, 20 to 30 seconds. Refrigerate for at least 1 hour to allow flavors to meld and soup to chill."
        },
        {
            "2": "Stir gazpacho before serving, as it has a tendency to separate, and divide between 4 bowls. Top with feta cheese and cilantro."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Cool Time": " 1 hrs"
        },
        {
            "Total Time": " 1 hrs 20 mins"
        },
        {
            "Servings": "4 "
        }
    ],
