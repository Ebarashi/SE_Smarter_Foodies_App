{
    "title": "Spanish Gazpacho",
    "Category": "Gazpacho",
    "Ingredients": [
        {
            "1": "2 pounds tomatoes, peeled and halved"
        },
        {
            "2": "1  cucumber, peeled and sliced"
        },
        {
            "3": "1  green bell pepper, sliced"
        },
        {
            "4": "0.5  onion, sliced"
        },
        {
            "5": "0.5 tablespoons red wine vinegar"
        },
        {
            "6": "1 clove garlic"
        },
        {
            "7": "1 salt to taste"
        },
        {
            "8": "0.25 cup extra-virgin olive oil"
        },
        {
            "9": "0.25 cup cold water, or as needed"
        },
        {
            "10": "1 Garnish-"
        },
        {
            "11": "0.5  onion, chopped"
        },
        {
            "12": "1  green bell pepper, chopped"
        },
        {
            "13": "1  cucumber, chopped"
        }
    ],
    "Directions": [
        {
            "1": "Combine tomatoes, cucumber, bell pepper, onion, vinegar, garlic, and salt in the bowl of a food processor; pulse until blended. Pour olive oil in slowly, with the processor running, until gazpacho is smooth. Add small amounts of cold water as needed to achieve desired consistency."
        },
        {
            "2": "Serve gazpacho chilled with chopped onion, bell pepper, and cucumber sprinkled over each serving."
        }
    ],
