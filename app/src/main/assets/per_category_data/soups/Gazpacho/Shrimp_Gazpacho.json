{
    "title": "Shrimp Gazpacho",
    "Category": "Gazpacho",
    "Ingredients": [
        {
            "1": "64 fluid ounces tomato and clam juice cocktail"
        },
        {
            "2": "3 pounds cooked shrimp, peeled and deveined"
        },
        {
            "3": "4  avocados, peeled and chopped"
        },
        {
            "4": "2  cucumbers, cubed"
        },
        {
            "5": "3 large tomatoes, diced"
        },
        {
            "6": "1  red onion, diced"
        },
        {
            "7": "1 bunch cilantro, chopped"
        },
        {
            "8": "2 tablespoons lemon juice"
        },
        {
            "9": "0.5 teaspoon salt"
        },
        {
            "10": "0.25 teaspoon pepper"
        }
    ],
    "Directions": [
        {
            "1": "In a large bowl, combine the tomato and clam juice cocktail, shrimp, avocados, cucumbers, tomatoes, red onion, cilantro, lemon juice, salt, and pepper."
        },
        {
            "2": "Refrigerate until ready to serve."
        }
    ],
    "Details": [
