{
    "title": "German Lentil Soup",
    "Category": "Lentil Soup",
    "Ingredients": [
        {
            "1": "2 cups dried brown lentils, rinsed and drained"
        },
        {
            "2": "3 cups chicken stock"
        },
        {
            "3": "1  bay leaf"
        },
        {
            "4": "1 cup chopped carrots"
        },
        {
            "5": "1 cup chopped celery"
        },
        {
            "6": "1 cup chopped onion"
        },
        {
            "7": "1 cup cooked, cubed ham"
        },
        {
            "8": "1 teaspoon Worcestershire sauce"
        },
        {
            "9": "0.5 teaspoon garlic powder"
        },
        {
            "10": "0.25 teaspoon freshly grated nutmeg"
        },
        {
            "11": "5 drops hot pepper sauce"
        },
        {
            "12": "0.25 teaspoon caraway seed"
        },
        {
            "13": "0.5 teaspoon celery salt"
        },
        {
            "14": "1 tablespoon chopped fresh parsley"
        },
        {
            "15": "0.5 teaspoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Place lentils in a 5- to 6-quart slow cooker. Add chicken stock, bay leaf, carrots, celery, onion, and ham. Season with Worcestershire sauce, garlic powder, nutmeg, hot pepper sauce, caraway seed, celery salt, parsley, and pepper."
        },
        {
            "2": "Cover, and cook on Low for 8 to 10 hours. Remove bay leaf before serving."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
