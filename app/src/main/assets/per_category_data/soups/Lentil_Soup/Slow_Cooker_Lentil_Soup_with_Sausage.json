{
    "title": "Slow Cooker Lentil Soup with Sausage",
    "Category": "Lentil Soup",
    "Ingredients": [
        {
            "1": "2  kielbasa sausages, chopped"
        },
        {
            "2": "2  carrots, diced"
        },
        {
            "3": "2 stalks celery, chopped"
        },
        {
            "4": "1  onion, chopped"
        },
        {
            "5": "1 (14.5 ounce) can crushed tomatoes"
        },
        {
            "6": "2 cups dried lentils"
        },
        {
            "7": "2 cups water"
        },
        {
            "8": "2 cups wine"
        },
        {
            "9": "0.25 cup olive oil"
        },
        {
            "10": "2 cloves garlic, minced"
        },
        {
            "11": "1 teaspoon dried oregano"
        },
        {
            "12": "1 teaspoon dried basil"
        },
        {
            "13": "0.5 teaspoon dried thyme"
        },
        {
            "14": "1  bay leaf"
        },
        {
            "15": "1 salt and ground black pepper to taste"
        },
        {
            "16": "0.5 cup fresh spinach leaves"
        }
    ],
    "Directions": [
        {
            "1": "Combine sausages, carrots, celery, onion, tomatoes, lentils, water, wine, oil, garlic, oregano, basil, thyme, bay leaf, salt, and pepper in the bottom of a slow cooker."
        },
        {
            "2": "Cover and cook on Low until lentils are tender and sausage is no longer pink, 6 to 8 hours."
        },
        {
            "3": "When ready to serve, stir in spinach and cook until it wilts, 7 to 10 minutes. Remove bay leaf."
        }
    ],
