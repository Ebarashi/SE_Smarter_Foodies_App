{
    "title": "German Potato Bacon Soup",
    "Category": "Potato Soup",
    "Ingredients": [
        {
            "1": "0.25 cup butter"
        },
        {
            "2": "2  yellow onions, diced small"
        },
        {
            "3": "4 slices bacon, diced small"
        },
        {
            "4": "1  bay leaf"
        },
        {
            "5": "2 quarts chicken broth"
        },
        {
            "6": "4  russet potatoes, diced"
        },
        {
            "7": "2 tablespoons flour"
        },
        {
            "8": "1 teaspoon dried marjoram"
        },
        {
            "9": "1 teaspoon caraway seed"
        },
        {
            "10": "1 salt and ground black pepper to taste"
        },
        {
            "11": "3 ounces sour cream"
        }
    ],
    "Directions": [
        {
            "1": "Melt butter in a stockpot over medium heat; add onions, bacon, and bay leaf. Cook and stir until onions are nearly transparent, 5 to 7 minutes."
        },
        {
            "2": "Pour chicken broth into the pot; add potatoes, flour, marjoram, caraway seed, salt, and pepper. Bring to a boil, reduce heat to medium-low, and simmer until potatoes are tender, about 20 minutes."
        },
        {
            "3": "Stir sour cream into soup; cook another 1 to 2 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Cook Time": "30 mins"
