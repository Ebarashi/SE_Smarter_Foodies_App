{
    "title": "Golden Potato Soup",
    "Category": "Potato Soup",
    "Ingredients": [
        {
            "1": "3 cups peeled and cubed potatoes"
        },
        {
            "2": "0.5 cup chopped celery"
        },
        {
            "3": "0.5 cup chopped onion"
        },
        {
            "4": "1 cube chicken bouillon"
        },
        {
            "5": "1 cup water"
        },
        {
            "6": "1 teaspoon dried parsley"
        },
        {
            "7": "0.5 teaspoon salt"
        },
        {
            "8": "1 pinch ground black pepper"
        },
        {
            "9": "2 teaspoons all-purpose flour"
        },
        {
            "10": "0.5 cups milk"
        },
        {
            "11": "0.5 cups shredded American cheese"
        },
        {
            "12": "1 cup chopped ham"
        }
    ],
    "Directions": [
        {
            "1": "In a large stock pot add potatoes, celery, onion, chicken bouillon, water and parsley flakes. Season with salt and pepper and simmer until vegetables become tender."
        },
        {
            "2": "In a separate bowl mix flour and milk. Once it is well blended, add to soup mixture and cook until soup becomes thick."
        },
        {
            "3": "Stir in cheese, cooked ham or hamburger and simmer until cheese is melted."
        }
    ],
