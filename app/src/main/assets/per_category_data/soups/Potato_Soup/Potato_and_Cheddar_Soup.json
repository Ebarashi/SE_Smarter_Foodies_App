{
    "title": "Potato and Cheddar Soup",
    "Category": "Potato Soup",
    "Ingredients": [
        {
            "1": "2 cups water"
        },
        {
            "2": "2 cups peeled and cubed red potatoes"
        },
        {
            "3": "3 tablespoons melted butter"
        },
        {
            "4": "1 small onion, chopped"
        },
        {
            "5": "3 tablespoons all-purpose flour"
        },
        {
            "6": "1 salt and pepper to taste"
        },
        {
            "7": "3 cups milk"
        },
        {
            "8": "0.5 teaspoon white sugar"
        },
        {
            "9": "1 cup shredded Cheddar cheese"
        },
        {
            "10": "1 cup diced ham"
        }
    ],
    "Directions": [
        {
            "1": "Using a medium sized stock pot bring water to a boil, add potatoes and cook until tender. Drain reserving 1 cup liquid."
        },
        {
            "2": "Stir in butter, onion and flour. Season with salt and pepper. Gradually stir in potatoes, reserved liquid, milk, sugar, cheese, and ham. Simmer for 30 minutes, stirring frequently."
        }
    ],
    "Details": [
        {
            "Servings": "10 "
        },
        {
            "Yield": "10 servings"
        }
    ],
