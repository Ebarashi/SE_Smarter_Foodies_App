{
    "title": "Slow Cooker 5-Mushroom Barley Soup",
    "Category": "Mushroom Soup",
    "Ingredients": [
        {
            "1": "3 cups water"
        },
        {
            "2": "1 cup barley"
        },
        {
            "3": "4 cups beef broth"
        },
        {
            "4": "1 cup milk"
        },
        {
            "5": "2 tablespoons olive oil"
        },
        {
            "6": "1 cup diced onion"
        },
        {
            "7": "0.5 cup diced celery"
        },
        {
            "8": "0.5 cup diced carrot"
        },
        {
            "9": "1 tablespoon finely chopped garlic"
        },
        {
            "10": "1 (6 ounce) package sliced white mushrooms"
        },
        {
            "11": "0.5 cup chopped brown beech mushrooms"
        },
        {
            "12": "0.5 cup chopped oyster mushrooms"
        },
        {
            "13": "0.25 cup dried shiitake mushrooms"
        },
        {
            "14": "0.25 cup dried black mushrooms, broken into small pieces"
        },
        {
            "15": "1 (10.75 ounce) can  condensed cream of mushroom soup (Optional)"
        },
        {
            "16": "0.5 teaspoon salt"
        },
        {
            "17": "0.5 teaspoon ground mixed peppercorns"
        }
    ],
    "Directions": [
        {
            "1": "Heat water in a slow cooker set to High until hot; add barley, beef broth, milk, olive oil, onion, celery, carrot, garlic, white mushrooms, brown beech mushrooms, oyster mushrooms, shiitake mushrooms, black mushrooms, cream of mushroom soup, salt, and ground mixed peppercorns to the slow cooker."
        },
        {
            "2": "Cover and cook on High for 1 hour."
        },
        {
            "3": "Remove shiitake mushrooms with a slotted spoon and chop into 1/2-inch pieces. Reduce heat setting to Low and continue cooking 7 hours more."
        }
    ],
