{
    "title": "Kelly's Slow Cooker Beef, Mushroom, and Barley Soup",
    "Category": "Mushroom Soup",
    "Ingredients": [
        {
            "1": "1 (32 ounce) carton beef stock"
        },
        {
            "2": "1 (8 ounce) can tomato sauce"
        },
        {
            "3": "1 cup water"
        },
        {
            "4": "0.5  onion, diced"
        },
        {
            "5": "0.75 cup diced carrots"
        },
        {
            "6": "1 cup barley"
        },
        {
            "7": "1 (6 ounce) package sliced fresh mushrooms"
        },
        {
            "8": "4 cloves garlic, minced"
        },
        {
            "9": "2 pounds beef sirloin, cut into chunks"
        },
        {
            "10": "1 pinch garlic salt, or to taste"
        },
        {
            "11": "1 salt and ground black pepper to taste"
        },
        {
            "12": "2  bay leaves"
        }
    ],
    "Directions": [
        {
            "1": "Stir beef stock, tomato sauce, water, onion, carrot, barley, mushrooms, and garlic together in a slow cooker."
        },
        {
            "2": "Season beef chunks with garlic salt, salt, and black pepper; add to the beef stock mixture. Add bay leaves to the slow cooker."
        },
        {
            "3": "Cook on Low until the soup thickens and the beef is tender, about 6 hours."
        },
        {
            "4": "Remove and discard the bay leaves to serve."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Cook Time": " 6 hrs"
        },
        {
            "Total Time": " 6 hrs 20 mins"
        },
        {
            "Servings": "6 "
        },
        {
            "Yield": "6 servings"
