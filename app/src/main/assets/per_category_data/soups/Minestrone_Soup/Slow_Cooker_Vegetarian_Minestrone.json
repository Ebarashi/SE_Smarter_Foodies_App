{
    "title": "Slow Cooker Vegetarian Minestrone",
    "Category": "Minestrone Soup",
    "Ingredients": [
        {
            "1": "6 cups vegetable broth"
        },
        {
            "2": "1 (28 ounce) can crushed tomatoes"
        },
        {
            "3": "1 (15 ounce) can kidney beans, drained"
        },
        {
            "4": "1 large onion, chopped"
        },
        {
            "5": "2  ribs celery, diced"
        },
        {
            "6": "2 large carrots, diced"
        },
        {
            "7": "1 cup green beans"
        },
        {
            "8": "1 small zucchini"
        },
        {
            "9": "3 cloves garlic, minced"
        },
        {
            "10": "1 tablespoon minced fresh parsley"
        },
        {
            "11": "0.5 teaspoons dried oregano"
        },
        {
            "12": "1 teaspoon salt"
        },
        {
            "13": "0.75 teaspoon dried thyme"
        },
        {
            "14": "0.25 teaspoon freshly ground black pepper"
        },
        {
            "15": "0.5 cup elbow macaroni"
        },
        {
            "16": "4 cups chopped fresh spinach"
        },
        {
            "17": "0.25 cup finely grated Parmesan cheese, or more to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine vegetable broth, tomatoes, kidney beans, onion, celery, carrots, green beans, zucchini, garlic, parsley, oregano, salt, thyme, and black pepper in a 6-quart slow cooker."
        },
        {
            "2": "Cook on Low for 6 to 8 hours."
        },
        {
            "3": "Bring a large pot of lightly salted water to a boil. Cook elbow macaroni in the boiling water, stirring occasionally until cooked through but firm to the bite, 8 minutes; drain."
        },
        {
            "4": "Stir spinach and macaroni into minestrone; cook another 15 minutes. Top with Parmesan cheese."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Cook Time": " 6 hrs 15 mins"
        },
        {
            "Total Time": " 6 hrs 35 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "8 servings"
        }
    ],
