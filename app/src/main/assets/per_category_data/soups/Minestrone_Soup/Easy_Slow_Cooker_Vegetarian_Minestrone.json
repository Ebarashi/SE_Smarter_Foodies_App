{
    "title": "Easy Slow Cooker Vegetarian Minestrone",
    "Category": "Minestrone Soup",
    "Ingredients": [
        {
            "1": "1 tablespoon olive oil"
        },
        {
            "2": "1 medium zucchini, diced"
        },
        {
            "3": "1 medium onion, minced"
        },
        {
            "4": "1 large carrot, chopped"
        },
        {
            "5": "1 stalk celery, chopped"
        },
        {
            "6": "2 cloves garlic, minced"
        },
        {
            "7": "1 salt and ground black pepper to taste"
        },
        {
            "8": "6 cups vegetable stock"
        },
        {
            "9": "1 (15.5 ounce) can chickpeas, rinsed and drained"
        },
        {
            "10": "1 (14.5 ounce) can undrained diced tomatoes"
        },
        {
            "11": "0.25 pound fresh green beans, trimmed"
        },
        {
            "12": "0.5 cup uncooked ditalini pasta"
        },
        {
            "13": "0.25 cup pesto"
        }
    ],
    "Directions": [
        {
            "1": "Heat oil in the bottom of a 4- or 6-quart slow cooker on High. Add zucchini, onion, carrot, celery, garlic, salt, and pepper; stir to combine."
        },
        {
            "2": "Pour in vegetable stock, chickpeas, undrained tomatoes, and green beans. Cover and cook on Low for 6 to 7 hours."
        },
        {
            "3": "Add ditalini and cook for 1 more hour. Stir in pesto and serve."
        }
    ],
    "Details": [
        {
            "Prep Time": "25 mins"
        },
        {
            "Cook Time": " 7 hrs"
        },
        {
            "Total Time": " 7 hrs 25 mins"
        },
