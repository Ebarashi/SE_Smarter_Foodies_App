{
    "title": "Caraway Rye Bread (for the bread machine)",
    "Category": "Wheat Bread",
    "Ingredients": [
        {
            "1": "0.25 cups lukewarm water (100 degrees F/38 degrees C)"
        },
        {
            "2": "2 tablespoons dry milk powder"
        },
        {
            "3": "1 teaspoon salt"
        },
        {
            "4": "2 tablespoons brown sugar"
        },
        {
            "5": "2 tablespoons molasses"
        },
        {
            "6": "2 tablespoons butter"
        },
        {
            "7": "0.75 cup whole wheat flour"
        },
        {
            "8": "0.75 cups bread flour"
        },
        {
            "9": "0.75 cup rye flour"
        },
        {
            "10": "0.5 tablespoons caraway seeds"
        },
        {
            "11": "0.75 teaspoons active dry yeast"
        }
    ],
    "Directions": [
        {
            "1": "Put lukewarm water, milk powder, salt, brown sugar, molasses, butter, whole wheat flour, bread flour, rye flour, caraway seeds, and yeast into the pan of a bread machine in the order suggested by the manufacturer. Select the Grain setting and 2-pound loaf size."
        }
    ],
