{
    "title": "Banana-Pineapple Bread",
    "Category": "Banana Bread",
    "Ingredients": [
        {
            "1": "3 cups all-purpose flour"
        },
        {
            "2": "2 cups white sugar"
        },
        {
            "3": "2 cups mashed bananas"
        },
        {
            "4": "1 cup vegetable oil"
        },
        {
            "5": "0.5 cup unsweetened applesauce"
        },
        {
            "6": "1 (8 ounce) can crushed pineapple"
        },
        {
            "7": "3 large eggs"
        },
        {
            "8": "1 teaspoon ground cinnamon"
        },
        {
            "9": "1 teaspoon baking soda"
        },
        {
            "10": "1 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 325 degrees F (165 degrees C). Grease and flour 2 loaf pans."
        },
        {
            "2": "Mix flour, sugar, bananas, oil, applesauce, pineapple, eggs, cinnamon, baking soda, and salt together in a large bowl until blended. Pour into the prepared loaf pans."
        },
        {
            "3": "Bake in the preheated oven until a toothpick inserted in the center comes out clean, 50 to 60 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "50 mins"
        },
        {
            "Total Time": " 1 hrs"
        },
        {
            "Servings": "24 "
        },
        {
            "Yield": "2 loaves"
        }
