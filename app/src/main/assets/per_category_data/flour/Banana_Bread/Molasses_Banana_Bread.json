{
    "title": "Molasses Banana Bread",
    "Category": "Banana Bread",
    "Ingredients": [
        {
            "1": "0.5 cup butter, softened"
        },
        {
            "2": "2 cups mashed banana"
        },
        {
            "3": "1 cup molasses"
        },
        {
            "4": "1 large egg"
        },
        {
            "5": "1 cup whole wheat flour"
        },
        {
            "6": "0.75 cup all-purpose flour"
        },
        {
            "7": "0.5 teaspoon baking soda"
        },
        {
            "8": "0.5 teaspoon salt"
        },
        {
            "9": "0.5 teaspoon ground nutmeg"
        },
        {
            "10": "0.5 cup chopped walnuts (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C). Lightly grease a 9x5-inch loaf pan."
        },
        {
            "2": "Cream butter in a large bowl. Beat in banana, molasses, and egg. Mix in both flours, baking soda, salt, and nutmeg. Stir in walnuts until well blended. Pour batter into the prepared pan."
        },
        {
            "3": "Bake in the preheated oven until a toothpick inserted into the center of the loaf comes out clean, 55 to 65 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "55 mins"
        },
        {
            "Total Time": " 1 hrs 10 mins"
        },
        {
            "Servings": "10 "
        },
        {
            "Yield": "1 9x5-inch loaf"
        }
    ],
