{
    "title": "Ezekiel Bread",
    "Category": "Yeast Bread",
    "Ingredients": [
        {
            "1": "4 cups warm water (110 degrees F/45 degrees C)"
        },
        {
            "2": "1 cup honey"
        },
        {
            "3": "0.5 cup olive oil"
        },
        {
            "4": "2 (.25 ounce) packages active dry yeast"
        },
        {
            "5": "1.0 cups wheat berries"
        },
        {
            "6": "0.5 cups spelt flour"
        },
        {
            "7": "0.5 cup barley"
        },
        {
            "8": "0.5 cup millet"
        },
        {
            "9": "0.25 cup dry green lentils"
        },
        {
            "10": "2 tablespoons dry great Northern beans"
        },
        {
            "11": "2 tablespoons dry kidney beans"
        },
        {
            "12": "2 tablespoons dried pinto beans"
        },
        {
            "13": "2 tablespoons salt"
        }
    ],
    "Directions": [
        {
            "1": "Mix water, honey, olive oil, and yeast in a large bowl. Let sit for 3 to 5 minutes."
        },
        {
            "2": "While yeast mixture sits, stir together wheat berries, spelt flour, barley, millet, lentils, great northern beans, kidney beans, and pinto beans together until well mixed. Grind in a flour mill."
        },
        {
            "3": "Add fresh milled flour-bean mixture and salt to yeast mixture; stir until well mixed, about 10 minutes. The dough will be loose like a batter bread."
        },
        {
            "4": "Pour dough into two greased 9x5-inch loaf pans."
        },
        {
            "5": "Let rise in a warm place until dough has reached the top of the pan, about 1 hour."
        },
        {
            "6": "Meanwhile, preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "7": "Bake in the preheated oven until loaves are golden brown, 45 to 50 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Cook Time": "45 mins"
        },
        {
            "Additional Time": " 1 hrs"
        },
        {
            "Total Time": " 2 hrs 5 mins"
        },
        {
            "Servings": "24 "
        },
        {
            "Yield": "2 loaves"
        }
    ],
