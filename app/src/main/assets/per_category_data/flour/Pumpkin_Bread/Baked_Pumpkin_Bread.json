{
    "title": "Baked Pumpkin Bread",
    "Category": "Pumpkin Bread",
    "Ingredients": [
        {
            "1": "0.5 cups all-purpose flour"
        },
        {
            "2": "0.25 teaspoons baking soda"
        },
        {
            "3": "1 teaspoon salt"
        },
        {
            "4": "1 teaspoon ground cinnamon"
        },
        {
            "5": "0.5 teaspoon ground nutmeg"
        },
        {
            "6": "1 cup solid pack pumpkin puree"
        },
        {
            "7": "1 cup packed brown sugar"
        },
        {
            "8": "0.5 cup buttermilk"
        },
        {
            "9": "1  egg"
        },
        {
            "10": "2 tablespoons butter, softened"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Sift the flour, baking soda, salt, cinnamon and nutmeg into a large bowl. Mix in the pumpkin, brown sugar, buttermilk, egg and butter until well blended. Pour into a 9x5 inch loaf pan and smooth the top."
        },
        {
            "3": "Bake for 1 hour in the preheated oven, or until a toothpick inserted into the center comes out clean."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": " 1 hrs"
        },
        {
            "Total Time": " 1 hrs 10 mins"
        },
        {
            "Servings": "12 "
        },
        {
            "Yield": "1 - 9x5 inch loaf"
        }
    ],
