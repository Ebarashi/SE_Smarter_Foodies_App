{
    "title": "Pumpkin-Walnut Bread",
    "Category": "Pumpkin Bread",
    "Ingredients": [
        {
            "1": "0.5 cups unsifted all-purpose flour"
        },
        {
            "2": "0.5 cups white sugar"
        },
        {
            "3": "1 teaspoon baking soda"
        },
        {
            "4": "0.75 teaspoon salt"
        },
        {
            "5": "0.5 teaspoon ground cinnamon"
        },
        {
            "6": "0.5 teaspoon ground nutmeg"
        },
        {
            "7": "0.5 teaspoon ground cloves"
        },
        {
            "8": "0.25 teaspoon baking powder"
        },
        {
            "9": "0.25 teaspoon ground ginger (Optional)"
        },
        {
            "10": "1 cup pumpkin puree"
        },
        {
            "11": "0.5 cup vegetable oil"
        },
        {
            "12": "2  eggs, beaten"
        },
        {
            "13": "1 cup chopped walnuts"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C). Grease a 9x5-inch loaf pan."
        },
        {
            "2": "Combine flour, sugar, baking soda, salt, cinnamon, nutmeg, cloves, baking powder, and ginger in a large bowl. Make a well in the center; add pumpkin, oil, and eggs. Stir well. Fold in walnuts."
        },
        {
            "3": "Pour batter into the prepared loaf pan."
        },
        {
            "4": "Bake in the preheated oven until a toothpick inserted into the center comes out clean, about 90 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": " 1 hrs 30 mins"
        },
        {
            "Total Time": " 1 hrs 45 mins"
        },
        {
            "Servings": "12 "
        },
        {
            "Yield": "1 loaf"
        }
