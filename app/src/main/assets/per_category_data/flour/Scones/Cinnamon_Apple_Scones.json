{
    "title": "Cinnamon-Apple Scones",
    "Category": "Scones",
    "Ingredients": [
        {
            "1": "0.75 cups all-purpose flour"
        },
        {
            "2": "1 teaspoon baking powder"
        },
        {
            "3": "0.5 teaspoon baking soda"
        },
        {
            "4": "0.5 teaspoon ground cinnamon"
        },
        {
            "5": "1 pinch salt"
        },
        {
            "6": "0.25 cup cold unsalted butter, cut into pieces"
        },
        {
            "7": "0.5 medium Granny Smith apples - peeled, cored, and diced"
        },
        {
            "8": "7 tablespoons brown sugar, divided"
        },
        {
            "9": "0.5 cup whole milk"
        },
        {
            "10": "1 tablespoon whole milk"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 300 degrees F (150 degrees C). Grease a baking sheet."
        },
        {
            "2": "Sift flour, baking powder, baking soda, cinnamon, and salt into a mixing bowl and rub in butter. Add apples and 6 tablespoons brown sugar. Add 1/2 cup milk and work into a dough."
        },
        {
            "3": "Press into a 6- to 7-inch round on the prepared baking sheet. Brush with 1 tablespoon milk and sprinkle with remaining 1 tablespoon brown sugar. Score into 8 segments without cutting all the way through."
        },
        {
            "4": "Bake in the preheated oven until golden, about 1 hour."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": " 1 hrs"
        },
        {
            "Total Time": " 1 hrs 15 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "8 scones"
        }
    ],
