{
    "title": "Healthy Greek Chicken Panini",
    "Category": "Panini",
    "Ingredients": [
        {
            "1": "1 (10 inch) multigrain flour tortillas"
        },
        {
            "2": "1 tablespoon hummus"
        },
        {
            "3": "1 (4 ounce) grilled chicken breast, sliced"
        },
        {
            "4": "2 medium roasted red peppers, drained"
        },
        {
            "5": "2 tablespoons crumbled reduced-fat feta cheese"
        },
        {
            "6": "1 cup baby spinach leaves, or to taste"
        },
        {
            "7": "1 cooking spray"
        }
    ],
    "Directions": [
        {
            "1": "Heat a panini maker."
        },
        {
            "2": "Lay tortilla on a work surface and spread with hummus. Top with sliced chicken, red peppers, feta cheese, and then spinach. Fold tortilla in half and spray with cooking spray."
        },
        {
            "3": "Grill in the preheated panini maker until outside is golden brown, 6 to 8 minutes."
        }
    ],
