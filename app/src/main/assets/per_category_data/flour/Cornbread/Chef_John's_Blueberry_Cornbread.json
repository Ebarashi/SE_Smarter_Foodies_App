{
    "title": "Chef John's Blueberry Cornbread",
    "Category": "Cornbread",
    "Ingredients": [
        {
            "1": "2 large eggs"
        },
        {
            "2": "1 cup white sugar"
        },
        {
            "3": "1 cup milk"
        },
        {
            "4": "1 cup buttermilk"
        },
        {
            "5": "12 tablespoons unsalted butter, melted"
        },
        {
            "6": "2 cups all-purpose flour"
        },
        {
            "7": "2 cups cornmeal"
        },
        {
            "8": "4 teaspoons baking powder"
        },
        {
            "9": "2 teaspoons fine salt"
        },
        {
            "10": "1 teaspoon baking soda"
        },
        {
            "11": "4 cups fresh blueberries"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 400 degrees F (200 degrees C). Generously butter a 9x13-inch baking dish."
        },
        {
            "2": "Whisk eggs and white sugar together in a bowl vigorously for 1 minute; add milk, buttermilk, and melted butter and mix together."
        },
        {
            "3": "Mix flour, cornmeal, baking powder, salt, and baking soda together in another bowl. Stir into the wet ingredients. Fold in blueberries."
        },
        {
            "4": "Transfer batter to the prepared baking dish."
        },
        {
            "5": "Bake in the center of the preheated oven until a skewer inserted into the center comes out clean, 50 to 60 minutes. Let cool before cutting and serving."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "50 mins"
        },
