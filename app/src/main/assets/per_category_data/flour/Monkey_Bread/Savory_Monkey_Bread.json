{
    "title": "Savory Monkey Bread",
    "Category": "Monkey Bread",
    "Ingredients": [
        {
            "1": "1 cup butter, melted"
        },
        {
            "2": "0.25 cup chopped fresh parsley"
        },
        {
            "3": "2 tablespoons finely chopped onion"
        },
        {
            "4": "1 tablespoon chopped fresh dill"
        },
        {
            "5": "2 (7.5 ounce) cans refrigerated buttermilk biscuit dough (such as Pillsbury\u00ae)"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 400 degrees F (200 degrees C)."
        },
        {
            "2": "Stir melted butter, parsley, onion, and dill together in a bowl. Dunk biscuit halves into the butter mixture to coat; arrange into a tube-cake pan."
        },
        {
            "3": "Bake in preheated oven until dough is cooked through and golden brown on top, about 30 minutes."
        }
    ],
