{
    "title": "Christy's Awesome Hot Ham and Cheese",
    "Category": "Sandwiches",
    "Ingredients": [
        {
            "1": "2 slices whole grain bread"
        },
        {
            "2": "2 teaspoons butter"
        },
        {
            "3": "2 slices Swiss cheese"
        },
        {
            "4": "2 thin slices  deli ham"
        },
        {
            "5": "1 teaspoon mayonnaise"
        },
        {
            "6": "1 teaspoon whole grain mustard"
        }
    ],
    "Directions": [
        {
            "1": "Preheat a skillet over medium-high heat."
        },
        {
            "2": "Spread one side of each slice of bread with 1 teaspoon butter."
        },
        {
            "3": "Place one slice butter-side down in the hot skillet. Top with Swiss cheese and ham. Spread the unbuttered side of second slice of bread with mayonnaise and mustard; place it butter-side up on sandwich."
        },
        {
            "4": "Cook in the hot skillet until golden brown and cheese is melted, about 3 minutes per side."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
