{
    "title": "Vicki's Hush Puppies",
    "Category": "Hushpuppies",
    "Ingredients": [
        {
            "1": "1 quart oil for frying, or as needed"
        },
        {
            "2": "1 large onion, diced"
        },
        {
            "3": "0.5 cup white sugar"
        },
        {
            "4": "2 large eggs, beaten"
        },
        {
            "5": "1 cup self-rising flour"
        },
        {
            "6": "1 cup self-rising cornmeal"
        }
    ],
    "Directions": [
        {
            "1": "Heat 2 inches oil in a deep saucepan over medium heat to 365 degrees F (185 degrees C)."
        },
        {
            "2": "While oil is heating, mix together onion, sugar, and eggs in a medium bowl. Blend in flour and cornmeal to make a batter."
        },
        {
            "3": "Drop batter by rounded teaspoonfuls, 4 at a time, into hot oil and fry until golden brown, 1 to 2 minutes per side. Transfer to a paper towel-lined plate to drain. Serve hot."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "15 mins"
        },
        {
            "Total Time": "25 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "24 hush puppies"
        }
    ],
