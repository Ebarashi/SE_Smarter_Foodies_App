{
    "title": "Skiers' French Toast",
    "Category": "French Toast",
    "Ingredients": [
        {
            "1": "2 tablespoons light corn syrup"
        },
        {
            "2": "0.5 cup butter"
        },
        {
            "3": "1 cup packed brown sugar"
        },
        {
            "4": "1 (1 pound) loaf unsliced white bread, with crust trimmed"
        },
        {
            "5": "5  eggs"
        },
        {
            "6": "0.5 cups milk"
        },
        {
            "7": "1 tablespoon vanilla extract"
        },
        {
            "8": "0.25 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Combine light corn syrup, butter and sugar in saucepan; simmer until syrup-like. Pour mixture over the bottom of a 9x 13 pan."
        },
        {
            "2": "Slice bread into 12-16 slices; place over the syrup. Layer as needed."
        },
        {
            "3": "Beat together the eggs, milk, vanilla, and salt. Pour over bread. Cover with saran wrap. Refrigerate overnight."
        },
        {
            "4": "Bake in a preheated oven 350 degree F (175 degrees C) for 45 minutes."
        },
        {
            "5": "Cut into squares. Invert and serve."
        }
    ],
    "Details": [
        {
            "Servings": "8 "
        },
        {
            "Yield": "8 servings"
        }
    ],
