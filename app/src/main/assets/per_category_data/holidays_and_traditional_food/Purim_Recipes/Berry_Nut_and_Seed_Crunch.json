{
    "title": "Berry Nut and Seed Crunch",
    "Category": "Purim Recipes",
    "Ingredients": [
        {
            "1": "0.5 cup brown sugar"
        },
        {
            "2": "0.5 cups walnuts"
        },
        {
            "3": "2 tablespoons sesame seeds"
        },
        {
            "4": "1 cup hulled pumpkin seeds"
        },
        {
            "5": "1 cup sunflower seeds"
        },
        {
            "6": "0.75 cup raspberry-flavored dried cranberries"
        }
    ],
    "Directions": [
        {
            "1": "Line a baking sheet with parchment paper."
        },
        {
            "2": "Heat brown sugar in a saucepan over medium-low heat until melted; stir in walnuts and sesame seeds until walnuts are coated with sugar and sesame seeds. Transfer coated walnuts to the prepared baking sheet; set aside until cooled."
        },
        {
            "3": "Mix coated walnuts, pumpkin seeds, sunflower seeds, and cranberries together in a bowl."
        }
    ],
