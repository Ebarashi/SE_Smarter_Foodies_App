{
    "title": "Vegan Sweet Potato and Bean Burritos",
    "Category": "Burritos",
    "Ingredients": [
        {
            "1": "1 cup sweet potato, peeled and cut into 1/4-inch cubes"
        },
        {
            "2": "0.5 cup yellow onion, sliced"
        },
        {
            "3": "2 tablespoons olive oil"
        },
        {
            "4": "1 tablespoon taco seasoning mix"
        },
        {
            "5": "0.5 cup vegan refried beans"
        },
        {
            "6": "4 large vegan tortillas"
        },
        {
            "7": "1 cup cooked brown rice"
        },
        {
            "8": "0.25 cup salsa"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 450 degrees F (230 degrees C)."
        },
        {
            "2": "Toss sweet potatoes, onion, olive oil, and taco seasoning together in a bowl. Spread out onto a baking sheet."
        },
        {
            "3": "Roast in the preheated oven, about 30 minutes or until desired level of doneness."
        },
        {
            "4": "Spread 1/4 of the beans down the center of each tortilla. Top each tortilla with 1/4 cup rice, 1/4 of the vegetable mixture, and 1 tablespoon salsa."
        }
    ],
