{
    "title": "Tomato Spinach and Bean Burrito",
    "Category": "Burritos",
    "Ingredients": [
        {
            "1": "2 tablespoons olive oil"
        },
        {
            "2": "1 cup diced onion"
        },
        {
            "3": "3 cloves garlic, minced"
        },
        {
            "4": "2 tablespoons chili powder, or to taste"
        },
        {
            "5": "1 teaspoon ground cumin"
        },
        {
            "6": "0.25 cup water"
        },
        {
            "7": "4 cups chopped fresh tomatoes"
        },
        {
            "8": "1 (15 ounce) can kidney beans, drained and rinsed"
        },
        {
            "9": "1 salt to taste"
        },
        {
            "10": "1 (10 ounce) package frozen chopped spinach, thawed and drained"
        },
        {
            "11": "4 (10 inch) flour tortillas"
        },
        {
            "12": "1  ripe avocado, sliced"
        },
        {
            "13": "4 tablespoons sour cream"
        },
        {
            "14": "4 tablespoons salsa"
        }
    ],
    "Directions": [
        {
            "1": "Heat oil in a large skillet on medium-high heat. Saute onion and garlic for 5 minutes. Stir in chili powder and cumin, and cook 1 minute. Stir in water, tomato, kidney beans and salt. Bring to a boil, then reduce heat and simmer for 20 minutes."
        },
        {
            "2": "Stir in spinach and cook 5 minutes more. Spoon 1/4 of the bean mixture into the middle of a warm tortilla. Wrap and garnish with avocado, sour cream and salsa."
        }
    ],
