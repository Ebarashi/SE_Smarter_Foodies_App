{
    "title": "Mediterranean Bean Salad",
    "Category": "Tex-Mex Recipes",
    "Ingredients": [
        {
            "1": "1 (15.5 ounce) can garbanzo beans, drained"
        },
        {
            "2": "1 (15 ounce) can kidney beans, drained"
        },
        {
            "3": "1  lemon, zested and juiced"
        },
        {
            "4": "1 medium tomato, chopped"
        },
        {
            "5": "0.25 cup chopped red onion"
        },
        {
            "6": "0.5 cup chopped fresh parsley"
        },
        {
            "7": "1 teaspoon capers, rinsed and drained"
        },
        {
            "8": "3 tablespoons extra virgin olive oil"
        },
        {
            "9": "0.5 teaspoon salt, or to taste"
        }
    ],
    "Directions": [
        {
            "1": "In a large bowl, stir together the garbanzo beans, kidney beans, lemon juice and zest, tomato, onion, parsley, capers, olive oil and salt. Cover, and refrigerate for about 2 hours, stirring occasionally, before serving."
        }
    ],
