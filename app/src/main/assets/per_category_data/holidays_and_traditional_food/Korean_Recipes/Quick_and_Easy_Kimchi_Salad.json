{
    "title": "Quick and Easy Kimchi Salad",
    "Category": "Korean Recipes",
    "Ingredients": [
        {
            "1": "1 small head cabbage, shredded"
        },
        {
            "2": "0.25 cup rock salt"
        },
        {
            "3": "1 small carrot, cut into thin strips"
        },
        {
            "4": "3 tablespoons vinegar"
        },
        {
            "5": "2 tablespoons vegetable oil"
        },
        {
            "6": "1 tablespoon white sugar, or to taste"
        },
        {
            "7": "1 teaspoon sesame oil"
        },
        {
            "8": "1 teaspoon toasted sesame seeds, ground"
        },
        {
            "9": "0.5 teaspoon cayenne pepper, or to taste"
        },
        {
            "10": "0.25 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Combine cabbage and rock salt together in a bowl. Let stand 15 minutes, stirring often. Rinse lightly and drain."
        },
        {
            "2": "Mix cabbage, carrot, vinegar, vegetable oil, sugar, sesame oil, sesame seeds, cayenne pepper, and salt together in a bowl. Refrigerate until chilled, at least 30 minutes."
        }
    ],
