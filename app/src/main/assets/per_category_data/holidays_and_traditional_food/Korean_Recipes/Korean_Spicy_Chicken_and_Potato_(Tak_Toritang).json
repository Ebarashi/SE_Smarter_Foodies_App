{
    "title": "Korean Spicy Chicken and Potato (Tak Toritang)",
    "Category": "Korean Recipes",
    "Ingredients": [
        {
            "1": "1.0 pounds chicken drumettes"
        },
        {
            "2": "2 large potatoes, cut into large chunks"
        },
        {
            "3": "2  carrots, cut into 2 inch pieces"
        },
        {
            "4": "1 large onion, cut into 8 pieces"
        },
        {
            "5": "4 cloves garlic, crushed"
        },
        {
            "6": "0.25 cup water"
        },
        {
            "7": "0.5 cup soy sauce"
        },
        {
            "8": "2 tablespoons white sugar"
        },
        {
            "9": "3 tablespoons gochujang (Korean hot pepper paste)"
        }
    ],
    "Directions": [
        {
            "1": "In a large pot over medium heat, mix the chicken, potatoes, carrots, onion, sugar, and garlic. Pour in water and soy sauce, and stir in sugar and hot pepper paste."
        },
        {
            "2": "Bring to a boil, reduce heat to low, and simmer 45 minutes, until chicken juices run clear, vegetables are tender, and liquid has thickened."
        }
    ],
