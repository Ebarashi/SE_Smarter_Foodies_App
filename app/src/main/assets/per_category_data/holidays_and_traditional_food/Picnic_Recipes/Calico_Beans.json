{
    "title": "Calico Beans",
    "Category": "Picnic Recipes",
    "Ingredients": [
        {
            "1": "1 pound lean ground beef"
        },
        {
            "2": "0.5 cup bacon, chopped"
        },
        {
            "3": "1 (15 ounce) can pork and beans"
        },
        {
            "4": "1 (15 ounce) can kidney beans, drained"
        },
        {
            "5": "1 (15 ounce) can butter beans"
        },
        {
            "6": "1 (15 ounce) can lima beans, drained"
        },
        {
            "7": "1 cup packed brown sugar"
        },
        {
            "8": "1 cup chopped onion"
        },
        {
            "9": "0.5 cup chopped celery"
        },
        {
            "10": "0.5 cup ketchup"
        },
        {
            "11": "3 tablespoons white wine vinegar"
        },
        {
            "12": "1 teaspoon mustard powder"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Cook ground beef and bacon in a large, deep skillet over medium-high heat until evenly brown. Drain and transfer meat to a 4-quart casserole dish."
        },
        {
            "3": "Add pork and beans, kidney beans, butter beans, lima beans, brown sugar, onion, celery, ketchup, vinegar, and dry mustard to the casserole dish; mix well."
        },
        {
            "4": "Bake, covered, in the preheated oven until bubbly and heated through, about 1 hour."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
