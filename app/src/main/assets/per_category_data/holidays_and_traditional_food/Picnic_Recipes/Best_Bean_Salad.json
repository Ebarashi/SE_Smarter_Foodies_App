{
    "title": "Best Bean Salad",
    "Category": "Picnic Recipes",
    "Ingredients": [
        {
            "1": "1 (15.5 ounce) can garbanzo beans, drained"
        },
        {
            "2": "1 (14.5 ounce) can kidney beans, drained"
        },
        {
            "3": "1 (14.5 ounce) can black beans, drained"
        },
        {
            "4": "1 (14.5 ounce) can green beans, drained"
        },
        {
            "5": "1 (14.5 ounce) can wax beans, drained"
        },
        {
            "6": "0.5 cup chopped green pepper"
        },
        {
            "7": "0.5 cup chopped onion"
        },
        {
            "8": "0.5 cup chopped celery"
        },
        {
            "9": "0.75 cup white sugar"
        },
        {
            "10": "0.5 cup salad oil"
        },
        {
            "11": "0.5 cup vinegar"
        },
        {
            "12": "0.5 teaspoon salt"
        },
        {
            "13": "0.5 teaspoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Combine garbanzo beans, kidney beans, black beans, green beans, wax beans, green pepper, onion, and celery in a large bowl; toss to mix."
        },
        {
            "2": "Whisk together sugar, oil, vinegar, salt, and pepper in a separate bowl until sugar is dissolved; pour over bean mixture. Mix well."
        },
        {
            "3": "Refrigerate salad for 8 hours to overnight before serving."
        }
    ],
    "Details": [
