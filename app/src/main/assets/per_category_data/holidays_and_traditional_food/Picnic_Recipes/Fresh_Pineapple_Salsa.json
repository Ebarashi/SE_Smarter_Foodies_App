{
    "title": "Fresh Pineapple Salsa",
    "Category": "Picnic Recipes",
    "Ingredients": [
        {
            "1": "1 cup finely chopped fresh pineapple"
        },
        {
            "2": "0.25 cup finely chopped red onion"
        },
        {
            "3": "0.25 cup red bell pepper, chopped"
        },
        {
            "4": "1  jalapeno pepper, seeded and minced"
        },
        {
            "5": "1 tablespoon finely chopped fresh cilantro"
        },
        {
            "6": "1 tablespoon lime juice"
        },
        {
            "7": "1 clove garlic, minced (Optional)"
        },
        {
            "8": "0.5 teaspoon white sugar"
        },
        {
            "9": "0.5 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Combine pineapple, red onion, bell pepper, jalapeno pepper, cilantro, lime juice, garlic, sugar, and salt. Stir to combine. Cover with plastic wrap and refrigerate for 1 hour so flavors can meld."
        }
    ],
