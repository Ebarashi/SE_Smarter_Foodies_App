{
    "title": "Quick and Easy Chicken Noodle Soup",
    "Category": "Everyday Leftovers",
    "Ingredients": [
        {
            "1": "1 tablespoon butter"
        },
        {
            "2": "0.5 cup chopped onion"
        },
        {
            "3": "0.5 cup chopped celery"
        },
        {
            "4": "4 (14.5 ounce) cans chicken broth"
        },
        {
            "5": "1 (14.5 ounce) can vegetable broth"
        },
        {
            "6": "0.5 pound chopped cooked chicken breast"
        },
        {
            "7": "0.5 cups egg noodles"
        },
        {
            "8": "1 cup sliced carrots"
        },
        {
            "9": "0.5 teaspoon dried basil"
        },
        {
            "10": "0.5 teaspoon dried oregano"
        },
        {
            "11": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Melt butter in a large pot over medium heat. Add onion and celery and cook until just tender, about 5 minutes."
        },
        {
            "2": "Add chicken broth, vegetable broth, chicken, egg noodles, carrots, basil, oregano, salt, and pepper. Stir to combine and bring to a boil."
        },
        {
            "3": "Reduce heat and simmer for 20 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "30 mins"
        },
        {
            "Total Time": "40 mins"
        },
        {
            "Servings": "6 "
