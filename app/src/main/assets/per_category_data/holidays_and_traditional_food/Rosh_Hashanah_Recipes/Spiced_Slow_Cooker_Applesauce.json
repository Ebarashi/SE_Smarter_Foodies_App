{
    "title": "Spiced Slow Cooker Applesauce",
    "Category": "Rosh Hashanah Recipes",
    "Ingredients": [
        {
            "1": "8  apples - peeled, cored, and thinly sliced"
        },
        {
            "2": "0.5 cup water"
        },
        {
            "3": "0.75 cup packed brown sugar"
        },
        {
            "4": "0.5 teaspoon pumpkin pie spice"
        }
    ],
    "Directions": [
        {
            "1": "Combine the apples and water in a slow cooker; cook on Low for 6 to 8 hours. Stir in the brown sugar and pumpkin pie spice; continue cooking another 30 minutes."
        }
