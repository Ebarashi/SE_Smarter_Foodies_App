{
    "title": "Boniet",
    "Category": "Antipasto",
    "Ingredients": [
        {
            "1": "2 bunches fresh parsley, finely chopped"
        },
        {
            "2": "1 (2 ounce) can anchovy fillets, chopped"
        },
        {
            "3": "3 cloves garlic, minced"
        },
        {
            "4": "0.5 tablespoons tomato paste"
        },
        {
            "5": "4 tablespoons distilled white vinegar"
        },
        {
            "6": "0.5 cup extra virgin olive oil"
        }
    ],
    "Directions": [
        {
            "1": "In a medium bowl, toss together the parsley, anchovies and garlic. Stir in the tomato paste, vinegar and olive oil. Serve at room temperature."
        }
    ],
