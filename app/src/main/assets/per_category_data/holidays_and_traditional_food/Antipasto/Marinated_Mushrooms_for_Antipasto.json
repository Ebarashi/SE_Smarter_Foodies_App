{
    "title": "Marinated Mushrooms for Antipasto",
    "Category": "Antipasto",
    "Ingredients": [
        {
            "1": "1 pound small mushrooms, stemmed"
        },
        {
            "2": "4 tablespoons extra-virgin olive oil"
        },
        {
            "3": "3 tablespoons white wine vinegar"
        },
        {
            "4": "0.5  red onion, finely chopped"
        },
        {
            "5": "2 tablespoons finely chopped fresh oregano"
        },
        {
            "6": "2 tablespoons finely chopped fresh thyme"
        },
        {
            "7": "2 cloves garlic cloves, pressed"
        },
        {
            "8": "0.5 teaspoon salt"
        },
        {
            "9": "0.5 teaspoon freshly ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Bring a large pot of lightly salted water to a boil. Add mushrooms and cook until softened, about 10 minutes. Drain and set aside to cool."
        },
        {
            "2": "Combine olive oil, white wine vinegar, red onion, oregano, thyme, garlic, salt, and pepper in a large jar with a lid. Close the jar and shake well. Add cooked mushrooms and refrigerate at least 5 hours. Remove from refrigerator 10 minutes prior to serving."
        }
    ],
