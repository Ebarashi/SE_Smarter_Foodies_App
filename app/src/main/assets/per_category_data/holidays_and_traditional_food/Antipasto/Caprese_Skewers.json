{
    "title": "Caprese Skewers",
    "Category": "Antipasto",
    "Ingredients": [
        {
            "1": "1 (8 ounce) container small fresh mozzarella balls"
        },
        {
            "2": "1 (4 ounce) package fresh basil leaves"
        },
        {
            "3": "1 (10.5 ounce) container cherry tomatoes, halved"
        },
        {
            "4": "24  small skewers, or as needed"
        },
        {
            "5": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Wrap each mozzarella ball with a basil leaf."
        },
        {
            "2": "Thread a tomato half, a basil-wrapped mozzarella ball, and another tomato half onto a skewer. Repeat until all tomatoes are on skewers. Sprinkle with salt and pepper."
        }
    ],
