{
    "title": "Easy Marinated Feta",
    "Category": "Antipasto",
    "Ingredients": [
        {
            "1": "1 pound feta cheese, cubed"
        },
        {
            "2": "1  chile pepper"
        },
        {
            "3": "1 tablespoon herbes de Provence"
        },
        {
            "4": "1 pinch lemon zest"
        },
        {
            "5": "1 cup olive oil, or as needed"
        }
    ],
    "Directions": [
        {
            "1": "Place feta cheese in a glass jar. Add chile pepper, herbes de Provence, and lemon zest. Fill jar with olive oil so feta is completely covered and seal."
        },
        {
            "2": "Marinate in a cool place, but not in the refrigerator, for 3 to 4 days. Remove chile pepper before serving."
        }
    ],
