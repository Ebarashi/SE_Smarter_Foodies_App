{
    "title": "Cosmopolitan",
    "Category": "Mother's Day Recipes",
    "Ingredients": [
        {
            "1": "1 (1.5 fluid ounce) jigger vodka"
        },
        {
            "2": "0.5 fluid ounce cointreau"
        },
        {
            "3": "1 teaspoon fresh lime juice"
        },
        {
            "4": "0.5 fluid ounces cranberry juice"
        },
        {
            "5": "1 twist lime zest, garnish"
        }
    ],
    "Directions": [
        {
            "1": "Pour all the ingredients into a shaker with lots of ice. Shake vigorously for several seconds and strain into a cocktail glass. Garnish with a lime twist."
        }
    ],
