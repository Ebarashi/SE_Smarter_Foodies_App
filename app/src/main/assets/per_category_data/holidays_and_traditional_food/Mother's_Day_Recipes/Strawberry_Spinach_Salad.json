{
    "title": "Strawberry Spinach Salad",
    "Category": "Mother's Day Recipes",
    "Ingredients": [
        {
            "1": "1 Dressing-"
        },
        {
            "2": "0.5 cup white sugar"
        },
        {
            "3": "0.5 cup olive oil"
        },
        {
            "4": "0.25 cup distilled white vinegar"
        },
        {
            "5": "2 tablespoons sesame seeds"
        },
        {
            "6": "1 tablespoon poppy seeds"
        },
        {
            "7": "1 tablespoon minced onion"
        },
        {
            "8": "0.25 teaspoon paprika"
        },
        {
            "9": "0.25 teaspoon Worcestershire sauce"
        },
        {
            "10": "1 Salad-"
        },
        {
            "11": "1 quart strawberries - cleaned, hulled and sliced"
        },
        {
            "12": "10 ounces fresh spinach - rinsed, dried and torn into bite-size pieces"
        },
        {
            "13": "0.25 cup almonds, blanched and slivered"
        }
    ],
    "Directions": [
        {
            "1": "Make dressing: Whisk together sugar, oil, vinegar, sesame seeds, poppy seeds, onion, paprika, and Worcestershire in a medium bowl. Cover and chill for 1 hour."
        },
        {
            "2": "Make salad: Combine strawberries, spinach, and almonds in a large bowl."
        },
        {
            "3": "Pour dressing over salad; toss to coat. Refrigerate for 10 to 15 minutes before serving."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
