{
    "title": "Hawaiian Cole Slaw",
    "Category": "Hawaiian",
    "Ingredients": [
        {
            "1": "1 (8.5 ounce) package coleslaw mix"
        },
        {
            "2": "1 (15 ounce) can crushed pineapple, drained"
        },
        {
            "3": "0.5 cup finely chopped onion"
        },
        {
            "4": "0.5 cup mayonnaise"
        },
        {
            "5": "0.25 cup apple cider vinegar"
        },
        {
            "6": "2 tablespoons brown sugar"
        },
        {
            "7": "1 teaspoon dried cilantro"
        },
        {
            "8": "0.5 teaspoon salt"
        },
        {
            "9": "0.25 teaspoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Mix coleslaw mix, pineapple, and onion together in a bowl."
        },
        {
            "2": "Whisk mayonnaise, vinegar, brown sugar, cilantro, salt, and pepper together in a bowl until dressing is smooth. Pour dressing over coleslaw mixture and toss to coat. Refrigerate."
        }
    ],
    "Details": [
