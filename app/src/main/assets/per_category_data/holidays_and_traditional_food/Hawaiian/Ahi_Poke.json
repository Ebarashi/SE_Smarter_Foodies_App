{
    "title": "Ahi Poke",
    "Category": "Hawaiian",
    "Ingredients": [
        {
            "1": "2 pounds fresh tuna steaks, cubed"
        },
        {
            "2": "1 cup soy sauce"
        },
        {
            "3": "0.75 cup chopped green onions"
        },
        {
            "4": "2 tablespoons sesame oil"
        },
        {
            "5": "2 tablespoons finely chopped macadamia nuts"
        },
        {
            "6": "1 tablespoon toasted sesame seeds"
        },
        {
            "7": "1 tablespoon crushed red pepper (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Place tuna in a medium non-reactive bowl. Add soy sauce, green onions, sesame oil, sesame seeds, macadamia nuts, and red pepper flakes; mix well. Cover and refrigerate at least 2 hours before serving."
        }
    ],
