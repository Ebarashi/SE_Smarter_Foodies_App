{
    "title": "Ahi Poke Salad",
    "Category": "Hawaiian",
    "Ingredients": [
        {
            "1": "0.25 pounds ahi tuna, cut into 1/2 inch cubes"
        },
        {
            "2": "0.25 cup minced onion"
        },
        {
            "3": "0.25 cup minced green onion"
        },
        {
            "4": "2 tablespoons lightly crumbled wakame seaweed"
        },
        {
            "5": "1 tablespoon ground roasted macadamia nuts"
        },
        {
            "6": "2 teaspoons sesame oil"
        },
        {
            "7": "0.5 teaspoons crushed red pepper flakes"
        },
        {
            "8": "1 teaspoon minced fresh ginger"
        },
        {
            "9": "8 leaves iceberg lettuce"
        },
        {
            "10": "2 tablespoons cilantro leaves, for garnish"
        }
    ],
    "Directions": [
        {
            "1": "Mix together ahi tuna, onion, green onion, seaweed, macadamia nuts, sesame oil, red pepper flakes, and ginger in a mixing bowl."
        },
        {
            "2": "Cover and refrigerate for the flavors to come together, at least 1 hour."
        },
        {
            "3": "Arrange lettuce leaves on a serving platter, spoon ahi poke on top, and garnish with cilantro."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
