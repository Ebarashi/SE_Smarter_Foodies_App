{
    "title": "Blue Hawaiian Cocktail",
    "Category": "Hawaiian",
    "Ingredients": [
        {
            "1": "1 cup crushed ice"
        },
        {
            "2": "2 fluid ounces pineapple juice"
        },
        {
            "3": "1 fluid ounce light rum"
        },
        {
            "4": "1 fluid ounce blue Curacao liqueur"
        },
        {
            "5": "1 fluid ounce cream of coconut"
        },
        {
            "6": "1  pineapple slice"
        },
        {
            "7": "1  maraschino cherry"
        }
    ],
    "Directions": [
        {
            "1": "Combine crushed ice, pineapple juice, rum, blue Curacao, and cream of coconut in a blender. Puree on high speed until smooth. Pour into a chilled highball glass."
        },
        {
            "2": "Garnish with a slice of pineapple and a maraschino cherry."
        }
    ],
    "Details": [
