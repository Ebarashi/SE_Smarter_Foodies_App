{
    "title": "Green Grape Salad",
    "Category": "Easter Recipes",
    "Ingredients": [
        {
            "1": "4 pounds seedless green grapes"
        },
        {
            "2": "1 (8 ounce) package cream cheese"
        },
        {
            "3": "1 (8 ounce) container sour cream"
        },
        {
            "4": "0.5 cup white sugar"
        },
        {
            "5": "1 teaspoon vanilla extract"
        },
        {
            "6": "4 ounces chopped pecans"
        },
        {
            "7": "2 tablespoons brown sugar"
        }
    ],
    "Directions": [
        {
            "1": "Wash and dry grapes."
        },
        {
            "2": "Mix together cream cheese, sour cream, sugar, and vanilla in a large bowl. Add grapes and mix until evenly incorporated."
        },
        {
            "3": "Sprinkle with brown sugar and pecans, mix again and refrigerate until serving."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Total Time": "15 mins"
        },
        {
            "Servings": "8 "
        }
