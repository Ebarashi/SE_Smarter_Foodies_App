{
    "title": "Charoset",
    "Category": "Seder Recipes",
    "Ingredients": [
        {
            "1": "2  Granny Smith apples, peeled and coarsely grated"
        },
        {
            "2": "1 cup walnuts, finely ground"
        },
        {
            "3": "2 tablespoons grape juice"
        },
        {
            "4": "2 teaspoons honey"
        },
        {
            "5": "1 teaspoon ground cinnamon"
        }
    ],
    "Directions": [
        {
            "1": "Mix apples, walnuts, grape juice, honey, and cinnamon together in a bowl. Cover the bowl with plastic wrap and refrigerate until chilled, 8 hours to overnight."
        }
