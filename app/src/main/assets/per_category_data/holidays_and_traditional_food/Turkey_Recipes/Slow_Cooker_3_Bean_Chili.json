{
    "title": "Slow Cooker 3-Bean Chili",
    "Category": "Turkey Recipes",
    "Ingredients": [
        {
            "1": "1 (20 ounce) package 93%-lean ground turkey"
        },
        {
            "2": "1 (28 ounce) can diced fire-roasted tomatoes"
        },
        {
            "3": "1 (16 ounce) can tomato sauce"
        },
        {
            "4": "1 (15.5 ounce) can pinto beans, rinsed and drained"
        },
        {
            "5": "1 (15.5 ounce) can kidney beans, rinsed and drained"
        },
        {
            "6": "1 (15 ounce) can reduced-sodium black beans, rinsed and drained"
        },
        {
            "7": "1 small onion, chopped"
        },
        {
            "8": "1 (4.5 ounce) can chopped green chiles"
        },
        {
            "9": "2 tablespoons chili powder"
        },
        {
            "10": "1 tablespoon minced garlic"
        },
        {
            "11": "1 teaspoon oregano"
        },
        {
            "12": "1 pinch ground cumin"
        }
    ],
    "Directions": [
        {
            "1": "Cook and stir turkey in a large skillet over medium-high heat until crumbly and browned, 7 to 10 minutes; transfer to a slow cooker."
        },
        {
            "2": "Stir in tomatoes, tomato sauce, pinto beans, kidney beans, black beans, onion, green chilies, chili powder, garlic, oregano, and cumin in the slow cooker."
        },
        {
            "3": "Cover and cook on Low for 7 hours or High for 4 hours."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
