{
    "title": "Virgin Strawberry Daiquiri",
    "Category": "Labor Day Recipes",
    "Ingredients": [
        {
            "1": "2 large strawberries, hulled"
        },
        {
            "2": "0.25 cup white sugar"
        },
        {
            "3": "0.75 cup chilled lemon-lime soda"
        },
        {
            "4": "1 tablespoon lemon juice"
        },
        {
            "5": "4 cubes  ice"
        }
    ],
    "Directions": [
        {
            "1": "Combine strawberries, sugar, lemon-lime soda, and lemon juice in the container of a blender. Add ice and blend until smooth."
        },
        {
            "2": "Pour into a fancy glass to serve."
        }
    ],
