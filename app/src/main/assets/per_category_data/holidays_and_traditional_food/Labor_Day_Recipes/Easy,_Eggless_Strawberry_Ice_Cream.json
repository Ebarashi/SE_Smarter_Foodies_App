{
    "title": "Easy, Eggless Strawberry Ice Cream",
    "Category": "Labor Day Recipes",
    "Ingredients": [
        {
            "1": "2 cups mashed fresh strawberries"
        },
        {
            "2": "2 cups whole milk"
        },
        {
            "3": "2 cups heavy cream"
        },
        {
            "4": "1 cup white sugar"
        },
        {
            "5": "2 teaspoons vanilla extract"
        },
        {
            "6": "0.25 teaspoon salt"
        },
        {
            "7": "2 drops red food coloring (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Combine strawberries, milk, cream, sugar, vanilla, salt, and food coloring in a large bowl."
        },
        {
            "2": "Pour strawberry mixture into the freezer bowl of an ice cream maker; freeze according to the manufacturer's directions."
        }
    ],
