{
    "title": "Oatmeal Chocolate Coconut Macaroons",
    "Category": "Christmas Cookies",
    "Ingredients": [
        {
            "1": "2 cups quick-cooking oats"
        },
        {
            "2": "1 cup shredded coconut"
        },
        {
            "3": "0.25 cup unsweetened cocoa powder"
        },
        {
            "4": "2 cups white sugar"
        },
        {
            "5": "0.5 cup butter"
        },
        {
            "6": "0.5 cup milk"
        }
    ],
    "Directions": [
        {
            "1": "Mix together oats, coconut, and cocoa powder in a large bowl."
        },
        {
            "2": "Bring sugar, butter, and milk to a boil in a saucepan, stirring occasionally; remove immediately from heat and stir into oat mixture until well combined."
        },
        {
            "3": "Drop 15 spoonfuls of dough onto a sheet of waxed paper. Let cool completely at room temperature before serving."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "5 mins"
        },
        {
            "Total Time": "20 mins"
        },
        {
            "Servings": "15 "
        },
        {
            "Yield": "15 macaroons"
        }
    ],
