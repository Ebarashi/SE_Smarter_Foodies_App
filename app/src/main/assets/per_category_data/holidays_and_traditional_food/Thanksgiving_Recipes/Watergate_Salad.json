{
    "title": "Watergate Salad",
    "Category": "Thanksgiving Recipes",
    "Ingredients": [
        {
            "1": "1 (3.4 ounce) package instant pistachio pudding mix"
        },
        {
            "2": "1 (8 ounce) can crushed pineapple, with juice"
        },
        {
            "3": "1 cup miniature marshmallows"
        },
        {
            "4": "0.5 cup chopped walnuts"
        },
        {
            "5": "4 ounces frozen whipped topping, thawed"
        }
    ],
    "Directions": [
        {
            "1": "Combine crushed pineapple with juice, instant pudding mix, mini marshmallows, and walnuts in a large bowl. Mix until well combined, then fold in whipped topping."
        },
        {
            "2": "Chill for 1 to 2 hours before serving."
        }
    ],
