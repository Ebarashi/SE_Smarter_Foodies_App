{
    "title": "The Best Veggie Sandwich",
    "Category": "Lent Recipes",
    "Ingredients": [
        {
            "1": "4  English muffins, split and toasted"
        },
        {
            "2": "1  avocado, mashed"
        },
        {
            "3": "1 cup alfalfa sprouts"
        },
        {
            "4": "1 small tomato, chopped"
        },
        {
            "5": "1 small sweet onion, chopped"
        },
        {
            "6": "4 tablespoons Ranch-style salad dressing"
        },
        {
            "7": "4 tablespoons toasted sesame seeds"
        },
        {
            "8": "1 cup shredded smoked Cheddar cheese"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to broil."
        },
        {
            "2": "Place each muffin open-faced on a cookie sheet. Spread each half with mashed avocado; place halves close together. Distributing ingredients evenly, cover each half with sprouts, tomatoes, onion, dressing, sesame seeds and cheese."
        },
        {
            "3": "Place under broiler for about 5 minutes, or until cheese is melted and bubbly."
        }
    ],
