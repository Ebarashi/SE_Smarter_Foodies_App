{
    "title": "Tomato Alfredo",
    "Category": "Fettuccini",
    "Ingredients": [
        {
            "1": "1 (14.5 ounce) can diced tomatoes"
        },
        {
            "2": "1 cup low-fat sour cream"
        },
        {
            "3": "0.75 cup grated Parmesan cheese"
        },
        {
            "4": "0.75 cup frozen green peas"
        },
        {
            "5": "1 pound dry fettuccine pasta"
        },
        {
            "6": "1 ground black pepper to taste"
        },
        {
            "7": "1 salt to taste"
        }
    ],
    "Directions": [
        {
            "1": "Cook pasta in a large pot of boiling water, adding peas just before pasta is done. Drain."
        },
        {
            "2": "Combine tomatoes, sour cream, parmesan, and 1/4 cup reserved juice. Add pasta and peas. Salt and pepper to taste."
        }
    ],
