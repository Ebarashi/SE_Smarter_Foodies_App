{
    "title": "Balela Salad",
    "Category": "Ramadan Recipes",
    "Ingredients": [
        {
            "1": "2 (15 ounce) cans garbanzo beans, drained and rinsed"
        },
        {
            "2": "2 (15 ounce) cans black beans, drained and rinsed"
        },
        {
            "3": "0.5 cup (packed) chopped fresh mint"
        },
        {
            "4": "0.5 cup (packed) chopped fresh parsley"
        },
        {
            "5": "0.5 cup chopped onion"
        },
        {
            "6": "1 pint grape tomatoes, halved"
        },
        {
            "7": "1  jalapeno pepper, finely chopped"
        },
        {
            "8": "1 clove garlic, minced"
        },
        {
            "9": "0.25 cup olive oil"
        },
        {
            "10": "3 tablespoons lemon juice"
        },
        {
            "11": "5 tablespoons apple cider vinegar"
        },
        {
            "12": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "In a salad bowl, lightly mix together the garbanzo beans, black beans, mint, parsley, onion, and grape tomatoes."
        },
        {
            "2": "Place the jalapeno pepper, garlic, olive oil, lemon juice, and apple cider vinegar into a blender, and blend until thoroughly combined. Pour the dressing over the salad, and toss to mix; sprinkle with salt and black pepper to serve."
        }
    ],
