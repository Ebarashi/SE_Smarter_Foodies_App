{
    "title": "Stuffed Dates",
    "Category": "Ramadan Recipes",
    "Ingredients": [
        {
            "1": "1 (3 ounce) package cream cheese, softened"
        },
        {
            "2": "3 ounces crumbled Gorgonzola cheese"
        },
        {
            "3": "36  pitted dates"
        },
        {
            "4": "36  walnut pieces"
        }
    ],
    "Directions": [
        {
            "1": "Mix cream cheese and Gorgonzola cheese together in a bowl."
        },
        {
            "2": "Cut a slit into each date so they can be spread flat. Spread cheese mixture into each date; top with a walnut piece."
        }
    ],
