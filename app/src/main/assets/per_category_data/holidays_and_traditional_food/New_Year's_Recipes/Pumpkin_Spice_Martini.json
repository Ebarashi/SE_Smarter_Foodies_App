{
    "title": "Pumpkin Spice Martini",
    "Category": "New Year's Recipes",
    "Ingredients": [
        {
            "1": "1 cup ice cubes"
        },
        {
            "2": "1 (1.5 fluid ounce) jigger vanilla flavored vodka (such as Stoli\u00ae)"
        },
        {
            "3": "1 (1.5 fluid ounce) jigger Irish cream liqueur (such as Bailey's\u00ae)"
        },
        {
            "4": "1 (1.5 fluid ounce) jigger pumpkin flavored liqueur (such as Hiram Walker\u00ae)"
        },
        {
            "5": "1 pinch ground cinnamon"
        },
        {
            "6": "1 pinch ground nutmeg"
        }
    ],
    "Directions": [
        {
            "1": "Fill a shaker with ice."
        },
        {
            "2": "Pour vanilla-flavored vodka, Irish cream liqueur, and pumpkin-flavored liqueur over ice. Cover and shake until the outside of the shaker has frosted."
        },
        {
            "3": "Strain into a chilled martini glass. Garnish with ground cinnamon and ground nutmeg."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Total Time": "5 mins"
        },
        {
            "Servings": "1 "
        }
    ],
