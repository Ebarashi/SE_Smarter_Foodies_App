{
    "title": "Pudding Shots",
    "Category": "New Year's Recipes",
    "Ingredients": [
        {
            "1": "1 cup milk"
        },
        {
            "2": "0.5 cup Irish cream liqueur (eg. Bailey's\u00ae)"
        },
        {
            "3": "0.5 cup vodka (eg. Smirnoff\u00ae)"
        },
        {
            "4": "1 (3.9 ounce) package instant chocolate pudding mix"
        }
    ],
    "Directions": [
        {
            "1": "Whisk milk, Irish cream liqueur, vodka, and instant pudding mix together in a bowl until thoroughly combined, about 2 minutes."
        },
        {
            "2": "Spoon the pudding mixture into shot glasses or disposable party shot cups. Chill until set, about 30 minutes. The shots will have the consistency of mousse."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Additional Time": "30 mins"
        },
        {
            "Total Time": "35 mins"
        },
        {
            "Servings": "15 "
        }
    ],
