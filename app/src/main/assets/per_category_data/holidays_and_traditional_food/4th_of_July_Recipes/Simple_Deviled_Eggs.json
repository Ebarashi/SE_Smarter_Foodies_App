{
    "title": "Simple Deviled Eggs",
    "Category": "4th of July Recipes",
    "Ingredients": [
        {
            "1": "6  hard-cooked eggs"
        },
        {
            "2": "2 tablespoons mayonnaise"
        },
        {
            "3": "1 teaspoon white sugar"
        },
        {
            "4": "1 teaspoon white vinegar"
        },
        {
            "5": "1 teaspoon prepared mustard"
        },
        {
            "6": "0.5 teaspoon salt"
        },
        {
            "7": "1 tablespoon finely chopped onion"
        },
        {
            "8": "1 tablespoon finely chopped celery"
        },
        {
            "9": "1 pinch paprika, or to taste"
        }
    ],
    "Directions": [
        {
            "1": "Slice eggs in half lengthwise and remove yolks; set whites aside."
        },
        {
            "2": "Mash yolks with a fork in a small bowl. Stir in mayonnaise, sugar, vinegar, mustard, salt, onion, and celery; mix well."
        },
        {
            "3": "Stuff or pipe egg yolk mixture into egg whites. Sprinkle with paprika. Refrigerate until serving."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Total Time": "15 mins"
        },
        {
            "Servings": "6 "
        },
        {
            "Yield": "12 deviled egg halves"
        }
    ],
