{
    "title": "Grandpa's Classic Coney Sauce",
    "Category": "4th of July Recipes",
    "Ingredients": [
        {
            "1": "2 pounds ground beef"
        },
        {
            "2": "0.5 cup chopped onion"
        },
        {
            "3": "0.5 cups ketchup"
        },
        {
            "4": "0.25 cup white sugar"
        },
        {
            "5": "0.25 cup white vinegar"
        },
        {
            "6": "0.25 cup prepared yellow mustard"
        },
        {
            "7": "0.75 teaspoon Worcestershire sauce"
        },
        {
            "8": "0.75 teaspoon salt"
        },
        {
            "9": "0.5 teaspoon celery seed"
        },
        {
            "10": "0.5 teaspoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Place ground beef and onion in a large skillet over medium-high heat. Cook, stirring to crumble, until beef is browned; drain."
        },
        {
            "2": "Transfer the beef and onion to a slow cooker and stir in ketchup, sugar, vinegar, and mustard. Season with Worcestershire, salt, celery seed, and pepper."
        },
        {
            "3": "Cover and simmer on Low for 3 hours before serving."
        }
    ],
