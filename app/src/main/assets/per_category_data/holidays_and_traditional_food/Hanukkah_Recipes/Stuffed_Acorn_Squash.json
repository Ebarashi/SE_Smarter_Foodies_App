{
    "title": "Stuffed Acorn Squash",
    "Category": "Hanukkah Recipes",
    "Ingredients": [
        {
            "1": "1  acorn squash, halved and seeded"
        },
        {
            "2": "4 tablespoons butter, divided"
        },
        {
            "3": "4 tablespoons brown sugar, divided"
        },
        {
            "4": "2 cups dry bread stuffing mix, divided"
        },
        {
            "5": "0.5 cups chicken broth, divided"
        },
        {
            "6": "1 salt and pepper to taste"
        },
        {
            "7": "1 garlic powder to taste"
        },
        {
            "8": "1 onion powder to taste"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 400 degrees F (200 degrees C)."
        },
        {
            "2": "Place squash, face up, in a shallow baking dish. In each half place 2 tablespoons butter, 2 tablespoons brown sugar, 1 cup stuffing mix, 3/4 cup chicken broth, salt, pepper, garlic powder and onion powder. Wrap each half tightly with aluminum foil."
        },
        {
            "3": "Bake in preheated oven for 60 minutes, or until squash is very tender."
        }
    ],
