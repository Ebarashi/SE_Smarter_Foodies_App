{
    "title": "Apricot Chicken I",
    "Category": "Hanukkah Recipes",
    "Ingredients": [
        {
            "1": "6  skinless, boneless chicken breast halves"
        },
        {
            "2": "0.5 (1 ounce) packages dry onion soup mix"
        },
        {
            "3": "1 (10 fluid ounce) bottle Russian-style salad dressing"
        },
        {
            "4": "1 cup apricot preserves"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Place the chicken pieces in a 4 quart casserole dish. Mix the soup mix, dressing and jam together, and pour over the chicken."
        },
        {
            "3": "Cover dish and bake for 1 hour in preheated oven."
        }
    ],
