{
    "title": "Annie's Turkey Brine",
    "Category": "Turkey Brine",
    "Ingredients": [
        {
            "1": "0.5 gallons water"
        },
        {
            "2": "1 quart apple cider"
        },
        {
            "3": "2 cups brown sugar"
        },
        {
            "4": "2  oranges, sliced"
        },
        {
            "5": "0.5 cups salt"
        },
        {
            "6": "0.75 cup celery leaves and diced celery stalk from the inside of a bunch"
        },
        {
            "7": "3 tablespoons dried rosemary"
        },
        {
            "8": "6 cloves garlic, minced"
        },
        {
            "9": "6  bay leaves"
        },
        {
            "10": "1 tablespoon dried thyme"
        }
    ],
    "Directions": [
        {
            "1": "Stir water, apple cider, brown sugar, orange slices, salt, celery, rosemary, garlic, bay leaves, and thyme together in a stock pot; bring to a boil and cook 5 minutes. Cool brine completely before using to brine turkey."
        }
    ],
