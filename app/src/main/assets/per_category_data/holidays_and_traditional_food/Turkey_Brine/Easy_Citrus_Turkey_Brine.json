{
    "title": "Easy Citrus Turkey Brine",
    "Category": "Turkey Brine",
    "Ingredients": [
        {
            "1": "96 ounces water"
        },
        {
            "2": "32 ounces vegetable broth"
        },
        {
            "3": "2  oranges, sliced"
        },
        {
            "4": "1  onion, chopped into large pieces"
        },
        {
            "5": "1 (6 ounce) can frozen limeade concentrate"
        },
        {
            "6": "1 (6 ounce) can frozen orange juice concentrate"
        },
        {
            "7": "0.5 cup brown sugar"
        },
        {
            "8": "0.5 cup rum"
        },
        {
            "9": "0.5 cup coarse salt"
        },
        {
            "10": "2 tablespoons crushed garlic"
        },
        {
            "11": "1 tablespoon ground black pepper"
        },
        {
            "12": "1 tablespoon ground thyme"
        },
        {
            "13": "1 tablespoon dried parsley"
        },
        {
            "14": "1 tablespoon juniper berries"
        },
        {
            "15": "2  cinnamon sticks"
        },
        {
            "16": "3  bay leaves"
        }
    ],
    "Directions": [
        {
            "1": "Combine water, broth, oranges, onion, limeade concentrate, orange juice concentrate, brown sugar, rum, salt, garlic, black pepper, thyme, parsley, juniper berries, cinnamon sticks, and bay leaves in a large pot and bring to a boil. Reduce temperature and simmer for 10 minutes. Remove from heat and cover pot. Allow to cool completely, at least 1 hour."
        }
    ],
