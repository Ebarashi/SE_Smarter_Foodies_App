{
    "title": "Honey Citrus Turkey Brine",
    "Category": "Turkey Brine",
    "Ingredients": [
        {
            "1": "4 cups orange juice"
        },
        {
            "2": "2 cups water"
        },
        {
            "3": "4 (8 ounce) jars honey"
        },
        {
            "4": "1 cup brown sugar"
        },
        {
            "5": "1  lemon, sliced"
        },
        {
            "6": "2  oranges, sliced"
        },
        {
            "7": "3 tablespoons ground black pepper"
        },
        {
            "8": "1 tablespoon red pepper flakes"
        },
        {
            "9": "2 cloves garlic"
        },
        {
            "10": "1  bay leaf"
        },
        {
            "11": "2 (7 pound) bags of ice, or as needed"
        }
    ],
    "Directions": [
        {
            "1": "Place orange juice, water, honey, brown sugar, lemon, oranges, black pepper, red pepper flakes, garlic, and bay leaf in a large pot; bring to a boil. Cook and stir until sugar is dissolved, 10 minutes. Remove from heat and gradually stir in ice to start chilling the brine."
        },
        {
            "2": "Pour brine into a large bucket; add more ice to finish cooling."
        }
    ],
