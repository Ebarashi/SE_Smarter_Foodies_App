{
    "title": "Apple Cider Turkey Brine",
    "Category": "Turkey Brine",
    "Ingredients": [
        {
            "1": "1 gallon water, or as needed, divided"
        },
        {
            "2": "2 cups apple cider"
        },
        {
            "3": "1 cup kosher salt"
        },
        {
            "4": "1 tablespoon fresh sage"
        },
        {
            "5": "1 tablespoon fresh rosemary"
        },
        {
            "6": "1 tablespoon fresh thyme"
        },
        {
            "7": "1 tablespoon black peppercorns"
        },
        {
            "8": "4 cups ice cubes"
        }
    ],
    "Directions": [
        {
            "1": "Combine 4 cups water, apple cider, kosher salt, sage, rosemary, thyme, and peppercorns together in a large stockpot. Bring to a boil; cook and stir brine until the salt is just dissolved. Remove from heat; add ice cubes."
        }
    ],
