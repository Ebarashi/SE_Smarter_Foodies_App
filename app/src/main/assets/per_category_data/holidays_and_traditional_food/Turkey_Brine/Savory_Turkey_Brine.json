{
    "title": "Savory Turkey Brine",
    "Category": "Turkey Brine",
    "Ingredients": [
        {
            "1": "2 gallons cold water"
        },
        {
            "2": "10 ounces Kikkoman Naturally Brewed Soy Sauce"
        },
        {
            "3": "0.5 cup kosher salt"
        },
        {
            "4": "0.5 cup white sugar"
        },
        {
            "5": "2 tablespoons dried sage"
        },
        {
            "6": "2 tablespoons dried celery seed"
        },
        {
            "7": "1 tablespoon dried thyme"
        }
    ],
    "Directions": [
        {
            "1": "Mix water with soy sauce, salt, sugar, sage, celery seed, and thyme in a large stockpot. Stir well until salt is dissolved. Let cool to room temperature."
        }
    ],
