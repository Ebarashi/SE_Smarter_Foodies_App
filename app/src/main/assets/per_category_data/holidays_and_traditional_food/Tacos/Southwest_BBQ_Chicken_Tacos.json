{
    "title": "Southwest BBQ Chicken Tacos",
    "Category": "Tacos",
    "Ingredients": [
        {
            "1": "1 cup barbecue sauce"
        },
        {
            "2": "1 tablespoon Southwest seasoning"
        },
        {
            "3": "0.5 teaspoon hot sauce"
        },
        {
            "4": "2 teaspoons olive oil"
        },
        {
            "5": "3 cups shredded rotisserie chicken"
        },
        {
            "6": "8 (6 inch) flour or corn tortillas"
        },
        {
            "7": "0.5 cup chopped tomatoes"
        },
        {
            "8": "0.5 cup Borden\u00ae Finely Shredded Four Cheese Mexican Shreds"
        },
        {
            "9": "0.5 cup coarsely chopped fresh cilantro"
        },
        {
            "10": "1 Lime wedges, for serving"
        }
    ],
    "Directions": [
        {
            "1": "Combine barbecue sauce, seasoning and hot sauce in a small bowl. Let stand 15 minutes."
        },
        {
            "2": "Heat oil in a large skillet over medium heat until hot."
        },
        {
            "3": "Add chicken; cook and stir 3 to 5 minutes or until warmed through."
        },
        {
            "4": "Add barbecue sauce mixture. Cook 3 to 5 minutes more or until chicken is coated and sauce thickens slightly."
        },
        {
            "5": "Heat tortillas according to package directions."
        },
        {
            "6": "Spoon chicken mixture evenly onto the center of each tortilla."
        },
        {
            "7": "Top each taco evenly with tomatoes, Borden\u00ae Queso Fresco Fresh Crumbling Cheese and cilantro. Serve with lime wedges."
        }
    ],
    "Details": [
        {
            "Servings": "4 "
        },
        {
            "Yield": "8 tacos"
        }
    ],
