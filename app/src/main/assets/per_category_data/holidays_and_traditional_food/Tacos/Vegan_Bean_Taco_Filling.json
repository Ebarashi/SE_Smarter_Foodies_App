{
    "title": "Vegan Bean Taco Filling",
    "Category": "Tacos",
    "Ingredients": [
        {
            "1": "1 tablespoon olive oil"
        },
        {
            "2": "1  onion, diced"
        },
        {
            "3": "2 cloves garlic, minced"
        },
        {
            "4": "1  bell pepper, chopped"
        },
        {
            "5": "2 (14.5 ounce) cans black beans, rinsed, drained, and mashed"
        },
        {
            "6": "2 tablespoons yellow cornmeal"
        },
        {
            "7": "0.5 tablespoons cumin"
        },
        {
            "8": "1 teaspoon paprika"
        },
        {
            "9": "1 teaspoon cayenne pepper"
        },
        {
            "10": "1 teaspoon chili powder"
        },
        {
            "11": "1 cup salsa"
        }
    ],
    "Directions": [
        {
            "1": "Heat olive oil in a medium skillet over medium heat. Stir in onion, garlic, and bell pepper; cook until tender. Stir in mashed beans. Add the cornmeal. Mix in cumin, paprika, cayenne, chili powder, and salsa. Cover, and cook 5 minutes."
        }
    ],
