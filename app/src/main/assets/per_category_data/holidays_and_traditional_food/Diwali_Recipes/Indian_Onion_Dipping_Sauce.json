{
    "title": "Indian Onion Dipping Sauce",
    "Category": "Diwali Recipes",
    "Ingredients": [
        {
            "1": "1  onion, chopped, or more to taste"
        },
        {
            "2": "0.5 tablespoons lemon juice"
        },
        {
            "3": "1 teaspoon ketchup, or more to taste (Optional)"
        },
        {
            "4": "0.5 teaspoon cayenne pepper"
        },
        {
            "5": "0.5 teaspoon Hungarian paprika"
        },
        {
            "6": "0.5 teaspoon salt"
        },
        {
            "7": "1 pinch white sugar (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Place onion in a food processor and pulse until onion is minced. Add lemon juice, ketchup, cayenne pepper, paprika, salt, and sugar and pulse until evenly combined. Transfer sauce to an airtight container and refrigerate until flavors combine, 2 to 3 hours."
        }
    ],
