{
    "title": "Air Fryer Pakoras",
    "Category": "Diwali Recipes",
    "Ingredients": [
        {
            "1": "2 cups chopped cauliflower"
        },
        {
            "2": "1 cup diced yellow potatoes"
        },
        {
            "3": "0.25 cups chickpea flour (besan)"
        },
        {
            "4": "0.75 cup water"
        },
        {
            "5": "0.5  red onion, chopped"
        },
        {
            "6": "1 tablespoon salt"
        },
        {
            "7": "1 clove garlic, minced"
        },
        {
            "8": "1 teaspoon curry powder"
        },
        {
            "9": "1 teaspoon coriander"
        },
        {
            "10": "0.5 teaspoon ground cayenne pepper, or more to taste"
        },
        {
            "11": "0.5 teaspoon cumin"
        },
        {
            "12": "1  serving cooking spray"
        }
    ],
    "Directions": [
        {
            "1": "Combine cauliflower, potatoes, chickpea flour, water, red onion, salt, garlic, curry powder, coriander, cayenne, and cumin in a large bowl. Set aside to rest for 10 minutes."
        },
        {
            "2": "Preheat air fryer to 350 degrees F (175 degrees C)."
        },
        {
            "3": "Spray the basket of the air fryer with cooking spray. Spoon 2 tablespoons of cauliflower mixture into the basket and flatten. Repeat this as many times as your basket space allows without the pakoras touching. Mist the top of each pakora with non-stick spray."
        },
        {
            "4": "Cook for 8 minutes. Flip and cook 8 additional minutes. Transfer to a paper towel-lined plate. Repeat with remaining batter."
        }
    ],
