{
    "title": "Buffalo Chicken Dip in the Slow Cooker",
    "Category": "Buffalo Chicken Dip",
    "Ingredients": [
        {
            "1": "2 cups diced, cooked rotisserie chicken"
        },
        {
            "2": "8 ounces cream cheese, softened"
        },
        {
            "3": "8 ounces shredded Colby-Jack Cheese"
        },
        {
            "4": "4 ounces honey-flavored goat cheese, softened"
        },
        {
            "5": "0.5 cup Buffalo wing sauce"
        },
        {
            "6": "0.5 cup blue cheese salad dressing"
        },
        {
            "7": "2 tablespoons chopped green bell pepper"
        },
        {
            "8": "1 tablespoon onion, chopped"
        },
        {
            "9": "1 tablespoon chopped celery"
        },
        {
            "10": "1 tablespoon chopped scallions for garnish (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Combine chicken, cream cheese, Colby-Jack cheese, goat cheese, Buffalo wing sauce, blue cheese dressing, bell pepper, onion, and celery in a slow cooker."
        },
        {
            "2": "Cook on Low for 4 hours. Transfer dip to a bowl and sprinkle with chopped scallions."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
