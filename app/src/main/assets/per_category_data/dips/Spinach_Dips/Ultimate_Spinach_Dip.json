{
    "title": "Ultimate Spinach Dip",
    "Category": "Spinach Dips",
    "Ingredients": [
        {
            "1": "1 bunch fresh spinach, stems removed, torn into small pieces"
        },
        {
            "2": "1 bunch green onions"
        },
        {
            "3": "1 (8 ounce) can water chestnuts, drained and sliced"
        },
        {
            "4": "1 (8 ounce) container sour cream"
        },
        {
            "5": "1.0 cups mayonnaise"
        },
        {
            "6": "1 (.4 ounce) packet dry vegetable soup mix"
        },
        {
            "7": "0.5 (1 ounce) package dry onion soup mix (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "In a medium bowl, toss together spinach, green onion stalks, and water chestnuts. Mix in the sour cream, mayonnaise, dry vegetable soup mix and dry onion soup mix."
        }
    ],
