{
    "title": "Spinach Artichoke Dip with Water Chestnuts",
    "Category": "Spinach Dips",
    "Ingredients": [
        {
            "1": "1 (14 ounce) can artichoke hearts, drained and chopped"
        },
        {
            "2": "0.5 (10 ounce) package frozen chopped spinach, thawed"
        },
        {
            "3": "0.5 (8 ounce) can sliced water chestnuts, drained and finely chopped"
        },
        {
            "4": "0.5 cup sour cream"
        },
        {
            "5": "0.25 cup mayonnaise"
        },
        {
            "6": "0.25 cup cream cheese"
        },
        {
            "7": "0.25 cup grated Romano cheese"
        },
        {
            "8": "0.25 teaspoon minced garlic"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 375 degrees F (190 degrees C)."
        },
        {
            "2": "Mix artichoke hearts, spinach, water chestnuts, sour cream, mayonnaise, cream cheese, Romano cheese, and garlic together in a small baking dish. Cover dish with aluminum foil."
        },
        {
            "3": "Bake in the preheated oven until cooked through and bubbling, about 25 minutes."
        }
    ],
