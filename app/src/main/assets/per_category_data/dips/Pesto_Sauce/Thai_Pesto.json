{
    "title": "Thai Pesto",
    "Category": "Pesto Sauce",
    "Ingredients": [
        {
            "1": "1 bunch cilantro"
        },
        {
            "2": "0.25 cup peanut butter"
        },
        {
            "3": "3 cloves garlic, minced"
        },
        {
            "4": "3 tablespoons extra-virgin olive oil"
        },
        {
            "5": "2 tablespoons minced fresh ginger"
        },
        {
            "6": "0.5 tablespoons fish sauce"
        },
        {
            "7": "1 tablespoon brown sugar"
        },
        {
            "8": "0.5 teaspoon cayenne pepper"
        }
    ],
    "Directions": [
        {
            "1": "Blend cilantro, peanut butter, garlic, olive oil, ginger, fish sauce, brown sugar, and cayenne pepper in a blender until smooth."
        }
    ],
