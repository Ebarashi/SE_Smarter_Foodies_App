{
    "title": "Homemade Best-O Pesto Ever",
    "Category": "Pesto Sauce",
    "Ingredients": [
        {
            "1": "0.25 cup pine nuts"
        },
        {
            "2": "0.25 cup almonds"
        },
        {
            "3": "3 cloves garlic"
        },
        {
            "4": "4 cups loosely packed fresh basil"
        },
        {
            "5": "0.25 cups olive oil, or more as needed"
        },
        {
            "6": "1 teaspoon salt"
        },
        {
            "7": "1 teaspoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Pulse pine nuts, almonds, and garlic in a food processor for about 20 seconds. Add basil, 1 cup olive oil, salt, and pepper. Blend for about 1 minute. Add additional olive oil until desired consistency is reached."
        }
    ],
