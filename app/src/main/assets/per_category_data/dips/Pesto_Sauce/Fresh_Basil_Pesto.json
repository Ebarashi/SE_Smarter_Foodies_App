{
    "title": "Fresh Basil Pesto",
    "Category": "Pesto Sauce",
    "Ingredients": [
        {
            "1": "2 cups packed fresh basil"
        },
        {
            "2": "0.5 cup extra-virgin olive oil"
        },
        {
            "3": "0.25 cup toasted pine nuts"
        },
        {
            "4": "3 cloves garlic, peeled"
        },
        {
            "5": "1 dash fresh lemon juice"
        },
        {
            "6": "0.5 cup freshly grated Pecorino Romano cheese"
        },
        {
            "7": "1 kosher salt and freshly ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine basil, oil, pine nuts, garlic, and lemon juice in a food processor or blender; puree until smooth. Stir in Pecorino Romano cheese, salt, and pepper."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Total Time": "10 mins"
        },
        {
            "Servings": "6 "
        },
        {
            "Yield": "1 cup"
        }
