{
    "title": "Easy Basil Pesto with Almonds",
    "Category": "Pesto Sauce",
    "Ingredients": [
        {
            "1": "2 cups fresh basil leaves"
        },
        {
            "2": "0.5 cup Parmesan cheese"
        },
        {
            "3": "0.25 cup whole raw almonds"
        },
        {
            "4": "0.25 cup extra-virgin olive oil, or more as needed"
        },
        {
            "5": "1 clove garlic, peeled"
        }
    ],
    "Directions": [
        {
            "1": "Combine basil, Parmesan cheese, almonds, olive oil, and garlic in a food processor. Pulse until blended. Pour in more olive oil, with the processor running, until pesto is thick and smooth."
        }
    ],
