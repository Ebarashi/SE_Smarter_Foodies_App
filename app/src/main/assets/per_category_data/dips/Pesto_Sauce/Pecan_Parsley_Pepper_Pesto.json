{
    "title": "Pecan Parsley Pepper Pesto",
    "Category": "Pesto Sauce",
    "Ingredients": [
        {
            "1": "2 cups flat leaf parsley with stems"
        },
        {
            "2": "2 cups shelled pecans"
        },
        {
            "3": "2 cups extra virgin olive oil"
        },
        {
            "4": "1 cup shaved Parmesan cheese"
        },
        {
            "5": "30  pitted kalamata olives"
        },
        {
            "6": "1  jalapeno pepper, seeded"
        },
        {
            "7": "3 cloves garlic"
        },
        {
            "8": "1  lemon, juiced and zested"
        }
    ],
    "Directions": [
        {
            "1": "Place the parsley, pecans, olive oil, Parmesan cheese, olives, jalapeno pepper, garlic, lemon juice, and lemon zest in the bowl of a blender or food processor, and pulse until smooth and creamy. Store in refrigerator."
        }
    ],
