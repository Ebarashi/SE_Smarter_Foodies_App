{
    "title": "Garlic Scape Pesto",
    "Category": "Pesto Sauce",
    "Ingredients": [
        {
            "1": "1 pound garlic scapes, cut into 2-inch pieces"
        },
        {
            "2": "0.25 cups grated Parmesan cheese"
        },
        {
            "3": "1 cup olive oil"
        },
        {
            "4": "1 tablespoon lemon juice"
        },
        {
            "5": "1 ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Blend garlic scapes, Parmesan cheese, olive oil, lemon juice, and pepper together in a food processor until smooth."
        }
    ],
