{
    "title": "Chef John's Garlic Scape Pesto",
    "Category": "Pesto Sauce",
    "Ingredients": [
        {
            "1": "1 cup roughly chopped garlic scapes"
        },
        {
            "2": "1  lemon, juiced"
        },
        {
            "3": "1 tablespoon white wine vinegar"
        },
        {
            "4": "0.5 teaspoon kosher salt, or to taste"
        },
        {
            "5": "0.25 cup olive oil"
        },
        {
            "6": "0.25 cup canola oil"
        },
        {
            "7": "0.5 cup toasted walnuts"
        }
    ],
    "Directions": [
        {
            "1": "Combine garlic scapes, lemon juice, vinegar, salt, olive oil, canola oil, and walnuts in a blender. Blend into a smooth paste. Adjust seasoning if needed."
        }
    ],
