{
    "title": "Faux Pesto",
    "Category": "Pesto Sauce",
    "Ingredients": [
        {
            "1": "4 cups fresh baby spinach"
        },
        {
            "2": "0.5 cup pecans"
        },
        {
            "3": "2 cloves garlic"
        },
        {
            "4": "1 cup Parmesan cheese"
        },
        {
            "5": "1 tablespoon lemon juice"
        },
        {
            "6": "0.5 cup extra virgin olive oil"
        },
        {
            "7": "1 pinch salt and freshly ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Mix the spinach, pecans, garlic, Parmesan cheese, and lemon juice in the bowl of a food processor; stream the olive oil into the mixture as it blends. Season with salt and pepper."
        }
    ],
