{
    "title": "Summer Herb Pesto",
    "Category": "Pesto Sauce",
    "Ingredients": [
        {
            "1": "2  garlic cloves, minced"
        },
        {
            "2": "1 cup fresh basil leaves"
        },
        {
            "3": "1 cup fresh flat-leaf parsley leaves"
        },
        {
            "4": "1 cup fresh spinach leaves"
        },
        {
            "5": "0.5 cup chopped fresh oregano"
        },
        {
            "6": "0.5 cup toasted pine nuts"
        },
        {
            "7": "0.5 cup grated Parmesan cheese"
        },
        {
            "8": "1 pinch salt and freshly ground black pepper to taste"
        },
        {
            "9": "0.75 cup olive oil"
        }
    ],
    "Directions": [
        {
            "1": "Blend garlic, basil, parsley, spinach, oregano, pine nuts, Parmesan cheese, salt, pepper, and olive oil in a blender or food processor until smooth."
        }
