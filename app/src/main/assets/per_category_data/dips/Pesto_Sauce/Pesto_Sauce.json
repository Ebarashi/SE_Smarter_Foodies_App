{
    "title": "Pesto Sauce",
    "Category": "Pesto Sauce",
    "Ingredients": [
        {
            "1": "3 cups fresh basil leaves"
        },
        {
            "2": "0.5 cups chopped walnuts"
        },
        {
            "3": "4 cloves garlic, peeled"
        },
        {
            "4": "0.25 cup grated Parmesan cheese"
        },
        {
            "5": "1 cup olive oil"
        },
        {
            "6": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Blend together basil leaves, nuts, garlic, and cheese in a food processor until finely chopped."
        },
        {
            "2": "Pour in oil slowly with the processor running. Season with salt and pepper."
        }
    ],
