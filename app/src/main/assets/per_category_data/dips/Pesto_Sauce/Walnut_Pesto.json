{
    "title": "Walnut Pesto",
    "Category": "Pesto Sauce",
    "Ingredients": [
        {
            "1": "2 cups basil leaves"
        },
        {
            "2": "0.5 cup walnuts"
        },
        {
            "3": "0.25 cup olive oil"
        },
        {
            "4": "2 cloves garlic"
        },
        {
            "5": "1 tablespoon lemon juice"
        }
    ],
    "Directions": [
        {
