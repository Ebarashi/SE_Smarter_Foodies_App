{
    "title": "Diane's No-Cook Red Hamburger Relish",
    "Category": "Relishes",
    "Ingredients": [
        {
            "1": "1 cup sweet green pickle relish"
        },
        {
            "2": "1 (5.5 ounce) can tomato paste"
        },
        {
            "3": "2 tablespoons jarred roasted red peppers with garlic, finely chopped"
        },
        {
            "4": "1 teaspoon white vinegar"
        },
        {
            "5": "0.5 teaspoon white sugar"
        },
        {
            "6": "6 drops liquid smoke flavoring"
        },
        {
            "7": "1 pinch ground black pepper"
        },
        {
            "8": "1 dash ground cinnamon"
        }
    ],
    "Directions": [
        {
            "1": "Mix relish, tomato paste, roasted red peppers, vinegar, sugar, liquid smoke, black pepper, and cinnamon together in a bowl. Spoon mixture into a clean glass jar and screw the lid on tightly. Store in the refrigerator."
        }
    ],
