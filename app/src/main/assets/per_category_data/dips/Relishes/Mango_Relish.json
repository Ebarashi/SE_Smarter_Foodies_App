{
    "title": "Mango Relish",
    "Category": "Relishes",
    "Ingredients": [
        {
            "1": "1  mango - peeled, seeded and diced"
        },
        {
            "2": "1 teaspoon extra virgin olive oil"
        },
        {
            "3": "0.5  red bell pepper, chopped"
        },
        {
            "4": "2  green onion, thinly sliced"
        },
        {
            "5": "1 tablespoon chopped cilantro"
        },
        {
            "6": "1  lime, juiced"
        },
        {
            "7": "0.25 teaspoon salt"
        },
        {
            "8": "1 pinch cracked black pepper"
        },
        {
            "9": "1 teaspoon honey"
        }
    ],
    "Directions": [
        {
            "1": "Combine mango, oil, bell pepper, scallions, cilantro, and lime juice in a small bowl. Stir to combine. Season with salt, pepper, and honey. Refrigerate or serve immediately."
        }
    ],
