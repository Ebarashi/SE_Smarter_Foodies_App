{
    "title": "Pool Hall Coleslaw",
    "Category": "Relishes",
    "Ingredients": [
        {
            "1": "3 cups distilled white vinegar"
        },
        {
            "2": "2 cups prepared yellow mustard"
        },
        {
            "3": "0.5 cups white sugar"
        },
        {
            "4": "0.5 cups packed light brown sugar"
        },
        {
            "5": "1 tablespoon ground turmeric"
        },
        {
            "6": "1 tablespoon salt"
        },
        {
            "7": "1 teaspoon ground black pepper"
        },
        {
            "8": "1 tablespoon red pepper flakes"
        },
        {
            "9": "16 cups shredded cabbage"
        },
        {
            "10": "3  carrots, shredded"
        },
        {
            "11": "3  green bell peppers, seeded and chopped"
        },
        {
            "12": "2 large onions, chopped"
        }
    ],
    "Directions": [
        {
            "1": "In a large stock pot, combine the vinegar, mustard, white sugar and brown sugar. Season with turmeric, salt, black pepper and red pepper flakes. Stir to dissolve sugar and bring to a boil."
        },
        {
            "2": "Once it is boiling, add the cabbage, carrots, peppers and onions. Return to a boil, then reduce heat to medium-low and simmer for 20 to 25 minutes, stirring frequently."
        }
    ],
