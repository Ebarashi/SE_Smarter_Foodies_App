{
    "title": "Sauerkraut Relish",
    "Category": "Relishes",
    "Ingredients": [
        {
            "1": "1 (20 ounce) can sauerkraut, drained"
        },
        {
            "2": "0.5 cup chopped green bell peppers"
        },
        {
            "3": "0.5 cup chopped red bell peppers"
        },
        {
            "4": "0.5 cup chopped celery"
        },
        {
            "5": "1 small onion, chopped"
        },
        {
            "6": "0.75 cup white vinegar"
        },
        {
            "7": "1 cup white sugar"
        }
    ],
    "Directions": [
        {
            "1": "In a medium bowl, mix together the sauerkraut, green peppers, red peppers, celery, onion, vinegar and sugar. Refrigerate 8 hours or overnight before serving."
        }
    ],
