{
    "title": "Chow Chow I",
    "Category": "Relishes",
    "Ingredients": [
        {
            "1": "6.0 pounds green tomatoes, chopped"
        },
        {
            "2": "8 large onions, chopped"
        },
        {
            "3": "10  green bell peppers, chopped"
        },
        {
            "4": "3 teaspoons salt"
        },
        {
            "5": "6  chopped green chile peppers"
        },
        {
            "6": "1 quart distilled white vinegar"
        },
        {
            "7": "0.75 cups white sugar"
        },
        {
            "8": "0.5 cup prepared horseradish"
        },
        {
            "9": "1 tablespoon ground cinnamon"
        },
        {
            "10": "1 tablespoon ground allspice"
        },
        {
            "11": "0.25 teaspoon ground cloves"
        }
    ],
    "Directions": [
        {
            "1": "In a large bowl combine tomatoes, onions, bell peppers and salt. Let stand overnight."
        },
        {
            "2": "Drain the tomato/pepper mixture and add the hot chile peppers, vinegar, sugar, and horseradish. Wrap the cinnamon, allspice, and cloves in cheesecloth or a porous bag, and add to tomato/pepper mixture."
        },
        {
            "3": "Boil for 15 minutes, or until tender."
        },
        {
            "4": "Pack tightly in sterilized jars and seal."
        }
    ],
