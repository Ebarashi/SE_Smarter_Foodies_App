{
    "title": "Pineapple Cranberry Relish",
    "Category": "Relishes",
    "Ingredients": [
        {
            "1": "1 (20 ounce) can crushed pineapple, drained"
        },
        {
            "2": "2 (16 ounce) cans whole cranberry sauce"
        },
        {
            "3": "0.5 cup chopped walnuts"
        },
        {
            "4": "1 (16 ounce) package frozen strawberries, thawed and drained"
        }
    ],
    "Directions": [
        {
            "1": "In a large bowl, mix together the pineapple, cranberry sauce, walnuts and strawberries. Cover and chill overnight before serving."
        }
