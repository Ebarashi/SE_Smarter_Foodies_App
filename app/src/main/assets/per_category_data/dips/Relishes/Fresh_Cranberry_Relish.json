{
    "title": "Fresh Cranberry Relish",
    "Category": "Relishes",
    "Ingredients": [
        {
            "1": "2 cups cranberries"
        },
        {
            "2": "1 (20 ounce) can crushed pineapple, drained"
        },
        {
            "3": "2  Granny Smith apples - peeled, cored and cut into wedges"
        },
        {
            "4": "2 (11 ounce) cans mandarin oranges, drained"
        },
        {
            "5": "1 teaspoon granulated sugar"
        }
    ],
    "Directions": [
        {
            "1": "In a food processor, combine cranberries, pineapple, apples and oranges. Blend into small chunks, add sugar if needed. Chill and serve."
        }
    ],
