{
    "title": "Beetroot Relish",
    "Category": "Relishes",
    "Ingredients": [
        {
            "1": "10 medium beets, scrubbed and chopped"
        },
        {
            "2": "1 cup chopped onion"
        },
        {
            "3": "1 cup chopped red bell pepper"
        },
        {
            "4": "1 stalk celery, chopped"
        },
        {
            "5": "2 tablespoons salt"
        },
        {
            "6": "0.5 cup white sugar"
        },
        {
            "7": "1 cup chopped cauliflower"
        },
        {
            "8": "3 tablespoons mustard seed"
        },
        {
            "9": "1 tablespoon celery seed"
        },
        {
            "10": "1 cup white vinegar"
        }
    ],
    "Directions": [
        {
            "1": "In a large pot, combine the beets, onion, red bell pepper, celery, sugar, and cauliflower. Season with salt, mustard seed, and celery seed, then stir in vinegar. Bring to a boil, then reduce heat to low."
        },
        {
            "2": "Simmer, stirring occasionally, until vegetables are tender, about 20 minutes."
        }
    ],
