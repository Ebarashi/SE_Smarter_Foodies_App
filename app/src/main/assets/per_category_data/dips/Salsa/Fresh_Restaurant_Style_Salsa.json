{
    "title": "Fresh Restaurant-Style Salsa",
    "Category": "Salsa",
    "Ingredients": [
        {
            "1": "1 (28 ounce) can Roma tomatoes, drained"
        },
        {
            "2": "4 small Roma tomatoes, quartered"
        },
        {
            "3": "1 cup chopped white onion"
        },
        {
            "4": "1 clove garlic, smashed"
        },
        {
            "5": "1 small bunch fresh cilantro, stems removed"
        },
        {
            "6": "1  serrano pepper, seeded and chopped"
        },
        {
            "7": "1 medium jalapeno pepper, seeded and chopped"
        },
        {
            "8": "1 tablespoon freshly squeezed lime juice"
        },
        {
            "9": "0.5 teaspoons kosher salt"
        },
        {
            "10": "0.25 teaspoon ground cumin"
        }
    ],
    "Directions": [
        {
            "1": "Combine canned and fresh tomatoes, onion, and garlic in a food processor; pulse 5 times to chop. Add cilantro, serrano and jalapeno peppers, lime juice, salt, and cumin and process for 10 seconds. Pulse further to desired consistency. Check and adjust seasoning as desired."
        }
    ],
