{
    "title": "Spicy Avocado Sauce",
    "Category": "Salsa",
    "Ingredients": [
        {
            "1": "5  fresh tomatillos, husks removed"
        },
        {
            "2": "4  serrano chile peppers, or to taste"
        },
        {
            "3": "2 tablespoons chopped fresh cilantro"
        },
        {
            "4": "1  ripe avocado - peeled, pitted, and quartered"
        },
        {
            "5": "1 salt to taste"
        }
    ],
    "Directions": [
        {
            "1": "Blend tomatillos, serrano chile peppers, and cilantro in a blender until smooth. Add 1 avocado quarter and continue blending until avocado is fully incorporated into mixture; repeat with each piece of avocado. Season with salt; continue processing until smooth. Serve immediately."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Total Time": "10 mins"
        },
        {
            "Servings": "6 "
        },
        {
            "Yield": "1 1/2 cups"
        }
    ],
