{
    "title": "Avocado, Tomato and Mango Salsa",
    "Category": "Salsa",
    "Ingredients": [
        {
            "1": "1  mango - peeled, seeded and diced"
        },
        {
            "2": "1  avocado - peeled, pitted, and diced"
        },
        {
            "3": "4 medium tomatoes, diced"
        },
        {
            "4": "1  jalapeno pepper, seeded and minced"
        },
        {
            "5": "0.5 cup chopped fresh cilantro"
        },
        {
            "6": "3 cloves garlic, minced"
        },
        {
            "7": "1 teaspoon salt"
        },
        {
            "8": "2 tablespoons fresh lime juice"
        },
        {
            "9": "0.25 cup chopped red onion"
        },
        {
            "10": "3 tablespoons olive oil"
        }
    ],
    "Directions": [
        {
            "1": "In a medium bowl, combine the mango, avocado, tomatoes, jalapeno, cilantro, and garlic. Stir in the salt, lime juice, red onion, and olive oil. To blend the flavors, refrigerate for about 30 minutes before serving."
        }
    ],
