{
    "title": "Chilean Pebre Sauce",
    "Category": "Salsa",
    "Ingredients": [
        {
            "1": "2  Roma tomatoes, quartered"
        },
        {
            "2": "1 bunch fresh cilantro (leaves and stems), chopped"
        },
        {
            "3": "6  scallions, chopped"
        },
        {
            "4": "5 cloves garlic"
        },
        {
            "5": "3 tablespoons red wine vinegar"
        },
        {
            "6": "2 tablespoons chile-garlic sauce (such as Huy Fong Foods\u00ae)"
        },
        {
            "7": "0.5 tablespoon olive oil"
        },
        {
            "8": "0.5 teaspoon garlic salt"
        }
    ],
    "Directions": [
        {
            "1": "Combine tomatoes, cilantro, scallions, garlic, red wine vinegar, chile-garlic sauce, olive oil, and garlic salt in a food processor or blender; process to the desired consistency. Refrigerate for 2 hours to let the flavors blend."
        }
    ],
