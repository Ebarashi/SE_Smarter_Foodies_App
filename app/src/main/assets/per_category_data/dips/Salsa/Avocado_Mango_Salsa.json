{
    "title": "Avocado Mango Salsa",
    "Category": "Salsa",
    "Ingredients": [
        {
            "1": "1  avocado - peeled, pitted and diced"
        },
        {
            "2": "1  lime, juiced"
        },
        {
            "3": "1  mango - peeled, seeded and diced"
        },
        {
            "4": "1 small red onion, chopped"
        },
        {
            "5": "1  habanero pepper, seeded and chopped"
        },
        {
            "6": "1 tablespoon chopped fresh cilantro"
        },
        {
            "7": "1 salt to taste"
        }
    ],
    "Directions": [
        {
            "1": "Place the avocado in a serving bowl, and mix with the lime juice. Mix in the mango, onion, habanero pepper, cilantro and salt."
        }
    ],
