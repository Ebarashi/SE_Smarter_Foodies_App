{
    "title": "Cool Cucumber Salsa",
    "Category": "Salsa",
    "Ingredients": [
        {
            "1": "2 medium cucumbers - peeled, seeded, and chopped"
        },
        {
            "2": "2 medium tomatoes, chopped"
        },
        {
            "3": "0.5 cup chopped green bell pepper"
        },
        {
            "4": "1  jalapeno pepper, seeded and minced"
        },
        {
            "5": "1 small onion, chopped"
        },
        {
            "6": "1 clove garlic, minced"
        },
        {
            "7": "2 tablespoons lime juice"
        },
        {
            "8": "1 teaspoon minced fresh parsley"
        },
        {
            "9": "2 teaspoons minced fresh cilantro"
        },
        {
            "10": "0.5 teaspoon dried dill weed"
        },
        {
            "11": "0.5 teaspoon salt"
        },
        {
            "12": "1 (12 ounce) package tortilla chips"
        }
    ],
    "Directions": [
        {
            "1": "In a medium bowl, stir together the cucumbers, tomatoes, green pepper, jalapeno pepper, onion, garlic, lime juice, parsley, cilantro, dill, and salt. Cover and refrigerate for 1 hour. Serve with tortilla chips."
        }
    ],
