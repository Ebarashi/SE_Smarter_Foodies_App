{
    "title": "Fresh Homemade Salsa",
    "Category": "Salsa",
    "Ingredients": [
        {
            "1": "2 cups chopped tomatoes"
        },
        {
            "2": "0.25 cup chopped red onion"
        },
        {
            "3": "0.25 cup chopped yellow onion"
        },
        {
            "4": "2 tablespoons canned green chilies"
        },
        {
            "5": "2 tablespoons fresh lime juice"
        },
        {
            "6": "2 tablespoons chopped fresh cilantro"
        },
        {
            "7": "2 cloves garlic, peeled"
        },
        {
            "8": "1 teaspoon ground cumin"
        },
        {
            "9": "0.25 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Combine tomatoes, red onion, yellow onion, green chilies, lime juice, cilantro, garlic, cumin, and salt in a food processor. Pulse until mixture is combined but still chunky."
        },
        {
            "2": "Transfer salsa to a bowl, cover with plastic wrap, and refrigerate at least 1 hour before serving."
        }
    ],
