{
    "title": "Fresh Mango Salsa",
    "Category": "Salsa",
    "Ingredients": [
        {
            "1": "2 cups diced Roma tomatoes"
        },
        {
            "2": "0.5 cups diced mango"
        },
        {
            "3": "0.5 cup diced onion"
        },
        {
            "4": "1 teaspoon white sugar"
        },
        {
            "5": "0.5 cup chopped fresh cilantro"
        },
        {
            "6": "2 tablespoons fresh lime juice"
        },
        {
            "7": "1 tablespoon cider vinegar"
        },
        {
            "8": "0.5 teaspoon salt"
        },
        {
            "9": "0.5 teaspoon black pepper"
        },
        {
            "10": "2 cloves garlic, minced"
        }
    ],
    "Directions": [
        {
            "1": "Stir the tomatoes, mango, onion, sugar, cilantro, lime juice, cider vinegar, salt, pepper, and garlic together in a bowl; refrigerate 1 hour before serving."
        }
    ],
