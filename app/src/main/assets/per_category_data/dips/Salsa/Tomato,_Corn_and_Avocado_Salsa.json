{
    "title": "Tomato, Corn and Avocado Salsa",
    "Category": "Salsa",
    "Ingredients": [
        {
            "1": "1 (11 ounce) can whole kernel corn, drained"
        },
        {
            "2": "1 (4 ounce) can sliced black olives, drained"
        },
        {
            "3": "0.5 cups diced roma tomatoes"
        },
        {
            "4": "0.75 cup diced red onion"
        },
        {
            "5": "1  red bell pepper, seeded and diced"
        },
        {
            "6": "0.5 teaspoons minced jalapeno pepper"
        },
        {
            "7": "1  avocados - peeled, pitted and diced"
        },
        {
            "8": "2 tablespoons olive oil"
        },
        {
            "9": "2 tablespoons fresh lime juice"
        },
        {
            "10": "1 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Mix together corn, olives, tomatoes, onion, red pepper, and jalapeno pepper in a large bowl. Gently fold in diced avocado, olive oil, lime juice, and salt."
        }
    ],
