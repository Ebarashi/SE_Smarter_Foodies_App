{
    "title": "Jen's Fresh and Spicy Salsa",
    "Category": "Salsa",
    "Ingredients": [
        {
            "1": "5  roma (plum) tomatoes, seeded and chopped"
        },
        {
            "2": "10  green onions"
        },
        {
            "3": "2  fresh jalapeno peppers, seeded"
        },
        {
            "4": "0.25 cup chopped fresh cilantro"
        },
        {
            "5": "2 tablespoons fresh lime juice"
        },
        {
            "6": "2 tablespoons hot pepper sauce"
        },
        {
            "7": "1 teaspoon ground black pepper"
        },
        {
            "8": "1 teaspoon garlic powder"
        },
        {
            "9": "1 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "In a blender or food processor, pulse the tomatoes, green onions, jalapeno peppers, and cilantro to desired consistency. Transfer to a bowl, and mix in the lime juice, hot pepper sauce, black pepper, garlic powder, and salt."
        }
    ],
