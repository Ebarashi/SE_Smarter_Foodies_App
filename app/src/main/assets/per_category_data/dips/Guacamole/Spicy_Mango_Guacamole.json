{
    "title": "Spicy Mango Guacamole",
    "Category": "Guacamole",
    "Ingredients": [
        {
            "1": "2  ripe avocados, chopped"
        },
        {
            "2": "1 small lime, juiced"
        },
        {
            "3": "0.25 teaspoon salt"
        },
        {
            "4": "1  ripe mango, chopped"
        },
        {
            "5": "0.5 cup finely chopped white onion"
        },
        {
            "6": "0.5 cup fresh cilantro, finely chopped"
        },
        {
            "7": "2  jalapeno peppers, seeded and chopped"
        },
        {
            "8": "0.5 teaspoon hot sauce, or to taste"
        }
    ],
    "Directions": [
        {
            "1": "Mash avocados, lime juice, and salt lightly together in a bowl, so some avocado chunks remain. Mix in mango, onion, cilantro, and jalapeno peppers."
        },
        {
            "2": "Refrigerate guacamole for about 1 hour. Drizzle hot sauce on top before serving."
        }
    ],
