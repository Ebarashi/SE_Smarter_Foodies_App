{
    "title": "Basic Guacamole Dip",
    "Category": "Guacamole",
    "Ingredients": [
        {
            "1": "4 medium ripe avocados - peeled, pitted, and mashed"
        },
        {
            "2": "2 medium tomatoes, diced"
        },
        {
            "3": "2 tablespoons minced onion"
        },
        {
            "4": "1 tablespoon lemon juice"
        }
