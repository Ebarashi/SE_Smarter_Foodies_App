{
    "title": "Fall in Love (with) Guacamole",
    "Category": "Guacamole",
    "Ingredients": [
        {
            "1": "3  avocados, peeled and pitted"
        },
        {
            "2": "1 teaspoon sea salt"
        },
        {
            "3": "1 teaspoon garlic powder"
        },
        {
            "4": "2 tablespoons fresh lime juice"
        },
        {
            "5": "0.5 cup diced onion"
        },
        {
            "6": "2  roma (plum) tomatoes, diced"
        },
        {
            "7": "3 tablespoons chopped cilantro"
        },
        {
            "8": "1 pinch cayenne pepper, or more to taste (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Mash avocados, sea salt, garlic powder, and lime juice in a bowl using a fork."
        },
        {
            "2": "Mix onion, tomatoes, and cilantro into avocado mixture; season with cayenne pepper."
        }
    ],
