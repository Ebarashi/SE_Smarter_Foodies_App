{
    "title": "Peach Guacamole",
    "Category": "Guacamole",
    "Ingredients": [
        {
            "1": "3 medium avocados, peeled and pitted"
        },
        {
            "2": "2 teaspoons lime juice"
        },
        {
            "3": "0.5 teaspoon ancho chile powder"
        },
        {
            "4": "1 pinch salt to taste"
        },
        {
            "5": "1 large fresh peach - peeled, pitted, and diced"
        },
        {
            "6": "2 tablespoons minced red onion"
        },
        {
            "7": "0.5 small jalapeno pepper, seeded and minced"
        },
        {
            "8": "2 tablespoons minced cilantro"
        }
    ],
    "Directions": [
        {
            "1": "Mash avocados in a small bowl. Mix in lime juice, ancho chile powder, and salt, leaving chunks if desired. Stir in peach, red onion, jalapeno, and cilantro. Serve immediately."
        }
    ],
