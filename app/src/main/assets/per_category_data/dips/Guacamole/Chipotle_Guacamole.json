{
    "title": "Chipotle Guacamole",
    "Category": "Guacamole",
    "Ingredients": [
        {
            "1": "2  avocados, peeled, seeded and cubed"
        },
        {
            "2": "1 tablespoon fresh lime juice"
        },
        {
            "3": "2 tablespoons sour cream"
        },
        {
            "4": "0.25 cup salsa, or to taste"
        },
        {
            "5": "0.25 teaspoon adobo sauce from canned chilies, or to taste"
        },
        {
            "6": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Mash together cubed avocado, lime juice, sour cream, salsa, and adobo sauce in a bowl until blended. Season with salt and pepper."
        }
    ],
