{
    "title": "Guacamole with Nopales (Mexican Cactus)",
    "Category": "Guacamole",
    "Ingredients": [
        {
            "1": "0.5 cup diced nopales (Mexican cactus)"
        },
        {
            "2": "1 large ripe avocado, halved and pitted"
        },
        {
            "3": "0.5  lime, juiced"
        },
        {
            "4": "1 tablespoon olive oil"
        },
        {
            "5": "1 salt to taste"
        },
        {
            "6": "1  plum tomato, seeded and chopped"
        },
        {
            "7": "1  fresh jalapeno pepper - seeded, deveined, and minced, or to taste"
        },
        {
            "8": "4 sprigs fresh cilantro, chopped"
        }
    ],
    "Directions": [
        {
            "1": "Heat a small nonstick skillet over medium heat. Add nopales and cook, stirring occasionally, until soft, about 10 minutes. Let cool."
        },
        {
            "2": "Mash avocado in a bowl with lime juice, olive oil, and salt. Stir in tomato, jalapeno, cilantro, and cooked nopales."
        }
    ],
