{
    "title": "Corn and Black Bean Guacamole",
    "Category": "Guacamole",
    "Ingredients": [
        {
            "1": "0.5  red bell pepper, minced"
        },
        {
            "2": "0.5  red onion, minced"
        },
        {
            "3": "2 cloves garlic, minced"
        },
        {
            "4": "0.5 (15.25 ounce) can sweet corn, drained"
        },
        {
            "5": "0.5 (15 ounce) can black beans, drained and rinsed"
        },
        {
            "6": "3  ripe avocados, peeled, pitted and sliced"
        },
        {
            "7": "2 tablespoons fresh lime juice"
        },
        {
            "8": "0.25 teaspoon salt"
        },
        {
            "9": "1 pinch ground black pepper, or to taste"
        },
        {
            "10": "0.5 cup chopped fresh cilantro, or more to taste"
        }
    ],
    "Directions": [
        {
            "1": "Stir red bell pepper, red onion, garlic, sweet corn, black beans, and avocado slices together in a large bowl."
        },
        {
            "2": "Mix lime juice, salt, black pepper, and cilantro into avocado mixture until desired texture is achieved."
        }
    ],
