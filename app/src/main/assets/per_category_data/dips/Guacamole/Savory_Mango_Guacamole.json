{
    "title": "Savory Mango Guacamole",
    "Category": "Guacamole",
    "Ingredients": [
        {
            "1": "3 large avocados, peeled and pitted"
        },
        {
            "2": "1 small red onion, chopped"
        },
        {
            "3": "1  mango - peeled, pitted, and chopped"
        },
        {
            "4": "1  lime, juiced"
        },
        {
            "5": "1 tablespoon minced garlic"
        },
        {
            "6": "1 tablespoon salt"
        },
        {
            "7": "3 tablespoons chopped fresh cilantro"
        }
    ],
    "Directions": [
        {
            "1": "Mash avocados in a medium bowl to a lumpy consistency. Mix onion, mango, lime juice, garlic, salt, and cilantro into avocado."
        }
    ],
