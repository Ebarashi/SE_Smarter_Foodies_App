{
    "title": "Guacamole with Corn",
    "Category": "Guacamole",
    "Ingredients": [
        {
            "1": "3 large avocados - peeled, pitted, and mashed"
        },
        {
            "2": "1  lime, juiced"
        },
        {
            "3": "1 salt to taste"
        },
        {
            "4": "1 large plum tomato, seeded and chopped"
        },
        {
            "5": "0.5  white onion, chopped"
        },
        {
            "6": "1  serrano chile, seeded and finely chopped"
        },
        {
            "7": "2 tablespoons chopped fresh cilantro"
        },
        {
            "8": "1 tablespoon olive oil"
        },
        {
            "9": "1 (8.75 ounce) can whole kernel corn, drained and rinsed"
        }
    ],
    "Directions": [
        {
            "1": "Mash avocados, lime juice, and salt together in a medium bowl. Mix in tomato, onion, serrano chile, and olive oil. Stir in corn kernels. Serve immediately."
        }
    ],
