{
    "title": "Simple Beef-Flavored Gravy",
    "Category": "Gravy",
    "Ingredients": [
        {
            "1": "0.25 cup butter"
        },
        {
            "2": "0.25 cup all-purpose flour"
        },
        {
            "3": "1  onion, chopped"
        },
        {
            "4": "0.5 cups water"
        },
        {
            "5": "3 teaspoons beef bouillon"
        }
    ],
    "Directions": [
        {
            "1": "Melt butter in a small saucepan over medium heat. Stir in flour to combine and add onion. Pour in water and season with bouillon."
        },
        {
            "2": "Bring to a boil over medium heat and cook until thickened."
        }
    ],
