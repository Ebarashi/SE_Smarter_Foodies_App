{
    "title": "Buttermilk Sausage Gravy",
    "Category": "Gravy",
    "Ingredients": [
        {
            "1": "1 (8 ounce) package pork link sausages (such as Farmer John(R))"
        },
        {
            "2": "0.25 cup bacon drippings"
        },
        {
            "3": "0.25 cup all-purpose flour"
        },
        {
            "4": "1.0 cups buttermilk"
        },
        {
            "5": "1 salt and ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Place sausages and bacon drippings in a skillet and cook over medium heat, breaking up sausage into bite-sized pieces, until golden brown, 4 to 6 minutes. Add flour, stirring constantly. Slowly add buttermilk and cook and stir until gravy has thickened, 3 to 5 minutes."
        }
    ],
