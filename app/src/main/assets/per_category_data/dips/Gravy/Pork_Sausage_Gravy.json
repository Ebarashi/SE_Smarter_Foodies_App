{
    "title": "Pork Sausage Gravy",
    "Category": "Gravy",
    "Ingredients": [
        {
            "1": "2 pounds fresh, ground pork sausage"
        },
        {
            "2": "0.5 cup all-purpose flour, or as needed"
        },
        {
            "3": "2 cups milk, or as needed"
        },
        {
            "4": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Break up sausage in a skillet and brown. Remove from the skillet and set aside."
        },
        {
            "2": "Add flour and stir until all drippings are absorbed, adding more flour if necessary."
        },
        {
            "3": "Stir in milk a little at a time until creamy. Add cooked sausage back to the skillet and season with salt and pepper."
        }
    ],
