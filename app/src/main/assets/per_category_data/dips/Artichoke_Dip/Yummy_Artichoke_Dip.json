{
    "title": "Yummy Artichoke Dip",
    "Category": "Artichoke Dip",
    "Ingredients": [
        {
            "1": "1 (6.5 ounce) jar marinated artichoke hearts, drained and quartered"
        },
        {
            "2": "1 cup mayonnaise"
        },
        {
            "3": "0.5 cups grated Parmesan cheese"
        },
        {
            "4": "1 (8 ounce) package cream cheese, softened"
        },
        {
            "5": "1 (4 ounce) can chopped green chile peppers"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "In a medium bowl, mix artichoke hearts, mayonnaise, 1 cup Parmesan cheese, cream cheese, and green chile peppers. Scoop mixture into a pie pan or medium baking pan. Top with remaining 1/2 cup of Parmesan cheese."
        },
        {
            "3": "Bake for 25 minutes or until bubbly and slightly browned. Serve warm."
        }
    ],
