{
    "title": "Dijon-Style Artichoke Dip",
    "Category": "Artichoke Dip",
    "Ingredients": [
        {
            "1": "1 (6.5 ounce) jar marinated artichoke hearts, chopped, liquid reserved"
        },
        {
            "2": "1 (8 ounce) package cream cheese, softened"
        },
        {
            "3": "0.25 cup prepared Dijon-style mustard"
        }
    ],
    "Directions": [
        {
            "1": "In a medium bowl, blend reserved liquid from marinated artichoke hearts, cream cheese and prepared Dijon-style mustard. When smooth, stir in marinated artichoke hearts. Chill approximately 1 hour before serving."
        }
    ],
