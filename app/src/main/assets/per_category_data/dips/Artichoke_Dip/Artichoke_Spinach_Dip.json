{
    "title": "Artichoke Spinach Dip",
    "Category": "Artichoke Dip",
    "Ingredients": [
        {
            "1": "0.25 cup butter"
        },
        {
            "2": "1 (10 ounce) package frozen chopped spinach, partially thawed"
        },
        {
            "3": "1 (14 ounce) can artichoke hearts, drained and chopped"
        },
        {
            "4": "2 (8 ounce) packages cream cheese, softened"
        },
        {
            "5": "2  (16 ounce) containers sour cream"
        },
        {
            "6": "1 cup grated Parmesan cheese"
        },
        {
            "7": "1 garlic salt to taste"
        }
    ],
    "Directions": [
        {
            "1": "Melt butter in a large saucepan over medium heat. Stir in the spinach and artichoke hearts. Cook until tender, about 5 minutes."
        },
        {
            "2": "Mix cream cheese and sour cream into the spinach mixture. Stir in Parmesan cheese and garlic salt. Cook, stirring occasionally, until thickened, 10 to 15 minutes. Serve warm."
        }
    ],
