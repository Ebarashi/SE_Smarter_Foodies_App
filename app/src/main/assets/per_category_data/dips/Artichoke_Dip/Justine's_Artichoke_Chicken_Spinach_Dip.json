{
    "title": "Justine's Artichoke Chicken Spinach Dip",
    "Category": "Artichoke Dip",
    "Ingredients": [
        {
            "1": "1 (10 ounce) box frozen chopped spinach, thawed and drained"
        },
        {
            "2": "1 (13.75 ounce) can  artichoke hearts, drained"
        },
        {
            "3": "1 (12.5 fl oz) can chunk chicken, drained"
        },
        {
            "4": "1 cup mayonnaise"
        },
        {
            "5": "2 cups shredded mozzarella cheese"
        },
        {
            "6": "2 cloves garlic, crushed"
        },
        {
            "7": "1 teaspoon liquid smoke flavoring"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Combine the spinach, artichokes, chicken, mayonnaise, cheese, garlic, and liquid smoke in a large bowl; mix well. Spoon mixture into a glass baking dish."
        },
        {
            "3": "Bake in preheated oven until the edges turn golden brown, about 30 minutes."
        }
    ],
