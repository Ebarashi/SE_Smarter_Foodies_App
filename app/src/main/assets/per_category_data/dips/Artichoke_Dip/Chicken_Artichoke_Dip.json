{
    "title": "Chicken Artichoke Dip",
    "Category": "Artichoke Dip",
    "Ingredients": [
        {
            "1": "1 (5 ounce) can chunk chicken, drained and flaked"
        },
        {
            "2": "1 (14 ounce) can artichoke hearts, drained and chopped"
        },
        {
            "3": "1 (.7 ounce) package dry Italian-style salad dressing mix"
        },
        {
            "4": "1 (8 ounce) container sour cream"
        },
        {
            "5": "0.25 cup mayonnaise"
        },
        {
            "6": "1 cup Havarti cheese"
        },
        {
            "7": "0.25 cup finely shredded Parmesan cheese"
        },
        {
            "8": "0.25 cup sliced almonds"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "In a medium bowl, blend chicken, artichoke, Italian-style dressing mix, sour cream, mayonnaise, 1/2 cup Havarti, and Parmesan. Transfer to a small baking dish. Top with remaining Havarti and almonds."
        },
        {
            "3": "Bake 25 minutes in the preheated oven, or until bubbly and lightly browned."
        }
    ],
