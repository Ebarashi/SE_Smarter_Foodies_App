{
    "title": "TriBeCa Artichoke Dip",
    "Category": "Artichoke Dip",
    "Ingredients": [
        {
            "1": "0.5 cups quartered artichoke hearts in water, drained and chopped"
        },
        {
            "2": "0.75 cup mayonnaise"
        },
        {
            "3": "0.75 cup shredded mozzarella cheese"
        },
        {
            "4": "0.5  white onion, chopped"
        },
        {
            "5": "0.5 cup grated Parmesan cheese"
        },
        {
            "6": "0.25 cup chopped pickled jalapeno peppers"
        },
        {
            "7": "1 teaspoon salt, or to taste"
        },
        {
            "8": "1 teaspoon ground black pepper, or to taste"
        },
        {
            "9": "2 pinches paprika"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Mix artichoke hearts, mayonnaise, mozzarella cheese, white onion, Parmesan cheese, jalapeno peppers, salt, and black pepper in a bowl; spread into a baking dish. Sprinkle paprika over the artichoke mixture."
        },
        {
            "3": "Bake in preheated oven until hot and beginning to set, about 25 minutes. Cool at least 15 minutes before serving."
        }
    ],
