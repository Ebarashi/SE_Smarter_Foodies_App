{
    "title": "Luscious Spinach Artichoke Dip",
    "Category": "Artichoke Dip",
    "Ingredients": [
        {
            "1": "1 (14 ounce) can artichoke hearts, drained and chopped"
        },
        {
            "2": "0.5 (10 ounce) package frozen chopped spinach, thawed"
        },
        {
            "3": "0.5 cup sour cream"
        },
        {
            "4": "0.25 cup mayonnaise"
        },
        {
            "5": "0.25 cup cream cheese"
        },
        {
            "6": "0.25 cup grated Romano cheese"
        },
        {
            "7": "0.25 teaspoon minced garlic"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 375 degrees F (190 degrees C)."
        },
        {
            "2": "In a small baking dish, mix together artichoke hearts, spinach, sour cream, mayonnaise, cream cheese, Romano cheese, and garlic. Cover dish."
        },
        {
            "3": "Bake until heated through and bubbly, about 25 minutes."
        }
    ],
    "Details": [
