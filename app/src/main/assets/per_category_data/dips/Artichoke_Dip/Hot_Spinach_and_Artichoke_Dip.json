{
    "title": "Hot Spinach and Artichoke Dip",
    "Category": "Artichoke Dip",
    "Ingredients": [
        {
            "1": "8 slices bacon"
        },
        {
            "2": "1 (14 ounce) can quartered marinated artichoke hearts, drained"
        },
        {
            "3": "1 (10 ounce) package frozen chopped spinach, thawed and drained"
        },
        {
            "4": "1 cup grated Parmesan cheese"
        },
        {
            "5": "1 (8 ounce) container sour cream"
        },
        {
            "6": "1 (5 ounce) container garlic-herb flavored cheese spread"
        },
        {
            "7": "0.5 cup mayonnaise"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 400 degrees F (200 degrees C)."
        },
        {
            "2": "Cook bacon in a large, deep skillet over medium-high heat until browned and crispy, 5 to 7 minutes. Drain on paper towels and crumble when cool enough to handle."
        },
        {
            "3": "Combine artichoke hearts, spinach, Parmesan, sour cream, cheese spread, and mayonnaise in a mixing bowl. Add crumbled bacon and mix thoroughly. Spread mixture into a 7x11-inch baking dish."
        },
        {
            "4": "Bake in the preheated oven until bubbly and lightly browned, about 20 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": "20 mins"
        },
        {
            "Total Time": "30 mins"
        },
        {
