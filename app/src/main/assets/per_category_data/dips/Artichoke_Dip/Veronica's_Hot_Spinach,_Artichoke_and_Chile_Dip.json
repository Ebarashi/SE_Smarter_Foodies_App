{
    "title": "Veronica's Hot Spinach, Artichoke and Chile Dip",
    "Category": "Artichoke Dip",
    "Ingredients": [
        {
            "1": "2 (8 ounce) packages cream cheese, softened"
        },
        {
            "2": "0.5 cup mayonnaise"
        },
        {
            "3": "1 (4.5 ounce) can chopped green chiles, drained"
        },
        {
            "4": "1 cup freshly grated Parmesan cheese"
        },
        {
            "5": "1 (12 ounce) jar marinated artichoke hearts, drained and chopped"
        },
        {
            "6": "0.25 cup canned chopped jalapeno peppers, drained"
        },
        {
            "7": "1 (10 ounce) box frozen chopped spinach, thawed and drained"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Mix together the cream cheese and mayonnaise in a bowl until smooth. Stir in the green chiles, Parmesan cheese, artichokes, peppers, and spinach. Spoon the mixture into a baking dish."
        },
        {
            "3": "Bake in preheated oven until slightly browned, about 30 minutes."
        }
    ],
