{
    "title": "Skinny Spinach and Artichoke Dip",
    "Category": "Artichoke Dip",
    "Ingredients": [
        {
            "1": "2 (6.5 ounce) jars marinated artichoke hearts, drained and chopped"
        },
        {
            "2": "1 (10 ounce) package frozen chopped spinach, thawed and drained"
        },
        {
            "3": "1 (8 ounce) package cream cheese, softened"
        },
        {
            "4": "1 cup fat-free sour cream"
        },
        {
            "5": "1 cup shredded Romano cheese"
        },
        {
            "6": "0.5 cup shredded Parmesan cheese"
        },
        {
            "7": "0.25 cup roasted red peppers, drained and chopped"
        },
        {
            "8": "2 cloves garlic, minced"
        },
        {
            "9": "0.5 teaspoon lemon juice"
        },
        {
            "10": "0.5 cup coarsely crushed garlic croutons"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 375 degrees F (190 degrees C)."
        },
        {
            "2": "Mix artichoke hearts, spinach, cream cheese, sour cream, Romano cheese, Parmesan cheese, roasted peppers, garlic, and lemon juice together in a baking dish and top with croutons."
        },
        {
            "3": "Bake in the preheated oven until bubbly and croutons are browned, about 35 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
