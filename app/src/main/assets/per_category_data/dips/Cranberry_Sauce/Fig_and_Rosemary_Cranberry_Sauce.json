{
    "title": "Fig and Rosemary Cranberry Sauce",
    "Category": "Cranberry Sauce",
    "Ingredients": [
        {
            "1": "1 cup water"
        },
        {
            "2": "1 cup dark brown sugar"
        },
        {
            "3": "2 teaspoons maple extract"
        },
        {
            "4": "1 (12 ounce) package fresh cranberries"
        },
        {
            "5": "10  dried Mission figs, chopped"
        },
        {
            "6": "1 (5 inch) fresh rosemary sprig"
        }
    ],
    "Directions": [
        {
            "1": "Bring water, brown sugar, and maple extract to a boil in a non-reactive saucepan. Add cranberries and figs. Cook over medium heat, stirring frequently, until slightly thickened, about 5 minutes. Add rosemary and cook until sauce is thickened, about 5 minutes more."
        },
        {
            "2": "Carefully remove rosemary sprig and discard. Remove pot from the burner and let cool, about 10 minutes. Pour sauce into a bowl and refrigerate until ready to serve."
        }
    ],
