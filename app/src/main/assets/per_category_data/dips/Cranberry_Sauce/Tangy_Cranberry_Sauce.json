{
    "title": "Tangy Cranberry Sauce",
    "Category": "Cranberry Sauce",
    "Ingredients": [
        {
            "1": "4 cups fresh cranberries"
        },
        {
            "2": "2 cups water"
        },
        {
            "3": "0.5 cups white sugar"
        },
        {
            "4": "1 tablespoon prepared horseradish"
        }
    ],
    "Directions": [
        {
            "1": "Place cranberries and water in a medium saucepan. Over high heat, cook until the cranberries burst."
        },
        {
            "2": "Lower heat to simmer. Stir in sugar. Continue cooking 15 minutes."
        },
        {
            "3": "Allow mixture to cool. Stir in horseradish. Chill in the refrigerator until serving."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
