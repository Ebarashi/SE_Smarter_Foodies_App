{
    "title": "Amazing Pineapple Cranberry Sauce",
    "Category": "Cranberry Sauce",
    "Ingredients": [
        {
            "1": "1 (16 ounce) can pineapple chunks with juice"
        },
        {
            "2": "1 water as needed"
        },
        {
            "3": "1 cup white sugar"
        },
        {
            "4": "1 (12 ounce) bag fresh cranberries"
        }
    ],
    "Directions": [
        {
            "1": "Drain juice from canned pineapple into a measuring cup. Fill the measuring cup with enough water to equal 1 cup liquid altogether."
        },
        {
            "2": "Stir pineapple juice mixture and sugar together in a saucepan; bring to a boil, stirring constantly. Add cranberries and return to a boil. Reduce heat to low and simmer until cranberries burst, about 10 minutes."
        },
        {
            "3": "Mix pineapple chunks into cranberry mixture. Cool to room temperature before serving."
        }
    ],
