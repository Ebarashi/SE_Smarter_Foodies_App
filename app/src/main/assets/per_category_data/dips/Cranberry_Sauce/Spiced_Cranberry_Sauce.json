{
    "title": "Spiced Cranberry Sauce",
    "Category": "Cranberry Sauce",
    "Ingredients": [
        {
            "1": "0.75 cup orange juice"
        },
        {
            "2": "0.25 cup water"
        },
        {
            "3": "1 cup white sugar"
        },
        {
            "4": "1  cinnamon stick"
        },
        {
            "5": "1 dash ground cloves"
        },
        {
            "6": "1 dash ground nutmeg"
        },
        {
            "7": "1 dash ground allspice"
        },
        {
            "8": "1 (12 ounce) bag fresh cranberries"
        }
    ],
    "Directions": [
        {
            "1": "Heat orange juice and water in a saucepan over medium heat until warm, about 3 minutes. Add sugar and stir until dissolved. Mix in cinnamon stick, cloves, nutmeg, and allspice. Add cranberries and cook, stirring occasionally, until they pop, about 10 minutes."
        },
        {
            "2": "Remove from heat and pour sauce into a bowl. Cover and refrigerate, 8 hours to overnight."
        }
    ],
