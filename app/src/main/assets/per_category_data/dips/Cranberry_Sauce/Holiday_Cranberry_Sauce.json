{
    "title": "Holiday Cranberry Sauce",
    "Category": "Cranberry Sauce",
    "Ingredients": [
        {
            "1": "3  cinnamon sticks"
        },
        {
            "2": "5  whole cloves"
        },
        {
            "3": "5  whole allspice berries"
        },
        {
            "4": "4 cups fresh cranberries"
        },
        {
            "5": "0.5 cups water"
        },
        {
            "6": "2 cups white sugar"
        }
    ],
    "Directions": [
        {
            "1": "Wrap cinnamon sticks, cloves, and allspice berries in a spice bag."
        },
        {
            "2": "Combine cranberries, water, and spice bag in a medium saucepan over medium heat. Cook until cranberries begin to burst, about 10 minutes."
        },
        {
            "3": "Stir in sugar and reduce the heat to low. Continue cooking until sugar dissolves, about 5 minutes. Remove from the heat and let cool to room temperature, about 30 minutes."
        },
        {
            "4": "Remove and discard spice bag. Cover cranberry sauce and chill in the refrigerator before serving, 8 hours to overnight."
        }
    ],
    "Details": [
