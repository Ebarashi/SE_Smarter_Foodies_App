{
    "title": "Easy Cranberry Applesauce",
    "Category": "Cranberry Sauce",
    "Ingredients": [
        {
            "1": "2 pounds apples - peeled, cored, and cut into 1-inch chunks"
        },
        {
            "2": "1 cup fresh cranberries"
        },
        {
            "3": "0.25 cup white sugar"
        },
        {
            "4": "3 tablespoons water"
        },
        {
            "5": "0.5 teaspoon ground cinnamon"
        },
        {
            "6": "1 pinch salt"
        },
        {
            "7": "0.5 teaspoons lemon juice"
        }
    ],
    "Directions": [
        {
            "1": "Stir apples, cranberries, sugar, water, cinnamon, and salt together in a saucepan. Bring to a boil, reduce heat to low, and cook until apples and cranberries are tender, about 20 minutes. Remove from heat."
        },
        {
            "2": "Stir lemon juice into cranberry mixture."
        },
        {
            "3": "Mash cranberry mixture with a wooden spoon or fork to make a chunky sauce."
        }
    ],
