{
    "title": "Cranberry Raspberry Sauce",
    "Category": "Cranberry Sauce",
    "Ingredients": [
        {
            "1": "12 ounces cranberries"
        },
        {
            "2": "2 cups raspberries"
        },
        {
            "3": "1 cup Merlot wine"
        },
        {
            "4": "0.75 cup white sugar"
        }
    ],
    "Directions": [
        {
            "1": "Put cranberries, raspberries, Merlot, and sugar in a large saucepan. Bring to a boil, stirring occasionally. Use a whisk to mash cranberries. Reduce heat and simmer for 15 minutes."
        },
        {
            "2": "Serve hot or pour sauce into a container and refrigerate to serve cold."
        }
    ],
