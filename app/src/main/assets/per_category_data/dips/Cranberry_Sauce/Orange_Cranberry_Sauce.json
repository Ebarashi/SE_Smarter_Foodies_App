{
    "title": "Orange Cranberry Sauce",
    "Category": "Cranberry Sauce",
    "Ingredients": [
        {
            "1": "2 medium oranges"
        },
        {
            "2": "1 pound fresh cranberries"
        },
        {
            "3": "1 cup white sugar"
        },
        {
            "4": "0.25 cups water, or as needed to cover"
        }
    ],
    "Directions": [
        {
            "1": "Zest and juice oranges; set juice aside."
        },
        {
            "2": "Combine cranberries, sugar, and orange zest in a saucepan; stir to combine. Cover with water and bring to a boil. Cook until cranberries begin to burst, 5 to 10 minutes."
        },
        {
            "3": "Reduce the heat to low, stir in orange juice, and simmer until flavors blend, about 1 hour."
        },
        {
            "4": "Remove from the heat and cool to room temperature, about 20 minutes. Refrigerate until jelled, at least 1 hour."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
