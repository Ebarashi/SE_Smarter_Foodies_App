{
    "title": "Cranberry-Blueberry Sauce",
    "Category": "Cranberry Sauce",
    "Ingredients": [
        {
            "1": "4 cups fresh cranberries"
        },
        {
            "2": "0.5 cups water"
        },
        {
            "3": "1  orange, zested and juiced"
        },
        {
            "4": "4  cinnamon sticks"
        },
        {
            "5": "1 (2 inch) piece fresh ginger, grated"
        },
        {
            "6": "1 teaspoon vanilla extract"
        },
        {
            "7": "6  whole cloves"
        },
        {
            "8": "0.5 teaspoon freshly grated nutmeg"
        },
        {
            "9": "0.25 teaspoon ground allspice"
        },
        {
            "10": "1 pint blueberries"
        },
        {
            "11": "0.75 cup maple syrup"
        }
    ],
    "Directions": [
        {
            "1": "Mix cranberries, water, orange zest and juice, cinnamon sticks, ginger, vanilla extract, cloves, nutmeg, and allspice together in a large saucepan until well combined; bring to a boil. Reduce heat to low and simmer, stirring frequently, until many of the cranberries pop and sauce starts to thicken, about 20 minutes."
        },
        {
            "2": "Add blueberries and maple syrup; cook and stir until mixture returns to a simmer. Cook, stirring frequently, until desired consistency, about 20 minutes. Allow to cool completely. Remove cinnamon sticks and cloves; refrigerate for 6 hours, to overnight."
        }
    ],
