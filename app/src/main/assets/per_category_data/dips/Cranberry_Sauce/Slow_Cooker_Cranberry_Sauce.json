{
    "title": "Slow Cooker Cranberry Sauce",
    "Category": "Cranberry Sauce",
    "Ingredients": [
        {
            "1": "0.5 cup orange juice"
        },
        {
            "2": "0.5 cup water"
        },
        {
            "3": "0.5 cup brown sugar"
        },
        {
            "4": "0.5 cup white sugar"
        },
        {
            "5": "0.25 teaspoon ground cinnamon"
        },
        {
            "6": "1 (12 ounce) package fresh cranberries"
        }
    ],
    "Directions": [
        {
            "1": "Combine orange juice, water, brown sugar, white sugar, and cinnamon in slow cooker; stir in cranberries. Cook on High for 3 hours, stirring once each hour."
        },
        {
            "2": "Remove lid; stir well. Cook on High until sauce has thickened and most cranberries have popped, about 45 more minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
