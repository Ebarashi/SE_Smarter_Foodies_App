{
    "title": "Cranberry Sauce with Bourbon",
    "Category": "Cranberry Sauce",
    "Ingredients": [
        {
            "1": "1 (12 ounce) package fresh cranberries"
        },
        {
            "2": "4 cups white sugar"
        },
        {
            "3": "0.5 cup bourbon"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Stir cranberries, sugar, and bourbon together in a baking dish."
        },
        {
            "3": "Bake in the preheated oven, stirring occasionally, until thick, about 1 hour."
        }
    ],
