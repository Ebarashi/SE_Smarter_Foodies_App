{
    "title": "Dragon Fruit Mojito",
    "Category": "Mojitos",
    "Ingredients": [
        {
            "1": "2 tablespoons white sugar"
        },
        {
            "2": "2 teaspoons white sugar"
        },
        {
            "3": "16 leaves fresh mint"
        },
        {
            "4": "1 cup light rum"
        },
        {
            "5": "1  dragon fruit, halved and flesh removed"
        },
        {
            "6": "1 liter chilled club soda"
        }
    ],
    "Directions": [
        {
            "1": "Mix 2 tablespoons plus 2 teaspoons sugar with mint in the bottom of a highball glass."
        },
        {
            "2": "Combine rum and dragon fruit flesh in a blender; blend until smooth. Pour rum mixture over the muddled mint mixture. Stir to incorporate."
        },
        {
            "3": "Divide mixture between 4 double old-fashioned glasses. Top each drink with equal amounts of club soda."
        }
    ],
    "Details": [
