{
    "title": "Fresh Raspberry Mojito",
    "Category": "Mojitos",
    "Ingredients": [
        {
            "1": "4  fresh mint leaves"
        },
        {
            "2": "4  fresh raspberries"
        },
        {
            "3": "0.5 fluid ounce simple syrup"
        },
        {
            "4": "0.5 fluid ounce lime juice"
        },
        {
            "5": "1 ice"
        },
        {
            "6": "1 fluid ounce raspberry-flavored vodka"
        },
        {
            "7": "1 fluid ounce silver rum"
        },
        {
            "8": "2 fluid ounces club soda"
        }
    ],
    "Directions": [
        {
            "1": "Muddle mint leaves, raspberries, simple syrup, and lime juice together in a shaker. Fill the shaker with ice and add vodka and rum; cover and shake until chilled. Pour entire contents of shaker into a Collins glass and top with club soda."
        }
    ],
