{
    "title": "Bubbly Mojito",
    "Category": "Mojitos",
    "Ingredients": [
        {
            "1": "10  fresh mint leaves"
        },
        {
            "2": "2 teaspoons white sugar"
        },
        {
            "3": "2 fluid ounces white rum"
        },
        {
            "4": "0.5  lime, quartered"
        },
        {
            "5": "4 fluid ounces sparkling white wine"
        },
        {
            "6": "0.25 cup crushed ice"
        }
    ],
    "Directions": [
        {
            "1": "Place mint leaves and sugar in the bottom of a collins glass. Crush mint using a muddler. Add rum and lime slices to glass and muddle again. Top with sparkling wine and crushed ice."
        }
