{
    "title": "Cocojito (Frozen Mojito)",
    "Category": "Mojitos",
    "Ingredients": [
        {
            "1": "4 cups ice cubes"
        },
        {
            "2": "1.0  limes, rind removed"
        },
        {
            "3": "7 fluid ounces sweetened cream of coconut (such as Coco Lopez\u00ae)"
        },
        {
            "4": "1.0 (1.5 fluid ounce) jiggers white rum (such as Bacardi\u00ae)"
        },
        {
            "5": "5 sprigs fresh mint leaves"
        },
        {
            "6": "2  mint sprigs, for garnish"
        },
        {
            "7": "2  lime wedges, for garnish"
        }
    ],
    "Directions": [
        {
            "1": "Blend the ice, limes, cream of coconut, rum, and mint leaves together in a blender until smooth. Pour into two glasses and garnish each with a mint sprig and lime wedge as desired."
        }
    ],
