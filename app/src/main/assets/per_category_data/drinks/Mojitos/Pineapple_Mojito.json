{
    "title": "Pineapple Mojito",
    "Category": "Mojitos",
    "Ingredients": [
        {
            "1": "10  fresh mint leaves, divided"
        },
        {
            "2": "1 tablespoon white sugar"
        },
        {
            "3": "1 tablespoon chopped fresh pineapple"
        },
        {
            "4": "1 ice"
        },
        {
            "5": "6 fluid ounces pineapple juice"
        },
        {
            "6": "2 fluid ounces rum"
        },
        {
            "7": "1 splash club soda"
        },
        {
            "8": "1 wedge fresh pineapple"
        }
    ],
    "Directions": [
        {
            "1": "Muddle mint leaves, sugar, and pineapple in a highball glass."
        },
        {
            "2": "Fill glass with ice cubes. Pour in pineapple juice and rum. Top with a splash of club soda. Garnish with pineapple wedge and remaining mint leaves."
        }
    ],
