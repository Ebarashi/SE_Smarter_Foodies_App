{
    "title": "Guava Mojito",
    "Category": "Mojitos",
    "Ingredients": [
        {
            "1": "1 ice"
        },
        {
            "2": "4 sprigs fresh mint, divided"
        },
        {
            "3": "2 teaspoons white sugar"
        },
        {
            "4": "3 fluid ounces passion fruit-flavored rum"
        },
        {
            "5": "1  lime, juiced"
        },
        {
            "6": "3 fluid ounces unsweetened guava nectar"
        },
        {
            "7": "2 fluid ounces club soda, or as needed"
        }
    ],
    "Directions": [
        {
            "1": "Fill a glass with ice."
        },
        {
            "2": "Muddle 3 sprigs mint and sugar together in a cocktail shaker; add ice, rum, lime juice, and guava nectar. Cover and shake 10 times. Strain drink into glass filled with ice; add club soda and garnish with 1 mint sprig."
        }
    ],
