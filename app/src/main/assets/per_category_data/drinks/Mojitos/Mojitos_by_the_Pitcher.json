{
    "title": "Mojitos by the Pitcher",
    "Category": "Mojitos",
    "Ingredients": [
        {
            "1": "0.5 cup white sugar"
        },
        {
            "2": "36 leaves fresh mint, or to taste"
        },
        {
            "3": "3 medium limes, quartered"
        },
        {
            "4": "1 cup rum (such as Bacardi\u00ae)"
        },
        {
            "5": "1 liter club soda"
        }
    ],
    "Directions": [
        {
            "1": "Muddle sugar and mint leaves together in a pitcher until leaves are well broken down. Add limes and muddle until limes are juiced. Pour rum into sugar mixture; add club soda and stir until sugar is dissolved."
        }
    ],
