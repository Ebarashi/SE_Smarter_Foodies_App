{
    "title": "Patron Mojito",
    "Category": "Mojitos",
    "Ingredients": [
        {
            "1": "2 fluid ounces silver tequila (such as Patron\u00ae)"
        },
        {
            "2": "12 leaves mint"
        },
        {
            "3": "1 tablespoon white sugar"
        },
        {
            "4": "3  lime wedges"
        },
        {
            "5": "0.25 cup club soda, or as needed"
        }
    ],
    "Directions": [
        {
            "1": "Combine tequila, mint, sugar, and lime in a tall glass; mash mint and lime well with a cocktail muddler. Top with club soda and stir gently to serve."
        }
    ],
