{
    "title": "Pinnacle Sunday Funday Bloody Mary",
    "Category": "Bloody Marys",
    "Ingredients": [
        {
            "1": "1 part Pinnacle\u00ae Vodka"
        },
        {
            "2": "2 parts Tomato Juice"
        },
        {
            "3": "1 dash Tabasco\u00ae"
        },
        {
            "4": "1 dash Horseradish"
        },
        {
            "5": "1 dash Worcestershire Sauce"
        },
        {
            "6": "1 Celery, for garnish"
        },
        {
            "7": "1 Ice"
        }
    ],
    "Directions": [
        {
            "1": "Combine all ingredients except celery in an ice-filled shaker and shake until well chilled."
        },
        {
            "2": "Strain into a chilled pint glass over fresh ice."
        },
        {
            "3": "Garnish with celery and any other fun toppings on hand."
        }
    ],
