{
    "title": "Bloody Mary Mix",
    "Category": "Bloody Marys",
    "Ingredients": [
        {
            "1": "1 (46 fluid ounce) bottle tomato-vegetable juice cocktail"
        },
        {
            "2": "0.5 medium lemons, juiced"
        },
        {
            "3": "1 teaspoon brown sugar"
        },
        {
            "4": "1 tablespoon steak sauce"
        },
        {
            "5": "1 tablespoon Worcestershire sauce"
        },
        {
            "6": "1 teaspoon prepared horseradish"
        },
        {
            "7": "1 teaspoon hot pepper sauce"
        },
        {
            "8": "0.5 teaspoon celery salt"
        }
    ],
    "Directions": [
        {
            "1": "In a large pitcher, combine juice cocktail, lemon juice and brown sugar. Season with Worcestershire sauce, horseradish, hot sauce and celery salt. Cover, and refrigerate 8 to 12 hours to allow flavors to meld."
        }
    ],
