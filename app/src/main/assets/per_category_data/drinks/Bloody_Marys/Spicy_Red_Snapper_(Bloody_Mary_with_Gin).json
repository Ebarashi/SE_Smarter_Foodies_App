{
    "title": "Spicy Red Snapper (Bloody Mary with Gin)",
    "Category": "Bloody Marys",
    "Ingredients": [
        {
            "1": "5  ice cubes"
        },
        {
            "2": "1 (6 fluid ounce) can tomato juice"
        },
        {
            "3": "2 fluid ounces dry gin"
        },
        {
            "4": "2 tablespoons fresh lemon juice"
        },
        {
            "5": "2 teaspoons hot pepper sauce"
        },
        {
            "6": "2 teaspoons Worcestershire sauce"
        },
        {
            "7": "0.5 teaspoon brine from olive jar"
        },
        {
            "8": "1 pinch hot chili powder"
        },
        {
            "9": "1 pinch garlic powder"
        },
        {
            "10": "1 pinch ground cumin"
        },
        {
            "11": "0.25 teaspoon celery salt"
        },
        {
            "12": "0.25 teaspoon fresh-ground black pepper"
        },
        {
            "13": "0.25 teaspoon prepared horseradish"
        },
        {
            "14": "1  celery stalk"
        },
        {
            "15": "1 small dill pickle"
        },
        {
            "16": "2  stuffed green olives"
        },
        {
            "17": "1 wedge lemon"
        }
    ],
    "Directions": [
        {
            "1": "Place ice cubes into a cocktail shaker, and pour in tomato juice, gin, lemon juice, hot sauce, and Worcestershire sauce. Season with chili powder, garlic powder, cumin, celery salt, pepper, and horseradish. Cover and shake until the outside of the shaker has frosted. Pour contents (including ice) into a 12 ounce highball glass. Garnish with celery, pickle, olives, and lemon wedge before serving."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Total Time": "10 mins"
        },
        {
            "Servings": "1 "
        },
        {
            "Yield": "1 spicy red snapper"
        }
    ],
