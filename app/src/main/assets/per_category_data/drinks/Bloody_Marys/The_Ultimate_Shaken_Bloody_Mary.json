{
    "title": "The Ultimate Shaken Bloody Mary",
    "Category": "Bloody Marys",
    "Ingredients": [
        {
            "1": "2 (12 fluid ounce) cans tomato juice"
        },
        {
            "2": "0.25 teaspoon hot sauce such as Tabasco"
        },
        {
            "3": "1 teaspoon Worcestershire sauce"
        },
        {
            "4": "1 teaspoon dill weed"
        },
        {
            "5": "1 tablespoon seafood seasoning, such as Old Bay\u2122"
        },
        {
            "6": "0.5 teaspoon celery salt"
        },
        {
            "7": "0.5 cup vodka"
        },
        {
            "8": "4  cornichons (small dill pickles)"
        },
        {
            "9": "2 cups ice cubes"
        }
    ],
    "Directions": [
        {
            "1": "Pour the tomato juice, hot sauce, Worcestershire sauce, dill, seafood seasoning, celery salt, and vodka into a cocktail shaker over ice. Cover, and shake until the outside of the shaker has frosted. Strain into ice-filled glasses, and garnish with cornichons to serve."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Total Time": "10 mins"
        },
        {
            "Servings": "2 "
        },
        {
            "Yield": "2 cocktails"
        }
    ],
