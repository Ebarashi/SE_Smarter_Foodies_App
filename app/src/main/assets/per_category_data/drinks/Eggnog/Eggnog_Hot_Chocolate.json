{
    "title": "Eggnog Hot Chocolate",
    "Category": "Eggnog",
    "Ingredients": [
        {
            "1": "1.0 cups milk"
        },
        {
            "2": "12 ounces bittersweet chocolate chips"
        },
        {
            "3": "4 cups eggnog"
        },
        {
            "4": "1 teaspoon vanilla extract"
        },
        {
            "5": "0.25 cup coffee flavored liqueur"
        },
        {
            "6": "0.5 cup whipped cream"
        },
        {
            "7": "8  cinnamon sticks, garnish"
        }
    ],
    "Directions": [
        {
            "1": "Melt milk with chocolate in a medium saucepan over medium-low heat. Remove from heat and let stand for 2 minutes. Whisk mixture until melted and smooth."
        },
        {
            "2": "Stir in eggnog. Heat gently over low heat until very warm but not boiling. Remove from heat; stir in vanilla and coffee liqueur."
        },
        {
            "3": "Pour into mugs. Serve topped with whipped cream and a cinnamon stick garnish."
        }
    ],
