{
    "title": "Polynesian Watermelon Drink ('Otai)",
    "Category": "Punch",
    "Ingredients": [
        {
            "1": "1  seedless watermelon, halved and sliced"
        },
        {
            "2": "3 cups water"
        },
        {
            "3": "1 (15.25 ounce) can crushed pineapple"
        },
        {
            "4": "1 (12 fluid ounce) can evaporated milk"
        },
        {
            "5": "0.5 cup shredded coconut"
        },
        {
            "6": "2 tablespoons white sugar, or to taste"
        },
        {
            "7": "0.5  lime, juiced"
        }
    ],
    "Directions": [
        {
            "1": "Grate watermelon with a fork from the rind into a large bowl, leaving no large chunks. Stir water, pineapple, evaporated milk, and coconut into the grated watermelon. Add sugar; stir until dissolved. Squeeze lime juice into the watermelon mixture."
        }
    ],
