{
    "title": "Pineapple Sangria",
    "Category": "Punch",
    "Ingredients": [
        {
            "1": "2 (750 milliliter) bottles Sauvignon Blanc"
        },
        {
            "2": "2 cups pineapple juice"
        },
        {
            "3": "1 cup triple sec"
        },
        {
            "4": "0.5 cup brandy"
        },
        {
            "5": "0.5 cup chopped fresh pineapple"
        },
        {
            "6": "0.5 cup chopped orange"
        },
        {
            "7": "1  lemon, seeded and chopped"
        },
        {
            "8": "0.25 cup chopped lime"
        },
        {
            "9": "2 cups lemon-lime flavored carbonated beverage (such as 7-Up\u00ae)"
        }
    ],
    "Directions": [
        {
            "1": "Mix Sauvignon Blanc, pineapple juice, triple sec, and brandy together in a large pitcher. Add chopped pineapple, orange, lemon, and lime. Cover and refrigerate, 8 hours to overnight."
        },
        {
            "2": "Stir in lemon-lime soda before serving."
        }
    ],
