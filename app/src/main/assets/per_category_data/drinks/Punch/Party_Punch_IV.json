{
    "title": "Party Punch IV",
    "Category": "Punch",
    "Ingredients": [
        {
            "1": "1 (16 ounce) can frozen pineapple juice concentrate"
        },
        {
            "2": "1 (12 fluid ounce) can frozen orange juice concentrate"
        },
        {
            "3": "12 fluid ounces vodka"
        },
        {
            "4": "2 (2 liter) bottles lemon-lime flavored carbonated beverage, chilled"
        },
        {
            "5": "1 (4 ounce) jar maraschino cherries"
        },
        {
            "6": "4  oranges, sliced into rounds"
        },
        {
            "7": "2 trays ice cubes"
        }
    ],
    "Directions": [
        {
            "1": "In a punch bowl, combine pineapple juice concentrate, orange juice concentrate and vodka. Pour in lemon-lime soda. Float cherries and orange slices on top. Add ice before serving."
        }
    ],
