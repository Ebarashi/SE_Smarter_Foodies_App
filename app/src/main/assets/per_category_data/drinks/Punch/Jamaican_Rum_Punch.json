{
    "title": "Jamaican Rum Punch",
    "Category": "Punch",
    "Ingredients": [
        {
            "1": "1.0 cups pineapple juice"
        },
        {
            "2": "1.0 cups orange juice"
        },
        {
            "3": "1 cup 151 proof rum (such as Bacardi\u00ae)"
        },
        {
            "4": "0.5 cup dark rum (such as Meyer's\u00ae)"
        },
        {
            "5": "0.25 cup coconut-flavored rum (such as Malibu\u00ae)"
        },
        {
            "6": "0.25 cup fresh lime juice"
        },
        {
            "7": "3 tablespoons grenadine syrup"
        },
        {
            "8": "1  orange, sliced"
        },
        {
            "9": "1  lime, sliced"
        },
        {
            "10": "1  lemon, sliced"
        }
    ],
    "Directions": [
        {
            "1": "Stir pineapple juice, orange juice, 151 proof rum, dark rum, coconut-flavored rum, lime juice, and grenadine syrup together in a punch bowl. Float orange slices, lime slices, and lemon slices in the punch."
        },
        {
            "2": "Ladle the punch into ice filled glasses, or chill the punch in the refrigerator until ready to serve."
        }
    ],
