{
    "title": "White Sangria",
    "Category": "Punch",
    "Ingredients": [
        {
            "1": "0.5 cup peach schnapps"
        },
        {
            "2": "0.5 cup cognac"
        },
        {
            "3": "0.25 cup white sugar"
        },
        {
            "4": "4  oranges, sliced into rounds"
        },
        {
            "5": "2  mangos, peeled and sliced"
        },
        {
            "6": "4 (750 milliliter) bottles dry white wine, chilled"
        },
        {
            "7": "1 liter ginger ale, chilled"
        }
    ],
    "Directions": [
        {
            "1": "In a pitcher, combine peach schnapps, cognac, sugar, sliced oranges and sliced mangos. Chill for at least an hour."
        },
        {
            "2": "Pour fruit mixture into a large punch bowl. Stir in white wine and ginger ale."
        }
    ],
