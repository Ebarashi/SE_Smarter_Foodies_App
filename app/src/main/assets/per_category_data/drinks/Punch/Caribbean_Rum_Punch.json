{
    "title": "Caribbean Rum Punch",
    "Category": "Punch",
    "Ingredients": [
        {
            "1": "1 cup fresh lime juice"
        },
        {
            "2": "2 cups simple syrup"
        },
        {
            "3": "3 cups amber rum"
        },
        {
            "4": "4 cups orange juice"
        },
        {
            "5": "4 dashes bitters"
        },
        {
            "6": "1 freshly grated nutmeg"
        }
    ],
    "Directions": [
        {
            "1": "In a pitcher, combine lime juice, simple syrup, rum and orange juice. Add a few dashes of bitters and some grated nutmeg to taste. Serve chilled over ice."
        }
    ],
