{
    "title": "Witches Brew Punch",
    "Category": "Punch",
    "Ingredients": [
        {
            "1": "1 cup boiling water"
        },
        {
            "2": "0.75 cup white sugar"
        },
        {
            "3": "2 (3 ounce) packages lime-flavored gelatin mix"
        },
        {
            "4": "1 (46 fluid ounce) can very cold pineapple juice"
        },
        {
            "5": "1 (2 liter) bottle ginger ale"
        },
        {
            "6": "0.5 gallon orange sherbet"
        },
        {
            "7": "3 cups orange drink"
        },
        {
            "8": "20  gummy candies, or more to taste"
        }
    ],
    "Directions": [
        {
            "1": "Mix boiling water, sugar, and lime-flavored gelatin together in a large bowl until sugar and gelatin are dissolved. Stir in pineapple juice."
        },
        {
            "2": "Pour pineapple juice mixture into a punch bowl. Add ginger ale, orange sherbet, orange drink, and gummy candy; stir well until blended."
        }
    ],
