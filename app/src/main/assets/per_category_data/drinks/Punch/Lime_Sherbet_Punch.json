{
    "title": "Lime Sherbet Punch",
    "Category": "Punch",
    "Ingredients": [
        {
            "1": "2 quarts lime sherbet"
        },
        {
            "2": "2 (2 liter) bottles ginger ale"
        },
        {
            "3": "1 (46 fluid ounce) can pineapple juice"
        },
        {
            "4": "1 (4 ounce) jar maraschino cherries, drained"
        },
        {
            "5": "1  lemon, sliced"
        },
        {
            "6": "1  lime, sliced"
        }
    ],
    "Directions": [
        {
            "1": "Scoop lime sherbet into a punch bowl; pour in ginger ale and pineapple juice. Stir well."
        },
        {
            "2": "Stir in maraschino cherries and float lemon and lime slices in the punch."
        }
    ],
