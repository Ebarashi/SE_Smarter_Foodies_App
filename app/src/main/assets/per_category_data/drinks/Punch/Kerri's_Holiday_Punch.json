{
    "title": "Kerri's Holiday Punch",
    "Category": "Punch",
    "Ingredients": [
        {
            "1": "1 (750 milliliter) bottle Southern Comfort liqueur"
        },
        {
            "2": "6 fluid ounces lemon juice"
        },
        {
            "3": "1 (6 ounce) can frozen lemonade concentrate, thawed"
        },
        {
            "4": "1 (6 ounce) can frozen orange juice concentrate"
        },
        {
            "5": "3 liters lemon-lime flavored carbonated beverage"
        },
        {
            "6": "2 drops red food coloring"
        },
        {
            "7": "1  orange, sliced into rounds"
        },
        {
            "8": "1  lemon, sliced into rounds"
        }
    ],
    "Directions": [
        {
            "1": "In a large punch bowl, combine Southern Comfort, lemon juice, lemonade concentrate and orange juice concentrate. Stir in lemon-lime soda, and add a few drops red food coloring. Float orange and lemon slices for garnish."
        }
    ],
