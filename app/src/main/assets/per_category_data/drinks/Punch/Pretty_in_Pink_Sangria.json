{
    "title": "Pretty in Pink Sangria",
    "Category": "Punch",
    "Ingredients": [
        {
            "1": "2 (10 ounce) packages frozen sliced strawberries, thawed"
        },
        {
            "2": "1 (12 fluid ounce) can frozen lemonade concentrate, thawed"
        },
        {
            "3": "2 (750 milliliter) bottles chilled rose wine, such as white Zinfandel"
        },
        {
            "4": "2 cups pineapple juice"
        },
        {
            "5": "1 liter ginger ale"
        }
    ],
    "Directions": [
        {
            "1": "Stir the strawberries, lemonade concentrate, rose wine, and pineapple juice in a punch bowl until combined. Stir in the ginger ale just before serving."
        }
    ],
