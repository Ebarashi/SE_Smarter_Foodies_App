{
    "title": "Mexico Chiquito Punch",
    "Category": "Punch",
    "Ingredients": [
        {
            "1": "1 (46 fluid ounce) can pineapple juice"
        },
        {
            "2": "1 (1 liter) bottle grenadine syrup"
        },
        {
            "3": "1 cup white sugar"
        },
        {
            "4": "0.25 cup lemon juice"
        },
        {
            "5": "1 (1 ounce) packet instant tea powder"
        },
        {
            "6": "5 quarts water, or as needed"
        }
    ],
    "Directions": [
        {
            "1": "Mix pineapple juice, grenadine, sugar, lemon juice, and instant tea powder together in a 2-gallon container; stir to dissolve sugar and tea powder. Add enough water to fill container; stir."
        }
    ],
