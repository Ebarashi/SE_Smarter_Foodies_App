{
    "title": "New Year's Champagne Punch",
    "Category": "Punch",
    "Ingredients": [
        {
            "1": "1 quart orange sherbet"
        },
        {
            "2": "2 quarts fresh orange juice, chilled"
        },
        {
            "3": "1 (2 liter) bottle ginger ale, chilled"
        },
        {
            "4": "2 (750 milliliter) bottles champagne, chilled"
        }
