    "Category": "Sangria",
    "Ingredients": [
        {
            "1": "2.0 cups red wine"
        },
        {
            "2": "0.75 cup ginger ale"
        },
        {
            "3": "0.5 cup orange juice"
        },
        {
            "4": "2 fluid ounces brandy"
        },
        {
