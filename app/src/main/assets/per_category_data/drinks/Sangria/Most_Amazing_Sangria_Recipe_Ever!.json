        },
        {
            "6": "1 teaspoon white sugar"
        },
        {
            "7": "1 pinch ground cinnamon"
        },
        {
            "8": "2 cups crushed ice"
        },
        {
            "9": "0.5  Granny Smith apple, diced"
        }
    ],
    "Directions": [
        {
            "1": "Stir red wine, ginger ale, orange juice, brandy, triple sec, sugar, cinnamon, and apple together in a large pitcher."
        },
        {
            "2": "Divide crushed ice evenly into six large wine glasses."
        },
        {
            "3": "Pour red wine mixture into each wine glass, making sure that each serving has a few pieces of apple."
        }
