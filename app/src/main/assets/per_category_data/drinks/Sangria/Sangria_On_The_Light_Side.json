{
    "title": "Sangria On The Light Side",
    "Category": "Sangria",
    "Ingredients": [
        {
            "1": "1 large Juice of 1 orange"
        },
        {
            "2": "1 large Juice of 1 lemon"
        },
        {
            "3": "0.25 cup lime juice"
        },
        {
            "4": "1 (1.5 liter) bottle Burgundy or other red wine"
        },
        {
            "5": "0.5 teaspoon Sweet'N Low granulated sugar substitute"
        },
        {
            "6": "1.5 cups seltzer"
        },
        {
            "7": "1  orange, chilled and cut into wedges"
        },
        {
            "8": "1  lemon, chilled and sliced"
        },
        {
            "9": "0.5 cup fresh strawberry slices"
        }
    ],
    "Directions": [
        {
            "1": "In large pitcher or punch bowl, stir together juices, wine, and Sweet 'N Low until Sweet 'N Low dissolves; refrigerate until well chilled. Just before serving, add seltzer, orange wedges, lemon slices, and strawberry slices."
        }
    ],
