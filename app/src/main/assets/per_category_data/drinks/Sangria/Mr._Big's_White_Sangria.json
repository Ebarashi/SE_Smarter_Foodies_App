{
    "title": "Mr. Big's White Sangria",
    "Category": "Sangria",
    "Ingredients": [
        {
            "1": "3  oranges, thinly sliced"
        },
        {
            "2": "4  lemons, thinly sliced"
        },
        {
            "3": "4  limes, thinly sliced"
        },
        {
            "4": "0.5 cups white sugar"
        },
        {
            "5": "0.5 cup tequila"
        },
        {
            "6": "2 trays ice cubes, or as needed"
        },
        {
            "7": "1 (750 milliliter) bottle dry white wine"
        },
        {
            "8": "1 (750 milliliter) bottle champagne"
        },
        {
            "9": "2 cups club soda"
        }
    ],
    "Directions": [
        {
            "1": "Put orange slices, lemon slices, and lime slices in a large punch bowl. Pour sugar over the fruit; add tequila and mix gently with a wooden spoon. Let soak 6 hours to overnight."
        },
        {
            "2": "Put plenty of ice in the punch bowl. Pour white wine, champagne, and club soda over the ice. Stir and serve immediately."
        }
    ],
