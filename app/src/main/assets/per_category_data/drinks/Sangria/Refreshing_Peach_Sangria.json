{
    "title": "Refreshing Peach Sangria",
    "Category": "Sangria",
    "Ingredients": [
        {
            "1": "3 fluid ounces white Zinfandel wine"
        },
        {
            "2": "3 fluid ounces lemon-lime flavored soda"
        },
        {
            "3": "1 fluid ounce peach schnapps"
        },
        {
            "4": "1 fluid ounce pineapple juice"
        },
        {
            "5": "1 cup ice cubes, or as needed"
        },
        {
            "6": "1 slice frozen peach"
        },
        {
            "7": "3  red seedless grapes, frozen"
        },
        {
            "8": "3  seedless green grapes, frozen"
        }
    ],
    "Directions": [
        {
            "1": "Mix wine, lemon-lime soda, peach schnapps, and pineapple juice in a cocktail shaker; add ice. Stir until wine mixture is chilled."
        },
        {
            "2": "Place frozen peach, red grapes, and green grapes in a martini glass; strain wine mixture into martini glass."
        }
    ],
