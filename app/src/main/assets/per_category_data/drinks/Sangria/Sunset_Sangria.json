{
    "title": "Sunset Sangria",
    "Category": "Sangria",
    "Ingredients": [
        {
            "1": "1 (750 milliliter) bottle rose wine"
        },
        {
            "2": "1 cup pineapple juice"
        },
        {
            "3": "0.5 cup vodka"
        },
        {
            "4": "0.25 cup triple sec"
        },
        {
            "5": "0.5 cup simple syrup"
        },
        {
            "6": "1  orange, sliced into rounds"
        },
        {
            "7": "1  lemon, sliced into rounds"
        },
        {
            "8": "1  lime, sliced into rounds"
        },
        {
            "9": "1 (6 ounce) container fresh raspberries"
        }
    ],
    "Directions": [
        {
            "1": "Combine wine, pineapple juice, vodka, triple sec, and simple syrup in a large bowl or pitcher. Refrigerate until chilled, about 1 hour."
        },
        {
            "2": "Stir the oranges, lemons, limes, and raspberries into the chilled sangria at least 30 minutes before serving. The longer the fruit marinates the better. Serve over ice."
        }
    ],
