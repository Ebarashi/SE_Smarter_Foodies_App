{
    "title": "Game Day Sangria",
    "Category": "Sangria",
    "Ingredients": [
        {
            "1": "2 cups lemon juice"
        },
        {
            "2": "2 cups orange juice"
        },
        {
            "3": "2 cups white sugar"
        },
        {
            "4": "4 cups red wine"
        },
        {
            "5": "4 cups club soda"
        },
        {
            "6": "2 cups brandy"
        },
        {
            "7": "1 ice cubes, for serving"
        }
    ],
    "Directions": [
        {
            "1": "Combine lemon juice, orange juice, and sugar in a pitcher; stir until sugar has dissolved completely."
        },
        {
            "2": "Add red wine, club soda, and brandy to the pitcher. Serve in glasses with ice."
        }
    ],
