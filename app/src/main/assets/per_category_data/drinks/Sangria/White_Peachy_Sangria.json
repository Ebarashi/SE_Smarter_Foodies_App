{
    "title": "White Peachy Sangria",
    "Category": "Sangria",
    "Ingredients": [
        {
            "1": "4 (750 milliliter) bottles moscato wine"
        },
        {
            "2": "4 (12 fluid ounce) cans or bottles lemon-lime soda (such as Sprite\u00ae)"
        },
        {
            "3": "1 cup brandy"
        },
        {
            "4": "1 cup peach schnapps"
        },
        {
            "5": "2  fresh peaches, pitted and sliced"
        },
        {
            "6": "1 (16 ounce) package fresh strawberries, sliced"
        },
        {
            "7": "2  mangos - peeled, seeded, and sliced"
        },
        {
            "8": "0.5  fresh pineapple - peeled, cored, and cut into chunks"
        },
        {
            "9": "1 (6 ounce) container fresh raspberries"
        }
    ],
    "Directions": [
        {
            "1": "Stir moscato wine, lemon-lime soda, brandy, peach schnapps, peaches, strawberries, mangos, pineapple chunks, and raspberries together in a large container and chill 8 hours or overnight."
        }
    ],
