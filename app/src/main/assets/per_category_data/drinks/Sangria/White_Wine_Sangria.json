{
    "title": "White Wine Sangria",
    "Category": "Sangria",
    "Ingredients": [
        {
            "1": "1 (750 milliliter) bottle white wine"
        },
        {
            "2": "0.5 cup cubed watermelon"
        },
        {
            "3": "0.5 cup cubed honeydew"
        },
        {
            "4": "0.5 cup sliced strawberries"
        },
        {
            "5": "0.5 cup fresh raspberries"
        },
        {
            "6": "6 splashes ginger ale, or to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine the white wine, watermelon, honeydew, strawberries, and raspberries in a large pitcher. Chill in refrigerator at least 1 hour to allow flavors to combine."
        },
        {
            "2": "Ladle wine and fruit into wine goblets; top each with a splash of ginger ale to serve."
        }
    ],
