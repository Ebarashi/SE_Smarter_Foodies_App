{
    "title": "White Sangria with Frozen Fruit",
    "Category": "Sangria",
    "Ingredients": [
        {
            "1": "1 (750 milliliter) bottle white wine"
        },
        {
            "2": "0.5 cup peach schnapps"
        },
        {
            "3": "0.25 cup white sugar"
        },
        {
            "4": "0.25 cup frozen sliced peaches, or to taste"
        },
        {
            "5": "0.25 cup frozen sliced mango, or to taste"
        },
        {
            "6": "0.25 cup frozen raspberries, or to taste"
        }
    ],
    "Directions": [
        {
            "1": "Pour wine and schnapps into a pitcher. Add sugar, peaches, mango, and raspberries and stir gently. Refrigerate for at least 1 hour."
        }
    ],
