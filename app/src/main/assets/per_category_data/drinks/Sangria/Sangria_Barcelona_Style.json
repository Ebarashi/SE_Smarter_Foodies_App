{
    "title": "Sangria Barcelona Style",
    "Category": "Sangria",
    "Ingredients": [
        {
            "1": "1  lemon, sliced"
        },
        {
            "2": "1  orange, sliced"
        },
        {
            "3": "3 tablespoons brown sugar"
        },
        {
            "4": "1 (1.5 fluid ounce) jigger vodka"
        },
        {
            "5": "1 (1.5 fluid ounce) jigger cointreau or triple sec"
        },
        {
            "6": "1 (1.5 fluid ounce) jigger gin"
        },
        {
            "7": "0.25 cup orange soda"
        },
        {
            "8": "0.25 cup lemon-lime soda"
        },
        {
            "9": "0.75 cup pineapple juice"
        },
        {
            "10": "0.75 cup orange juice"
        },
        {
            "11": "1 cup ice cubes"
        },
        {
            "12": "1 (750 milliliter) bottle dry red wine"
        }
    ],
    "Directions": [
        {
            "1": "Place the lemon, orange, and brown sugar in a large pitcher. Pour in vodka, Cointreau, gin, orange soda, lemon-lime soda, pineapple juice, and orange juice. Add ice cubes and pour in red wine; stir well until the sugar has dissolved."
        }
    ],
