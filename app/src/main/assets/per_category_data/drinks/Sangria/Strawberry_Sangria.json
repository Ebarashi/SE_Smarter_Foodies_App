{
    "title": "Strawberry Sangria",
    "Category": "Sangria",
    "Ingredients": [
        {
            "1": "1 (750 milliliter) bottle white wine"
        },
        {
            "2": "0.5 cup strawberry schnapps"
        },
        {
            "3": "0.25 cup white sugar"
        },
        {
            "4": "2 cups sliced strawberries"
        },
        {
            "5": "1 ice"
        },
        {
            "6": "6  whole strawberries for garnish"
        }
    ],
    "Directions": [
        {
            "1": "Combine wine, strawberry schnapps, and sugar in a large pitcher. Stir until sugar is dissolved. Add sliced strawberries and refrigerate, 2 to 3 hours."
        },
        {
            "2": "Pour over ice and garnish with whole strawberries before serving."
        }
    ],
