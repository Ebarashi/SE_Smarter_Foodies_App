{
    "title": "Indian Summer Raspberry Peach Sangria",
    "Category": "Sangria",
    "Ingredients": [
        {
            "1": "1 (750 milliliter) bottle red wine"
        },
        {
            "2": "24 fluid ounces raspberry-flavored soda water"
        },
        {
            "3": "0.5 cup peach schnapps"
        },
        {
            "4": "0.5 cup pomegranate juice"
        },
        {
            "5": "0.5 cup fresh lemon juice"
        },
        {
            "6": "1 cup fresh raspberries"
        },
        {
            "7": "2  peaches, sliced"
        },
        {
            "8": "2  lemons, sliced"
        },
        {
            "9": "1  orange, sliced"
        },
        {
            "10": "4 cups ice cubes, or as desired"
        }
    ],
    "Directions": [
        {
            "1": "Stir red wine, raspberry-flavored soda water, peach schnapps, pomegranate juice, and lemon juice together in a large pitcher or punch bowl; add raspberries, peach slices, lemon slices, and orange slices. Float ice cubes in the beverage to chill."
        }
    ],
