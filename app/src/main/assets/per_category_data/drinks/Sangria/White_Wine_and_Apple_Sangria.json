{
    "title": "White Wine and Apple Sangria",
    "Category": "Sangria",
    "Ingredients": [
        {
            "1": "0.5 cup apple juice"
        },
        {
            "2": "3 tablespoons white sugar"
        },
        {
            "3": "1 (750 milliliter) bottle dry white wine"
        },
        {
            "4": "1 cup apple slices"
        },
        {
            "5": "0.5 cup sliced peaches"
        },
        {
            "6": "2 medium lemons, sliced"
        },
        {
            "7": "0.25 cup brandy"
        },
        {
            "8": "0.5 cups club soda"
        },
        {
            "9": "3 cups ice, or as needed"
        },
        {
            "10": "8 sprigs fresh thyme"
        }
    ],
    "Directions": [
        {
            "1": "Stir together apple juice and sugar in a pitcher until sugar dissolves. Add wine, apples, peaches, lemon slices, and brandy."
        },
        {
            "2": "Cover and chill until ready to serve, 1 to 24 hours."
        },
        {
            "3": "Gently stir in club soda before serving over ice. Garnish with fresh thyme sprigs."
        }
    ],
