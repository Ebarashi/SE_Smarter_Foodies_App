{
    "title": "Spanish Sangria",
    "Category": "Sangria",
    "Ingredients": [
        {
            "1": "1 (750 milliliter) bottle Shiraz red wine"
        },
        {
            "2": "0.5 cup white sugar"
        },
        {
            "3": "0.5 cup vodka"
        },
        {
            "4": "0.5 cup gin"
        },
        {
            "5": "0.5 cup rum"
        },
        {
            "6": "0.5 cup tequila"
        },
        {
            "7": "1 medium orange, halved and sliced"
        },
        {
            "8": "1 medium lemon, juiced"
        },
        {
            "9": "1 liter ginger ale"
        }
    ],
    "Directions": [
        {
            "1": "Mix wine with sugar in a bowl to dissolve. Add vodka, gin, rum, and tequila."
        },
        {
            "2": "Cut orange and lemon in half, then into slices. Stir into sangria. Refrigerate for 8 hours, or overnight."
        },
        {
            "3": "Add ginger ale. Serve over a lot of ice."
        }
    ],
