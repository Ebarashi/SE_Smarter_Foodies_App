{
    "title": "Creamy Mango Smoothie",
    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "0.75 cup cold milk"
        },
        {
            "2": "0.25 cup vanilla yogurt"
        },
        {
            "3": "0.75 teaspoon vanilla extract"
        },
        {
            "4": "0.5 cups chopped fresh mango"
        },
        {
            "5": "3  ice cubes"
        }
    ],
    "Directions": [
        {
            "1": "Blend the milk, yogurt, vanilla extract, mango, and ice cubes in a blender until smooth and creamy."
        }
