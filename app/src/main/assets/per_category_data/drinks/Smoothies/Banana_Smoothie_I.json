{
    "title": "Banana Smoothie I",
    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "1  banana"
        },
        {
            "2": "1 cup milk"
        },
        {
            "3": "1 teaspoon vanilla extract"
        },
        {
            "4": "1  egg"
        },
        {
            "5": "2 tablespoons white sugar"
        },
        {
            "6": "1 pinch ground cinnamon"
        }
    ],
    "Directions": [
        {
            "1": "In a blender, combine banana, milk, vanilla, egg and sugar. Blend until smooth. Pour into a tall glass and top with a pinch of cinnamon."
        }
    ],
