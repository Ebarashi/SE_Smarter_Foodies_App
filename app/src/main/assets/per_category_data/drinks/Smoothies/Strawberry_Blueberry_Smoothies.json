{
    "title": "Strawberry Blueberry Smoothies",
    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "0.5 cup almond milk"
        },
        {
            "2": "0.5 cup frozen strawberries"
        },
        {
            "3": "0.5 cup frozen blueberries"
        },
        {
            "4": "0.5 cup low-fat plain yogurt"
        },
        {
            "5": "1 teaspoon flax seed oil"
        },
        {
            "6": "1 teaspoon agave nectar"
        }
    ],
    "Directions": [
        {
            "1": "Blend almond milk, strawberries, blueberries, yogurt, flax seed oil, and agave nectar in a blender until smooth."
        }
    ],
