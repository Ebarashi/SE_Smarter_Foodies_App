{
    "title": "Healthy Blueberry Breakfast Smoothie",
    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "1 cup fresh blueberries"
        },
        {
            "2": "0.5 cup Greek yogurt"
        },
        {
            "3": "0.25 cup orange juice"
        },
        {
            "4": "1 tablespoon white sugar, or to taste"
        },
        {
            "5": "0.25 teaspoon vanilla extract"
        },
        {
            "6": "1 pinch ground cinnamon, or to taste"
        },
        {
            "7": "3  ice cubes"
        }
    ],
    "Directions": [
        {
            "1": "Blend blueberries, yogurt, orange juice, sugar, vanilla extract, and cinnamon together in a blender on low speed for 30 seconds; increase speed to high and blend until smooth, about 2 minutes. Add ice and blend on high until smooth, about 1 minute more."
        }
    ],
    "Details": [
