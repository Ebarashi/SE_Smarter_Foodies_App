{
    "title": "Healthy Chocolate Smoothie",
    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "2  very ripe bananas"
        },
        {
            "2": "1 cup milk"
        },
        {
            "3": "6  ice cubes"
        },
        {
            "4": "0.5 tablespoons unsweetened cocoa powder"
        },
        {
            "5": "1 tablespoon peanut butter"
        },
        {
            "6": "1 teaspoon vanilla extract"
        }
    ],
    "Directions": [
        {
            "1": "Combine bananas, milk, ice cubes, cocoa powder, peanut butter, and vanilla in a blender; blend on high speed until smooth. Pour into 2 glasses."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
