{
    "title": "Orange Banana Smoothie",
    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "1 cup cold milk"
        },
        {
            "2": "2  oranges, peeled and segmented"
        },
        {
            "3": "1  banana"
        },
        {
            "4": "0.25 cup sugar"
        },
        {
            "5": "1 pinch salt"
        },
        {
            "6": "0.5 (8 ounce) container vanilla fat-free yogurt"
        },
        {
            "7": "4 cubes  ice"
        }
    ],
    "Directions": [
        {
            "1": "In a blender, combine milk, oranges, banana, sugar, salt and yogurt. Blend for about 1 minute. Insert ice cubes, and blend until smooth. Pour into glasses and serve."
        }
    ],
