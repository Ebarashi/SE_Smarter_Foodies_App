{
    "title": "Breakfast Banana Green Smoothie",
    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "2 cups baby spinach leaves, or to taste"
        },
        {
            "2": "1  banana"
        },
        {
            "3": "1  carrot, peeled and cut into large chunks"
        },
        {
            "4": "0.75 cup plain fat-free Greek yogurt, or to taste"
        },
        {
            "5": "0.75 cup ice"
        },
        {
            "6": "2 tablespoons honey"
        }
    ],
    "Directions": [
        {
            "1": "Put spinach, banana, carrot, yogurt, ice, and honey in a blender; blend until smooth."
        }
