    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "1 scoop whey protein powder"
        },
        {
            "2": "1 tablespoon flax seed meal"
        },
        {
            "3": "1 tablespoon unsweetened cocoa powder"
        },
        {
            "4": "1 teaspoon brown sugar (Optional)"
        },
        {
            "5": "1 cup unsweetened almond milk"
        },
        {
            "6": "1  banana"
        },
        {
            "7": "0.5 cup frozen mixed berries"
        },
        {
            "8": "1 tablespoon smooth peanut butter"
        }
    ],
    "Directions": [
        {
            "1": "Layer protein powder, flax meal, cocoa powder, and brown sugar in a blender; add almond milk, banana, berries, and peanut butter. Blend until smooth, about 1 minute."
        }
