{
    "title": "Mango Watermelon Smoothie",
    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "5 cups cubed seeded watermelon"
        },
        {
            "2": "1  mango - peeled, seeded, and diced"
        },
        {
            "3": "0.5 cup water"
        },
        {
            "4": "1 tablespoon white sugar"
        },
        {
            "5": "4 cubes  ice cubes"
        }
    ],
    "Directions": [
        {
            "1": "Blend watermelon, mango, water, and sugar together in a blender until smooth."
        },
        {
            "2": "Place ice into glasses and pour smoothie over ice."
        }
    ],
