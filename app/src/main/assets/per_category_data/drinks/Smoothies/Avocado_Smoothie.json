{
    "title": "Avocado Smoothie",
    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "1 cup milk"
        },
        {
            "2": "1  ripe avocado, halved and pitted"
        },
        {
            "3": "0.5 cup vanilla yogurt"
        },
        {
            "4": "3 tablespoons honey"
        },
        {
            "5": "8  ice cubes"
        }
    ],
    "Directions": [
        {
            "1": "Place milk, avocado, yogurt, and honey into a blender; add ice cubes and blend until smooth."
        }
