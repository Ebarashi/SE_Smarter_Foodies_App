{
    "title": "Healthy Cocoa, Banana, PB Smoothie",
    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "1 cup milk"
        },
        {
            "2": "0.5  chopped frozen banana, or more to taste"
        },
        {
            "3": "2 tablespoons peanut butter"
        },
        {
            "4": "2 teaspoons unsweetened cocoa powder"
        },
        {
            "5": "1 teaspoon honey"
        }
    ],
    "Directions": [
        {
            "1": "Blend milk, banana, peanut butter, cocoa powder, and honey together in a blender until smooth."
        }
    ],
