{
    "title": "Spinach and Kale Smoothie",
    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "2 cups fresh spinach"
        },
        {
            "2": "1 cup almond milk"
        },
        {
            "3": "1 leaf kale"
        },
        {
            "4": "1 tablespoon peanut butter"
        },
        {
            "5": "1 tablespoon chia seeds (Optional)"
        },
        {
            "6": "1  sliced frozen banana"
        }
    ],
    "Directions": [
        {
            "1": "Combine spinach, almond milk, kale, peanut butter, and chia seeds in a blender; blend until smooth. Add banana and blend until smooth."
        }
    ],
