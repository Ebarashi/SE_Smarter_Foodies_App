{
    "title": "B and L's Strawberry Smoothie",
    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "8  strawberries, hulled"
        },
        {
            "2": "0.5 cup skim milk"
        },
        {
            "3": "0.5 cup plain yogurt"
        },
        {
            "4": "3 tablespoons white sugar"
        },
        {
            "5": "2 teaspoons vanilla extract"
        },
        {
            "6": "6 cubes  ice, crushed"
        }
    ],
    "Directions": [
        {
            "1": "Combine strawberries, milk, yogurt, sugar, and vanilla in a blender. Add ice and blend until smooth and creamy."
        },
        {
            "2": "Pour into glasses and serve immediately."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Total Time": "5 mins"
        },
        {
            "Servings": "2 "
        }
