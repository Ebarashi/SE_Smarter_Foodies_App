{
    "title": "Green Monster Smoothie",
    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "1 cup fat-free milk"
        },
        {
            "2": "0.5 cup fat-free plain yogurt"
        },
        {
            "3": "1  banana, frozen and chunked"
        },
        {
            "4": "1 tablespoon natural peanut butter"
        },
        {
            "5": "2 cups fresh spinach"
        },
        {
            "6": "1 cup ice cubes (Optional)"
        }
    ],
    "Directions": [
