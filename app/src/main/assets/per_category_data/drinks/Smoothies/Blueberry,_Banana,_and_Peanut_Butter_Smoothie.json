{
    "title": "Blueberry, Banana, and Peanut Butter Smoothie",
    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "1 tablespoon flax seed meal or wheat germ"
        },
        {
            "2": "1  banana"
        },
        {
            "3": "0.5 cup frozen blueberries"
        },
        {
            "4": "1 tablespoon peanut butter"
        },
        {
            "5": "1 teaspoon honey"
        },
        {
            "6": "0.5 cup plain yogurt"
        },
        {
            "7": "1 cup milk"
        }
    ],
    "Directions": [
        {
            "1": "Put ground flax seed meal or wheat germ into blender to grind and further breakdown. This will also eliminate any bitterness from the flax seed."
        },
        {
            "2": "Place the banana, blueberries, peanut butter, honey, yogurt, and milk into the blender. Cover, and puree until smooth. Pour into glasses to serve."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Total Time": "10 mins"
        },
        {
            "Servings": "2 "
        },
        {
            "Yield": "2 cups"
        }
    ],
