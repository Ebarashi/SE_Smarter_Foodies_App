{
    "title": "Strawberry Raspberry Smoothie",
    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "1 cup hulled fresh strawberries"
        },
        {
            "2": "0.5 cup frozen raspberries"
        },
        {
            "3": "0.5 cup lowfat milk"
        },
        {
            "4": "0.5 cup vanilla yogurt"
        },
        {
            "5": "1 tablespoon honey"
        },
        {
            "6": "1 teaspoon vanilla extract"
        }
    ],
    "Directions": [
        {
            "1": "Blend strawberries, raspberries, milk, yogurt, honey, and vanilla extract in a blender until smooth."
        }
    ],
