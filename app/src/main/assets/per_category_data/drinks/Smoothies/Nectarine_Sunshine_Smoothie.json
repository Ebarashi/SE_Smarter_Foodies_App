{
    "title": "Nectarine Sunshine Smoothie",
    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "2 large  nectarines, pitted and quartered"
        },
        {
            "2": "1  banana, cut into pieces and frozen"
        },
        {
            "3": "1 large orange, peeled and quartered"
        },
        {
            "4": "1 cup vanilla yogurt"
        },
        {
            "5": "1 cup orange juice"
        },
        {
            "6": "1 tablespoon honey"
        }
    ],
    "Directions": [
        {
            "1": "Place the nectarines, frozen banana chunks, orange, vanilla yogurt, orange juice, and honey into a blender, and blend until smooth."
        }
    ],
