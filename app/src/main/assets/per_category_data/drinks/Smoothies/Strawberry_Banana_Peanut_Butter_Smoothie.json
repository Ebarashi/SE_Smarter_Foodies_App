{
    "title": "Strawberry-Banana-Peanut Butter Smoothie",
    "Category": "Smoothies",
    "Ingredients": [
        {
            "1": "0.5 cup nonfat plain yogurt"
        },
        {
            "2": "2 tablespoons peanut butter"
        },
        {
            "3": "1  banana"
        },
        {
            "4": "4  fresh strawberries, hulled"
        },
        {
            "5": "10  ice cubes"
        }
    ],
    "Directions": [
        {
            "1": "Place yogurt, peanut butter, banana, strawberries, and ice cubes into a blender. Puree until smooth."
        }
    ],
