{
    "title": "Bethy's Cucumber Basil Lemonade",
    "Category": "Lemonade",
    "Ingredients": [
        {
            "1": "8 cups cold water, divided"
        },
        {
            "2": "1 cup white sugar"
        },
        {
            "3": "0.5 cups lemon juice"
        },
        {
            "4": "0.5  cucumber, thinly sliced"
        },
        {
            "5": "10  fresh basil leaves, torn"
        }
    ],
    "Directions": [
        {
            "1": "Combine 1 cup water and sugar together in a small saucepan; bring to a boil. Cook and stir until sugar is dissolved, about 5 minutes. Remove saucepan from heat and cool syrup to room temperature."
        },
        {
            "2": "Stir syrup, remaining water, lemon juice, cucumber, and basil together in a pitcher; chill in the refrigerator for at least 2 hours."
        }
    ],
