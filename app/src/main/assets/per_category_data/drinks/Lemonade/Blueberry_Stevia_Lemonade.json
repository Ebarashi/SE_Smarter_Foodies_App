{
    "title": "Blueberry Stevia Lemonade",
    "Category": "Lemonade",
    "Ingredients": [
        {
            "1": "10  fresh organic blueberries, plus more for garnish"
        },
        {
            "2": "2 ounces fresh lemon juice"
        },
        {
            "3": "2 sprigs fresh thyme"
        },
        {
            "4": "2 packets Wholesome Organic Stevia, or more to taste"
        },
        {
            "5": "1 Sparkling water to taste"
        },
        {
            "6": "1 Lemon wheel for garnish"
        }
    ],
    "Directions": [
        {
            "1": "Muddle blueberries with lemon juice in glass."
        },
        {
            "2": "Stir in thyme sprigs and Wholesome Organic Stevia."
        },
        {
            "3": "Top with sparkling water to taste."
        },
        {
            "4": "Garnish with additional blueberries and lemon wheel."
        }
    ],
