{
    "title": "Icy Blender Lemonade",
    "Category": "Lemonade",
    "Ingredients": [
        {
            "1": "1 large lemon, peeled and seeded"
        },
        {
            "2": "0.5 cup white sugar, or to taste"
        },
        {
            "3": "3 cups cold water"
        },
        {
            "4": "6  ice cubes"
        }
    ],
    "Directions": [
        {
            "1": "Place lemon, sugar, cold water, and ice cubes into the container of a blender. Blend until smooth, and serve immediately."
        }
    ],
