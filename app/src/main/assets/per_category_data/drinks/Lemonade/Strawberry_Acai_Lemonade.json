{
    "title": "Strawberry Acai Lemonade",
    "Category": "Lemonade",
    "Ingredients": [
        {
            "1": "1 cup sliced fresh strawberries"
        },
        {
            "2": "0.5 cup freshly squeezed lemon juice"
        },
        {
            "3": "2 teaspoons acai powder"
        },
        {
            "4": "1 tablespoon stevia sweetener (such as Truvia\u00ae), or more to taste"
        },
        {
            "5": "3 cups water, divided"
        },
        {
            "6": "1 ice as needed"
        }
    ],
    "Directions": [
        {
            "1": "Combine strawberries, lemon juice, acai powder, and 1 cup water in a blender; blend until smooth."
        },
        {
            "2": "Pour into a pitcher and add remaining 2 cups water and 1 tablespoon sweetener. Add more sweetener according to sweetness desired. Serve over ice."
        }
    ],
