{
    "title": "Strawberry-Ginger-Mint Lemonade",
    "Category": "Lemonade",
    "Ingredients": [
        {
            "1": "18  fresh strawberries, stemmed"
        },
        {
            "2": "1 bunch fresh mint leaves"
        },
        {
            "3": "2 cups freshly squeezed lemon juice"
        },
        {
            "4": "6 cups water, divided, or more as needed"
        },
        {
            "5": "0.25 cup white sugar"
        },
        {
            "6": "2 tablespoons minced fresh ginger root, or more to taste"
        }
    ],
    "Directions": [
        {
            "1": "Place strawberries, mint, lemon juice, 1 cup water, sugar, and ginger into an electric blender and blend well."
        },
        {
            "2": "Pour into a pitcher, add 5 cups water, and stir well. Refrigerate or serve immediately over crushed ice."
        }
    ],
    "Details": [
