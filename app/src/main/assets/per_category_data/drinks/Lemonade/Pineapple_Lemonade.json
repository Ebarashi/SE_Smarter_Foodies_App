{
    "title": "Pineapple Lemonade",
    "Category": "Lemonade",
    "Ingredients": [
        {
            "1": "1  fresh pineapple"
        },
        {
            "2": "4 cups water"
        },
        {
            "3": "0.5 cup freshly squeezed lemon juice"
        },
        {
            "4": "1 packet sweetener of your choice, or to taste"
        },
        {
            "5": "2 cups ice cubes"
        }
    ],
    "Directions": [
        {
            "1": "Core pineapple using a pineapple corer or slicer. Break up into chunks and measure out 2 cups. Save juices."
        },
        {
            "2": "Combine 2 cups pineapple and juices, 2 cups water, and lemon juice in a blender. Blend until smooth. Pour into a pitcher. Add remaining 2 cups water and sweetener. Stir and serve over ice."
        }
    ],
