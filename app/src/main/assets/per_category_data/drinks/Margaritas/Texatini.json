{
    "title": "Texatini",
    "Category": "Margaritas",
    "Ingredients": [
        {
            "1": "1 tablespoon coarse salt"
        },
        {
            "2": "1 (1.5 fluid ounce) jigger tequila"
        },
        {
            "3": "1 (1.5 fluid ounce) jigger orange liqueur (Cointreau, Triple Sec or Grand Marnier)"
        },
        {
            "4": "0.5 cup sweet and sour mix"
        },
        {
            "5": "1 fluid ounce orange juice"
        },
        {
            "6": "1 cup crushed ice"
        },
        {
            "7": "1  jalapeno-stuffed green olive"
        }
    ],
    "Directions": [
        {
            "1": "Slightly moisten rim of a large martini glass, and dip in course salt to rim the glass."
        },
        {
            "2": "Combine tequila, orange liqueur, sweet and sour mix, orange juice, and ice in a shaker. Shake vigorously, and strain into martini glass. Garnish with jalapeno-stuffed green olives."
        }
    ],
