{
    "title": "Strawberry Basil Margarita",
    "Category": "Margaritas",
    "Ingredients": [
        {
            "1": "1 cup hulled strawberries"
        },
        {
            "2": "0.25 cup tequila"
        },
        {
            "3": "1 tablespoon orange-flavored liqueur, such as Cointreau \u00ae"
        },
        {
            "4": "1 tablespoon lemon juice"
        },
        {
            "5": "2 tablespoons white sugar"
        },
        {
            "6": "3 large basil leaves"
        },
        {
            "7": "8  ice cubes"
        }
    ],
    "Directions": [
        {
            "1": "Combine the strawberries, tequila, orange-flavored liqueur, lemon juice, sugar, and basil leaves in a blender; mix on low until smooth. Add the ice and puree until the ice is crushed, 30 to 60 seconds."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Total Time": "5 mins"
        },
        {
            "Servings": "2 "
        },
        {
            "Yield": "2 cocktails"
        }
    ],
