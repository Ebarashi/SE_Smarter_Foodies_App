{
    "title": "Beer Margaritas",
    "Category": "Margaritas",
    "Ingredients": [
        {
            "1": "1 (12 fluid ounce) can frozen limeade concentrate"
        },
        {
            "2": "12 fluid ounces tequila"
        },
        {
            "3": "12 fluid ounces water, or more as needed"
        },
        {
            "4": "12 fluid ounces beer"
        },
        {
            "5": "1 ice"
        },
        {
            "6": "1  lime, cut into wedges"
        }
    ],
    "Directions": [
        {
            "1": "Pour limeade concentrate, tequila, water, and beer into a large pitcher. Stir until limeade has melted."
        },
        {
            "2": "Add plenty of ice to the pitcher and garnish with lime wedges. Adjust with additional water if needed."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Total Time": "5 mins"
        },
        {
            "Servings": "6 "
        },
        {
            "Yield": "6 1-cup servings"
        }
    ],
