{
    "title": "Margaritas",
    "Category": "Margaritas",
    "Ingredients": [
        {
            "1": "4 cups crushed ice, or as needed"
        },
        {
            "2": "1 (6 ounce) can frozen limeade concentrate"
        },
        {
            "3": "6 fluid ounces tequila"
        },
        {
            "4": "2 fluid ounces triple sec"
        }
    ],
    "Directions": [
        {
            "1": "Fill blender with crushed ice. Pour in limeade concentrate, tequila, and triple sec. Blend until smooth. Pour into glasses and serve."
        }
    ],
