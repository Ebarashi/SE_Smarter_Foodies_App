{
    "title": "Creamy Coconut Margarita",
    "Category": "Margaritas",
    "Ingredients": [
        {
            "1": "1 (15 ounce) can cream of coconut (such as Coco Lopez\u00ae)"
        },
        {
            "2": "1 cup ice"
        },
        {
            "3": "0.75 cup tequila (such as Sauza\u00ae 100% Blue Agave)"
        },
        {
            "4": "0.5 cup freshly squeezed lime juice"
        },
        {
            "5": "0.25 cup brandy-based orange liqueur (such as Grand Marnier\u00ae)"
        }
    ],
    "Directions": [
        {
            "1": "Blend cream of coconut, ice, tequila, lime juice, and orange liqueur in a blender until smooth."
        }
    ],
