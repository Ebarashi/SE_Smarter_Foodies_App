{
    "title": "The Perfect Blended Margarita",
    "Category": "Margaritas",
    "Ingredients": [
        {
            "1": "0.25 cups fresh lime juice"
        },
        {
            "2": "1 cup Triple Sec or other orange liqueur"
        },
        {
            "3": "1 cup silver tequila"
        },
        {
            "4": "3 tablespoons fresh lemon juice"
        },
        {
            "5": "3 cups ice cubes"
        }
    ],
    "Directions": [
        {
            "1": "Combine the lime juice, triple sec, tequila, lemon juice, and 2 cups ice in a blender; blend until smooth."
        },
        {
            "2": "Place 1 cup ice cubes in a cocktail shaker; add desired amount of margarita; cover and shake well; serve."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Total Time": "5 mins"
        },
        {
            "Servings": "4 "
        }
