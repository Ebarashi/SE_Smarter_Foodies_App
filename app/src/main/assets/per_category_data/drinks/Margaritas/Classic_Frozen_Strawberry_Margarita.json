{
    "title": "Classic Frozen Strawberry Margarita",
    "Category": "Margaritas",
    "Ingredients": [
        {
            "1": "0.25 cup sliced fresh strawberries"
        },
        {
            "2": "0.5 fluid ounces tequila"
        },
        {
            "3": "1 fluid ounce lime juice"
        },
        {
            "4": "0.5 fluid ounce triple sec"
        },
        {
            "5": "1 teaspoon white sugar, or to taste"
        },
        {
            "6": "1 cup ice cubes"
        },
        {
            "7": "1 wedge lime"
        },
        {
            "8": "1 teaspoon white sugar"
        }
    ],
    "Directions": [
        {
            "1": "Place strawberries, tequila, lime juice, triple sec, and 1 teaspoon sugar in a blender; blend until smooth."
        },
        {
            "2": "Add ice cubes; blend on high until ice is crushed, about 15 seconds."
        },
        {
            "3": "Rub lime wedge around the rim of a glass. Spread 1 teaspoon sugar onto a plate. Dip glass rim in sugar to coat; pour margarita into the glass."
        }
    ],
