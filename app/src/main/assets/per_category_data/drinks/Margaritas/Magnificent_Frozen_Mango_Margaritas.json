{
    "title": "Magnificent Frozen Mango Margaritas",
    "Category": "Margaritas",
    "Ingredients": [
        {
            "1": "1 cup ice, or as needed"
        },
        {
            "2": "1 cup frozen mango chunks"
        },
        {
            "3": "3 fluid ounces tequila"
        },
        {
            "4": "3 fluid ounces triple sec"
        },
        {
            "5": "3 fluid ounces ginger ale (such as Vernors\u00ae)"
        },
        {
            "6": "3 fluid ounces simple syrup"
        },
        {
            "7": "0.5 fluid ounces lemon juice"
        }
    ],
    "Directions": [
        {
            "1": "Blend ice, mango, tequila, triple sec, ginger ale, simple syrup, and lemon juice in a blender until smooth."
        }
    ],
