{
    "title": "Frozen Banana Margaritas",
    "Category": "Margaritas",
    "Ingredients": [
        {
            "1": "2 large bananas"
        },
        {
            "2": "0.75 cup banana liqueur"
        },
        {
            "3": "0.5 cup tequila"
        },
        {
            "4": "0.25 cup triple sec (orange-flavored liqueur)"
        },
        {
            "5": "2 tablespoons lemon juice"
        },
        {
            "6": "2 tablespoons lime juice"
        },
        {
            "7": "6  ice cubes, or as needed"
        }
    ],
    "Directions": [
        {
            "1": "Combine bananas, banana liqueur, tequila, triple sec, lemon juice, and lime juice in the container of a blender. Add ice cubes until the mixture reaches the 6-cup line. Cover and blend until smooth."
        },
        {
            "2": "Pour into margarita glasses to serve."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Total Time": "15 mins"
        },
        {
            "Servings": "8 "
        }
    ],
