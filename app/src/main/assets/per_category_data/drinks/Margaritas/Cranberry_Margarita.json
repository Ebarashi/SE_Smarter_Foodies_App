{
    "title": "Cranberry Margarita",
    "Category": "Margaritas",
    "Ingredients": [
        {
            "1": "1 teaspoon white sugar, or as needed"
        },
        {
            "2": "1 teaspoon salt, or as needed"
        },
        {
            "3": "1 ice cubes"
        },
        {
            "4": "1  orange, sliced"
        },
        {
            "5": "1  lime, sliced"
        },
        {
            "6": "4 ounces 100% cranberry juice (such as Ocean Spray\u00ae Organic)"
        },
        {
            "7": "2 ounces silver tequila (such as Patron\u00ae)"
        },
        {
            "8": "1 ounce triple sec"
        },
        {
            "9": "1  orange, juiced"
        },
        {
            "10": "1  lime, juiced"
        },
        {
            "11": "8 ounces lemon-lime flavored carbonated beverage (such as Squirt\u00ae)"
        }
    ],
    "Directions": [
        {
            "1": "Rim 2 wine glasses with sugar and salt. Fill with ice. Add an orange slice and lime slice to each glass."
        },
        {
            "2": "Pour cranberry juice, tequila, triple sec, orange juice, and lime juice into a shaker. Shake well."
        },
        {
            "3": "Pour mixture evenly over ice in rimmed glasses. Top with lemon-lime beverage."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Total Time": "10 mins"
        },
        {
            "Servings": "2 "
        },
        {
            "Yield": "2 margaritas"
        }
    ],
