{
    "title": "Presidente Margarita",
    "Category": "Margaritas",
    "Ingredients": [
        {
            "1": "1 salt for rimming glass"
        },
        {
            "2": "3 cubes  ice, or as desired"
        },
        {
            "3": "4 fluid ounces sweet-and-sour cocktail mix"
        },
        {
            "4": "0.25 fluid ounces tequila"
        },
        {
            "5": "0.5 fluid ounce orange-flavored liqueur (such as Cointreau\u00ae)"
        },
        {
            "6": "0.5 fluid ounce brandy (such as Presidente\u00ae)"
        },
        {
            "7": "1 splash lime juice"
        }
    ],
    "Directions": [
        {
            "1": "Rim a margarita glass with salt; place ice into glass. Pour cocktail mix, tequila, orange liqueur, brandy, and lime juice into a cocktail shaker, cover, and shake to combine. Pour into prepared glass."
        }
