{
    "title": "Awesome and Easy Cranberry Sauce",
    "Category": "Applesauce",
    "Ingredients": [
        {
            "1": "1 (12 ounce) bag cranberries"
        },
        {
            "2": "1  Gala apple, cored and cut into chunks"
        },
        {
            "3": "1  pear, cored and cut into bite-size chunks"
        },
        {
            "4": "1 teaspoon lemon juice"
        },
        {
            "5": "0.25 cup water"
        },
        {
            "6": "0.25 cup white sugar, or more to taste"
        }
    ],
    "Directions": [
        {
            "1": "Stir the cranberries, Gala apple, pear, lemon juice, water, and white sugar together in a saucepan over medium heat; bring to a boil, reduce heat to low, and cook at a simmer until the cranberries pop and the liquid is absorbed, 20 to 30 minutes."
        }
    ],
