{
    "title": "Rosy Strawberry Rhubarb Applesauce",
    "Category": "Applesauce",
    "Ingredients": [
        {
            "1": "5 large Red Delicious apples, finely chopped"
        },
        {
            "2": "4 cups hulled and halved strawberries"
        },
        {
            "3": "4 cups finely chopped rhubarb"
        },
        {
            "4": "0.5 cup white sugar"
        },
        {
            "5": "0.25 cup water"
        },
        {
            "6": "2 teaspoons vanilla extract, or to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine apples, strawberries, rhubarb, sugar, and water in a large pot. Bring to a boil; cover and simmer, stirring occasionally, until apples are tender, about 20 minutes. Remove from heat and stir in vanilla extract."
        }
    ],
