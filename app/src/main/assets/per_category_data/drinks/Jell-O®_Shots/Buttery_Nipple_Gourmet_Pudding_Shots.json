{
    "title": "Buttery Nipple Gourmet Pudding Shots",
    "Category": "Jell-O\u00ae Shots",
    "Ingredients": [
        {
            "1": "0.75 cups cold milk"
        },
        {
            "2": "2 fluid ounces butterscotch schnapps"
        },
        {
            "3": "2 fluid ounces Irish cream liqueur (such as Bailey's\u00ae)"
        },
        {
            "4": "1 fluid ounce vodka"
        },
        {
            "5": "1 fluid ounce coffee-flavored liqueur (such as Kahlua\u00ae)"
        },
        {
            "6": "1 (3.4 ounce) package instant butterscotch pudding mix"
        }
    ],
    "Directions": [
        {
            "1": "Stir milk, schnapps, Irish cream liqueur, vodka, and coffee-flavored liqueur together in a pitcher. Add pudding mix and blend thoroughly."
        },
        {
            "2": "Pour mixture into disposable shot cups and refrigerate until chilled, at least 30 minutes."
        }
    ],
