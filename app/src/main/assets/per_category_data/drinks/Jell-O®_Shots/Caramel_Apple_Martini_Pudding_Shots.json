{
    "title": "Caramel Apple Martini Pudding Shots",
    "Category": "Jell-O\u00ae Shots",
    "Ingredients": [
        {
            "1": "0.75 cup milk"
        },
        {
            "2": "1 (3.4 ounce) package butterscotch instant pudding mix"
        },
        {
            "3": "0.5 cup sour apple schnapps (such as DeKuyper\u00ae Sour Apple Pucker \u00ae)"
        },
        {
            "4": "2 tablespoons butterscotch schnapps (such as DeKuyper\u00ae Buttershots\u00ae)"
        },
        {
            "5": "2 tablespoons vodka"
        },
        {
            "6": "1 (8 ounce) container frozen whipped topping (such as Cool Whip\u00ae), thawed"
        }
    ],
    "Directions": [
        {
            "1": "Beat milk and pudding mix together in a bowl until smooth; add apple schnapps, butterscotch schnapps, and vodka and stir until blended. Fold whipped topping into pudding mixture until incorporated. Spoon the pudding mixture into shot glasses or disposable 'party shot' cups."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Total Time": "10 mins"
        },
        {
            "Servings": "12 "
        },
        {
            "Yield": "12 pudding shots"
        }
    ],
