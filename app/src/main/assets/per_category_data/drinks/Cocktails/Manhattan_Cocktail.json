{
    "title": "Manhattan Cocktail",
    "Category": "Cocktails",
    "Ingredients": [
        {
            "1": "2 fluid ounces rye whiskey"
        },
        {
            "2": "0.5 fluid ounce sweet vermouth"
        },
        {
            "3": "1 dash Angostura bitters"
        },
        {
            "4": "1 cup ice cubes"
        },
        {
            "5": "1  maraschino cherry"
        }
    ],
    "Directions": [
        {
            "1": "Combine whiskey, vermouth, and bitters in a cocktail mixing glass. Add ice and stir until chilled."
        },
        {
            "2": "Strain into a chilled cocktail glass and garnish with a maraschino cherry."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Total Time": "5 mins"
        },
        {
            "Servings": "1 "
        }
