{
    "title": "The Real Rum Runner",
    "Category": "Cocktails",
    "Ingredients": [
        {
            "1": "2 cups ice, divided, or as needed"
        },
        {
            "2": "1 fluid ounce pineapple juice"
        },
        {
            "3": "1 fluid ounce orange juice"
        },
        {
            "4": "1 fluid ounce blackberry liqueur"
        },
        {
            "5": "1 fluid ounce banana liqueur"
        },
        {
            "6": "1 fluid ounce light rum"
        },
        {
            "7": "1 fluid ounce dark rum"
        },
        {
            "8": "1 splash grenadine syrup, or more to taste"
        },
        {
            "9": "1 fluid ounce high-proof rum (such as Bacardi\u00ae151)"
        },
        {
            "10": "1 slice orange"
        }
    ],
    "Directions": [
        {
            "1": "Combine 1 cup ice, pineapple juice, orange juice, blackberry liqueur, banana liqueur, light rum, dark rum, and grenadine in a cocktail shaker; shake to combine."
        },
        {
            "2": "Fill a hurricane glass with ice. Pour in mixture. Add high-proof rum for a floater and garnish with orange slice."
        }
    ],
