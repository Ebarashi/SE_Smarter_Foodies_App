{
    "title": "Electric Lemonade",
    "Category": "Cocktails",
    "Ingredients": [
        {
            "1": "1 ice cubes"
        },
        {
            "2": "2 fluid ounces sour mix"
        },
        {
            "3": "1 (1.5 fluid ounce) jigger citron vodka"
        },
        {
            "4": "0.5 fluid ounce Blue Curacao"
        },
        {
            "5": "1 (12 fluid ounce) can or bottle lemon-lime flavored carbonated beverage"
        },
        {
            "6": "1  lemon - cut into wedges, for garnish"
        }
    ],
    "Directions": [
        {
            "1": "Fill a Collins glass with ice. Pour sour mix, vodka, and blue curacao over ice."
        },
        {
            "2": "Fill to the top with lemon-lime soda. Garnish with lemon wedge; serve with a straw."
        }
    ],
