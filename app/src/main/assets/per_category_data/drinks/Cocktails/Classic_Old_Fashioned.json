{
    "title": "Classic Old Fashioned",
    "Category": "Cocktails",
    "Ingredients": [
        {
            "1": "2 teaspoons simple syrup"
        },
        {
            "2": "1 teaspoon water"
        },
        {
            "3": "2 dashes bitters"
        },
        {
            "4": "1 cup ice cubes"
        },
        {
            "5": "1 (1.5 fluid ounce) jigger bourbon whiskey"
        },
        {
            "6": "1 slice orange"
        },
        {
            "7": "1  maraschino cherry"
        }
    ],
    "Directions": [
        {
            "1": "Pour simple syrup, water, and bitters into a whiskey glass; stir to combine. Add ice cubes and pour in bourbon. Garnish with orange slice and maraschino cherry."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {
            "Total Time": "5 mins"
        },
        {
            "Servings": "1 "
        }
    ],
