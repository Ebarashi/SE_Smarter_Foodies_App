{
    "title": "Hot Toddy",
    "Category": "Cocktails",
    "Ingredients": [
        {
            "1": "2 fluid ounces boiling water"
        },
        {
            "2": "0.5 fluid ounces whiskey"
        },
        {
            "3": "1 teaspoon honey"
        },
        {
            "4": "3  whole cloves"
        },
        {
            "5": "1  cinnamon stick"
        },
        {
            "6": "1 slice lemon"
        },
        {
            "7": "1 pinch ground nutmeg"
        }
    ],
    "Directions": [
        {
            "1": "Pour boiling water, whiskey, and honey into a mug. Add cloves, cinnamon, and lemon slice. Let mixture stand for 5 minutes so flavors can mingle, then sprinkle with a pinch of nutmeg before serving."
        }
    ],
    "Details": [
