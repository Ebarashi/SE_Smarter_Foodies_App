{
    "title": "Pineapple Upside-Down Cake in a Glass",
    "Category": "Cocktails",
    "Ingredients": [
        {
            "1": "1 ice"
        },
        {
            "2": "1 splash grenadine syrup"
        },
        {
            "3": "3 ounces pineapple juice"
        },
        {
            "4": "1 (1.5 fluid ounce) jigger cake-flavored vodka"
        },
        {
            "5": "1  maraschino cherry"
        }
    ],
    "Directions": [
        {
            "1": "Fill a glass with ice."
        },
        {
            "2": "Splash grenadine over ice. Pour in pineapple juice and cake-flavored vodka; stir."
        },
        {
            "3": "Garnish with maraschino cherry to serve."
        }
    ],
    "Details": [
