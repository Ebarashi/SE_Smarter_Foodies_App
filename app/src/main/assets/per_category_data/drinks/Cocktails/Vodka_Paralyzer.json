{
    "title": "Vodka Paralyzer",
    "Category": "Cocktails",
    "Ingredients": [
        {
            "1": "1 cup ice cubes"
        },
        {
            "2": "4 fluid ounces cola"
        },
        {
            "3": "1 fluid ounce coffee flavored liqueur"
        },
        {
            "4": "1 (1.5 fluid ounce) jigger vodka"
        },
        {
            "5": "2 fluid ounces milk"
        },
        {
            "6": "1  maraschino cherry"
        }
    ],
    "Directions": [
        {
            "1": "Fill a highball glass with ice."
        },
        {
            "2": "In the order listed, pour in cola, coffee liqueur, vodka, then milk. Stir briefly and top with a maraschino cherry."
        }
    ],
