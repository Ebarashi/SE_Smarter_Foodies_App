{
    "title": "Moscow Mule Cocktail",
    "Category": "Cocktails",
    "Ingredients": [
        {
            "1": "0.5 fluid ounces vodka"
        },
        {
            "2": "0.5 fluid ounce lime juice"
        },
        {
            "3": "1 ice cubes"
        },
        {
            "4": "0.5 cup ginger beer"
        },
        {
            "5": "1  lime wedge for garnish"
        }
    ],
    "Directions": [
        {
            "1": "Pour vodka and lime juice into a mug; add ice cubes and ginger beer. Stir to combine."
        },
        {
            "2": "Drop a lime wedge into the mug for garnish."
        }
    ],
