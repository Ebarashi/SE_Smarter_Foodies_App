{
    "title": "Old Fashioned Cocktail",
    "Category": "Cocktails",
    "Ingredients": [
        {
            "1": "1  sugar cube"
        },
        {
            "2": "1 teaspoon water"
        },
        {
            "3": "1 dash bitters"
        },
        {
            "4": "2 fluid ounces whiskey (rye or bourbon)"
        },
        {
            "5": "1  lemon twist"
        },
        {
            "6": "0.5 cup ice cubes, or as needed"
        },
        {
            "7": "1  orange slice, for garnish"
        },
        {
            "8": "1  maraschino cherry, for garnish"
        }
    ],
    "Directions": [
        {
            "1": "Muddle sugar cube, water, and bitters in an old fashioned glass for 1 minute. Pour in whiskey; stir for 1 minute."
        },
        {
            "2": "Squeeze lemon twist over the glass and drop it in. Add ice cubes. Garnish with orange slice and maraschino cherry; serve with a swizzle stick."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
