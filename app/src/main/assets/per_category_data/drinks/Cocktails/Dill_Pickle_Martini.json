{
    "title": "Dill Pickle Martini",
    "Category": "Cocktails",
    "Ingredients": [
        {
            "1": "2 cups ice"
        },
        {
            "2": "2.0 fluid ounces vodka"
        },
        {
            "3": "0.5 fluid ounces dill pickle juice"
        },
        {
            "4": "2  dill pickle spears"
        }
    ],
    "Directions": [
        {
            "1": "Pour ice into a cocktail shaker; pour in vodka and dill pickle juice. Cover the shaker and shake vigorously for about 20 seconds; strain into martini glasses. Garnish each cocktail with a dill pickle spear."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
