{
    "title": "Classic Canadian Caesar",
    "Category": "Cocktails",
    "Ingredients": [
        {
            "1": "1  lime wedge"
        },
        {
            "2": "1 tablespoon celery salt, or as needed"
        },
        {
            "3": "1 ice cubes, as needed"
        },
        {
            "4": "1 fluid ounce vodka"
        },
        {
            "5": "1 dash Worcestershire sauce, or to taste"
        },
        {
            "6": "1 dash hot pepper sauce (such as Tabasco\u00ae), or to taste"
        },
        {
            "7": "8 fluid ounces tomato and clam juice cocktail (such as Clamato\u00ae)"
        },
        {
            "8": "1  celery stick"
        }
    ],
    "Directions": [
        {
            "1": "Wet the rim of a cocktail glass with lime wedge; set lime wedge aside for garnish."
        },
        {
            "2": "Place celery salt in a small dish; press the rim of the glass into salt to coat. Add ice to the glass."
        },
        {
            "3": "Pour vodka, Worcestershire sauce, and hot pepper sauce over ice; top with tomato-clam juice."
        },
        {
            "4": "Garnish with lime wedge and celery stick. Serve with a straw."
        }
    ],
