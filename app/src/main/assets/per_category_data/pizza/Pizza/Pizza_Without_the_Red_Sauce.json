{
    "title": "Pizza Without the Red Sauce",
    "Category": "Pizza",
    "Ingredients": [
        {
            "1": "2 tablespoons butter, melted"
        },
        {
            "2": "1 tablespoon olive oil"
        },
        {
            "3": "3 tablespoons minced garlic"
        },
        {
            "4": "2 tablespoons sun-dried tomato pesto"
        },
        {
            "5": "1 teaspoon dried basil leaves"
        },
        {
            "6": "1 teaspoon dried oregano"
        },
        {
            "7": "1 tablespoon grated Parmesan cheese"
        },
        {
            "8": "1  unbaked pizza crust"
        },
        {
            "9": "1  tomato, sliced"
        },
        {
            "10": "1 bunch fresh spinach, torn"
        },
        {
            "11": "1  sweet onion, sliced"
        },
        {
            "12": "1  fresh jalapeno pepper, chopped"
        },
        {
            "13": "1 (6 ounce) package feta cheese, crumbled"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven according to pizza crust package directions."
        },
        {
            "2": "In a small bowl combine butter, olive oil, garlic, pesto, basil, oregano and Parmesan cheese. Spread mixture evenly on pizza crust."
        },
        {
            "3": "Arrange tomato, spinach, onion and jalapeno on pizza. Top with crumbled feta cheese."
        },
        {
            "4": "Bake according to pizza crust package directions."
        }
    ],
