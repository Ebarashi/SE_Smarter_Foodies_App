{
    "title": "World's Easiest Bread Machine Pizza Dough",
    "Category": "Pizza Dough and Crusts",
    "Ingredients": [
        {
            "1": "1 cup warm water"
        },
        {
            "2": "3 tablespoons olive oil"
        },
        {
            "3": "3 tablespoons white sugar"
        },
        {
            "4": "1 teaspoon sea salt"
        },
        {
            "5": "3 cups all-purpose flour"
        },
        {
            "6": "1 teaspoon minced garlic"
        },
        {
            "7": "0.25 teaspoon dried oregano"
        },
        {
            "8": "0.25 teaspoon dried basil"
        },
        {
            "9": "0.25 teaspoon ground black pepper"
        },
        {
            "10": "0.25 teaspoon dried cilantro"
        },
        {
            "11": "0.25 teaspoon paprika"
        },
        {
            "12": "0.5 teaspoons active dry yeast"
        }
    ],
    "Directions": [
        {
            "1": "Put the water, olive oil, sugar, sea salt, flour, garlic, oregano, basil, black pepper, cilantro, paprika, and dry yeast into the pan of a bread machine individually in order as listed. Select Dough cycle; press Start. Let dough rest 5 to 30 minutes before using; the longer it rests, the thicker the crust."
        }
    ],
    "Details": [
        {
            "Prep Time": " 2 hrs"
        },
        {
            "Additional Time": "5 mins"
        },
        {
            "Total Time": " 2 hrs 5 mins"
        },
        {
            "Servings": "4 "
        },
        {
            "Yield": "1 crust"
        }
