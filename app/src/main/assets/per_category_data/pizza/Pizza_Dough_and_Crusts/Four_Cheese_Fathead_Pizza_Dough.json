{
    "title": "Four-Cheese Fathead Pizza Dough",
    "Category": "Pizza Dough and Crusts",
    "Ingredients": [
        {
            "1": "1 cup shredded mozzarella cheese"
        },
        {
            "2": "0.25 cup shredded sharp Cheddar cheese"
        },
        {
            "3": "0.25 cup shredded Parmesan cheese"
        },
        {
            "4": "2 tablespoons cream cheese"
        },
        {
            "5": "0.75 cup almond flour"
        },
        {
            "6": "1  egg"
        },
        {
            "7": "1 teaspoon granulated garlic"
        },
        {
            "8": "1 teaspoon dried parsley"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 400 degrees F (200 degrees C)."
        },
        {
            "2": "Combine mozzarella cheese, Cheddar cheese, Parmesan cheese, and cream cheese in a microwave safe bowl. Microwave for 1 minute. Remove and mix in almond flour, egg, garlic, and parsley until well combined. Spread dough onto a baking sheet and poke holes into the dough with a fork."
        },
        {
            "3": "Bake in the preheated oven for 10 minutes. Remove and let cool for 20 minutes."
        }
    ],
