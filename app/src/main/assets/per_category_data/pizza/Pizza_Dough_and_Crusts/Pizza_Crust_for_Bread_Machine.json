{
    "title": "Pizza Crust for Bread Machine",
    "Category": "Pizza Dough and Crusts",
    "Ingredients": [
        {
            "1": "0.25 cups warm water (110 degrees F/45 degrees C)"
        },
        {
            "2": "2 cups all-purpose flour"
        },
        {
            "3": "1 cup semolina flour"
        },
        {
            "4": "0.5 teaspoon white sugar"
        },
        {
            "5": "1 teaspoon salt"
        },
        {
            "6": "2 teaspoons olive oil"
        },
        {
            "7": "2 teaspoons active dry yeast"
        }
    ],
    "Directions": [
        {
            "1": "Place all ingredients in the bread machine pan in the order suggested by the manufacturer. Select dough cycle."
        },
        {
            "2": "Remove after rise cycle and use with your favorite pizza recipe."
        }
    ],
    "Details": [
        {
            "Servings": "12 "
        },
        {
            "Yield": "1 large pizza crust"
        }
    ],
