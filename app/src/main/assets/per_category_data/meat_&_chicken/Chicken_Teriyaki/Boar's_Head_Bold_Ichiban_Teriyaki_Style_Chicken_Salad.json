{
    "title": "Boar's Head Bold Ichiban Teriyaki Style Chicken Salad",
    "Category": "Chicken Teriyaki",
    "Ingredients": [
        {
            "1": "1 Salad Ingredients-"
        },
        {
            "2": "3 (1/4 inch thick) slices Boar\u2019s Head Bold\u00ae Ichiban Teriyaki\u2122 Style Oven Roasted Chicken, cubed"
        },
        {
            "3": "0.25 cup celery, diced"
        },
        {
            "4": "0.25 cup scallions, diced"
        },
        {
            "5": "0.5 cup Mandarin oranges, diced"
        },
        {
            "6": "1 Teriyaki Sauce Ingredients-"
        },
        {
            "7": "4 tablespoons soy sauce"
        },
        {
            "8": "4 tablespoons rice vinegar"
        },
        {
            "9": "1 teaspoon honey"
        },
        {
            "10": "1 teaspoon brown sugar"
        }
    ],
    "Directions": [
        {
            "1": "Slice Teriyaki Chicken into small 1/4-inch cubes. Finely dice celery and scallions. Drain mandarin oranges and slice into small pieces. Place the ingredients into a large mixing bowl and set aside."
        },
        {
            "2": "In a separate large mixing bowl, combine the Teriyaki Sauce ingredients. Mix the sauce and pour into the large bowl with the other ingredients. Mix the salad and serve."
        }
    ],
