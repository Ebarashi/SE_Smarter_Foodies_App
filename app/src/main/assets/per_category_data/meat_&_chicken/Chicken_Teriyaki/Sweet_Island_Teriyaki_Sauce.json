{
    "title": "Sweet Island Teriyaki Sauce",
    "Category": "Chicken Teriyaki",
    "Ingredients": [
        {
            "1": "0.5 cup light soy sauce"
        },
        {
            "2": "0.25 cup sake"
        },
        {
            "3": "0.25 cup minced fresh pineapple"
        },
        {
            "4": "0.25 cup honey"
        },
        {
            "5": "0.25 cup white sugar"
        },
        {
            "6": "2 cloves garlic, minced"
        },
        {
            "7": "1 tablespoon grated fresh ginger"
        },
        {
            "8": "0.25 teaspoon fish sauce"
        }
    ],
    "Directions": [
        {
            "1": "Combine soy sauce, sake, pineapple, honey, white sugar, garlic, ginger, and fish sauce in a saucepan over medium heat; cook and stir until just boiling, about 7 minutes. Remove from heat, cool, and store in refrigerator."
        }
    ],
