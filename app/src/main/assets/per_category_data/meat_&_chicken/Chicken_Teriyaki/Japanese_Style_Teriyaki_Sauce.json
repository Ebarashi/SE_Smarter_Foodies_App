{
    "title": "Japanese Style Teriyaki Sauce",
    "Category": "Chicken Teriyaki",
    "Ingredients": [
        {
            "1": "3 cups soy sauce"
        },
        {
            "2": "0.5 cups mirin (Japanese sweet wine)"
        },
        {
            "3": "1  orange, juiced"
        },
        {
            "4": "6 ounces chopped fresh pineapple with skin"
        },
        {
            "5": "1  Granny Smith apple, cored and chopped"
        },
        {
            "6": "2 cloves garlic"
        },
        {
            "7": "1 teaspoon grated fresh ginger root"
        },
        {
            "8": "4 tablespoons white sugar, divided"
        }
    ],
    "Directions": [
        {
            "1": "In a saucepan, combine soy sauce, mirin, orange juice, pineapple, Granny Smith apple, garlic, ginger, and half of the sugar; bring to a boil. Reduce heat, and simmer 20 minutes. Adjust sweetness to taste with remaining sugar. Strain through a fine mesh sieve; cool."
        }
    ],
