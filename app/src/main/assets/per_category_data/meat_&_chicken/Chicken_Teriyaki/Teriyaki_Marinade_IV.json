{
    "title": "Teriyaki Marinade IV",
    "Category": "Chicken Teriyaki",
    "Ingredients": [
        {
            "1": "0.25 cup white sugar"
        },
        {
            "2": "0.5 teaspoon garlic powder"
        },
        {
            "3": "0.5 teaspoon ground ginger"
        },
        {
            "4": "1 teaspoon salt"
        },
        {
            "5": "1 teaspoon Worcestershire sauce"
        },
        {
            "6": "1 (10 fluid ounce) bottle reduced sodium teriyaki sauce"
        }
    ],
    "Directions": [
        {
            "1": "In a medium bowl, mix white sugar, garlic powder, ground ginger and salt. Mix in Worcestershire sauce and reduced sodium teriyaki sauce. Stir until all dry ingredients have dissolved. Place desired meat into the mixture. Cover and marinate in the refrigerator at least 6 hours before grilling as desired."
        }
    ],
