{
    "title": "Grilled Flank Steak",
    "Category": "Flank Steak",
    "Ingredients": [
        {
            "1": "0.5 cup olive oil"
        },
        {
            "2": "0.25 cup lemon juice"
        },
        {
            "3": "2 cloves garlic, minced"
        },
        {
            "4": "1 tablespoon dried minced onion"
        },
        {
            "5": "0.5 teaspoons flavor enhancer (such as Accent\u00ae)"
        },
        {
            "6": "0.5 teaspoons salt"
        },
        {
            "7": "1 teaspoon black pepper"
        },
        {
            "8": "1 teaspoon white sugar"
        },
        {
            "9": "0.5 teaspoon dry mustard"
        },
        {
            "10": "0.5 teaspoon dried rosemary"
        },
        {
            "11": "0.25 teaspoon ground ginger"
        },
        {
            "12": "1 (1 pound) flank steak"
        }
    ],
    "Directions": [
        {
            "1": "Mix oil, lemon juice, garlic, minced onion, flavor enhancer, salt, pepper, sugar, dry mustard, rosemary, and ginger together in a glass jar. Place flank steak in a zip-top bag and add marinade. Refrigerate for 4 hours."
        },
        {
            "2": "Preheat an outdoor grill for medium-high heat and lightly oil the grate."
        },
        {
            "3": "Remove flank steak from the marinade and shake off excess. Discard the remaining marinade."
        },
        {
            "4": "Cook on the preheated grill, turning once, until firm and slightly pink in the center, 5 to 8 minutes per side. Slice on a diagonal across the grain to serve."
        }
    ],
