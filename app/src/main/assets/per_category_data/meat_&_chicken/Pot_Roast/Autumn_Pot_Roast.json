{
    "title": "Autumn Pot Roast",
    "Category": "Pot Roast",
    "Ingredients": [
        {
            "1": "1 tablespoon salt"
        },
        {
            "2": "1 tablespoon ground black pepper"
        },
        {
            "3": "2 teaspoons dried parsley"
        },
        {
            "4": "2 teaspoons dried oregano"
        },
        {
            "5": "2 pounds beef pot roast"
        },
        {
            "6": "4  potatoes, cut into chunks"
        },
        {
            "7": "2 cups baby carrots"
        },
        {
            "8": "1  tomato, cut into chunks"
        },
        {
            "9": "0.5  yellow onion, cut into chunks"
        },
        {
            "10": "1 (6.5 ounce) can tomato sauce"
        },
        {
            "11": "0.5 cup water"
        }
    ],
    "Directions": [
        {
            "1": "Sprinkle salt, black pepper, parsley, and oregano evenly over beef pot roast. Place roast in a slow cooker; add potatoes, carrots, tomato, and onion atop the beef. Pour tomato sauce and water over the beef and vegetables."
        },
        {
            "2": "Cook on High for 4 hours."
        }
    ],
