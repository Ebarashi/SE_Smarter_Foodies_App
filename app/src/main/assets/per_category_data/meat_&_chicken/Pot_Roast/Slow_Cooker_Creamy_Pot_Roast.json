{
    "title": "Slow Cooker Creamy Pot Roast",
    "Category": "Pot Roast",
    "Ingredients": [
        {
            "1": "1 (5 pound) boneless beef chuck roast"
        },
        {
            "2": "1 salt and ground black pepper to taste"
        },
        {
            "3": "2 (10.75 ounce) cans  condensed golden mushroom soup"
        },
        {
            "4": "2 (8 ounce) cans sliced mushrooms, drained"
        },
        {
            "5": "1 (10.75 ounce) can  condensed cream of mushroom soup"
        },
        {
            "6": "1 (1 ounce) package dry onion soup mix"
        },
        {
            "7": "0.5 teaspoons minced garlic"
        }
    ],
    "Directions": [
        {
            "1": "Season roast all over with salt and black pepper and place roast in a slow cooker. Whisk golden mushroom soup, sliced mushrooms, cream of mushroom soup, onion soup mix, and garlic together in a bowl; pour over beef roast."
        },
        {
            "2": "Cook on Low for 8 to 10 hours."
        }
    ],
