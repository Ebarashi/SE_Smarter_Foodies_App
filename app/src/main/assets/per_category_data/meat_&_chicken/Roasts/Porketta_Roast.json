{
    "title": "Porketta Roast",
    "Category": "Roasts",
    "Ingredients": [
        {
            "1": "1 tablespoon dill seed"
        },
        {
            "2": "1 tablespoon fennel seed"
        },
        {
            "3": "1 teaspoon dried oregano"
        },
        {
            "4": "1 teaspoon lemon-pepper seasoning"
        },
        {
            "5": "0.25 teaspoon onion powder"
        },
        {
            "6": "0.25 teaspoon garlic powder"
        },
        {
            "7": "4 pounds boneless pork roast"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 325 degrees F (165 degrees C)."
        },
        {
            "2": "Mix dill seed, fennel seed, oregano, lemon-pepper seasoning, onion powder, and garlic powder together in a small bowl until well combined. Rub onto all surfaces of pork roast. Place roast into a 10x15-inch roasting pan."
        },
        {
            "3": "Bake in the preheated oven until pork is slightly pink in the center, 1 to 1 1/2 hours. An instant-read thermometer inserted into the center should read at least 145 degrees F (63 degrees C)."
        }
    ],
