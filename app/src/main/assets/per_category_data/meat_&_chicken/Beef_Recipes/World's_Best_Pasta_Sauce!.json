{
    "title": "World's Best Pasta Sauce!",
    "Category": "Beef Recipes",
    "Ingredients": [
        {
            "1": "1 pound sweet Italian sausage, sliced"
        },
        {
            "2": "0.75 pound lean ground beef"
        },
        {
            "3": "0.5 cup minced onion"
        },
        {
            "4": "2 cloves garlic, crushed"
        },
        {
            "5": "1 (28 ounce) can crushed tomatoes"
        },
        {
            "6": "2 (6.5 ounce) cans tomato sauce"
        },
        {
            "7": "2 (6 ounce) cans tomato paste"
        },
        {
            "8": "0.5 cup water"
        },
        {
            "9": "2 tablespoons white sugar"
        },
        {
            "10": "0.5 teaspoons dried basil"
        },
        {
            "11": "1 teaspoon Italian seasoning"
        },
        {
            "12": "0.5 teaspoon fennel seed"
        },
        {
            "13": "0.5 teaspoon salt"
        },
        {
            "14": "0.25 teaspoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Cook sausage, beef, onion, and garlic in a large pot or Dutch oven over medium heat until browned; drain fat."
        },
        {
            "2": "Stir in crushed tomatoes, tomato sauce, tomato paste, and water. Mix in sugar, basil, Italian seasoning, fennel seed, salt, and pepper."
        },
        {
            "3": "Cover and simmer, stirring occasionally, until cooked through, about 1 1/2 hours."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
