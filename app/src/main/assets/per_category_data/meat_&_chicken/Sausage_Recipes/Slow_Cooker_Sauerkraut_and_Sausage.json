{
    "title": "Slow Cooker Sauerkraut and Sausage",
    "Category": "Sausage Recipes",
    "Ingredients": [
        {
            "1": "1 (20 ounce) can sauerkraut"
        },
        {
            "2": "0.25 cup brown sugar"
        },
        {
            "3": "0.5 pounds bratwurst sausages"
        },
        {
            "4": "1 medium onion, sliced"
        },
        {
            "5": "0.5 cup water, or as needed (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Mix sauerkraut and brown sugar together in a medium bowl."
        },
        {
            "2": "Pour sauerkraut mixture into a slow cooker; arrange sausages and onion over top. Cook on High for 2 hours. Check for dryness, adding water as necessary."
        },
        {
            "3": "Reduce the heat to Low and cook for 2 more hours."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": " 4 hrs"
        },
