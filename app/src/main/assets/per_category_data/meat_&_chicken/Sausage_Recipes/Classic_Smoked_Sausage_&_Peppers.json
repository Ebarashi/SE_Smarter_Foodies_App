{
    "title": "Classic Smoked Sausage & Peppers",
    "Category": "Sausage Recipes",
    "Ingredients": [
        {
            "1": "1 (14 ounce) package Hillshire Farm\u00ae Smoked Sausage, diagonally cut into 1/4-inch slices"
        },
        {
            "2": "2 tablespoons olive oil"
        },
        {
            "3": "1 cup onion, cut into 1-inch pieces"
        },
        {
            "4": "2 cups red and green peppers, cut into 1-inch pieces"
        },
        {
            "5": "0.5 teaspoon dried oregano"
        },
        {
            "6": "0.5 cup grated Parmesan cheese (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Saute sausage in a large skillet over medium-high heat for 3 minutes, turning occasionally."
        },
        {
            "2": "Add olive oil, onion, peppers and oregano, cooking until tender, about 3 minutes."
        },
        {
            "3": "Remove from heat, stir in Parmesan cheese."
        }
    ],
