{
    "title": "Slow Cooker Ground Beef Barbecue",
    "Category": "Sloppy Joes",
    "Ingredients": [
        {
            "1": "3 pounds lean ground beef"
        },
        {
            "2": "1 large onion, chopped"
        },
        {
            "3": "2 cloves garlic, minced"
        },
        {
            "4": "5 stalks celery, finely chopped"
        },
        {
            "5": "0.5 teaspoons salt"
        },
        {
            "6": "0.5 teaspoon ground black pepper"
        },
        {
            "7": "1 tablespoon cider vinegar"
        },
        {
            "8": "2 tablespoons prepared mustard"
        },
        {
            "9": "0.25 cup firmly packed brown sugar"
        },
        {
            "10": "1.5 cups ketchup"
        }
    ],
    "Directions": [
        {
            "1": "Place ground beef in a large skillet over medium heat, and cook until brown, breaking up the meat as it cooks, about 15 minutes. Drain excess grease."
        },
        {
            "2": "Place the cooked meat, onion, garlic, celery, salt, black pepper, cider vinegar, mustard, brown sugar, and ketchup into a slow cooker, and stir to combine. Set the cooker on Low, and cook for 6 to 8 hours."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": " 6 hrs 15 mins"
        },
        {
            "Total Time": " 6 hrs 25 mins"
        },
