{
    "title": "Beef Gyro",
    "Category": "Gyros",
    "Ingredients": [
        {
            "1": "2 (8 ounce) containers plain yogurt"
        },
        {
            "2": "2  cucumbers - peeled, seeded, and diced"
        },
        {
            "3": "3 tablespoons olive oil, divided"
        },
        {
            "4": "1 tablespoon chopped fresh dill"
        },
        {
            "5": "0.5  lemon, juiced"
        },
        {
            "6": "3 cloves garlic, peeled"
        },
        {
            "7": "1 salt and ground black pepper to taste"
        },
        {
            "8": "1 (1 pound) beef top sirloin steak, cut into thin strips"
        },
        {
            "9": "5  pita bread rounds"
        },
        {
            "10": "1  tomato, chopped"
        },
        {
            "11": "0.5 small onion, thinly sliced"
        },
        {
            "12": "1 (4 ounce) package crumbled feta cheese"
        },
        {
            "13": "0.5 cup shredded lettuce"
        },
        {
            "14": "1 (2.25 ounce) can sliced ripe olives, drained"
        }
    ],
    "Directions": [
        {
            "1": "Blend yogurt, cucumbers, 2 tablespoons olive oil, dill, lemon juice, garlic, salt, and pepper in a blender until tzatziki sauce is evenly combined."
        },
        {
            "2": "Heat remaining 1 tablespoon olive oil in a large skillet over medium heat; cook and stir beef in the hot oil until no longer pink, 5 to 10 minutes."
        },
        {
            "3": "Layer half of each pita with steak, tomato, onion, feta cheese, lettuce, olives, and tzatziki sauce. Bring edge of each pita over the filling and secure with a toothpick."
        }
    ],
