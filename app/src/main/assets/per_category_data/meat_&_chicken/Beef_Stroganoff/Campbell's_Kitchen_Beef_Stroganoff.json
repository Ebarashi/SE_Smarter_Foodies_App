{
    "title": "Campbell's Kitchen Beef Stroganoff",
    "Category": "Beef Stroganoff",
    "Ingredients": [
        {
            "1": "1 pound boneless beef sirloin steak or beef top round steak"
        },
        {
            "2": "2 tablespoons vegetable oil"
        },
        {
            "3": "1 medium onion, chopped"
        },
        {
            "4": "1 (10.75 ounce) can  Campbell's\u00ae Condensed Cream of Mushroom Soup or Campbell's\u00ae Condensed 98% Fat Free Cream of Mushroom Soup"
        },
        {
            "5": "0.5 teaspoon paprika"
        },
        {
            "6": "0.5 cup sour cream or yogurt"
        },
        {
            "7": "4 cups hot cooked medium egg noodles"
        },
        {
            "8": "1 Chopped fresh parsley"
        }
    ],
    "Directions": [
        {
            "1": "Slice beef into very thin strips."
        },
        {
            "2": "Heat half the oil in skillet over medium-high heat. Cook beef until browned, stirring often. Set beef aside."
        },
        {
            "3": "Add remaining oil. Add onion and cook over medium heat until tender. Pour off fat."
        },
        {
            "4": "Add soup and paprika. Heat to a boil. Stir in sour cream and return beef to skillet. Heat through. Serve over noodles. Sprinkle with parsley."
        }
    ],
    "Details": [
        {
            "Cook Time": "25 mins"
        },
        {
            "Total Time": "25 mins"
        },
        {
            "Servings": "4 "
        },
        {
            "Yield": "4 servings"
        }
    ],
