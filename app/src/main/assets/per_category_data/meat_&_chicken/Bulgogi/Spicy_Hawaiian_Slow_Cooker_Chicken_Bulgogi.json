{
    "title": "Spicy Hawaiian Slow Cooker Chicken Bulgogi",
    "Category": "Bulgogi",
    "Ingredients": [
        {
            "1": "2 pounds skinless, boneless chicken breasts, cut into 1-inch pieces"
        },
        {
            "2": "1  onion, chopped"
        },
        {
            "3": "1 (20 ounce) can pineapple tidbits with juice"
        },
        {
            "4": "1 (10 ounce) bottle Korean bulgogi marinade"
        },
        {
            "5": "0.5 cup gochujang (Korean hot pepper paste), or to taste"
        },
        {
            "6": "1 (8 ounce) package shredded carrots"
        },
        {
            "7": "1 (8 ounce) package shredded broccoli stalks"
        },
        {
            "8": "0.5 bunch fresh cilantro, chopped"
        }
    ],
    "Directions": [
        {
            "1": "Place chicken and onion in a slow cooker. Stir in pineapple, bulgogi marinade, and gochujang."
        },
        {
            "2": "Cook on High for 3 hours."
        },
        {
            "3": "Mix shredded carrots and broccoli into the slow cooker and continue cooking for 1 hour."
        },
        {
            "4": "Stir in cilantro and cook 15 minutes more. Serve."
        }
    ],
