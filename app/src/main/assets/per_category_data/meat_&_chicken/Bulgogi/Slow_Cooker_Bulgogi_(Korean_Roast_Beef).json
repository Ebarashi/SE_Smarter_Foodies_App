            "1": "1 pound beef tenderloin"
        },
        {
            "2": "3 tablespoons water"
        },
        {
            "3": "2 tablespoons soy sauce"
        },
        {
            "4": "2 tablespoons white sugar"
        },
        {
            "5": "1  green onion, finely chopped"
        },
        {
            "6": "1 tablespoon sesame oil"
        },
        {
            "7": "2 cloves garlic, minced"
        },
        {
            "8": "0.5 teaspoon roasted sesame seeds, or to taste"
        },
        {
            "9": "1 ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine beef tenderloin, water, soy sauce, sugar, green onion, sesame oil, garlic, sesame seeds, and black pepper in a slow cooker. Cook on Low until meat is tender, 10 to 12 hours."
        }
