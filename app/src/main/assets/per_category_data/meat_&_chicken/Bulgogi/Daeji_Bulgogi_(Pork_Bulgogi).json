{
    "title": "Daeji Bulgogi (Pork Bulgogi)",
    "Category": "Bulgogi",
    "Ingredients": [
        {
            "1": "3 tablespoons soy sauce"
        },
        {
            "2": "2 tablespoons Korean barbeque sauce"
        },
        {
            "3": "2 tablespoons Korean hot pepper paste"
        },
        {
            "4": "1 tablespoon white sugar"
        },
        {
            "5": "1 tablespoon minced ginger root"
        },
        {
            "6": "2 teaspoons red pepper flakes"
        },
        {
            "7": "0.5 teaspoon sesame seeds"
        },
        {
            "8": "0.25 teaspoon ground black pepper"
        },
        {
            "9": "0.5 pounds boneless pork shoulder, thinly sliced"
        },
        {
            "10": "1  onion, sliced"
        }
    ],
    "Directions": [
        {
            "1": "Whisk soy sauce, barbeque sauce, hot pepper paste, sugar, ginger root, red pepper flakes, sesame seeds, and black pepper together in a small bowl."
        },
        {
            "2": "Place pork in a large bowl. Pour soy sauce mixture over pork; toss to coat. Marinate in the refrigerator, at least 3 hours and up to overnight."
        },
        {
            "3": "Preheat a large skillet or grill pan over medium-high heat. Add pork, discarding marinade. Cook, stirring frequently, until pork begins to brown, about 5 minutes. Stir in onion; cook until soft, about 10 minutes."
        }
    ],
