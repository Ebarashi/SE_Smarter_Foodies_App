{
    "title": "Hamburger Goulash",
    "Category": "Goulash",
    "Ingredients": [
        {
            "1": "1 pound lean ground beef"
        },
        {
            "2": "4  potatoes, peeled and cubed"
        },
        {
            "3": "0.25 cup sliced onion"
        },
        {
            "4": "0.5 cups frozen corn kernels"
        },
        {
            "5": "1 (15 ounce) can cut green beans, drained"
        },
        {
            "6": "1 cup elbow macaroni"
        },
        {
            "7": "1 teaspoon salt"
        },
        {
            "8": "1 teaspoon ground black pepper"
        },
        {
            "9": "2 (8 ounce) cans tomato sauce"
        },
        {
            "10": "2 teaspoons Worcestershire sauce (Optional)"
        },
        {
            "11": "1 teaspoon garlic salt"
        },
        {
            "12": "0.5 teaspoon dried basil"
        }
    ],
    "Directions": [
        {
            "1": "Place ground beef in a large, deep skillet. Cook over medium-high heat until evenly brown. Drain, crumble, and set aside."
        },
        {
            "2": "Add all ingredients (except noodles) to browned ground meat and simmer for 1 hour, then add noodles."
        }
    ],
