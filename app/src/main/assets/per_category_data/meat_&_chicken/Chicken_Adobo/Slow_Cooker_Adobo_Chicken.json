{
    "title": "Slow Cooker Adobo Chicken",
    "Category": "Chicken Adobo",
    "Ingredients": [
        {
            "1": "1 small sweet onion, sliced"
        },
        {
            "2": "8 cloves garlic, crushed"
        },
        {
            "3": "0.75 cup low sodium soy sauce"
        },
        {
            "4": "0.5 cup vinegar"
        },
        {
            "5": "1 (3 pound) whole chicken, cut into pieces"
        }
    ],
    "Directions": [
        {
            "1": "Place chicken in a slow cooker. In a small bowl mix the onion, garlic, soy sauce, and vinegar, and pour over the chicken. Cook on Low for 6 to 8 hours."
        }
