{
    "title": "Best Turkey Burgers",
    "Category": "Burgers",
    "Ingredients": [
        {
            "1": "3 pounds ground turkey"
        },
        {
            "2": "0.25 cup seasoned bread crumbs"
        },
        {
            "3": "0.25 cup finely diced onion"
        },
        {
            "4": "2  egg whites, lightly beaten"
        },
        {
            "5": "0.25 cup chopped fresh parsley"
        },
        {
            "6": "1 clove garlic, peeled and minced"
        },
        {
            "7": "1 teaspoon salt"
        },
        {
            "8": "0.25 teaspoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Mix ground turkey, seasoned bread crumbs, onion, egg whites, parsley, garlic, salt, and pepper together in a large bowl."
        },
        {
            "2": "Form into 12 patties."
        },
        {
            "3": "Cook the patties in a medium skillet over medium heat, turning once, to an internal temperature of 180 degrees F (85 degrees C)."
        }
    ],
    "Details": [
