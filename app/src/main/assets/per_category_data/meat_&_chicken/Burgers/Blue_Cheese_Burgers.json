{
    "title": "Blue Cheese Burgers",
    "Category": "Burgers",
    "Ingredients": [
        {
            "1": "3 pounds lean ground beef"
        },
        {
            "2": "0.5 cup minced fresh chives"
        },
        {
            "3": "4 ounces blue cheese, crumbled"
        },
        {
            "4": "0.5 teaspoons salt"
        },
        {
            "5": "1 teaspoon Worcestershire sauce"
        },
        {
            "6": "1 teaspoon coarsely ground black pepper"
        },
        {
            "7": "1 teaspoon dry mustard"
        },
        {
            "8": "0.25 teaspoon hot pepper sauce"
        },
        {
            "9": "12  hamburger buns, split"
        }
    ],
    "Directions": [
        {
            "1": "Mix together ground beef, chives, blue cheese, salt, Worcestershire sauce, pepper, mustard, and hot pepper sauce in a large bowl until well combined. Cover and refrigerate for 2 hours."
        },
        {
            "2": "Preheat an outdoor grill for high heat and lightly oil the grate."
        },
        {
            "3": "Meanwhile, form burger mixture into 12 patties."
        },
        {
            "4": "Cook patties on the preheated grill until an instant-read thermometer inserted into the center reads at least 160 degrees F (71 degrees C), about 5 minutes per side."
        },
        {
            "5": "Serve on hamburger buns."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
