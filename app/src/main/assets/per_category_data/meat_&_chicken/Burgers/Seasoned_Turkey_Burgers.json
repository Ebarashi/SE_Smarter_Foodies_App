{
    "title": "Seasoned Turkey Burgers",
    "Category": "Burgers",
    "Ingredients": [
        {
            "1": "0.5 pounds ground turkey"
        },
        {
            "2": "1 large egg, lightly beaten (Optional)"
        },
        {
            "3": "1 (1 ounce) package dry onion soup mix"
        },
        {
            "4": "0.5 tablespoons soy sauce"
        },
        {
            "5": "0.5 teaspoon ground black pepper"
        },
        {
            "6": "0.5 teaspoon garlic powder"
        },
        {
            "7": "1 cooking spray"
        },
        {
            "8": "6  hamburger buns, split"
        }
    ],
    "Directions": [
        {
            "1": "Mix turkey with egg, onion soup mix, soy sauce, pepper, and garlic powder in a large bowl. Refrigerate the mixture for about 10 minutes, then form into 6 patties."
        },
        {
            "2": "Preheat the grill for medium-high heat."
        },
        {
            "3": "Lightly oil the grill grate. Place patties on the grill. Cook, turning once, until well done, about 20 minutes. The inside of the burgers will look whitish in color when cooked through. Serve on buns."
        }
    ],
