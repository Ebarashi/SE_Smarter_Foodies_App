{
    "title": "Zucchini Patties",
    "Category": "Burgers",
    "Ingredients": [
        {
            "1": "2 cups grated zucchini"
        },
        {
            "2": "2  eggs, beaten"
        },
        {
            "3": "0.5 cup all-purpose flour"
        },
        {
            "4": "0.5 cup grated Parmesan cheese"
        },
        {
            "5": "0.5 cup shredded mozzarella cheese"
        },
        {
            "6": "0.25 cup chopped onion"
        },
        {
            "7": "1 salt to taste"
        },
        {
            "8": "2 tablespoons vegetable oil"
        }
    ],
    "Directions": [
        {
            "1": "Combine zucchini, eggs, flour, Parmesan cheese, mozzarella cheese, onion, and salt in a medium bowl. Stir well enough to distribute ingredients evenly."
        },
        {
            "2": "Heat oil in a skillet over medium-high heat."
        },
        {
            "3": "Working in batches, scoop tablespoonfuls of zucchini mixture into hot oil and fry until golden brown, about 2 minutes per side."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Cook Time": "10 mins"
        },
        {
            "Total Time": "30 mins"
        },
        {
            "Servings": "4 "
        },
        {
            "Yield": "12 patties"
        }
    ],
