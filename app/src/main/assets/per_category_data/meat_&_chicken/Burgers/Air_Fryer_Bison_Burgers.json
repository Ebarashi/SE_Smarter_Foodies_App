{
    "title": "Air Fryer Bison Burgers",
    "Category": "Burgers",
    "Ingredients": [
        {
            "1": "1 pound ground bison"
        },
        {
            "2": "0.5 cup finely chopped onion"
        },
        {
            "3": "1  egg, beaten"
        },
        {
            "4": "2 tablespoons dry bread crumbs"
        },
        {
            "5": "1 tablespoon Worcestershire sauce"
        },
        {
            "6": "0.5 teaspoon liquid smoke, or to taste"
        },
        {
            "7": "1 pinch salt and ground black pepper to taste"
        },
        {
            "8": "4  hamburger buns"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the air fryer to 380 degrees F (190 degrees C)."
        },
        {
            "2": "Combine ground bison, onion, egg, bread crumbs, Worcestershire sauce, liquid smoke, salt, and pepper in a bowl. Form into 4 equal-sized patties."
        },
        {
            "3": "Carefully place in the bottom of the preheated air fryer and set the timer for 8 minutes. Open air fryer, carefully turn the patties, and continue frying until juices run clear, 4 to 5 minutes more. Place each patty on a bun."
        }
    ],
