{
    "title": "Keto Chicken Crust Pizza",
    "Category": "Ground Chicken",
    "Ingredients": [
        {
            "1": "1 pound ground chicken"
        },
        {
            "2": "0.5 cup mozzarella cheese, shredded"
        },
        {
            "3": "0.25 cup freshly grated Parmesan cheese"
        },
        {
            "4": "3 cloves garlic, minced"
        },
        {
            "5": "1 teaspoon Italian seasoning"
        },
        {
            "6": "0.5 teaspoon salt"
        },
        {
            "7": "0.25 teaspoon pepper"
        },
        {
            "8": "1 tablespoon chopped fresh basil"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 400 degrees F (200 degrees C). Line a baking sheet with parchment paper."
        },
        {
            "2": "Mix chicken, mozzarella cheese, Parmesan cheese, garlic, Italian seasoning, salt, and pepper in a large bowl with your hands. Fold in chopped basil."
        },
        {
            "3": "Place chicken mixture between two pieces of parchment paper. Gently spread mixture into a circle or rectangle about 1/4-1/2-inch thick. Remove top parchment paper."
        },
        {
            "4": "Bake in the preheated oven until edges start to brown, 35 to 45 minutes. Top with your favorite toppings."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
