{
    "title": "Homemade Spaghetti Sauce with Ground Beef",
    "Category": "Ground Beef",
    "Ingredients": [
        {
            "1": "1 pound ground beef"
        },
        {
            "2": "1 medium onion, chopped"
        },
        {
            "3": "4 cloves garlic, minced"
        },
        {
            "4": "1 small green bell pepper, diced"
        },
        {
            "5": "1 (28 ounce) can diced tomatoes"
        },
        {
            "6": "1 (16 ounce) can tomato sauce"
        },
        {
            "7": "1 (6 ounce) can tomato paste"
        },
        {
            "8": "2 teaspoons dried oregano"
        },
        {
            "9": "2 teaspoons dried basil"
        },
        {
            "10": "1 teaspoon salt"
        },
        {
            "11": "0.5 teaspoon ground black pepper"
        }
    ],
    "Directions": [
        {
            "1": "Combine ground beef, onion, garlic, and green pepper in a large saucepan over medium-high heat. Cook and stir until meat is browned and crumbly and vegetables are tender, 5 to 7 minutes. Drain grease."
        },
        {
            "2": "Stir diced tomatoes, tomato sauce, and tomato paste into the pan. Season with oregano, basil, salt, and pepper. Simmer spaghetti sauce for 1 hour, stirring occasionally."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },
        {
            "Cook Time": " 1 hrs 5 mins"
        },
        {
            "Total Time": " 1 hrs 15 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "8 servings"
        }
