{
    "title": "Slow Cooker Buffalo Cauliflower Casserole",
    "Category": "Buffalo Chicken Wings",
    "Ingredients": [
        {
            "1": "1 head cauliflower, broken into large pieces"
        },
        {
            "2": "1 stalk celery, sliced"
        },
        {
            "3": "4 ounces cream cheese, cubed"
        },
        {
            "4": "0.5 cup Buffalo wing sauce"
        },
        {
            "5": "0.25 cup ranch salad dressing"
        },
        {
            "6": "0.5 cups shredded mozzarella cheese"
        }
    ],
    "Directions": [
        {
            "1": "Place cauliflower florets and celery in the bottom of a slow cooker in an even layer. Top with cream cheese."
        },
        {
            "2": "Whisk Buffalo wing sauce and ranch dressing together in a bowl. Stir in mozzarella cheese and pour mixture over the cauliflower. Cook on Low for 3 hours, stirring every hour."
        }
    ],
