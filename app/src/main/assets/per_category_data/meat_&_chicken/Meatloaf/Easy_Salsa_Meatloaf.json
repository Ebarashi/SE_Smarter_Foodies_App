{
    "title": "Easy Salsa Meatloaf",
    "Category": "Meatloaf",
    "Ingredients": [
        {
            "1": "1 pound ground beef sirloin"
        },
        {
            "2": "0.5 cup salsa"
        },
        {
            "3": "0.5 cup Italian seasoned bread crumbs"
        },
        {
            "4": "0.5 cup shredded Cheddar cheese"
        },
        {
            "5": "1  egg, lightly beaten"
        },
        {
            "6": "3 cloves garlic, minced"
        },
        {
            "7": "1 dried parsley to taste"
        },
        {
            "8": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Preheat oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "In a large bowl, mix the ground sirloin, salsa, bread crumbs, cheese, egg, and garlic. Season with parsley, salt, and pepper. Transfer to a 5x9 inch loaf pan."
        },
        {
            "3": "Bake 45 minutes in the preheated oven, or to a minimum internal temperature of 160 degrees F (72 degrees C)."
        }
    ],
