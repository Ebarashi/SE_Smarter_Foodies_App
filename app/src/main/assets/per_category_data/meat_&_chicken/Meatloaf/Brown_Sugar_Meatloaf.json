{
    "title": "Brown Sugar Meatloaf",
    "Category": "Meatloaf",
    "Ingredients": [
        {
            "1": "1 cooking spray"
        },
        {
            "2": "0.5 cup packed brown sugar"
        },
        {
            "3": "0.5 cup ketchup"
        },
        {
            "4": "0.5 pounds lean ground beef"
        },
        {
            "5": "2 large eggs"
        },
        {
            "6": "1 small onion, chopped"
        },
        {
            "7": "0.75 cup milk"
        },
        {
            "8": "0.75 cup finely crushed saltine cracker crumbs"
        },
        {
            "9": "0.5 teaspoons salt"
        },
        {
            "10": "0.25 teaspoon ground black pepper"
        },
        {
            "11": "0.25 teaspoon ground ginger"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C). Lightly grease a 5x9-inch loaf pan."
        },
        {
            "2": "Press brown sugar into the bottom of the prepared loaf pan and spread ketchup over top sugar."
        },
        {
            "3": "Mix all remaining ingredients thoroughly in a mixing bowl and shape into a loaf. Place on top of the ketchup and sugar crust."
        },
        {
            "4": "Bake in the preheated oven for 1 hour or until juices are clear."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Cook Time": " 1 hrs"
        },
