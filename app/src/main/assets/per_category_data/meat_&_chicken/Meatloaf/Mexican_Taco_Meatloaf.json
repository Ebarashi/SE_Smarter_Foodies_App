{
    "title": "Mexican Taco Meatloaf",
    "Category": "Meatloaf",
    "Ingredients": [
        {
            "1": "0.5 pounds lean ground beef"
        },
        {
            "2": "1 small onion, chopped"
        },
        {
            "3": "1 cup crushed tortilla chips"
        },
        {
            "4": "0.75 cup shredded pepper Jack cheese"
        },
        {
            "5": "1 (1 ounce) packet taco seasoning mix"
        },
        {
            "6": "2 large eggs, beaten"
        },
        {
            "7": "0.5 cup milk"
        },
        {
            "8": "0.25 cup mild red taco sauce, or more to taste"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Mix together beef, onion, tortilla chips, pepper Jack cheese, and taco seasoning in a large bowl until combined. Whisk together eggs, milk, and taco sauce in a medium bowl. Add to beef mixture and stir until well combined."
        },
        {
            "3": "Press mixture into a 9x5x3-inch loaf pan. Pour a strip of taco sauce down the center of loaf."
        },
        {
            "4": "Bake in the preheated oven until cooked through and browned on top, 45 to 60 minutes. An instant-read thermometer inserted into the center of loaf should read at least 160 degrees F (72 degrees C)."
        }
    ],
    "Details": [
        {
            "Prep Time": "15 mins"
        },
        {
            "Cook Time": "45 mins"
        },
        {
            "Total Time": " 1 hrs"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "1 9x5x3-inch loaf"
        }
