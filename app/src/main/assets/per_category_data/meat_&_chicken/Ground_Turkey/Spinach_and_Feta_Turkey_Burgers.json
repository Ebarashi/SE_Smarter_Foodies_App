{
    "title": "Spinach and Feta Turkey Burgers",
    "Category": "Ground Turkey",
    "Ingredients": [
        {
            "1": "1 cooking spray"
        },
        {
            "2": "2 pounds ground turkey"
        },
        {
            "3": "1 (10 ounce) box frozen chopped spinach, thawed and squeezed dry"
        },
        {
            "4": "4 ounces feta cheese"
        },
        {
            "5": "2 large eggs, beaten"
        },
        {
            "6": "2 cloves garlic, minced"
        }
    ],
    "Directions": [
        {
            "1": "Preheat an outdoor grill for medium-high heat and lightly oil the grate."
        },
        {
            "2": "Mix together turkey, spinach, feta, eggs, and garlic in a large bowl until well combined; form into 8 patties."
        },
        {
            "3": "Cook patties on the preheated grill until no longer pink in the center, 15 to 20 minutes. An instant-read thermometer inserted into the center of patties should read at least 165 degrees F (74 degrees C)."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },
        {
            "Cook Time": "15 mins"
        },
        {
            "Total Time": "35 mins"
        },
        {
            "Servings": "8 "
        },
        {
            "Yield": "8 turkey burgers"
        }
    ],
